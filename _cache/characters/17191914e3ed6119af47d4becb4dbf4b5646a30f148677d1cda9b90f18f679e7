             <button type=\\\"button\\\" class=\\\"modal-btn modal-btn-save\\\" id=\\\"save-profile\\\">ä¿å­˜</button>\\n                </div>\\n            </form>\\n        </div>\\n    </div>\\n\\n    <!-- å¤´åƒé€‰æ‹©å¼¹çª— -->\\n    <div class=\\\"modal-overlay\\\" id=\\\"avatar-select-modal\\\">\\n        <div class=\\\"modal\\\">\\n            <div class=\\\"modal-header\\\">\\n                <div class=\\\"modal-title\\\">é€‰æ‹©å¤´åƒ</div>\\n            </div>\\n            <div class=\\\"avatar-grid\\\">\\n                <!-- å¤´åƒé€‰é¡¹å°†ç”±JavaScriptåŠ¨æ€æ·»åŠ  -->\\n            </div>\\n            <div class=\\\"modal-footer\\\">\\n                <button type=\\\"button\\\" class=\\\"modal-btn modal-btn-cancel\\\" id=\\\"cancel-avatar\\\">å–æ¶ˆ</button>\\n                <button type=\\\"button\\\" class=\\\"modal-btn modal-btn-save\\\" id=\\\"save-avatar\\\">ç¡®å®š</button>\\n            </div>\\n        </div>\\n    </div>\\n\\n    <!-- åº•éƒ¨å¯¼èˆªæ  -->\\n    <div class=\\\"bottom-navbar\\\">\\n        <a href=\\\"javascript:void(0)\\\" class=\\\"nav-item\\\" data-page=\\\"home\\\">\\n            <i class=\\\"fas fa-home\\\"></i>\\n            <span>é¦–é¡µ</span>\\n        </a>\\n        <a href=\\\"javascript:void(0)\\\" class=\\\"nav-item\\\" data-page=\\\"explore\\\">\\n            <i class=\\\"fas fa-compass\\\"></i>\\n            <span>å‘ç°</span>\\n        </a>\\n        <div class=\\\"publish-btn-container\\\">\\n            <div class=\\\"publish-btn\\\" data-page=\\\"publish\\\">\\n                <i class=\\\"fas fa-plus\\\"></i>\\n            </div>\\n        </div>\\n        <a href=\\\"javascript:void(0)\\\" class=\\\"nav-item\\\" data-page=\\\"message\\\">\\n            <i class=\\\"fas fa-bell\\\"></i>\\n            <span>æ¶ˆæ¯</span>\\n        </a>\\n        <a href=\\\"javascript:void(0)\\\" class=\\\"nav-item active\\\" data-page=\\\"profile\\\">\\n            <i class=\\\"fas fa-user\\\"></i>\\n            <span>æˆ‘çš„</span>\\n        </a>\\n    </div>\\n\\n    <script>\\n        // å¤„ç†å¤´åƒURLï¼Œæå–è‹±æ–‡éƒ¨åˆ†å¹¶æ·»åŠ å‰ç¼€\\n        function processAvatarUrl(avatar) {\\n            // å¦‚æœæ²¡æœ‰æä¾›å¤´åƒæˆ–ä¸ºç©ºï¼Œè¿”å›é»˜è®¤å¤´åƒ\\n            if (!avatar || avatar.trim() === '') {\\n                return 'https://files.catbox.moe/kz8xdq.JPG';\\n            }\\n            \\n            try {\\n                // å¦‚æœå·²ç»æ˜¯å®Œæ•´URLï¼Œç›´æ¥è¿”å›\\n                if (avatar.startsWith('http')) {\\n                    return avatar;\\n                }\\n                \\n                // æå–æ–‡ä»¶åéƒ¨åˆ†ï¼ˆå»é™¤å¯èƒ½çš„ä¸­æ–‡å‰ç¼€ï¼‰\\n                let fileName = avatar;\\n                const match = avatar.match(/[a-zA-Z0-9_-]+\\\\.[a-zA-Z0-9]+$/);\\n                if (match) {\\n                    fileName = match[0];\\n                }\\n                \\n                // è¿”å›å®Œæ•´URL\\n                return `https://files.catbox.moe/${fileName}`;\\n            } catch (error) {\\n                console.error('å¤„ç†å¤´åƒURLæ—¶å‡ºé”™:', error);\\n                return 'https://files.catbox.moe/kz8xdq.JPG';\\n            }\\n        }\\n\\n        // é¡µé¢åŠ è½½å®Œæˆåæ‰§è¡Œåˆå§‹åŒ–\\n        document.addEventListener('DOMContentLoaded', function() {\\n            // åˆå§‹åŒ–æ ‡ç­¾åˆ‡æ¢\\n            initTabs();\\n            \\n            // è·å–å¹¶æ˜¾ç¤ºæ•°æ®\\n            loadUserData();\\n\\n            // åˆå§‹åŒ–åº•éƒ¨å¯¼èˆªæ \\n            initBottomNavbar();\\n\\n            // åˆå§‹åŒ–ç¼–è¾‘ä¸ªäººèµ„æ–™åŠŸèƒ½\\n            initProfileEdit();\\n\\n            // åˆå§‹åŒ–å¤´åƒé€‰æ‹©åŠŸèƒ½\\n            avatarSelector.init();\\n        });\\n        \\n        // åˆå§‹åŒ–æ ‡ç­¾åˆ‡æ¢åŠŸèƒ½\\n        function initTabs() {\\n            const tabs = document.querySelectorAll('.tab');\\n            const contentSections = document.querySelectorAll('.content-section');\\n            \\n            tabs.forEach(tab => {\\n                tab.addEventListener('click', () => {\\n                    // ç§»é™¤æ‰€æœ‰æ ‡ç­¾çš„activeç±»\\n                    tabs.forEach(t => t.classList.remove('active'));\\n                    \\n                    // ä¸ºå½“å‰ç‚¹å‡»çš„æ ‡ç­¾æ·»åŠ activeç±»\\n                    tab.classList.add('active');\\n                    \\n                    // è·å–ç›®æ ‡å†…å®¹åŒºåŸŸçš„ID\\n                    const targetId = tab.getAttribute('data-tab') + '-section';\\n                    \\n                    // éšè—æ‰€æœ‰å†…å®¹åŒºåŸŸ\\n                    contentSections.forEach(section => {\\n                        section.classList.remove('active');\\n                    });\\n                    \\n                    // æ˜¾ç¤ºç›®æ ‡å†…å®¹åŒºåŸŸ\\n                    document.getElementById(targetId).classList.add('active');\\n                });\\n            });\\n        }\\n        \\n        // åˆå§‹åŒ–åº•éƒ¨å¯¼èˆªæ \\n        function initBottomNavbar() {\\n            const navItems = document.querySelectorAll('.bottom-navbar .nav-item, .publish-btn');\\n            \\n            navItems.forEach(item => {\\n                item.addEventListener('click', function() {\\n                    const page = this.getAttribute('data-page');\\n                    \\n                    // æ¨¡æ‹Ÿé¡µé¢è·³è½¬\\n                    switch(page) {\\n                        case 'home':\\n                            viewHome();\\n                            break;\\n                        case 'explore':\\n                            viewDiscovery();\\n                            break;\\n                        case 'publish':\\n                            // è°ƒç”¨å‘å¸–å‡½æ•°\\n                            createPost();\\n                            break;\\n                        case 'message':\\n                            viewMessages();\\n                            break;\\n                        case 'profile':\\n                            // å·²ç»åœ¨ä¸ªäººé¡µï¼Œä¸åšæ“ä½œ\\n                            break;\\n                    }\\n                });\\n            });\\n\\n            // ç»‘å®šæœç´¢æ¡†äº‹ä»¶\\n            const searchInput = document.querySelector('.search-bar input');\\n            searchInput.addEventListener('keypress', async function(e) {\\n                if (e.key === 'Enter') {\\n                    e.preventDefault();\\n                    const searchText = this.value.trim();\\n                    if (searchText) {\\n                        if (typeof triggerSlash === 'function') {\\n                            await triggerSlash(`/send ã€è®ºå›æœç´¢ã€‘æŸ¥çœ‹\\\"${searchText}\\\"çš„æœç´¢ç»“æœ|/trigger`);\\n                        } else {\\n                            console.log('éSTç¯å¢ƒï¼Œæœç´¢è¯:', searchText);\\n                        }\\n                    }\\n                }\\n            });\\n\\n            // ä¸ºæœç´¢å›¾æ ‡æ·»åŠ ç‚¹å‡»äº‹ä»¶\\n            const searchIcon = document.querySelector('.search-bar i');\\n            searchIcon.addEventListener('click', async function() {\\n                const searchText = searchInput.value.trim();\\n                if (searchText) {\\n                    if (typeof triggerSlash === 'function') {\\n                        await triggerSlash(`/send ã€è®ºå›æœç´¢ã€‘æŸ¥çœ‹\\\"${searchText}\\\"çš„æœç´¢ç»“æœ|/trigger`);\\n                    } else {\\n                        console.log('éSTç¯å¢ƒï¼Œæœç´¢è¯:', searchText);\\n                    }\\n                }\\n            });\\n        }\\n\\n        // åˆå§‹åŒ–ä¸ªäººèµ„æ–™ç¼–è¾‘åŠŸèƒ½\\n        function initProfileEdit() {\\n            const editProfileBtn = document.getElementById('edit-profile-btn');\\n            const editAvatarBtn = document.getElementById('edit-avatar-btn');\\n            const modalOverlay = document.getElementById('edit-profile-modal');\\n            const closeModalBtn = document.getElementById('close-modal');\\n            const cancelBtn = document.getElementById('cancel-e