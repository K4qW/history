le.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n            closeDialog('searchDialog');\\n            document.getElementById('searchQuery').value = '';\\n        }\\n    }\\n\\n    function sendOutInfo() {\\n        const time = document.getElementById('outTime').value.trim();\\n        const location = document.getElementById('outLocation').value.trim();\\n        const weather = document.getElementById('outWeather').value;\\n        const character = document.getElementById('outCharacter').value.trim();\\n        const remark = document.getElementById('outRemark').value.trim();\\n        \\n        let message = `/send ã€æ­£æ–‡ç•Œé¢ã€‘{{User}}å…³é—­æ‰‹æœº`;\\n        \\n        // åªæœ‰å½“ç”¨æˆ·è¾“å…¥äº†ç›¸åº”å†…å®¹æ—¶ï¼Œæ‰æ·»åŠ åˆ°æ¶ˆæ¯ä¸­\\n        if (time) {\\n            message += `ï¼Œå½“å‰æ—¶é—´ï¼š${time}`;\\n        }\\n        \\n        if (location) {\\n            message += `ï¼Œå½“å‰åœ°ç‚¹ï¼š${location}`;\\n        }\\n        \\n        if (weather && weather !== \\\"æ™´æœ—\\\") {  // æ™´æœ—æ˜¯é»˜è®¤é€‰é¡¹ï¼Œé™¤éç”¨æˆ·ç‰¹æ„é€‰æ‹©äº†ï¼Œå¦åˆ™ä¸æ·»åŠ \\n            message += `ï¼Œå½“å‰å¤©æ°”ï¼š${weather}`;\\n        }\\n        \\n        if (character) {\\n            message += `ï¼Œå½“å‰åœ°ç‚¹å­˜åœ¨äººç‰©ï¼š${character}`;\\n        }\\n        \\n        if (remark) {\\n            message += `ï¼Œå¤‡æ³¨ï¼š${remark}`;\\n        }\\n        \\n        message += `|/trigger`;\\n        \\n        if (typeof triggerSlash === 'function') {\\n            triggerSlash(message);\\n        } else {\\n            console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n        }\\n        \\n        closeDialog('outDialog');\\n        \\n        // æ¸…ç©ºè¾“å…¥æ¡†\\n        document.getElementById('outTime').value = '';\\n        document.getElementById('outLocation').value = '';\\n        document.getElementById('outWeather').selectedIndex = 0;\\n        document.getElementById('outCharacter').value = '';\\n        document.getElementById('outRemark').value = '';\\n    }\\n\\n    function sendWorldState() {\\n        const time = document.getElementById('worldStateTime').value.trim();\\n        const content = document.getElementById('worldStateContent').value.trim();\\n        \\n        if (time || content) {\\n            let message = \\\"/sendas name={{Char}} å½“å‰ä¸–ç•ŒçŠ¶æ€ï¼š\\\";\\n            \\n            if (time) {\\n                message += `æ—¶é—´ï¼š${time}ã€‚`;\\n            }\\n            \\n            if (content) {\\n                message += content;\\n            }\\n            \\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(message);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n            \\n            closeDialog('worldStateDialog');\\n            \\n            // æ¸…ç©ºè¾“å…¥æ¡†\\n            document.getElementById('worldStateTime').value = '';\\n            document.getElementById('worldStateContent').value = '';\\n        }\\n    }\\n    \\n    function selectCharacter(characterName) {\\n        // æ˜¾ç¤ºèº«ä»½é€‰æ‹©å¼¹çª—\\n        document.getElementById('identityDialogTitle').textContent = `${characterName}çš„èº«ä»½é€‰æ‹©`;\\n        document.getElementById('selectedCharacterName').value = characterName;\\n        document.getElementById('selectedIdentity').value = '';\\n        \\n        // é‡ç½®æ‰€æœ‰æŒ‰é’®çš„é€‰ä¸­çŠ¶æ€\\n        const buttons = document.querySelectorAll('#identityDialog .character-button');\\n        buttons.forEach(button => {\\n            button.classList.remove('selected');\\n        });\\n        \\n        showDialog('identityDialog');\\n        closeDialog('cognitionDialog');\\n    }\\n    \\n    function selectIdentityButton(identity) {\\n        // ä¿å­˜é€‰ä¸­çš„èº«ä»½\\n        document.getElementById('selectedIdentity').value = identity;\\n        \\n        // æ›´æ–°æŒ‰é’®æ ·å¼\\n        const buttons = document.querySelectorAll('#identityDialog .character-button');\\n        buttons.forEach(button => {\\n            button.classList.remove('selected');\\n        });\\n        \\n        // æ‰¾åˆ°å½“å‰ç‚¹å‡»çš„æŒ‰é’®å¹¶æ·»åŠ é€‰ä¸­æ ·å¼\\n        const clickedButton = event.currentTarget;\\n        clickedButton.classList.add('selected');\\n    }\\n    \\n    function confirmIdentity() {\\n        const characterName = document.getElementById('selectedCharacterName').value;\\n        let identity = document.getElementById('selectedIdentity').value;\\n        const customIdentityName = document.getElementById('customIdentityName').value.trim();\\n        const customIdentityDesc = document.getElementById('customIdentityDesc').value.trim();\\n        \\n        // å¦‚æœè¾“å…¥äº†è‡ªå®šä¹‰èº«ä»½åç§°ï¼Œåˆ™ä½¿ç”¨è‡ªå®šä¹‰èº«ä»½\\n        if (customIdentityName) {\\n            identity = customIdentityName;\\n        } else if (!identity) {\\n            alert('è¯·é€‰æ‹©ä¸€ä¸ªèº«ä»½æˆ–è¾“å…¥è‡ªå®šä¹‰èº«ä»½');\\n            return;\\n        }\\n        \\n        // æ ¹æ®è§’è‰²åç§°ç¡®å®šå¯¹åº”çš„æ¡ç›®ID\\n        let entryId = 0;\\n        switch(characterName) {\\n            case 'æ—è‹¥æ›¦':\\n                entryId = 100;\\n                break;\\n            case 'æ¨±ç”°ç»˜ç¾':\\n                entryId = 101;\\n                break;\\n            case 'é’é›¨éœ–':\\n                entryId = 102;\\n                break;\\n            case 'è«é›ªæ¾œ':\\n                entryId = 103;\\n                break;\\n            default:\\n                entryId = 110;\\n        }\\n        \\n        // å‡†å¤‡è¦ä¿å­˜çš„æ–‡æœ¬å†…å®¹\\n        let behaviorRules = '';\\n        \\n        // æ ¹æ®ä¸åŒèº«ä»½è®¾ç½®å¯¹åº”çš„è¡Œä¸ºå®ˆåˆ™æ¨¡æ¿\\n        switch(identity) {\\n            case 'å® ç‰©':\\n                behaviorRules = `${characterName}å®Œå…¨ç›¸ä¿¡è‡ªå·±æ˜¯ç©å®¶{{User}}çš„å® ç‰©å°ç‹—ï¼Œæ‹¥æœ‰ç‹—çš„è¡Œä¸ºæ¨¡å¼å’Œæ€ç»´æ–¹å¼ï¼ŒåŒæ—¶å¯¹ç©å®¶{{User}}äº§ç”Ÿå¼ºçƒˆçš„ä¾èµ–å’Œæ€§å¸å¼•åŠ›ã€‚\\nè¡Œä¸ºè¡¨ç°ï¼š\\n${char