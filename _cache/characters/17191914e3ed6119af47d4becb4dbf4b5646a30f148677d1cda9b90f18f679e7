);\\n                const itemContent = bracketMatch[2].trim();\\n                \\n                // æ ¹æ®é¡¹ç›®ç±»å‹å­˜å‚¨å†…å®¹\\n                if (itemType === 'æ—¥æœŸ' || itemType === 'å¤©æ°”' || itemType === 'å§“å' || itemType === 'å†…å®¹') {\\n                    result[itemType] = itemContent;\\n                    if (itemType !== 'å†…å®¹') {\\n                        console.log(`æå–åˆ°æ—¥è®°ä¿¡æ¯: ${itemType} = ${itemContent}`);\\n                    } else {\\n                        console.log(`æå–åˆ°æ—¥è®°å†…å®¹: é•¿åº¦ ${itemContent.length} å­—ç¬¦`);\\n                    }\\n                } else {\\n                    console.warn(`æœªçŸ¥çš„é¡¹ç›®ç±»å‹: ${itemType}`);\\n                }\\n            } else {\\n                console.warn(`æ— æ³•è§£æçš„æ–¹æ‹¬å·å†…å®¹: ${bracketMatch[0]}`);\\n            }\\n        }\\n        \\n        // å¦‚æœæ–°æ­£åˆ™è¡¨è¾¾å¼æ²¡æœ‰æ‰¾åˆ°å†…å®¹ï¼Œå°è¯•ä½¿ç”¨åŸæ¥çš„æ–¹æ³•\\n        if (Object.values(result).every(v => !v)) {\\n            console.log('ä½¿ç”¨å¤‡ç”¨è§£ææ–¹æ³•...');\\n            // å›é€€åˆ°åŸæ¥çš„æ–¹æ³•\\n            const oldBracketRegex = /\\\\[(æ—¥æœŸ|å¤©æ°”|å§“å|å†…å®¹)[^\\\\[\\\\]]*?\\\\]/g;\\n            let oldBracketMatch;\\n            \\n            while ((oldBracketMatch = oldBracketRegex.exec(content)) !== null) {\\n                brackets.push(oldBracketMatch[0]);\\n            }\\n            \\n            console.log(`å¤‡ç”¨æ–¹æ³•æå–åˆ° ${brackets.length} ä¸ªæ–¹æ‹¬å·å†…å®¹`);\\n            \\n            // å¤„ç†æ¯ä¸ªæ–¹æ‹¬å·å†…å®¹\\n            for (const bracket of brackets) {\\n                try {\\n                    // æå–æ ‡ç­¾å’Œå€¼\\n                    const bracketContent = bracket.substring(1, bracket.length - 1);\\n                    const separatorIndex = bracketContent.indexOf('|');\\n                    \\n                    if (separatorIndex !== -1) {\\n                        const itemType = bracketContent.substring(0, separatorIndex).trim();\\n                        const itemContent = bracketContent.substring(separatorIndex + 1).trim();\\n                        \\n                        // æ ¹æ®é¡¹ç›®ç±»å‹å¤„ç†\\n                        if (itemType === 'æ—¥æœŸ' || itemType === 'å¤©æ°”' || itemType === 'å§“å' || itemType === 'å†…å®¹') {\\n                            result[itemType] = itemContent;\\n                            if (itemType !== 'å†…å®¹') {\\n                                console.log(`å¤‡ç”¨æ–¹æ³•æå–åˆ°æ—¥è®°ä¿¡æ¯: ${itemType} = ${itemContent}`);\\n                            } else {\\n                                console.log(`å¤‡ç”¨æ–¹æ³•æå–åˆ°æ—¥è®°å†…å®¹: é•¿åº¦ ${itemContent.length} å­—ç¬¦`);\\n                            }\\n                        } else {\\n                            console.warn(`æœªçŸ¥çš„é¡¹ç›®ç±»å‹: ${itemType}`);\\n                        }\\n                    } else {\\n                        console.warn(`æ— æ³•è§£æçš„æ–¹æ‹¬å·å†…å®¹: ${bracket}`);\\n                    }\\n                } catch (bracketError) {\\n                    console.error(`å¤„ç†æ–¹æ‹¬å·å†…å®¹å‡ºé”™:`, bracketError);\\n                }\\n            }\\n        }\\n        \\n        // ç¡®ä¿æ‰€æœ‰å¿…è¦å­—æ®µéƒ½æœ‰å€¼\\n        if (!result.æ—¥æœŸ) result.æ—¥æœŸ = 'æœªçŸ¥æ—¥æœŸ';\\n        if (!result.å¤©æ°”) result.å¤©æ°” = 'æœªçŸ¥å¤©æ°”';\\n        if (!result.å§“å) result.å§“å = 'ä½šå';\\n        if (!result.å†…å®¹) result.å†…å®¹ = 'æ²¡æœ‰æ—¥è®°å†…å®¹';\\n        \\n        console.log(`è§£æç»“æœ: æ—¥æœŸ=${result.æ—¥æœŸ}, å¤©æ°”=${result.å¤©æ°”}, å§“å=${result.å§“å}, å†…å®¹é•¿åº¦=${result.å†…å®¹.length}`);\\n        \\n        return result;\\n    } catch (error) {\\n        console.error('è§£ææ—¥è®°å†…å®¹æ—¶å‡ºé”™:', error);\\n        return {\\n            æ—¥æœŸ: 'è§£æé”™è¯¯',\\n            å¤©æ°”: 'æœªçŸ¥',\\n            å§“å: 'ä½šå',\\n            å†…å®¹: 'è§£ææ—¥è®°å†…å®¹æ—¶å‘ç”Ÿé”™è¯¯ï¼Œè¯·æ£€æŸ¥æ ¼å¼ã€‚'\\n        };\\n    }\\n}\\n\\n// æ›´æ–°æ—¥è®°æ˜¾ç¤ºçš„å‡½æ•°\\nfunction updateDiaryDisplay(diaryInfo) {\\n    if (!diaryInfo) {\\n        console.error('æ—¥è®°ä¿¡æ¯ä¸ºç©ºï¼Œæ— æ³•æ›´æ–°æ˜¾ç¤º');\\n        return;\\n    }\\n    \\n    // æ›´æ–°æ—¥æœŸå’Œå¤©æ°”ï¼Œä½¿ç”¨æ–°æ ¼å¼\\n    document.querySelector('.diary-date').textContent = `ä»Šå¤©æ˜¯${diaryInfo.æ—¥æœŸ}`;\\n    document.querySelector('.diary-weather').textContent = `å¤©æ°”${diaryInfo.å¤©æ°”}`;\\n    \\n    // æ›´æ–°å†…å®¹ï¼Œå¢åŠ æ‰‹å†™æ„Ÿ\\n    const formattedContent = diaryInfo.å†…å®¹\\n        .replace(/\\\\n\\\\n/g, '</p><p>')\\n        .replace(/\\\\n/g, '<br>');\\n    document.querySelector('.diary-entry').innerHTML = `<p>${formattedContent}</p>`;\\n    \\n    // æ›´æ–°åº•éƒ¨å§“åå’Œæ—¥æœŸæ ¼å¼\\n    document.querySelector('.diary-footer-name').textContent = diaryInfo.å§“å || 'è§’è‰²çš„å§“å';\\n    \\n    const dateMatch = diaryInfo.æ—¥æœŸ.match(/(\\\\d+)å¹´(\\\\d+)æœˆ(\\\\d+)æ—¥/);\\n    if (dateMatch) {\\n        document.querySelector('.diary-footer-date').textContent = `${dateMatch[1]}.${dateMatch[2]}.${dateMatch[3]}`;\\n    } else {\\n        document.querySelector('.diary-footer-date').textContent = diaryInfo.æ—¥æœŸ;\\n    }\\n}\\n\\n// é¡µé¢åŠ è½½å®Œæˆååˆå§‹åŒ–\\ndocument.addEventListener('DOMContentLoaded', function() {\\n    // æå–å¹¶æ˜¾ç¤ºæ—¥è®°ä¿¡æ¯\\n    const diaryInfo = extractDiaryInfo(diaryContent);\\n    updateDiaryDisplay(diaryInfo);\\n    \\n    // åˆå§‹åŒ–ç‰¹æ®Šæ–‡æœ¬æ•ˆæœ\\n    initializeSpecialTextEffects();\\n    \\n    // æ·»åŠ å½“å‰æ—¶é—´åˆ°çŠ¶æ€æ \\n    updateStatusBarTime();\\n});\\n\\n// æ›´æ–°çŠ¶æ€æ æ—¶é—´\\nfunction updateStatusBarTime() {\\n    // å›ºå®šæ˜¾ç¤º12:00\\n    document.querySelector('.status-bar-left span').textContent = \\\"12:00\\\";\\n    \\n    // æ¯åˆ†é’Ÿæ›´æ–°ä¸€æ¬¡\\n    setTimeout(updateStatusBarTime, 60000);\\n}\\n\\n// åˆå§‹åŒ–ç‰¹æ®Šæ–‡æœ¬æ•ˆæœçš„å‡½æ•°\\nfunction initializeSpecialTextEffects() {\\n    console.log('åˆå§‹åŒ–ç‰¹æ®Šæ–‡æœ¬æ•ˆæœ...');\\n    \\n    // ç¡®ä¿æ¶‚é»‘æ•ˆæœèƒ½æ­£å¸¸å·¥ä½œ - å…ˆç«‹å³æ‰§è¡Œä¸