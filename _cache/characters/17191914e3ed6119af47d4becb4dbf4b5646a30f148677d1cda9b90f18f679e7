     }\\n\\n        .input-field {\\n            flex: 1;\\n            border: 1px solid var(--border-color);\\n            border-radius: 20px;\\n            padding: 10px 14px;\\n            font-size: 15px;\\n            background-color: var(--bg-color);\\n            margin: 0 8px;\\n            resize: none;\\n            height: 40px;\\n            max-height: 100px;\\n            overflow-y: auto;\\n            line-height: 1.4;\\n            outline: none;\\n            transition: all 0.3s ease;\\n            color: var(--text-primary);\\n        }\\n\\n        .input-field:focus {\\n            border-color: var(--primary-color);\\n        }\\n\\n        .input-field::placeholder {\\n            color: var(--text-light);\\n        }\\n\\n        .send-btn {\\n            background: none;\\n            border: none;\\n            width: 40px;\\n            height: 40px;\\n            border-radius: 50%;\\n            background-color: var(--primary-color);\\n            color: white;\\n            font-size: 16px;\\n            display: flex;\\n            align-items: center;\\n            justify-content: center;\\n            cursor: pointer;\\n            transition: all 0.3s ease;\\n        }\\n\\n        .send-btn:hover, .send-btn:active {\\n            opacity: 0.9;\\n            transform: scale(1.05);\\n        }\\n\\n        /* æ»šåŠ¨æ¡æ ·å¼ */\\n        .chat-messages::-webkit-scrollbar {\\n            width: 4px;\\n        }\\n\\n        .chat-messages::-webkit-scrollbar-thumb {\\n            background-color: var(--text-light);\\n            border-radius: 4px;\\n        }\\n\\n        .chat-messages::-webkit-scrollbar-track {\\n            background-color: transparent;\\n        }\\n\\n        /* æ¶ˆæ¯çŠ¶æ€æŒ‡ç¤ºå™¨ */\\n        .typing-indicator {\\n            display: flex;\\n            align-items: center;\\n            justify-content: center;\\n            padding: 5px 0;\\n        }\\n        \\n        .typing-dot {\\n            display: inline-block;\\n            width: 6px;\\n            height: 6px;\\n            margin: 0 2px;\\n            background-color: var(--text-light);\\n            border-radius: 50%;\\n            animation: typing-dot 1.4s infinite ease-in-out both;\\n        }\\n        \\n        .typing-dot:nth-child(1) {\\n            animation-delay: -0.32s;\\n        }\\n        \\n        .typing-dot:nth-child(2) {\\n            animation-delay: -0.16s;\\n        }\\n        \\n        @keyframes typing-dot {\\n            0%, 80%, 100% { transform: scale(0.7); opacity: 0.5; }\\n            40% { transform: scale(1); opacity: 1; }\\n        }\\n        \\n        /* å“åº”å¼è®¾è®¡ */\\n        @media (max-width: 768px) {\\n            .message {\\n                max-width: 98%;\\n            }\\n            \\n            .message-wrapper {\\n                max-width: 75%;\\n                padding: 10px 12px;\\n            }\\n            \\n            .chat-messages {\\n                padding: 16px 4px;\\n            }\\n            \\n            .sent {\\n                padding-right: 2px;\\n                padding-left: 6px;\\n            }\\n            \\n            .received {\\n                padding-left: 2px;\\n                padding-right: 6px;\\n            }\\n            \\n            .message-avatar {\\n                margin: 0 4px;\\n            }\\n            \\n            .sent .message-avatar {\\n                margin-left: 4px;\\n                margin-right: 2px;\\n            }\\n            \\n            .received .message-avatar {\\n                margin-right: 4px;\\n                margin-left: 2px;\\n            }\\n            \\n            .message-content {\\n                font-size: 14px;\\n            }\\n        }\\n    </style>\\n</head>\\n<body>\\n    <div class=\\\"chat-container\\\">\\n        <div class=\\\"header\\\">\\n            <div class=\\\"back-btn\\\"><i class=\\\"fas fa-chevron-left\\\"></i></div>\\n            <div class=\\\"avatar-container\\\">\\n                <img src=\\\"https://files.catbox.moe/kz8xdq.JPG\\\" alt=\\\"å¯¹æ–¹å¤´åƒ\\\" class=\\\"avatar\\\">\\n            </div>\\n            <div class=\\\"header-text\\\">\\n                <h2>è®¾è®¡çˆ±å¥½è€…</h2>\\n                <p>æœ€è¿‘æ´»è·ƒ</p>\\n            </div>\\n            <button class=\\\"profile-btn\\\">ä¸ªäººä¸»é¡µ</button>\\n        </div>\\n\\n        <div class=\\\"chat-messages\\\">\\n            <!-- èŠå¤©æ¶ˆæ¯ç”±JavaScriptåŠ¨æ€ç”Ÿæˆ -->\\n        </div>\\n        \\n        <div class=\\\"chat-input\\\">\\n            <button class=\\\"emoji-btn\\\"><i class=\\\"far fa-grin\\\"></i></button>\\n            <div class=\\\"emoji-picker-container\\\">\\n                <emoji-picker></emoji-picker>\\n            </div>\\n            <textarea class=\\\"input-field\\\" placeholder=\\\"å‘é€æ¶ˆæ¯...\\\"></textarea>\\n            <button class=\\\"send-btn\\\"><i class=\\\"fas fa-paper-plane\\\"></i></button>\\n        </div>\\n    </div>\\n\\n    <script>\\n        // åˆå§‹èŠå¤©æ•°æ®\\n        let gameText = `\\n<private_chat>$1</private_chat>`;\\n\\n        // å¤„ç†å¤´åƒURLï¼Œæå–è‹±æ–‡éƒ¨åˆ†å¹¶æ·»åŠ å‰ç¼€\\n        function processAvatarUrl(avatar) {\\n            // å¦‚æœæ²¡æœ‰æä¾›å¤´åƒæˆ–ä¸ºç©ºï¼Œè¿”å›é»˜è®¤å¤´åƒ\\n            if (!avatar || avatar.trim() === '') {\\n                return 'https://files.catbox.moe/kz8xdq.JPG';\\n            }\\n            \\n            try {\\n                // å¦‚æœå·²ç»æ˜¯å®Œæ•´URLï¼Œç›´æ¥è¿”å›\\n                if (avatar.startsWith('http')) {\\n                    return avatar;\\n                }\\n                \\n                // æå–æ–‡ä»¶åéƒ¨åˆ†ï¼ˆå»é™¤å¯èƒ½çš„ä¸­æ–‡å‰ç¼€ï¼‰\\n                let fileName = avatar;\\n                const match = avatar.match(/[a-zA-Z0-9_-]+\\\\.[a-zA-Z0-9]+$/);\\n                if (match) {\\n                    fileName = match[0];\\n                }\\n                \\n                // è¿”å›å®Œæ•´URL\\n                return `https://files.catbox.moe/${fileName}`;\\n            } catch (error) {\\n                console.error('å¤„ç†å¤´åƒURLæ—¶å‡ºé”™:', error);\\n                return 'https://files.catbox.moe/kz8xdq.JPG';\\n            }\\n        }\\n        \\n        // è·å–ç”¨æˆ·å¤´åƒURL\\n        function getUserAvatar() {\\n            // é»˜è®¤å¤´åƒ\\n            let avatarUrl = 'https://files.catbox.moe/kz8xdq.JPG';\\n            \\n            // å°è¯•ä»å·²åŠ è½½çš„å˜é‡ä¸­è·å–\\n            if (window.userAvatarUrl) {\\n                return window.userAvatarUrl;\\n            }\\n            \\n            return avatarUrl;\\n        }\\n\\n        // æ•°æ®æå–å™¨\\n        const DataExtractor = {\\n            // è§£ægameText\\n            parseGameText(gameText) {\\n                // ç›´æ¥è§£æç§èŠæ•°æ®\\n                return this.parseChatData(gameText);\\n            },\\n            \\n            // è§£æç§èŠæ•°æ®\\n            parseChatData(content) {\\n                try {\\n                    // æå– <private_chat> æ ‡ç­¾å†…çš„å†…å®¹\\n                    const chatMatch = content.match(/<private_chat>([\\\\s\\\\S]*?)<\\\\/private_chat>/);\\n                    if (!chatMatch) return null;\\n                    \\n                    const chatContent = chatMatch[1].trim();\\n                    const lines = chatContent.split('\\\\n');\\n                    \\n                    // åˆå§‹åŒ–æ•°æ®ç»“æ„\\n                    const chatData = {\\n                        date: '',\\n                        time: '',\\n                        contactInfo: {\\n                            avatar: '',\\n                            username: '',\\n                            realName: ''\\n                        },\\n                        location: '',\\n                        status: '',\\n                        thoughts: '',\\n                        messages: []\\n                    };\\n                    \\n                    // è§£ææ¯è¡Œæ•°æ®\\n                    for (const line of lines) {\\n                        const trimmedLine = line.trim();\\n                        if (!trimmedLine || trimmedLine.startsWith('<!--')) continue;\\n                        \\n                        // æ—¶é—´ä¿¡æ¯\\n                        if (trimmedLine.startsWith('[æ—¶é—´|')) {\\n                            const timeMatch = trimmedLine.match(/\\\\[æ—¶é—´\\\\|(.*?)\\\\|(.*?)\\\\]/);\\n                            if (timeMatch) {\\n                                chatData.date = timeMatch[1] || '';\\n                                chatData.time = timeMatch[2] || '';\\n                            }\\n                            continue;\\n                        }\\n                        \\n                        // å¯¹æ–¹ä¿¡æ¯\\n                        if (trimmedLine.startsWith('[å¯¹æ–¹ä¿¡æ¯|')) {\\n                            const infoMatch = trimmedLine.match(/\\\\[å¯¹æ–¹ä¿¡æ¯\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/);\\n                            if (infoMatch) {\\n                                // å­˜å‚¨åŸå§‹å¤´åƒå€¼\\n                                const originalAvatar = infoMatch[1] || '';\\n                                // å¤„ç†å¤´åƒå¹¶å­˜å‚¨\\n                                chatData.contactInfo.avatar = originalAvatar;\\n                                chatData.contactInfo.processedAvatar = processAvatarUrl(originalAvatar);\\n                                chatData.contactInfo.username = infoMatch[2] || '';\\n                                chatData.contactInfo.realName = infoMatch[3] || '';\\n                            }\\n                            continue;\\n                        }\\n                        \\n                        // å¯¹æ–¹åœ°ç‚¹\\n                        if (trimmedLine.startsWith('[å¯¹æ–¹åœ°ç‚¹|')) {\\n                            const locationMatch = trimmedLine.match(/\\\\[å¯¹æ–¹åœ°ç‚¹\\\\|(.*?)\\\\]/);\\n                            if (locationMatch) {\\n                                chatData.location = locationMatch[1] || '';\\n                            }\\n                            continue;\\n                        }\\n                        \\n                        // å¯¹æ–¹çŠ¶æ€\\n                        if (trimmedLine.startsWith('[å¯¹æ–¹çŠ¶æ€|')) {\\n                            const statusMatch = trimmedLine.match(/\\\\[å¯¹æ–¹çŠ¶æ€\\\\|(.*?)\\\\]/);\\n                            if (statusMatch) {\\n                                chatData.status = statusMatch[1] || '';\\n                            }\\n                            continue;\\n                        }\\n                        \\n                        // å¯¹æ–¹æƒ³æ³•\\n                        if (trimmedLine.startsWith('[å¯¹æ–¹æƒ³æ³•|')) {\\n                            const thoughtsMatch = trimmedLine.match(/\\\\[å¯¹æ–¹æƒ³æ³•\\\\|(.*?)\\\\]/);\\n                            if (thoughtsMatch) {\\n                                chatData.thoughts = thoughtsMatch[1] || '';\\n                            }\\n                            continue;\\n                        }\\n                        \\n                        // æˆ‘æ–¹æ¶ˆæ¯\\n                        if (trimmedLine.startsWith('[æˆ‘æ–¹æ¶ˆæ¯|')) {\\n                            const messageMatch = trimmedLine.match(/\\\\[æˆ‘æ–¹æ¶ˆæ¯\\\\|(.*?)\\\\|(.*?)\\\\]/);\\n                            if (messageMatch) {\\n                                chatData.messages.push({\\n                                    type: 'sent',\\n                                    content: messageMatch[1] || '',\\n                                    time: messageMatch[2] || '',\\n                                    avatar: typeof getUserAvatar === 'function' ? getUserAvatar() : 'https://files.catbox.moe/kz8xdq.JPG'\\n                                });\\n                            }\\n                            continue;\\n                        }\\n                        \\n                        // å¯¹æ–¹æ¶ˆæ¯\\n                        if (trimmedLine.startsWith('[å¯¹æ–¹æ¶ˆæ¯|')) {\\n                            const messageMatch = trimmedLine.match(/\\\\[å¯¹æ–¹æ¶ˆæ¯\\\\|(.*?)\\\\|(.*?)\\\\]/);\\n                            if (messageMatch) {\\n                                chatData.messages.push({\\n                                    type: 'received',\\n                                    content: messageMatch[1] || '',\\n                                    time: messageMatch[2] || '',\\n                                    avatar: chatData.contactInfo.processedAvatar || processAvatarUrl(chatData.contactInfo.avatar)\\n                                });\\n                            }\\n                            continue;\\n                        }\\n                    }\\n                    \\n                    // æ·»åŠ ç³»ç»Ÿæ¶ˆæ¯ï¼ˆæ—¶é—´ï¼‰\\n                    if (chatData.date && chatData.time) {\\n                        chatData.messages.unshift({\\n                            type: 'system',\\n                            content: `${chatData.date} ${chatData.time}`\\n                        });\\n                    }\\n                    \\n                    return chatData;\\n                    \\n                } catch (error) {\\n                    console.error('è§£æç§èŠæ•°æ®å¤±è´¥:', error);\\n                    return null;\\n                }\\n            }\\n        };\\n\\n        // è·å–å½“å‰æ—¶é—´\\n        function getCurrentTime() {\\n            const now = new Date();\\n            const hours = now.getHours().toString().padStart(2, '0');\\n            const minutes = now.getMinutes().toString().padStart(2, '0');\\n            return `${hours}:${minutes}`;\\n        }\\n\\n        // å‘é€æ¶ˆæ¯å‡½æ•°\\n        function sendMessage(text) {\\n            if (!text.trim()) return;\\n            \\n            // è·å–å½“å‰æ—¶é—´\\n            const timeString = getCurrentTime();\\n            \\n            // åˆ›å»ºæ–°æ¶ˆæ¯å¯¹è±¡\\n            const newMessage = {\\n                type: 'sent',\\n                content: text,\\n                time: timeString,\\n                avatar: getUserAvatar()\\n            };\\n            \\n            // å°†æ–°æ¶ˆæ¯æ·»åŠ åˆ°DOM\\n            const chatMessages = document.querySelector('.chat-messages');\\n            const messageElement = document.createElement('div');\\n            \\n            messageElement.className = 'message sent';\\n            messageElement.style.display = 'flex'; // ç¡®ä¿ç«‹å³æ˜¾ç¤º\\n            messageElement.innerHTML = `\\n                <div class=\\\"message-avatar\\\">\\n                    <img src=\\\"${newMessage.avatar}\\\" alt=\\\"æˆ‘çš„å¤´åƒ\\\">\\n                </div>\\n                <div class=\\\"message-wrapper\\\">\\n                    <div class=\\\"message-content\\\">${text}</div>\\n                    <div class=\\\"message-time\\\">${timeString}</div>\\n                </div>\\n            `;\\n            \\n            chatMessages.appendChild(messageElement);\\n            \\n            // æ»šåŠ¨åˆ°åº•éƒ¨\\n            chatMessages.scrollTop = chatMessages.scrollHeight;\\n            \\n            // ä½¿ç”¨triggerSlashå‘é€æ¶ˆæ¯\\n            if (typeof triggerSlash === 'function') {\\n                // è·å–èŠå¤©å¯¹è±¡åç§°\\n                const contactName = document.querySelector('.header-text h2').textContent || 'å¯¹æ–¹';\\n                triggerSlash(`/send ã€è®ºå›ç§ä¿¡ã€‘æˆ‘æ–¹å›å¤ï¼š${text}|/trigger`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n\\n        // ä»èŠå¤©å˜é‡ä¸­åŠ è½½ç”¨æˆ·å¤´åƒ\\n        async function loadUserAvatar() {\\n            try {\\n                // é»˜è®¤å¤´åƒ\\n                let avatarUrl = 'https://files.catbox.moe/kz8xdq.JPG';\\n                \\n                // å°è¯•ä»èŠå¤©å˜é‡è·å–å¤´åƒ\\n                if (typeof getVariables === 'function') {\\n                    const variables = await getVariables();\\n                    \\n                    if (variables && variables.æˆ‘çš„ä¸ªäººèµ„æ–™ && variables.æˆ‘çš„ä¸ªäººèµ„æ–™.å¤´åƒ) {\\n                        // æå–å¤´åƒæ–‡ä»¶åï¼ˆè‹±æ–‡éƒ¨åˆ†å’Œåç¼€ï¼‰\\n                        const avatarMatch = variables.æˆ‘çš„ä¸ªäººèµ„æ–™.å¤´åƒ.match(/[a-zA-Z0-9_-]+\\\\.[a-zA-Z0-9]+$/);\\n                        if (avatarMatch) {\\n                            const avatarFileName = avatarMatch[0];\\n                            avatarUrl = `https://files.catbox.moe/${avatarFileName}`;\\n                        }\\n                    }\\n                }\\n                \\n                // ä¿å­˜åˆ°å…¨å±€å˜é‡ï¼Œä»¥ä¾¿å‘é€æ¶ˆæ¯æ—¶ä½¿ç”¨\\n                window.userAvatarUrl = avatarUrl;\\n                \\n                // æ›´æ–°ç°æœ‰çš„å·²å‘é€æ¶ˆæ¯å¤´åƒ\\n                document.querySelectorAll('.message.sent .message-avatar img').forEach(img => {\\n                    img.src = avatarUrl;\\n                });\\n                \\n                return avatarUrl;\\n            } catch (error) {\\n                console.error('åŠ è½½ç”¨æˆ·å¤´åƒæ—¶å‡ºé”™:', error);\\n                return 'https://files.catbox.moe/kz8xdq.JPG';\\n            }\\n        }\\n\\n        // æ¨¡æ‹Ÿå›å¤(å·²ä¸ä½¿ç”¨)\\n        function simulateReply() {\\n            // æ­¤åŠŸèƒ½å·²ç§»é™¤ï¼Œä¸å†æ¨¡æ‹Ÿå¯¹æ–¹å›å¤\\n        }\\n\\n        // UIæ›´æ–°å™¨\\n        const UIUpdater = {\\n            // æ›´æ–°ç§èŠç•Œé¢\\n            updateChatUI(data) {\\n                if (!data) {\\n                    console.error('æ— æ•ˆçš„èŠå¤©æ•°æ®ï¼Œæ— æ³•æ›´æ–°UI');\\n                    return;\\n                }\\n                \\n                // æ›´æ–°å¤´éƒ¨ä¿¡æ¯\\n                this.updateHeader(data);\\n                \\n                // æ›´æ–°æ¶ˆæ¯åˆ—è¡¨\\n                this.updateMessages(data.messages);\\n                \\n                // åŠ è½½å®Œæˆåè·å–ç”¨æˆ·å¤´åƒå¹¶æ›´æ–°å·²å‘é€æ¶ˆæ¯çš„å¤´åƒ\\n                setTimeout(async () => {\\n                    const userAvatar = await loadUserAvatar();\\n                    document.querySelectorAll('.message.sent .message-avatar img').forEach(img => {\\n                        img.src = userAvatar;\\n                    });\\n                }, 200);\\n            },\\n            \\n            // æ›´æ–°å¤´éƒ¨ä¿¡æ¯\\n            updateHeader(data) {\\n                // æ›´æ–°è”ç³»äººåç§°\\n                const headerTitle = document.querySelector('.header-text h2');\\n                if (headerTitle) {\\n                    headerTitle.textContent = data.contactInfo.username || 'æœªçŸ¥ç”¨æˆ·';\\n                }\\n                \\n                // æ›´æ–°çŠ¶æ€ä¿¡æ¯\\n                const headerStatus = document.querySelector('.header-text p');\\n                if (headerStatus) {\\n                    // å¦‚æœæœ‰ä½ç½®ä¿¡æ¯ï¼Œæ˜¾ç¤ºä½ç½®ï¼›å¦åˆ™æ˜¾ç¤º\\\"æœ€è¿‘æ´»è·ƒ\\\"\\n                    headerStatus.textContent = data.location || 'æœ€è¿‘æ´»è·ƒ';\\n                }\\n                \\n                // æ›´æ–°å¤´åƒï¼ˆå¦‚æœ‰éœ€è¦ï¼‰\\n                const avatarImg = document.querySelector('.avatar-container img');\\n                if (avatarImg) {\\n                    avatarImg.src = data.contactInfo.processedAvatar || processAvatarUrl(data.contactInfo.avatar || '');\\n                    avatarImg.alt = data.contactInfo.username || 'å¯¹æ–¹å¤´åƒ';\\n                }\\n            },\\n            \\n            // æ›´æ–°æ¶ˆæ¯åˆ—è¡¨\\n            updateMessages(messages) {\\n                if (!messages || !messages.length) {\\n                    console.warn('æ²¡æœ‰æ¶ˆæ¯å¯æ˜¾ç¤º');\\n                    return;\\n                }\\n                \\n                const chatMessages = document.querySelector('.chat-messages');\\n                if (!chatMessages) return;\\n                \\n                // æ¸…ç©ºç°æœ‰æ¶ˆæ¯\\n                chatMessages.innerHTML = '';\\n                \\n                // åˆ›å»ºæ‰€æœ‰æ¶ˆæ¯å…ƒç´ \\n                const allMessages = [];\\n                \\n                // æ·»åŠ æ¶ˆæ¯\\n                messages.forEach(msg => {\\n                    const messageElement = document.createElement('div');\\n                    \\n                    if (msg.type === 'system') {\\n                        // ç³»ç»Ÿæ¶ˆæ¯ï¼ˆæ—¥æœŸæ—¶é—´ï¼‰\\n                        messageElement.className = 'system-message';\\n                        messageElement.style.width = '100%';\\n                        messageElement.style.display = 'flex';\\n                        messageElement.style.justifyContent = 'center';\\n                        messageElement.innerHTML = `\\n                            <div class=\\\"system-message-content\\\">${msg.content}</div>\\n                        `;\\n                    } else if (msg.type === 'sent') {\\n                        // å‘é€çš„æ¶ˆæ¯\\n                        messageElement.className = 'message sent';\\n                        messageElement.innerHTML = `\\n                            <div class=\\\"message-avatar\\\">\\n                                <img src=\\\"${msg.avatar}\\\" alt=\\\"æˆ‘çš„å¤´åƒ\\\">\\n                            </div>\\n                            <div class=\\\"message-wrapper\\\">\\n                                <div class=\\\"message-content\\\">${msg.content}</div>\\n                                <div class=\\\"message-time\\\">${msg.time}</div>\\n                            </div>\\n                        `;\\n                    } else if (msg.type === 'received') {\\n                        // æ¥æ”¶çš„æ¶ˆæ¯\\n                        messageElement.className = 'message received';\\n                        messageElement.innerHTML = `\\n                            <div class=\\\"message-avatar\\\">\\n                                <img src=\\\"${msg.avatar}\\\" alt=\\\"å¯¹æ–¹å¤´åƒ\\\">\\n           