    });\\n    });\\n\\n    // æ·»åŠ ç‚¹èµå’Œè½¬å‘æŒ‰é’®çš„äº¤äº’åé¦ˆ\\n    document.querySelectorAll('.tweet-action').forEach(action => {\\n        action.addEventListener('click', (event) => {\\n            playClickSoundOnce();\\n            createClickEffect(action);\\n\\n            if (action.querySelector('.fa-heart')) {\\n                const heartIcon = action.querySelector('.fa-heart');\\n                const countSpan = action.querySelector('span');\\n                \\n                // åˆ‡æ¢ç‚¹èµçŠ¶æ€\\n                if (heartIcon.classList.contains('far')) {\\n                    // ç‚¹èµ\\n                    heartIcon.classList.remove('far');\\n                    heartIcon.classList.add('fas');\\n                    heartIcon.style.color = '#ff6b8b'; // ç²‰è‰²\\n                    countSpan.textContent = parseInt(countSpan.textContent) + 1;\\n                    showKawaiiToast('ç‚¹èµæˆåŠŸï¼ğŸ’–');\\n                    \\n                    // åˆ›å»ºçˆ±å¿ƒæ•ˆæœ\\n                    const heart = document.createElement('div');\\n                    heart.className = 'heart';\\n                    heart.innerHTML = 'â™¥';\\n                    heart.style.left = (event.clientX - 10) + 'px';\\n                    heart.style.top = (event.clientY - 10) + 'px';\\n                    document.body.appendChild(heart);\\n                    \\n                    setTimeout(() => {\\n                        heart.remove();\\n                    }, 3000);\\n                } else {\\n                    // å–æ¶ˆç‚¹èµ\\n                    heartIcon.classList.remove('fas');\\n                    heartIcon.classList.add('far');\\n                    heartIcon.style.color = '';\\n                    countSpan.textContent = parseInt(countSpan.textContent) - 1;\\n                    showKawaiiToast('å–æ¶ˆç‚¹èµ');\\n                }\\n            } else if (action.querySelector('.fa-retweet')) {\\n                const retweetIcon = action.querySelector('.fa-retweet');\\n                const countSpan = action.querySelector('span');\\n                \\n                // åˆ‡æ¢è½¬å‘çŠ¶æ€\\n                if (!retweetIcon.style.color || retweetIcon.style.color === '') {\\n                    // è½¬å‘\\n                    retweetIcon.style.color = '#75a8ff'; // è“è‰²\\n                    countSpan.textContent = parseInt(countSpan.textContent) + 1;\\n                    showKawaiiToast('è½¬å‘æˆåŠŸï¼âœ¨');\\n                } else {\\n                    // å–æ¶ˆè½¬å‘\\n                    retweetIcon.style.color = '';\\n                    countSpan.textContent = parseInt(countSpan.textContent) - 1;\\n                    showKawaiiToast('å–æ¶ˆè½¬å‘');\\n                }\\n            } else if (action.querySelector('.fa-comment')) {\\n                showKawaiiToast('è¯„è®ºåŠŸèƒ½å¼€å‘ä¸­ï¼ğŸ€');\\n            }\\n        });\\n    });\\n}\\n\\n    function createClickEffect(element) {\\n        const sparkleCount = 5;\\n        const rect = element.getBoundingClientRect();\\n\\n        for(let i = 0; i < sparkleCount; i++) {\\n            const sparkle = document.createElement('div');\\n            sparkle.className = 'sparkle';\\n            sparkle.style.left = rect.left + Math.random() * rect.width + 'px';\\n            sparkle.style.top = rect.top + Math.random() * rect.height + 'px';\\n            document.body.appendChild(sparkle);\\n            setTimeout(() => sparkle.remove(), 1500);\\n        }\\n    }\\n\\n    function showKawaiiToast(message) {\\n        const toast = document.createElement('div');\\n        toast.className = 'kawaii-toast';\\n        toast.textContent = message;\\n        document.body.appendChild(toast);\\n\\n        toast.offsetHeight;\\n        toast.classList.add('show');\\n\\n        setTimeout(() => {\\n            toast.classList.remove('show');\\n            setTimeout(() => toast.remove(), 300);\\n        }, 2000);\\n    }\\n\\n// åˆå§‹åŒ–\\ndocument.addEventListener('DOMCont