/div>\\n    <div class=\\\"search-modal-buttons\\\">\\n        <button class=\\\"search-modal-button search-cancel-btn\\\" id=\\\"searchCancelBtn\\\">å–æ¶ˆ</button>\\n        <button class=\\\"search-modal-button search-confirm-btn\\\" id=\\\"searchConfirmBtn\\\">ç¡®å®š</button>\\n    </div>\\n</div>\\n\\n<script>\\n// ä¸ªäººä¸»é¡µå†…å®¹æ¨¡æ¿ï¼ˆä¼šè¢«å®é™…å†…å®¹æ›¿æ¢ï¼‰\\nlet homepageContent = `<homepage>$1</homepage>`;\\n\\n// æ”¹è¿›URLå¤„ç†å‡½æ•°\\nfunction processImageUrl(imageUrl) {\\n    if (!imageUrl || typeof imageUrl !== 'string') {\\n        return 'https://files.catbox.moe/8mjvdg.jpg'; // é»˜è®¤å›¾ç‰‡\\n    }\\n    \\n    if (imageUrl.startsWith('http')) {\\n        return imageUrl;\\n    }\\n    \\n    // å°è¯•æå–è‹±æ–‡æ–‡ä»¶åçš„å‡ ç§æ–¹æ³•\\n    // æ–¹æ³•1: æå–å­—æ¯æ•°å­—åŠç‚¹å·ç»„æˆçš„æ–‡ä»¶å\\n    const fileNameMatch = imageUrl.match(/[a-zA-Z0-9]+\\\\.[a-zA-Z0-9]+$/i);\\n    if (fileNameMatch) {\\n        return 'https://files.catbox.moe/' + fileNameMatch[0];\\n    }\\n    \\n    // æ–¹æ³•2: å°è¯•åŒ¹é…ä»»ä½•.jpg, .png, .gifç­‰åç¼€\\n    const extensionMatch = imageUrl.match(/[a-zA-Z0-9]+\\\\.(jpg|jpeg|png|gif|bmp|webp)$/i);\\n    if (extensionMatch) {\\n        return 'https://files.catbox.moe/' + extensionMatch[0];\\n    }\\n    \\n    // æ–¹æ³•3: å¯»æ‰¾å­ä¸²ä¸­çš„æ–‡ä»¶åæ¨¡å¼\\n    const partialMatch = imageUrl.match(/([a-zA-Z0-9]{6,})\\\\.(jpg|jpeg|png|gif|bmp|webp)/i);\\n    if (partialMatch) {\\n        return 'https://files.catbox.moe/' + partialMatch[0];\\n    }\\n    \\n    return 'https://files.catbox.moe/8mjvdg.jpg'; // é»˜è®¤å›¾ç‰‡\\n}\\n\\n// æå–ä¸ªäººä¸»é¡µä¿¡æ¯çš„å‡½æ•°\\nfunction extractHomepageInfo(input) {\\n    // ä½¿ç”¨æ­£åˆ™è¡¨è¾¾å¼åŒ¹é…<homepage>æ ‡ç­¾ä¹‹é—´çš„å†…å®¹\\n    const regex = /<homepage>([\\\\s\\\\S]*?)<\\\\/homepage>/;\\n    const match = input.match(regex);\\n    \\n    if (!match) {\\n        console.error('æ— æ³•è§£æè¾“å…¥:', input);\\n        return null;\\n    }\\n    \\n    const content = match[1].trim();\\n    const result = {\\n        åŸºæœ¬ä¿¡æ¯: {},\\n        åŠ¨æ€: []\\n    };\\n    \\n    // ç›´æ¥æå–æ‰€æœ‰æ–¹æ‹¬å·å†…å®¹\\n    const brackets = [];\\n    const bracketRegex = /\\\\[(.*?)\\\\]/g;\\n    let bracketMatch;\\n    \\n    while ((bracketMatch = bracketRegex.exec(content)) !== null) {\\n        brackets.push(bracketMatch[0]);\\n    }\\n    \\n    console.log(`æå–åˆ° ${brackets.length} ä¸ªæ–¹æ‹¬å·å†…å®¹`);\\n    \\n    // å¤„ç†æ¯ä¸ªæ‹¬å·å†…å®¹\\n    for (const bracket of brackets) {\\n        try {\\n            // æå–æ ‡ç­¾å’Œå€¼\\n            const bracketContent = bracket.substring(1, bracket.length - 1);\\n            const firstSeparator = bracketContent.indexOf('|');\\n            \\n            if (firstSeparator !== -1) {\\n                const label = bracketContent.substring(0, firstSeparator).trim();\\n                const value = bracketContent.substring(firstSeparator + 1).trim();\\n                \\n                if (label === 'åŠ¨æ€') {\\n                    // è§£æåŠ¨æ€ä¿¡æ¯\\n                    const parts = value.split('|');\\n                    \\n                    if (parts.length >= 7) {\\n                        const dynamic = {\\n                            ç½‘å: parts[0].trim(),\\n                            é‚®ç®±: parts[1].trim(),\\n                            å†…å®¹: parts[2].trim(),\\n                            é…å›¾: parts[3].trim(),\\n                            ç‚¹èµ: parts[4].trim(),\\n                            è½¬å‘: parts[5].trim(),\\n                            è¯„è®ºæ•°: parts[6].trim(),\\n                            è¯„è®º: []\\n                        };\\n                        \\n                        // è§£æè¯„è®º - ä»ç´¢å¼•7å¼€å§‹æ˜¯è¯„è®ºè€…å’Œå†…å®¹çš„äº¤æ›¿\\n                        if (parts.length > 7) {\\n                            console.log(`åŠ¨æ€è§£æ: å…±æœ‰ ${(parts.length - 7) / 2} æ¡è¯„è®º`);\\n                            \\n                            for (let i = 7; i < parts.length; i += 2) {\\n                                if (i + 1 < parts.length) {\\n                                    dynamic.è¯„è®º.push({\\n                                        è¯„è®ºè€…: parts[i].trim(),\\n                                        å†…å®¹: parts[i + 1].trim()\\n                                    });\\n                                }\\n                            }\\n                        }\\n                        \\n                        result.åŠ¨æ€.push(dynamic);\\n                        console.log(`æ·»åŠ åŠ¨æ€: ${dynamic.å†…å®¹.substring(0, 15)}... (${dynamic.è¯„è®º.length} æ¡è¯„è®º)`);\\n                    } else {\\n                        console.warn(`åŠ¨æ€æ•°æ®æ ¼å¼ä¸æ­£ç¡®: ${parts.length} ä¸ªéƒ¨åˆ†, éœ€è¦è‡³å°‘7ä¸ª`);\\n                    }\\n                } else {\\n                    result.åŸºæœ¬ä¿¡æ¯[label] = value;\\n                    console.log(`æ·»åŠ åŸºæœ¬ä¿¡æ¯: ${label} = ${value}`);\\n                }\\n            }\\n        } catch (error) {\\n            console.error(`å¤„ç†æ–¹æ‹¬å·å†…å®¹æ—¶å‡ºé”™: ${bracket}`, error);\\n        }\\n    }\\n    \\n    console.log(`è§£æç»“æœ: ${Object.keys(result.åŸºæœ¬ä¿¡æ¯).length} ä¸ªåŸºæœ¬ä¿¡æ¯, ${result.åŠ¨æ€.length} æ¡åŠ¨æ€`);\\n    return result;\\n}\\n\\n// æ›´æ–°åŠ¨æ€é¡µé¢æ˜¾ç¤º\\nfunction updateHomepageDisplay(info) {\\n    if (!info) return;\\n    \\n    // æ›´æ–°åŸºæœ¬ä¿¡æ¯\\n    if (info.åŸºæœ¬ä¿¡æ¯['å¤´åƒ']) {\\n        let avatarUrl = processImageUrl(info.åŸºæœ¬ä¿¡æ¯['å¤´åƒ']);\\n        document.getElementById('profile-avatar').style.backgroundImage = `url('${avatarUrl}')`;\\n    }\\n    \\n    if (info.åŸºæœ¬ä¿¡æ¯['èƒŒæ™¯']) {\\n        let bgUrl = processImageUrl(info.åŸºæœ¬ä¿¡æ¯['èƒŒæ™¯']);\\n        document.getElementById('profile-section').style.backgroundImage = `url('${bgUrl}')`;\\n    }\\n    \\n    if (info.åŸºæœ¬ä¿¡æ¯['ç½‘å']) {\\n        document.getElementById('profile-name').textContent = info.åŸºæœ¬ä¿¡æ¯['ç½‘å'];\\n    }\\n    \\n    if (info.åŸºæœ¬ä¿¡æ