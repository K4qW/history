              <div class=\\\"post-comment-input\\\">\\n                    <img src=\\\"\\\" alt=\\\"æˆ‘çš„å¤´åƒ\\\" class=\\\"add-comment-avatar\\\">\\n                    <input type=\\\"text\\\" placeholder=\\\"è¯´ç‚¹ä»€ä¹ˆ...\\\" class=\\\"comment-input\\\">\\n                    <button class=\\\"comment-send-btn\\\">å‘é€</button>\\n                </div>\\n            </div>\\n        </div>\\n        \\n        <!-- è¯„è®ºåŒº -->\\n        <div class=\\\"comments-section\\\">\\n            <div class=\\\"comments-header\\\">\\n                <div class=\\\"comments-title\\\">è¯„è®º <span class=\\\"comments-count\\\">0</span></div>\\n                <div class=\\\"comments-sort\\\">\\n                    <div class=\\\"sort-option active\\\" data-type=\\\"new\\\">æœ€æ–°</div>\\n                    <div class=\\\"sort-option\\\" data-type=\\\"hot\\\">æœ€çƒ­</div>\\n                </div>\\n            </div>\\n            \\n            <!-- æœ€æ–°è¯„è®º -->\\n            <div class=\\\"new-comments\\\">\\n                <!-- åŠ¨æ€ç”Ÿæˆçš„è¯„è®ºå°†æ·»åŠ åˆ°è¿™é‡Œ -->\\n            </div>\\n            \\n            <!-- çƒ­é—¨è¯„è®º -->\\n            <div class=\\\"hot-comments\\\">\\n                <!-- åŠ¨æ€ç”Ÿæˆçš„çƒ­é—¨è¯„è®ºå°†æ·»åŠ åˆ°è¿™é‡Œ -->\\n            </div>\\n        </div>\\n        \\n        <!-- ç›¸å…³æ¨è -->\\n        <div class=\\\"related-posts\\\">\\n            <div class=\\\"related-header\\\">\\n                <div class=\\\"related-title\\\">ç›¸å…³æ¨è</div>\\n            </div>\\n            <div class=\\\"related-list\\\">\\n                <!-- åŠ¨æ€ç”Ÿæˆçš„ç›¸å…³å¸–å­å°†æ·»åŠ åˆ°è¿™é‡Œ -->\\n            </div>\\n        </div>\\n    </div>\\n\\n    <!-- åº•éƒ¨å¯¼èˆªæ  -->\\n    <div class=\\\"bottom-navbar\\\">\\n        <a href=\\\"javascript:void(0)\\\" class=\\\"nav-item\\\">\\n            <i class=\\\"fas fa-home\\\"></i>\\n            <span>é¦–é¡µ</span>\\n        </a>\\n        <a href=\\\"javascript:void(0)\\\" class=\\\"nav-item\\\">\\n            <i class=\\\"fas fa-compass\\\"></i>\\n            <span>å‘ç°</span>\\n        </a>\\n        <div class=\\\"publish-btn-container\\\">\\n            <div class=\\\"publish-btn\\\">\\n                <i class=\\\"fas fa-plus\\\"></i>\\n            </div>\\n        </div>\\n        <a href=\\\"javascript:void(0)\\\" class=\\\"nav-item\\\">\\n            <i class=\\\"fas fa-bell\\\"></i>\\n            <span>æ¶ˆæ¯</span>\\n        </a>\\n        <a href=\\\"javascript:void(0)\\\" class=\\\"nav-item\\\">\\n            <i class=\\\"fas fa-user\\\"></i>\\n            <span>æˆ‘çš„</span>\\n        </a>\\n    </div>\\n\\n    <script>\\n        // 1. åŸºç¡€æ¨¡æ¿å®šä¹‰\\n        let gameText = `\\n<post>$1</post>`;\\n\\n        // 2. æ•°æ®æå–å™¨\\n        const DataExtractor = {\\n            // å¤„ç†å¤´åƒURLï¼Œæå–è‹±æ–‡éƒ¨åˆ†å¹¶æ·»åŠ å‰ç¼€\\n            processAvatarUrl(avatar) {\\n                // å¦‚æœå¤´åƒæ ¼å¼åŒ…å«ä¸­æ–‡ï¼Œåˆ™æå–è‹±æ–‡éƒ¨åˆ†\\n                if (/[\\\\u4e00-\\\\u9fa5]/.test(avatar)) {\\n                    const match = avatar.match(/([a-zA-Z0-9]+\\\\.[a-zA-Z]+)$/);\\n                    if (match) {\\n                        return `https://files.catbox.moe/${match[1]}`;\\n                    }\\n                }\\n                return `https://files.catbox.moe/${avatar}`;\\n            },\\n            // è§£æå¸–å­é¡µé¢æ•°æ®\\n            parsePostData(text) {\\n                try {\\n                    // åŒ¹é… <post> æ ‡ç­¾å†…çš„å†…å®¹\\n                    const postMatch = text.match(/<post>([\\\\s\\\\S]*?)<\\\\/post>/);\\n                    if (!postMatch) return null;\\n                    \\n                    const postContent = postMatch[1].trim();\\n                    \\n                    // æå–å¸–å­å¤´éƒ¨ä¿¡æ¯\\n                    const headerMatch = postContent.match(/<post_header>([\\\\s\\\\S]*?)<\\\\/post_header>/);\\n                    const headerContent = headerMatch ? headerMatch[1].trim() : '';\\n                    \\n                    // æå–æœ€æ–°è¯„è®º\\n                    const latestCommentsMatch = postContent.match(/<latest_comments>([\\\\s\\\\S]*?)<\\\\/latest_comments>/);\\n                    const latestCommentsContent = latestCommentsMatch ? latestCommentsMatch[1].trim() : '';\\n                    \\n                    // æå–çƒ­é—¨è¯„è®º\\n                    const hotCommentsMatch = postContent.match(/<hot_comments>([\\\\s\\\\S]*?)<\\\\/hot_comments>/);\\n                    const hotCommentsContent = hotCommentsMatch ? hotCommentsMatch[1].trim() : '';\\n                    \\n                    // æå–ç›¸å…³å¸–å­\\n                    const relatedPostsMatch = postContent.match(/<related_posts>([\\\\s\\\\S]*?)<\\\\/related_posts>/);\\n                    const relatedPostsContent = relatedPostsMatch ? relatedPostsMatch[1].trim() : '';\\n                    \\n                    // è§£æå¸–å­å¤´éƒ¨\\n                    const postHeader = this.extractPostHeader(headerContent);\\n                    \\n                    // è§£ææœ€æ–°è¯„è®º\\n                    const latestComments = this.extractComments(latestCommentsContent);\\n                    \\n                    // è§£æçƒ­é—¨è¯„è®º\\n                    const hotComments = this.extractComments(hotCommentsContent);\\n                    \\n                    // è§£æç›¸å…³å¸–å­\\n                    const relatedPosts = this.extractRelatedPosts(relatedPostsContent);\\n                    \\n                    return {\\n                        postHeader,\\n                        latestComments,\\n                        hotComments,\\n                        relatedPosts\\n                    };\\n                } catch (error) {\\n                    console.error('è§£æå¸–å­æ•°æ®å¤±è´¥:', error);\\n                    return {\\n                        postHeader: {},\\n                        latestComments: [],\\n                        hotComments: [],\\n                        relatedPosts: []\\n                    };\\n                }\\n            },\\n            \\n            // æå–å¸–å­å¤´éƒ¨ä¿¡æ¯\\n            extractPostHeader(content) {\\n                if (!content) return {};\\n                \\n                const header = {};\\n                \\n                // æå–æ—¶é—´ä¿¡æ¯\\n                const timeRegex = /\\\\[æ—¶é—´\\\\|(.*?)\\\\|(.*?)\\\\]/;\\n                const timeMatch = content.match(timeRegex);\\n                if (timeMatch) {\\n                    header.date = timeMatch[1] || '';\\n                    header.time = timeMatch[2] || '';\\n                }\\n                \\n                // æå–å¸–å­åŸºæœ¬ä¿¡æ¯\\n                const infoRegex = /\\\\[å¸–å­ä¿¡æ¯\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/;\\n                const infoMatch = content.match(infoRegex);\\n                if (infoMatch) {\\n                    header.author = infoMatch[1] || '';\\n                    header.avatar = this.processAvatarUrl(infoMatch[2]) || '';\\n                    header.title = infoMatch[3] || '';\\n                    header.publishTime = infoMatch[4] || '';\\n                    header.likes = parseInt(infoMatch[5]) || 0;\\n                    header.favorites = parseInt(infoMatch[6]) || 0;\\n                    header.comments = parseInt(infoMatch[7]) || 0;\\n                }\\n                \\n                // æå–å¸–å­å›¾ç‰‡\\n                const imageRegex = /\\\\[å¸–å­å›¾ç‰‡\\\\|(.*?)\\\\]/;\\n                const imageMatch = content.match(imageRegex);\\n                if (imageMatch && imageMatch[1]) {\\n                    header.images = imageMatch[1].split('|').filter(img => img.trim() !== '');\\n                } else {\\n                    header.images = [];\\n                }\\n                \\n                // æå–å¸–å­å†…å®¹\\n                const contentRegex = /\\\\[å¸–å­å†…å®¹\\\\|([\\\\s\\\\S]*?)\\\\]/;\\n                const contentMatch = content.match(contentRegex);\\n                if (contentMatch) {\\n                    header.content = contentMatch[1] || '';\\n                }\\n                \\n                // æå–å¸–å­æ ‡ç­¾\\n                const tagsRegex = /\\\\[å¸–å­æ ‡ç­¾\\\\|(.*?)\\\\]/;\\n                const tagsMatch = content.match(tagsRegex);\\n                if (tagsMatch && tagsMatch[1]) {\\n                    header.tags = tagsMatch[1].split('|').filter(tag => tag.trim() !== '');\\n                } else {\\n                    header.tags = [];\\n                }\\n                \\n                return header;\\n            },\\n            \\n            // æå–è¯„è®ºæ•°æ®\\n            extractComments(content) {\\n                if (!content) return [];\\n                \\n                const comments = [];\\n                let currentComment = null;\\n                \\n                // æŒ‰è¡Œåˆ†å‰²å†…å®¹\\n                const lines = content.split('\\\\n');\\n                \\n                for (const line of lines) {\\n                    const trimmedLine = line.trim();\\n                    if (!trimmedLine) continue;\\n                    \\n                    // åŒ¹é…ä¸»è¯„è®º\\n                    const commentRegex = /\\\\[è¯„è®º\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/;\\n                    const commentMatch = trimmedLine.match(commentRegex);\\n                    \\n                    if (commentMatch) {\\n                        // å¦‚æœå­˜åœ¨ä¸Šä¸€æ¡è¯„è®ºï¼Œå…ˆæ·»åŠ åˆ°åˆ—è¡¨ä¸­\\n                        if (currentComment) {\\n                            comments.push(currentComment);\\n                        }\\n                        \\n                        // åˆ›å»ºæ–°çš„è¯„è®ºå¯¹è±¡\\n                        currentComment = {\\n                            author: commentMatch[1] || '',\\n                            avatar: this.processAvatarUrl(commentMatch[2]) || '',\\n                            content: commentMatch[3] || '',\\n                            time: commentMatch[4] || '',\\n                            likes: parseInt(commentMatch[5]) || 0,\\n                            replies: []\\n                        };\\n                    } else {\\n                        // åŒ¹é…æ¥¼ä¸­æ¥¼è¯„è®º\\n                        const replyRegex = /\\\\[è¯„è®ºæ¥¼ä¸­æ¥¼\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/;\\n                        const replyMatch = trimmedLine.match(replyRegex);\\n                        \\n                        if (replyMatch && currentComment) {\\n                            currentComment.replies.push({\\n                                author: replyMatch[1] || '',\\n                                content: replyMatch[2] || '',\\n                                time: replyMatch[3] || '',\\n                                likes: parseInt(replyMatch[4]) || 0\\n                            });\\n                        }\\n                    }\\n                }\\n                \\n                // æ·»åŠ æœ€åä¸€æ¡è¯„è®º\\n                if (currentComment) {\\n                    comments.push(currentComment);\\n                }\\n                \\n                return comments;\\n            },\\n            \\n            // æå–ç›¸å…³å¸–å­\\n            extractRelatedPosts(content) {\\n                if (!content) return [];\\n                \\n                const posts = [];\\n                const lines = content.split('\\\\n');\\n                \\n                for (const line of lines) {\\n                    const trimmedLine = line.trim();\\n                    if (!trimmedLine) continue;\\n                    \\n                    // æ–°æ ¼å¼: [ç›¸å…³å¸–å­|ä½œè€…|æ ‡é¢˜]\\n                    const postRegex = /\\\\[ç›¸å…³å¸–å­\\\\|(.*?)\\\\|(.*?)\\\\]/;\\n                    const match = trimmedLine.match(postRegex);\\n                    \\n                    if (match) {\\n                        posts.push({\\n                            author: match[1] || '',\\n                            title: match[2] || ''\\n                        });\\n                    }\\n                }\\n                \\n                return posts;\\n            },\\n            \\n            // è§£æ gameText\\n            parseGameText(gameText) {\\n                // ç›´æ¥è§£æå¸–å­æ•°æ®\\n                return this.parsePostData(gameText);\\n            }\\n        };\\n\\n        // 3. UI æ›´æ–°å™¨\\n        const UIUpdater = {\\n            // æ›´æ–°å¸–å­å¤´éƒ¨ä¿¡æ¯\\n            updatePostHeader(postHeader) {\\n                if (!postHeader) {\\n                    console.warn('æ²¡æœ‰å¸–å­å¤´éƒ¨æ•°æ®å¯æ˜¾ç¤º');\\n                    return;\\n                }\\n                \\n                // æ›´æ–°å¸–å­ä½œè€…ä¿¡æ¯\\n                document.querySelector('.author-name').textContent = postHeader.author || 'æœªçŸ¥ä½œè€…';\\n                document.querySelector('.post-time').textContent = postHeader.publishTime || '';\\n                \\n                // æ›´æ–°ä½œè€…å¤´åƒ\\n                const avatarImg = document.querySelector('.author-avatar');\\n                if (avatarImg) {\\n                    avatarImg.src = postHeader.avatar;\\n                    avatarImg.alt = postHeader.author || 'ä½œè€…å¤´åƒ';\\n                    \\n                    // æ·»åŠ ç‚¹å‡»äº‹ä»¶ï¼Œè·³è½¬åˆ°ä½œè€…ä¸»é¡µ\\n                    avatarImg.addEventListener('click', function() {\\n                        viewUserProfile(postHeader.author);\\n                    });\\n                }\\n                \\n                // ä¸ºä½œè€…åç§°æ·»åŠ ç‚¹å‡»äº‹ä»¶\\n                const authorName = document.querySelector('.author-name');\\n                if (authorName) {\\n                    authorName.addEventListener('click', function() {\\n                        viewUserProfile(postHeader.author);\\n                    });\\n                }\\n                \\n                // æ›´æ–°å¸–å­æ ‡é¢˜\\n                document.querySelector('.post-title').textContent = postHeader.title || 'æ— æ ‡é¢˜';\\n                \\n                // æ›´æ–°å¸–å­å†…å®¹\\n                // åˆ¤æ–­æ˜¯å¦ç›´æ¥åŒ…å«HTMLæ ‡ç­¾ï¼Œå¦‚æœä¸åŒ…å«åˆ™ç”¨pæ ‡ç­¾åŒ…è£¹\\n                const postContentEl = document.querySelector('.post-content');\\n                const titleEl = postContentEl.querySelector('.post-title');\\n                \\n                // ç§»é™¤æ‰€æœ‰ç°æœ‰çš„å¸–å­å†…å®¹\\n                const elementsToRemove = Array.from(postContentEl.children).filter(el => \\n                    !el.classList.contains('post-title') && \\n                    !el.classList.contains('post-tags')\\n                );\\n                \\n                elementsToRemove.forEach(el => el.remove());\\n                \\n                // æ·»åŠ æ»‘åŠ¨å›¾ç‰‡å®¹å™¨ï¼Œå¦‚æœæœ‰å›¾ç‰‡\\n                const images = postHeader.images || [];\\n                if (images.length > 0) {\\n                    const sliderContainer = document.createElement('div');\\n                    sliderContainer.className = 'post-images-slider';\\n                    \\n                    // åˆ›å»ºæ»‘åŠ¨å®¹å™¨\\n                    const imagesContainer = document.createElement('div');\\n                    imagesContainer.className = 'slider-container';\\n                    \\n                    // æ·»åŠ å›¾ç‰‡\\n                    images.forEach((image, index) => {\\n                        const imageContainer = document.createElement('div');\\n                        imageContainer.className = 'image-container';\\n                        // ç§»é™¤å†…è”onclickäº‹ä»¶ï¼Œæ”¹ç”±initPhotoSliderå‡½æ•°ç»Ÿä¸€å¤„ç†\\n                        \\n                        // æ·»åŠ é®ç½©å±‚\\n                        const imageMask = document.createElement('div');\\n                        imageMask.className = 'image-mask';\\n                        imag