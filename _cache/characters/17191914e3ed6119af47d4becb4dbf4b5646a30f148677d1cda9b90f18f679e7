ons\\\" id=\\\"defaultModalActions\\\">\\n            <button class=\\\"modal-btn cancel\\\" onclick=\\\"triggerActionWithSound('hideNewChatModal')\\\">å–æ¶ˆ</button>\\n            <button class=\\\"modal-btn confirm\\\" onclick=\\\"triggerActionWithSound('createNewChat')\\\">ç¡®è®¤</button>\\n        </div>\\n    </div>\\n</div>\\n\\n    <audio id=\\\"clickSound\\\" src=\\\"https://assets.mixkit.co/active_storage/sfx/2568/2568-preview.mp3\\\" preload=\\\"auto\\\"></audio>\\n\\n    <script>\\n    // é»˜è®¤å¤´åƒURL\\n    const DEFAULT_AVATAR_URL = 'https://files.catbox.moe/jr6orr.JPG';\\n    \\n    // æ¶ˆæ¯åˆ—è¡¨å†…å®¹æ¨¡æ¿\\n    let messageContent = `\\n<message_list>$1</message_list>`;\\n\\n    // æå–æ¶ˆæ¯åˆ—è¡¨ä¿¡æ¯çš„å‡½æ•° - æ”¹è¿›ç‰ˆ\\n    function extractMessageInfo(input) {\\n        console.log('å¼€å§‹è§£ææ¶ˆæ¯åˆ—è¡¨å†…å®¹...');\\n        try {\\n            // ä½¿ç”¨æ­£åˆ™è¡¨è¾¾å¼åŒ¹é…<message_list>æ ‡ç­¾ä¹‹é—´çš„å†…å®¹\\n            const regex = /<message_list>([\\\\s\\\\S]*?)<\\\\/message_list>/;\\n            const match = input.match(regex);\\n            \\n            if (!match) {\\n                console.error('æ— æ³•è§£æè¾“å…¥:', input);\\n                return { time: getCurrentTime(), messages: [] };\\n            }\\n            \\n            const content = match[1].trim();\\n            console.log(`æå–åˆ°å†…å®¹é•¿åº¦: ${content.length}å­—ç¬¦`);\\n            \\n            // å®šä¹‰ç»“æœå¯¹è±¡\\n            let time = '';\\n            const messages = [];\\n            \\n            // ä¼˜å…ˆä½¿ç”¨æ›´ç²¾ç¡®çš„æ­£åˆ™è¡¨è¾¾å¼æå–æ—¶é—´\\n            const timeRegex = /\\\\[æ—¶é—´\\\\|(.*?)\\\\|(.*?)\\\\]/;\\n            const timeMatch = content.match(timeRegex);\\n            if (timeMatch && timeMatch[1] && timeMatch[2]) {\\n                time = timeMatch[2]; // ä½¿ç”¨ç¬¬ä¸‰ä¸ªæ•è·ç»„ä½œä¸ºæ—¶é—´\\n                console.log(`æå–åˆ°æ—¶é—´: ${time}`);\\n            }\\n            \\n            // æå–ç¾¤èŠæ¶ˆæ¯\\n            const groupMessageRegex = /\\\\[ç¾¤èŠ\\\\|(æ–°æ¶ˆæ¯|æ— æ–°æ¶ˆæ¯)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)(?=\\\\])/g;\\n            let groupMsgMatch;\\n            while ((groupMsgMatch = groupMessageRegex.exec(content)) !== null) {\\n                if (groupMsgMatch.length >= 7) {\\n                    const message = {\\n                        messageStatus: groupMsgMatch[1].trim(),\\n                        name: groupMsgMatch[2].trim(),\\n                        avatar: groupMsgMatch[3].trim(),\\n                        preview: groupMsgMatch[4].trim() === 'æ— ' ? '' : groupMsgMatch[4].trim(),\\n                        time: groupMsgMatch[5].trim(),\\n                        unread: groupMsgMatch[6].trim(),\\n                        messageType: 'group',\\n                        members: 'ç¾¤æˆå‘˜ä¿¡æ¯' // é»˜è®¤ç¾¤æˆå‘˜ä¿¡æ¯\\n                    };\\n                    messages.push(message);\\n                    console.log(`æå–åˆ°ç¾¤èŠæ¶ˆæ¯: ${message.name} - ${message.messageStatus}`);\\n                }\\n            }\\n            \\n            // æå–æ–°æ¶ˆæ¯\\n            const newMessageRegex = /\\\\[æ–°æ¶ˆæ¯\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)(?=\\\\])/g;\\n            let msgMatch;\\n            while ((msgMatch = newMessageRegex.exec(content)) !== null) {\\n                if (msgMatch.length >= 6) {\\n                    const message = {\\n                        name: msgMatch[1].trim(),\\n                        avatar: msgMatch[2].trim(),\\n                        preview: msgMatch[3].trim(),\\n                        time: msgMatch[4].trim(),\\n                        unread: msgMatch[5].trim(),\\n                        messageType: 'new'\\n                    };\\n                    messages.push(message);\\n                    console.log(`æå–åˆ°æ–°æ¶ˆæ¯: ${message.name} - ${message.preview}`);\\n                }\\n            }\\n            \\n            // æå–æ— æ–°æ¶ˆæ¯\\n            const noNewMessageRegex = /\\\\[æ— æ–°æ¶ˆæ¯\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)(?=\\\\])/g;\\n            let noMsgMatch;\\n            while ((noMsgMatch = noNewMessageRegex.exec(content)) !== null) {\\n                if (noMsgMatch.length >= 6) {\\n                    const message = {\\n                        name: noMsgMatch[1].trim(),\\n                        avatar: noMsgMatch[2].trim(),\\n                        preview: noMsgMatch[3].trim() === 'æ— ' ? '' : noMsgMatch[3].trim(),\\n                        time: noMsgMatch[4].trim(),\\n                        unread: noMsgMatch[5].trim(),\\n                        messageType: 'noNew'\\n                    };\\n                    messages.push(message);\\n                    console.log(`æå–åˆ°æ— æ–°æ¶ˆæ¯: ${message.name}`);\\n                }\\n            }\\n            \\n            // å¦‚æœä»ç„¶æ²¡æœ‰æ—¶é—´ï¼Œä½¿ç”¨å½“å‰æ—¶é—´\\n            if (!time) {\\n                time = getCurrentTime();\\n                console.log(`ä½¿ç”¨å½“å‰æ—¶é—´ä½œä¸ºé»˜è®¤å€¼: ${time}`);\\n            }\\n            \\n            console.log(`è§£æç»“æœ: æ—¶é—´=${time}, æ¶ˆæ¯æ•°é‡=${messages.length}`);\\n            return { time, messages };\\n        } catch (error) {\\n            console.error('è§£ææ¶ˆæ¯åˆ—è¡¨å†…å®¹æ—¶å‡ºé”™:', error);\\n            return { time: getCurrentTime(), messages: [] };\\n        }\\n    }\\n\\n    // è·å–å½“å‰æ—¶é—´çš„è¾…åŠ©å‡½æ•°\\n    function getCurrentTime() {\\n        const now = new Date();\\n        return `${String(now.getHours()).padStart(2, '0')}:${String(now.getMinutes()).padStart(2, '0')}`;\\n    }\\n\\n    // æ›´æ–°æ˜¾ç¤ºçš„å‡½æ•°\\n    function updateMessageDisplay(data) {\\n        const statusTimeElement = document.getElementById('status-time');\\n        const chatListContainer = document.querySelector('.chat-list-container');\\n        \\n        // æ›´æ–°æ—¶é—´\\n        if (statusTimeElement && data.time) {\\n            statusTimeElement.textContent = data.time;\\n        }\\n        \\n        // æ¸…é™¤ç°æœ‰çš„èŠå¤©åˆ—è¡¨ï¼ˆä¿ç•™çŠ¶æ€æ ï¼‰\\n        const existingList = chatListContainer.querySelectorAll('.chat-list-item');\\n        existingList.forEach(item => item.remove());\\n        \\n        // å®Œå…¨é¢å¤–æ’åºé€»è¾‘ - æ— æ–°æ¶ˆæ¯åœ¨å‰ï¼Œæœ‰æ–°æ¶ˆæ¯åœ¨å\\n        const sortedMessages = [...data.messages].sort((a, b) => {\\n            // æ£€æµ‹æ˜¯å¦æ˜¯ã€Œæ— æ–°æ¶ˆæ¯ã€\\n            const isNoNewA = a.hasOwnProperty('messageType') && a.messageType === 'noNew';\\n            const isNoNewB = b.hasOwnProperty('messageType') && b.messageType === 'noNew';\\n            \\n            // å¦‚æœä¸€ä¸ªæ˜¯æ— æ–°æ¶ˆæ¯ï¼Œä¸€ä¸ªæ˜¯æœ‰æ–°æ¶ˆæ¯ï¼Œåˆ™æ— æ–°æ¶ˆæ¯æ˜¾ç¤ºåœ¨åˆ—è¡¨å‰é¢\\n            if (isNoNewA && !isNoNewB) return -1; // æ— æ–°æ¶ˆæ¯åœ¨å‰\\n            if (!isNoNewA && isNoNewB) return 1;  // æœ‰æ–°æ¶ˆæ¯åœ¨å\\n            \\n            // å¦‚æœä¸¤æ¡æ¶ˆæ¯ç±»å‹ç›¸åŒï¼ˆéƒ½æ˜¯æœ‰æ–°æ¶ˆæ¯æˆ–éƒ½æ˜¯æ— æ–°æ¶ˆæ¯ï¼‰\\n            // åˆ™æŒ‰ç…§æœªè¯»æ•°é‡å‡åºæ’åˆ—ï¼ˆæœªè¯»æ•°é‡å°‘çš„åœ¨å‰é¢ï¼‰\\n            const unreadA = parseInt(a.unread || '0');\\n            const unreadB = parseInt(b.unread || '0');\\n            \\n            if (unreadA !== unreadB) {\\n                return unreadA - unreadB; // æœªè¯»æ•°é‡å°‘çš„åœ¨å‰\\n            }\\n            \\n            // å¦‚æœæœªè¯»æ•°é‡ç›¸åŒï¼ŒæŒ‰ç…§æ—¶é—´å‡åºæ’åˆ—ï¼ˆè¿™é‡Œæˆ‘ä»¬å°†è¾ƒæ—©çš„æ—¶é—´æ”¾åœ¨å‰é¢ï¼‰\\n            const timeA = a.time ? convertTimeToMinutes(a.time) : 0;\\n            const timeB = b.time ? convertTimeToMinutes(b.time) : 0;\\n            return timeA - timeB; // æ—©çš„æ—¶é—´åœ¨å‰\\n        });\\n        \\n        // å°†æ—¶é—´è½¬æ¢ä¸ºåˆ†é’Ÿæ•°çš„è¾…åŠ©å‡½æ•°\\n        function convertTimeToMinutes(timeStr) {\\n            const parts = timeStr.split(':');\\n            if (parts.length === 2) {\\n                const hours = parseInt(parts[0], 10);\\n                const minutes = parseInt(parts[1], 10);\\n                return hours * 60 + minutes;\\n            }\\n            return 0;\\n        }\\n        \\n        // æ·»åŠ æ–°çš„èŠå¤©åˆ—è¡¨\\n        sortedMessages.forEach(msg => {\\n            const chatItem = document.createElement('div');\\n            chatItem.className = 'chat-list-item';\\n            chatItem.setAttribute('data-name', msg.name);\\n            chatItem.setAttribute('data-unread', msg.unread);\\n            \\n            // å¤„ç†å¤´åƒURL\\n            let avatarUrl = msg.avatar;\\n            if (!avatarUrl.startsWith('http')) {\\n                // æå–æ–‡ä»¶åä¸­çš„è‹±æ–‡éƒ¨åˆ†ï¼ˆé€šå¸¸æ˜¯æœ€åçš„IDå’Œæ‰©å±•åï¼‰\\n                const filenameParts = msg.avatar.match(/([a-zA-Z0-9]+\\\\.[a-zA-Z]+)$/);\\n                if (filenameParts && filenameParts[1]) {\\n                    // æ‰¾åˆ°ç¬¦åˆæ¡ä»¶çš„è‹±æ–‡éƒ¨åˆ†\\n                    avatarUrl = `https://files.catbox.moe/${filenameParts[1]}`;\\n                } else {\\n                    // æœªæ‰¾åˆ°ç¬¦åˆæ¡ä»¶çš„éƒ¨åˆ†ï¼Œä½¿ç”¨æ•´ä¸ªæ–‡ä»¶å\\n                    avatarUrl = `https://files.catbox.moe/${msg.avatar}`;\\n                }\\n            }\\n            \\n            // å‡†å¤‡HTMLå†…å®¹ï¼Œå¦‚æœunreadä¸º0ï¼Œåˆ™ä¸æ˜¾ç¤ºè“è‰²æœªè¯»æ ‡è®°\\n            let unreadBadgeHTML = '';\\n            if (parseInt(msg.unread) > 0) {\\n                unreadBadgeHTML = `<div class=\\\"unread-badge\\\">${msg.unread}</div>`;\\n            } else {\\n                unreadBadgeHTML = `<div class=\\\"unread-badge\\\" style=\\\"display:none;\\\">0</div>`;\\n            }\\n            \\n            chatItem.innerHTML = `\\n                <img src=\\\"${avatarUrl}\\\" class=\\\"chat-avatar\\\" \\n                    onerror=\\\"this.onerror=null;console.log('å¤´åƒåŠ è½½å¤±è´¥: ${avatarUrl}');this.src='${DEFAULT_AVATAR_URL}';\\\"\\n                    onload=\\\"console.log('å¤´åƒåŠ è½½æˆåŠŸ: ${avatarUrl}')\\\">\\n                <div class=\\\"chat-info\\\">\\n                    <div class=\\\"chat-header\\\">\\n                        <spa