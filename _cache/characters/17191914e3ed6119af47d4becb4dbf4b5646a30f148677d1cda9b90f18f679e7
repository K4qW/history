   <script>\\n        // æ·»åŠ å…¨å±€é”™è¯¯å¤„ç†\\n        window.onerror = function(message, source, lineno, colno, error) {\\n            console.error(\\\"å…¨å±€é”™è¯¯æ•è·:\\\");\\n            console.error(\\\"é”™è¯¯æ¶ˆæ¯:\\\", message);\\n            console.error(\\\"é”™è¯¯æº:\\\", source);\\n            console.error(\\\"è¡Œå·:\\\", lineno);\\n            console.error(\\\"åˆ—å·:\\\", colno);\\n            console.error(\\\"é”™è¯¯å¯¹è±¡:\\\", error);\\n            \\n            // åœ¨é¡µé¢ä¸Šæ˜¾ç¤ºé”™è¯¯ä¿¡æ¯\\n            const errorInfo = document.createElement('div');\\n            errorInfo.style.cssText = \\\"position: fixed; top: 10px; left: 10px; background: rgba(255,0,0,0.8); color: white; padding: 10px; border-radius: 5px; z-index: 9999; max-width: 80%; word-break: break-word;\\\";\\n            errorInfo.innerHTML = `<strong>é”™è¯¯ä¿¡æ¯:</strong> ${message}<br><strong>ä½ç½®:</strong> ç¬¬${lineno}è¡Œ, ç¬¬${colno}åˆ—<br><strong>æ¥æº:</strong> ${source}`;\\n            document.body.appendChild(errorInfo);\\n            \\n            return true; // é˜»æ­¢é»˜è®¤çš„é”™è¯¯å¤„ç†\\n        };\\n        \\n        // æ·»åŠ æœªæ•è·çš„Promiseé”™è¯¯å¤„ç†\\n        window.addEventListener('unhandledrejection', function(event) {\\n            console.error(\\\"æœªå¤„ç†çš„Promiseæ‹’ç»:\\\", event.reason);\\n        });\\n\\n        // 1. æ­£æ–‡å†…å®¹æ¨¡æ¿\\n        let contentTemplate = `<content>$1</content>`;\\n\\n        // 2. æ•°æ®æå–å™¨\\n        const ContentExtractor = {\\n            // æå–ä¸»æ–‡æœ¬å†…å®¹\\n            extractMainText(content) {\\n                try {\\n                    const regex = /<maintext>([\\\\s\\\\S]*?)<\\\\/maintext>/;\\n                    const match = content.match(regex);\\n                    \\n                    if (!match) return null;\\n                    \\n                    // è·å–maintextæ ‡ç­¾å†…çš„å†…å®¹\\n                    const mainTextContent = match[1].trim();\\n                    \\n                    // å°è¯•æå–æ—§æ ¼å¼ [æ­£æ–‡|å†…å®¹]\\n                    const oldFormatMatch = mainTextContent.match(/\\\\[æ­£æ–‡\\\\|([\\\\s\\\\S]*?)\\\\]/);\\n                    if (oldFormatMatch) {\\n                        console.log('æ£€æµ‹åˆ°æ—§æ ¼å¼æ­£æ–‡å†…å®¹ï¼Œä½¿ç”¨[æ­£æ–‡|]æ ‡ç­¾è§£æ');\\n                        return oldFormatMatch[1].trim();\\n                    }\\n                    \\n                    // æ–°æ ¼å¼ç›´æ¥è¿”å›maintextæ ‡ç­¾å†…çš„å†…å®¹\\n                    console.log('ä½¿ç”¨æ–°æ ¼å¼è§£ææ­£æ–‡å†…å®¹ï¼Œç›´æ¥è·å–maintextæ ‡ç­¾å†…å®¹');\\n                    return mainTextContent;\\n                } catch (error) {\\n                    console.error('æå–ä¸»æ–‡æœ¬å†…å®¹å‡ºé”™:', error);\\n                    return null;\\n                }\\n            },\\n            \\n            // æå–åŸºæœ¬ä¿¡æ¯\\n            extractBasicInfo(content) {\\n                try {\\n                    const regex = /<basic_info>([\\\\s\\\\S]*?)<\\\\/basic_info>/;\\n                    const match = content.match(regex);\\n                    \\n                    if (!match) return null;\\n                    \\n                    const basicInfoContent = match[1].trim();\\n                    const result = {\\n                        date: '',\\n                        time: '',\\n                        weekday: '',\\n                        location: '',\\n                        weather: ''\\n                    };\\n                    \\n                    // æå–æ—¶é—´ä¿¡æ¯ [æ—¶é—´|æ—¥æœŸ|æ—¶é—´|æ˜ŸæœŸ]\\n                    const timeMatch = basicInfoContent.match(/\\\\[æ—¶é—´\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/);\\n                    if (timeMatch) {\\n                        result.date = timeMatch[1].trim();\\n                        result.time = timeMatch[2].trim();\\n                        result.weekday = timeMatch[3].trim();\\n                    }\\n                    \\n                    // æå–åœ°ç‚¹\\n                    const locationMatch = basicInfoContent.match(/\\\\[åœ°ç‚¹\\\\|(.*?)\\\\]/);\\n                    if (locationMatch) {\\n                        result.location = locationMatch[1].trim();\\n                    }\\n                    \\n                    // æå–å¤©æ°”\\n                    const weatherMatch = basicInfoContent.match(/\\\\[å¤©æ°”\\\\|(.*?)\\\\]/);\\n                    if (weatherMatch) {\\n                        result.weather = weatherMatch[1].trim();\\n                    }\\n                    \\n                    return result;\\n                } catch (error) {\\n                    console.error('æå–åŸºæœ¬ä¿¡æ¯å‡ºé”™:', error);\\n                    return null;\\n                }\\n            },\\n            \\n            // æå–å½“å‰äººç‰©ä¿¡æ¯\\n            extractCharacterInfo(content) {\\n                try {\\n                    const regex = /<current_character>([\\\\s\\\\S]*?)<\\\\/current_character>/;\\n                    const match = content.match(regex);\\n                    \\n                    if (!match) return null;\\n                    \\n                    const characterContent = match[1].trim();\\n                    const result = {\\n                        name: '',\\n                        thought: '',\\n                        status: ''\\n                    };\\n                    \\n                    // æå–äººç‰©å§“å\\n                    const nameMatch = characterContent.match(/\\\\[å½“å‰äººç‰©å§“å\\\\|(.*?)\\\\]/);\\n                    if (nameMatch) {\\n                        result.name = nameMatch[1].trim();\\n                    }\\n                    \\n                    // æå–å†…å¿ƒæƒ³æ³•\\n                    const thoughtMatch = characterContent.match(/\\\\[å†…å¿ƒæƒ³æ³•\\\\|([\\\\s\\\\S]*?)\\\\]/);\\n                    if (thoughtMatch) {\\n                        result.thought = thoughtMatch[1].trim();\\n                    }\\n                    \\n                    // æå–äººç‰©çŠ¶æ€\\n                    const statusMatch = characterContent.match(/\\\\[äººç‰©çŠ¶æ€\\\\|([\\\\s\\\\S]*?)\\\\]/);\\n                    if (statusMatch) {\\n                        result.status = statusMatch[1].trim();\\n                    }\\n                    \\n                    return result;\\n                } catch (error) {\\n                    console.error('æå–äººç‰©ä¿¡æ¯å‡ºé”™:', error);\\n                    return null;\\n                }\\n            },\\n            \\n            // æå–äº‹ä»¶åˆ—è¡¨\\n            extractEventList(content) {\\n                try {\\n                    const regex = /<event_list>([\\\\s\\\\S]*?)<\\\\/event_list>/;\\n                    const match = content.match(regex);\\n                    \\n                    if (!match) return [];\\n                    \\n                    const eventListContent = match[1].trim();\\n                    const events = [];\\n                    \\n                    // æå–æ‰€æœ‰äº‹ä»¶ä¿¡æ¯ [äº‹ä»¶|åç§°|ç®€ä»‹|æ—¶é—´|çŠ¶æ€]\\n                    const eventRegex = /\\\\[äº‹ä»¶\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/g;\\n                    let eventMatch;\\n                    \\n                    while ((eventMatch = eventRegex.exec(eventListContent)) !== null) {\\n                        events.push({\\n                            name: eventMatch[1].trim(),\\n                            description: eventMatch[2].trim(),\\n                            time: eventMatch[3].trim(),\\n                            status: eventMatch[4].trim()\\n                        });\\n                    }\\n                    \\n                    return events;\\n                } catch (error) {\\n                    console.error('æå–äº‹ä»¶åˆ—è¡¨å‡ºé”™:', error);\\n                    return [];\\n                }\\n            },\\n            \\n            // æå–å¥½æ„Ÿåº¦ä¿¡æ¯\\n            extractFavorability(content) {\\n                try {\\n                    const regex = /<favorability>([\\\\s\\\\S]*?)<\\\\/favorability>/;\\n                    const match = content.match(regex);\\n                    \\n                    if (!match) return [];\\n                    \\n                    const favorabilityContent = match[1].trim();\\n                    const favorabilityItems = [];\\n                    \\n                    // æå–æ‰€æœ‰å¥½æ„Ÿåº¦ä¿¡æ¯ [å¥½æ„Ÿåº¦|å§“å|å€¼|å˜åŒ–å€¼]\\n                    const favorRegex = /\\\\[å¥½æ„Ÿåº¦\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/g;\\n                    let favorMatch;\\n                    \\n                    while ((favorMatch = favorRegex.exec(favorabilityContent)) !== null) {\\n                        favorabilityItems.push({\\n                            name: favorMatch[1].trim(),\\n                            value: parseInt(favorMatch[2].trim()) || 0,\\n                            change: parseInt(favorMatch[3].trim()) || 0\\n                        });\\n                    }\\n                    \\n                    return favorabilityItems;\\n                } catch (error) {\\n                    console.error('æå–å¥½æ„Ÿåº¦ä¿¡æ¯å‡ºé”™:', error);\\n                    return [];\\n                }\\n            },\\n            \\n            // æå–å›¾ç‰‡ä¿¡æ¯\\n            extractImage(content) {\\n                try {\\n                    const regex = /<image>([\\\\s\\\\S]*?)<\\\\/image>/;\\n                    const match = content.match(regex);\\n                    \\n                    if (!match) return null;\\n                    \\n                    const imageContent = match[1].trim();\\n                    // æå–å›¾ç‰‡é“¾æ¥\\n                    const imageMatch = imageContent.match(/\\\\[å›¾ç‰‡\\\\|(.*?)\\\\]/);\\n                    \\n                    if (imageMatch) {\\n                        let imageUrl = imageMatch[1].trim();\\n                        // å¦‚æœæ³¨é‡Šä¸­æåˆ°çš„å¤„ç†å›¾ç‰‡é“¾æ¥ï¼Œè¿™é‡Œå¯ä»¥æ·»åŠ ç›¸åº”çš„é€»è¾‘\\n                        if (imageUrl.match(/[\\\\u4e00-\\\\u9fa5]+.*?\\\\.\\\\w+/)) {\\n                            // æå–æ–‡ä»¶åç¼€\\n                            const extension = imageUrl.match(/\\\\.([^.]+)$/)[1];\\n                            // ç§»é™¤ä¸­æ–‡å­—ç¬¦ï¼Œåªä¿ç•™å­—æ¯ã€æ•°å­—å’Œåç¼€\\n                            const fileCode = imageUrl.replace(/[\\\\u4e00-\\\\u9fa5]+/g, '').trim();\\n                            // æ·»åŠ å‰ç¼€\\n                            imageUrl = `https://files.catbox.moe/${fileCode}`;\\n                        }\\n                        return imageUrl;\\n                    }\\n                    \\n                    return null;\\n                } catch (error) {\\n                    console.error('æå–å›¾ç‰‡ä¿¡æ¯å‡ºé”™:', error);\\n                    return null;\\n                }\\n            },\\n            \\n            // æå–è¡ŒåŠ¨é€‰é¡¹\\n            extractActionOptions(content) {\\n                try {\\n                    const regex = /<action_option>([\\\\s\\\\S]*?)<\\\\/action_option>/;\\n                    const match = content.match(regex);\\n                    \\n                    if (!match) return [];\\n                    \\n                    const optionContent = match[1].trim();\\n                    // æå–è¡ŒåŠ¨é€‰é¡¹ [è¡ŒåŠ¨é€‰é¡¹|é€‰é¡¹1ï¼šå†…å®¹|é€‰é¡¹2ï¼šå†…å®¹|é€‰é¡¹3ï¼šå†…å®¹|é€‰é¡¹4ï¼šå†…å®¹]\\n                    const optionMatch = optionContent.match(/\\\\[è¡ŒåŠ¨é€‰é¡¹\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/);\\n                    \\n                    if (optionMatch) {\\n                        return [\\n                            optionMatch[1].trim().replace(/é€‰é¡¹\\\\d+ï¼š/, ''),\\n                            optionMatch[2].trim().replace(/é€‰é¡¹\\\\d+ï¼š/, ''),\\n                            optionMatch[3].trim().replace(/é€‰é¡¹\\\\d+ï¼š/, ''),\\n                            optionMatch[4].trim().replace(/é€‰é¡¹\\\\d+ï¼š/, '')\\n                        ];\\n                    }\\n                    \\n                    return [];\\n                } catch (error) {\\n                    console.error('æå–è¡ŒåŠ¨é€‰é¡¹å‡ºé”™:', error);\\n                    return [];\\n                }\\n            }\\n        };\\n\\n        // 3. UIæ›´æ–°å™¨\\n        const UIUpdater = {\\n            updateMainContent(container, data) {\\n                // å¦‚æœæ²¡æœ‰æ•°æ®ï¼Œæ˜¾ç¤ºä¸€ä¸ªå ä½ç¬¦å†…å®¹\\n                if (!data.mainText && !data.basicInfo) {\\n                    container.innerHTML = `\\n                        <div class=\\\"story-card\\\">\\n                            <div class=\\\"info-bar\\\">\\n                                <div class=\\\"info-group\\\">\\n                                    <svg class=\\\"icon\\\" viewBox=\\\"0 0 24 24\\\">\\n                                        <path d=\\\"M12,20A8,8 0 0,0 20,12A8,8 0 0,0 12,4A8,8 0 0,0 4,12A8,8 0 0,0 12,20M12,2A10,10 0 0,1 22,12A10,10 0 0,1 12,22C6.47,22 2,17.5 2,12A10,10 0 0,1 12,2M12.5,7V12.25L17,14.92L16.25,16.15L11,13V7H12.5Z\\\"/>\\n                                    </svg>\\n                                    <span>åŠ è½½ä¸­...</span>\\n                                </div>\\n                            </div>\\n                            <div class=\\\"story-content\\\">æ­£åœ¨åŠ è½½å†…å®¹...</div>\\n                        </div>\\n                    `;\\n                    return;\\n                }\\n\\n                // æ„å»ºåŸºæœ¬ä¿¡æ¯æ \\n                let infoBarHTML = '';\\n                if (data.basicInfo) {\\n                    const { date, time, weekday, location, weather } = data.basicInfo;\\n                    \\n                    infoBarHTML = `\\n                        <div class=\\\"info-bar\\\">\\n                            <div class=\\\"info-group\\\">\\n                                <svg class=\\\"icon\\\" viewBox=\\\"0 0 24 24\\\">\\n                                    <path d=\\\"M12,20A8,8 0 0,0 20,12A8,8 0 0,0 12,4A8,8 0 0,0 4,12A8,8 0 0,0 12,20M12,2A10,10 0 0,1 22,12A10,10 0 0,1 12,22C6.47,22 2,17.5 2,12A10,10 0 0,1 12,2M12.5,7V12.25L17,14.92L16.25,16.15L11,13V7H12.5Z\\\"/>\\n                                </svg>\\n                                <span>${date || '--'} ${time || '--'} ${weekday || ''}</span>\\n                            </div>\\n                            <div class=\\\"info-group\\\">\\n                                <svg class=\\\"icon\\\" viewBox=\\\"0 0 24 24\\\">\\n                                    <path d=\\\"M12,11.5A2.5,2.5 0 0,1 9.5,9A2.5,2.5 0 0,1 12,6.5A2.5,2.5 0 0,1 14.5,9A2.5,2.5 0 0,1 12,11.5M12,2A7,7 0 0,0 5,9C5,14.25 12,22 12,22C12,22 19,14.25 19,9A7,7 0 0,0 12,2Z\\\"/>\\n                                </svg>\\n                                <span>${location || 'æœªçŸ¥åœ°ç‚¹'}</span>\\n                            </div>\\n                            <div class=\\\"info-group\\\">\\n                                <svg class=\\\"icon\\\" viewBox=\\\"0 0 24 24\\\">\\n                                    <path d=\\\"M6,19A5,5 0 0,1 1,14A5,5 0 0,1 6,9C7,6.65 9.3,5 12,5C15.43,5 18.24,7.66 18.5,11.03L19,11A4,4 0 0,1 23,15A4,4 0 0,1 19,19H6M19,13H17V12A5,5 0 0,0 12,7C9.5,7 7.45,8.82 7.06,11.19C6.73,11.07 6.37,11 6,11A3,3 0 0,0 3,14A3,3 0 0,0 6,17H19A2,2 0 0,0 21,15A2,2 0 0,0 19,13Z\\\"/>\\n                                </svg>\\n                                <span>${weather || 'æ™´æœ—'}</span>\\n                            </div>\\n                        </div>\\n                    `;\\n                }\\n\\n                // æ„å»ºæ­£æ–‡å†…å®¹\\n                let storyContentHTML = '';\\n                if (data.mainText) {\\n                    storyContentHTML = `\\n                        <div class=\\\"story-content\\\">${data.mainText}</div>\\n                    `;\\n                }\\n\\n                // æ„å»ºæ•…äº‹å¡ç‰‡\\n                const storyCardHTML = `\\n                    <div class=\