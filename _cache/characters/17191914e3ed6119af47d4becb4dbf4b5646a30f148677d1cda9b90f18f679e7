          \\n                    // ä¿å­˜åˆ°èŠå¤©å˜é‡\\n                    if (typeof updateVariablesWith === 'function') {\\n                        updateVariablesWith(variables => {\\n                            // ä¿å­˜ä¸º\\\"æˆ‘å‘å¸ƒçš„å¸–å­\\\"å˜é‡\\n                            variables['æˆ‘å‘å¸ƒçš„å¸–å­'] = postData;\\n                            return variables;\\n                        }).then(() => {\\n                            console.log('å¸–å­æ•°æ®å·²ä¿å­˜åˆ°èŠå¤©å˜é‡:', postData);\\n                        }).catch(error => {\\n                            console.error('ä¿å­˜å¸–å­æ•°æ®åˆ°èŠå¤©å˜é‡æ—¶å‡ºé”™:', error);\\n                        });\\n                    } else if (typeof setVariable === 'function') {\\n                        setVariable('æˆ‘å‘å¸ƒçš„å¸–å­', postData).then(() => {\\n                            console.log('å¸–å­æ•°æ®å·²ä¿å­˜åˆ°èŠå¤©å˜é‡:', postData);\\n                        }).catch(error => {\\n                            console.error('ä¿å­˜å¸–å­æ•°æ®åˆ°èŠå¤©å˜é‡æ—¶å‡ºé”™:', error);\\n                        });\\n                    } else {\\n                        console.warn('æ— æ³•ä¿å­˜å¸–å­æ•°æ®åˆ°èŠå¤©å˜é‡ï¼Œç›¸å…³APIä¸å¯ç”¨');\\n                    }\\n                } catch (error) {\\n                    console.error('ä¿å­˜å¸–å­æ•°æ®æ—¶å‡ºé”™:', error);\\n                }\\n            }\\n        });\\n    </script>\\n</body>\\n</html>\\n\\n```\",\"trimStrings\":[],\"placement\":[2],\"disabled\":false,\"markdownOnly\":true,\"promptOnly\":false,\"runOnEdit\":true,\"substituteRegex\":0,\"minDepth\":null,\"maxDepth\":2},{\"id\":\"1189119b-40ed-4bb3-8c8b-ef5b3fc41610\",\"scriptName\":\"è®ºå›å‘ç°\",\"findRegex\":\"<find>([\\\\s\\\\S]*?)</find>\",\"replaceString\":\"```\\n<!DOCTYPE html>\\n<html lang=\\\"zh-CN\\\">\\n<head>\\n    <meta charset=\\\"UTF-8\\\">\\n    <meta name=\\\"viewport\\\" content=\\\"width=device-width, initial-scale=1.0\\\">\\n    <title>å°çº¢ä¹¦é£æ ¼å‘ç°é¡µ</title>\\n    <style>\\