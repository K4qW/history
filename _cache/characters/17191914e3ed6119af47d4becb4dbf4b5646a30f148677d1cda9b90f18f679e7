         statusClass = 'in-progress';\\n                        } else if (event.status === 'æœªå¼€å§‹') {\\n                            statusClass = 'pending';\\n                        }\\n                        \\n                        eventsItemsHTML += `\\n                            <div class=\\\"event-item\\\">\\n                                <div class=\\\"event-name\\\">${event.name}</div>\\n                                <div class=\\\"event-description\\\">${event.description}</div>\\n                                <div class=\\\"event-footer\\\">\\n                                    <div class=\\\"event-time\\\">${event.time}</div>\\n                                    <div class=\\\"event-status ${statusClass}\\\">${event.status}</div>\\n                                </div>\\n                            </div>\\n                        `;\\n                    });\\n                    \\n                    eventsCardHTML = `\\n                        <div class=\\\"events-card\\\">\\n                            <div class=\\\"events-header\\\" onclick=\\\"toggleEvents()\\\">\\n                                <div class=\\\"events-title\\\">äº‹ä»¶åˆ—è¡¨</div>\\n                                <div class=\\\"events-toggle\\\">\\n                                    <svg viewBox=\\\"0 0 24 24\\\">\\n                                        <path d=\\\"M7,10L12,15L17,10H7Z\\\" />\\n                                    </svg>\\n                                </div>\\n                            </div>\\n                            <div class=\\\"events-content\\\" id=\\\"eventsContent\\\">\\n                                ${eventsItemsHTML}\\n                            </div>\\n                        </div>\\n                    `;\\n                }\\n\\n                // æ„å»ºå¥½æ„Ÿåº¦å®¹å™¨\\n                let favorabilityHTML = '';\\n                if (data.favorability && data.favorability.length > 0) {\\n                    let favorabilityItemsHTML = '';\\n                    \\n                    data.favorability.forEach(item => {\\n                        const percentage = Math.min(Math.max(item.value, 0), 100);\\n                        const changeClass = item.change > 0 ? 'positive' : (item.change < 0 ? 'negative' : '');\\n                        const changeSign = item.change > 0 ? '+' : '';\\n                        \\n                        favorabilityItemsHTML += `\\n                            <div class=\\\"favorability-item\\\">\\n                                <div class=\\\"favorability-name\\\">${item.name}</div>\\n                                <div class=\\\"favorability-bar\\\">\\n                                    <div class=\\\"favorability-value\\\" style=\\\"width: ${percentage}%;\\\"></div>\\n                                </div>\\n                                <div class=\\\"favorability-number\\\">${item.value}</div>\\n                                <div class=\\\"favorability-change-container\\\">\\n                                    ${item.change !== 0 ? `<div class=\\\"favorability-change ${changeClass}\\\">${changeSign}${item.change}</div>` : ''}\\n                                </div>\\n                            </div>\\n                        `;\\n                    });\\n                    \\n                    favorabilityHTML = `\\n                        <div class=\\\"favorability-container\\\">\\n                            <div class=\\\"favorability-title\\\">äººç‰©å¥½æ„Ÿåº¦</div>\\n                            ${favorabilityItemsHTML}\\n                        </div>\\n                    `;\\n                }\\n\\n                // æ„å»ºå›¾ç‰‡å®¹å™¨\\n                let imageHTML = '';\\n                if (data.image) {\\n                    imageHTML = `\\n                        <div class=\\\"image-container\\\">\\n                            <img src=\\\"${data.image}\\\" class=\\\"story-image\\\" alt=\\\"æ•…äº‹å›¾ç‰‡\\\">\\n                        </div>\\n                    `;\\n                }\\n\\n                // æ„å»ºé€‰é¡¹æŒ‰é’®\\n                let optionsHTML = '';\\n                if (data.options && data.options.length > 0) {\\n                    let optionButtonsHTML = '';\\n                    \\n                    data.options.forEach((option, index) => {\\n                        if (option) {\\n                            // å®Œå…¨é‡å†™è½¬ä¹‰å’ŒæŒ‰é’®HTMLç”Ÿæˆé€»è¾‘\\n                            // ä¸ºé¿å…å­—ç¬¦ä¸²æ¨¡æ¿æ’å€¼å¸¦æ¥çš„é—®é¢˜ï¼Œä½¿ç”¨DOM APIåˆ›å»ºå…ƒç´ \\n                            console.log(`å¤„ç†é€‰é¡¹ ${index + 1}:`, option);\\n                            \\n                            // ä½¿ç”¨å‡½æ•°ç”Ÿæˆå®‰å…¨çš„HTML\\n                            const safeOptionHtml = createSafeOptionButton(option, index + 1);\\n                            optionButtonsHTML += safeOptionHtml;\\n                        }\\n                    });\\n                    \\n                    optionsHTML = `\\n                        <div class=\\\"options-container\\\">\\n                            ${optionButtonsHTML}\\n                        </div>\\n                    `;\\n                }\\n\\n                // æ„å»ºæ¶ˆæ¯è¾“å…¥åŒºåŸŸ\\n                const inputAreaHTML = `\\n                    <div class=\\\"input-area\\\">\\n                        <input type=\\\"text\\\" class=\\\"message-input\\\" id=\\\"messageInput\\\" placeholder=\\\"è¾“å…¥è‡ªå®šä¹‰å›å¤...\\\" onkeypress=\\\"if(event.key === 'Enter') sendMessage()\\\">\\n                        <button class=\\\"send-button\\\" onclick=\\\"sendMessage()\\\">å‘é€</button>\\n                    </div>\\n                `;\\n\\n                // ç»„åˆæ‰€æœ‰HTMLå¹¶æ›´æ–°å®¹å™¨\\n                container.innerHTML = `\\n                    ${storyCardHTML}\\n                    ${characterCardHTML}\\n                    ${thoughtCardHTML}\\n                    ${eventsCardHTML}\\n                    ${favorabilityHTML}\\n                    ${imageHTML}\\n                    ${optionsHTML}\\n                    ${inputAreaHTML}\\n                `;\\n                \\n                // é‡è¦ï¼šåœ¨æ’å…¥HTMLåé‡æ–°ç»‘å®šæ‰€æœ‰é€‰é¡¹æŒ‰é’®çš„ç‚¹å‡»äº‹ä»¶\\n                this.bindOptionButtonEvents(container, data.options);\\n            },\\n            \\n            // æ·»åŠ ç»‘å®šé€‰é¡¹æŒ‰é’®äº‹ä»¶çš„æ–¹æ³•\\n            bindOptionButtonEvents(container, options) {\\n                if (!options || !options.length) return;\\n                \\n                const buttonElements = container.querySelectorAll('.option-button');\\n                if (!buttonElements || buttonElements.length === 0) {\\n                    console.warn('æœªæ‰¾åˆ°é€‰é¡¹æŒ‰é’®å…ƒç´ ');\\n                    return;\\n                }\\n                \\n                console.log(`æ‰¾åˆ° ${buttonElements.length} ä¸ªé€‰é¡¹æŒ‰é’®ï¼Œå¼€å§‹ç»‘å®šäº‹ä»¶`);\\n                \\n                buttonElements.forEach((button, index) => {\\n                    // åªå¤„ç†å­˜åœ¨çš„é€‰é¡¹\\n                    if (index < options.length && options[index]) {\\n                        const optionText = options[index];\\n                        const optionNumber = index + 1;\\n                        \\n                        // æ¸…é™¤ç°æœ‰äº‹ä»¶ï¼ˆå¦‚æœæœ‰ï¼‰\\n                        button.onclick = null;\\n                        \\n                        // æ·»åŠ æ–°çš„ç‚¹å‡»äº‹ä»¶\\n                        button.onclick = function() {\\n                            console.log(`ç‚¹å‡»äº†é€‰é¡¹ ${optionNumber}: ${optionText}`);\\n                            selectOption(optionNumber, optionText);\\n                        };\\n                        \\n                        console.log(`å·²ç»‘å®šé€‰é¡¹ ${optionNumber} çš„ç‚¹å‡»äº‹ä»¶`);\\n                    }\\n                });\\n            }\\n        };\\n\\n        // 4. é€‰é¡¹é€‰æ‹©å¤„ç†\\n        function selectOption(optionNumber, optionContent) {\\n            try {\\n                console.log(`é€‰é¡¹è¢«ç‚¹å‡»: optionNumber=${optionNumber}, optionContent=${optionContent}`);\\n                \\n                if (typeof triggerSlash === 'function') {\\n                    console.log(`å‡†å¤‡è§¦å‘Slashå‘½ä»¤: /send ã€æ­£æ–‡ç•Œé¢ã€‘<Request::å»¶ç»­å½“å‰å‰§æƒ…{{User}}å†³å®š${optionContent}>|/trigger`);\\n                    triggerSlash(`/send ã€æ­£æ–‡ç•Œé¢ã€‘<Request::å»¶ç»­å½“å‰å‰§æƒ…{{User}}å†³å®š${optionContent}>|/trigger`);\\n                } else {\\n                    console.log(`é€‰æ‹©é€‰é¡¹: ${optionNumber} - ${optionContent}`);\\n                }\\n            } catch (error) {\\n                console.error(\\\"selectOptionå‡½æ•°æ‰§è¡Œé”™è¯¯:\\\", error);\\n                console.error(\\\"é”™è¯¯å †æ ˆ:\\\", error.stack);\\n                alert(`é€‰é¡¹é€‰æ‹©é”™è¯¯: ${error.message}`);\\n            }\\n        }\\n\\n        // åˆ‡æ¢äº‹ä»¶åˆ—è¡¨çš„å±•å¼€å’ŒæŠ˜å \\n        function toggleEvents() {\\n            try {\\n                const eventsContent = document.getElementById('eventsContent');\\n                if (eventsContent) {\\n                    eventsContent.classList.toggle('expanded');\\n                    \\n                    // è·å–åˆ‡æ¢æŒ‰é’®å›¾æ ‡å¹¶æ—‹è½¬\\n                    const toggleIcon = document.querySelector('.events-toggle svg');\\n                    if (toggleIcon) {\\n                        if (eventsContent.classList.contains('expanded')) {\\n                            toggleIcon.style.transform = 'rotate(180deg)';\\n                        } else {\\n                            toggleIcon.style.transform = 'rotate(0deg)';\\n                        }\\n                    }\\n                    \\n                    console.log('äº‹ä»¶åˆ—è¡¨çŠ¶æ€åˆ‡æ¢æˆåŠŸ');\\n                }\\n            } catch (error) {\\n                console.error(\\\"toggleEventså‡½æ•°æ‰§è¡Œé”™è¯¯:\\\", error);\\n                consol