      if (streamerMatch && streamerMatch[1]) {\\n                        const streamerInfo = streamerMatch[1].split('|');\\n                        result.basicInfo.streamerName = streamerInfo[0] || '';\\n                        result.basicInfo.streamerNickname = streamerInfo[1] || '';\\n                        let avatarName = streamerInfo[2] || '';\\n                        // å¤„ç†å¤´åƒæ–‡ä»¶åï¼Œå»é™¤ä¸­æ–‡å‰ç¼€\\n                        if (avatarName && !avatarName.startsWith('http')) {\\n                            const match = avatarName.match(/[a-zA-Z0-9]+\\\\.[a-zA-Z]+$/i);\\n                            if (match) {\\n                                result.basicInfo.streamerAvatar = 'https://files.catbox.moe/' + match[0];\\n                            }\\n                        }\\n                    }\\n                    \\n                    // æå–ç›´æ’­é—´æ ‡é¢˜\\n                    const titleMatch = basicInfoText.match(/\\\\[ç›´æ’­é—´æ ‡é¢˜\\\\|([^\\\\]]+)\\\\]/);\\n                    if (titleMatch && titleMatch[1]) {\\n                        result.basicInfo.roomTitle = titleMatch[1];\\n                    }\\n                    \\n                    // æå–è§‚çœ‹äººæ•°\\n                    const viewerMatch = basicInfoText.match(/\\\\[è§‚çœ‹äººæ•°\\\\|([^\\\\]]+)\\\\]/);\\n                    if (viewerMatch && viewerMatch[1]) {\\n                        result.basicInfo.viewerCount = viewerMatch[1];\\n                    }\\n                    \\n                    // æå–ç‚¹èµäººæ•°\\n                    const likeMatch = basicInfoText.match(/\\\\[ç‚¹èµäººæ•°\\\\|([^\\\\]]+)\\\\]/);\\n                    if (likeMatch && likeMatch[1]) {\\n                        result.basicInfo.likeCount = likeMatch[1];\\n                    }\\n                    \\n                    // æå–å…³æ³¨äººæ•°\\n                    const followMatch = basicInfoText.match(/\\\\[å…³æ³¨äººæ•°\\\\|([^\\\\]]+)\\\\]/);\\n                    if (followMatch && followMatch[1]) {\\n                        result.basicInfo.followCount = followMatch[1];\\n                    }\\n                    \\n                    // æå–æ’è¡Œæ¦œ\\n                    const rankMatch = basicInfoText.match(/\\\\[ç§¯åˆ†æ’è¡Œæ¦œ\\\\|([^\\\\]]+)\\\\]/);\\n                    if (rankMatch && rankMatch[1]) {\\n                        const rankInfo = rankMatch[1].split('|');\\n                        result.basicInfo.rankList = [];\\n                        \\n                        for (let i = 0; i < rankInfo.length; i += 2) {\\n                            if (rankInfo[i] && rankInfo[i+1]) {\\n                                result.basicInfo.rankList.push({\\n                                    name: rankInfo[i],\\n                                    score: rankInfo[i+1]\\n                                });\\n                            }\\n                        }\\n                    }\\n                }\\n                \\n                // ä¿®æ”¹ï¼šç›´æ¥ä»æ•´ä¸ªXMLä¸­æå–ä¸»æ’­çŠ¶æ€å’Œæƒ³æ³•ï¼Œä¸ä¾èµ–äºæ ‡ç­¾ç»“æ„\\n                console.log(\\\"å°è¯•ç›´æ¥æå–ä¸»æ’­çŠ¶æ€å’Œæƒ³æ³•...\\\");\\n                const statusMatch = xmlText.match(/\\\\[ä¸»æ’­å½“å‰çŠ¶æ€\\\\|([\\\\s\\\\S]*?)\\\\]/);\\n                if (statusMatch && statusMatch[1]) {\\n                    result.characterInfo.streamerStatus = statusMatch[1];\\n                    console.log(\\\"æˆåŠŸæå–ä¸»æ’­çŠ¶æ€:\\\", result.characterInfo.streamerStatus);\\n                } else {\\n                    console.log(\\\"æœªæ‰¾åˆ°ä¸»æ’­çŠ¶æ€\\\");\\n                }\\n                \\n                const thoughtMatch = xmlText.match(/\\\\[ä¸»æ’­å½“å‰æƒ³æ³•\\\\|([\\\\s\\\\S]*?)\\\\]/);\\n                if (thoughtMatch && thoughtMatch[1]) {\\n                    result.characterInfo.streamerThought = thoughtMatch[1];\\n                    console.log(\\\"æˆåŠŸæå–ä¸»æ’­æƒ³æ³•:\\\", result.characterInfo.streamerThought);\\n                } else {\\n                    console.log(\\\"æœªæ‰¾åˆ°ä¸»æ’­æƒ³æ³•\\\");\\n                }\\n                \\n                // æå–ä¸»æ’­å‘è¨€\\n                const dialogueMatch = xmlText.match(/<dialogue>([\\\\s\\\\S]*?)<\\\\/dialogue>/i);\\n                if (dialogueMatch && dialogueMatch[1]) {\\n                    const dialogueText = dialogueMatch[1].trim();\\n                    const dialogueRegex = /\\\\[ä¸»æ’­å‘è¨€\\\\|([^\\\\]]+)\\\\]/g;\\n                    let match;\\n                    \\n                    while ((match = dialogueRegex.exec(dialogueText)) !== null) {\\n                        if (match[1]) {\\n                            result.dialogue.push(match[1]);\\n                        }\\n                    }\\n                }\\n                \\n                // æå–å¼¹å¹•\\n                const barrageMatch = xmlText.match(/<barrage>([\\\\s\\\\S]*?)<\\\\/barrage>/i);\\n                if (barrageMatch && barrageMatch[1]) {\\n                    const barrageText = barrageMatch[1].trim();\\n                    const barrageRegex = /\\\\[å¼¹å¹•\\\\|([^\\\\]]+)\\\\]/g;\\n                    let match;\\n                    \\n                    while ((match = barrageRegex.exec(barrageText)) !== null) {\\n                        if (match[1]) {\\n                            const parts = match[1].split('|');\\n                            if (parts.length >= 2) {\\n                                result.barrage.push({\\n                                    user: parts[0],\\n                                    content: parts[1]\\n                                });\\n                            }\\n                        }\\n                    }\\n                }\\n                \\n                // æå–å›¾ç‰‡\\n                const imageMatch = xmlText.match(/<image>([\\\\s\\\\S]*?)<\\\\/image>/i);\\n                if (imageMatch && imageMatch[1]) {\\n                    const imageText = imageMatch[1].trim();\\n                    const imgMatch = imageText.match(/\\\\[å›¾ç‰‡\\\\|([^\\\\]]+)\\\\]/);\\n                    \\n                    if (imgMatch && imgMatch[1]) {\\n                        // å¤„ç†å›¾ç‰‡é“¾æ¥ï¼Œå»é™¤ä¸­æ–‡å¹¶æ·»åŠ å‰ç¼€\\n                        let imgUrl = imgMatch[1];\\n                        if (!imgUrl.startsWith('http')) {\\n                            const match = imgUrl.match(/[a-zA-Z0-9]+\\\\.[a-zA-Z]+$/i);\\n                            if (match) {\\n                                imgUrl = 'https://files.catbox.moe/' + match[0];\\n                            } else {\\n                                imgUrl = 'https://files.catbox.moe/' + imgUrl;\\n                            }\\n                        }\\n                        result.image = imgUrl;\\n                    }\\n                }\\n                \\n                console.log(\\\"XMLè§£æå®Œæˆï¼Œè¿”å›ç»“æœ:\\\", result);\\n                return result;\\n            } catch (error) {\\n                console.error('è§£æç›´æ’­XMLå‡ºé”™:', error);\\n                return result;\\n            }\\n        }\\n        \\n        // åº”ç”¨ç›´æ’­ä¿¡æ¯åˆ°ç•Œé¢\\n        function applyLiveInfo(liveInfo) {\\n            console.log(\\\"å¼€å§‹åº”ç”¨ç›´æ’­ä¿¡æ¯åˆ°ç•Œé¢...\\\");\\n            \\n            // æ›´æ–°æ—¶é—´\\n            if (liveInfo.basicInfo.time) {\\n                document.getElementById('liveTime').textContent = liveInfo.basicInfo.time;\\n                document.getElementById('liveTime').setAttribute('data-from-xml', 'true');\\n            }\\n            \\n            // æ›´æ–°ä¸»æ’­ä¿¡æ¯\\n            if (liveInfo.basicInfo.streamerName || liveInfo.basicInfo.streamerNickname) {\\n                const displayName = liveInfo.basicInfo.streamerNickname || liveInfo.basicInfo.streamerName;\\n                document.getElementById('streamerName').textContent = displayName;\\n            }\\n            \\n            if (liveInfo.basicInfo.streamerAvatar) {\\n                document.getElementById('streamerAvatar').src = liveInfo.basicInfo.streamerAvatar;\\n            }\\n            \\n            // æ›´æ–°ç›´æ’­é—´ä¿¡æ¯\\n            if (liveInfo.basicInfo.viewerCount) {\\n                document.getElementById('onlineCount').textContent = liveInfo.basicInfo.viewerCount + ' äººæ­£åœ¨è§‚çœ‹';\\n            }\\n            \\n            if (liveInfo.basicInfo.likeCount) {\\n                document.getElementById('likeCount').textContent = liveInfo.basicInfo.likeCount;\\n            }\\n            \\n            if (liveInfo.basicInfo.followCount) {\\n                document.getElementById('followCount').textContent = liveInfo.basicInfo.followCount;\\n            }\\n            \\n            // æ›´æ–°ç›´æ’­å›¾ç‰‡\\n            if (liveInfo.image) {\\n                document.getElementById('streamImage').src = liveInfo.image;\\n            }\\n            \\n            // æ›´æ–°ä¸»æ’­æ¶ˆæ¯\\n            if (liveInfo.dialogue && liveInfo.dialogue.length > 0) {\\n                let messageIndex = 0;\\n                const streamerMessageElement = document.getElementById('streamerMessage');\\n                const streamerName = liveInfo.basicInfo.streamerNickname || liveInfo.basicInfo.streamerName || 'ä¸»æ’­';\\n                \\n                // æ›´æ–°æ˜¾ç¤ºæ–¹å¼ï¼Œæ·»åŠ ä¸»æ’­åå­—å‰ç¼€\\n                function updateMessageWithName() {\\n                    const namePrefix = `<span class=\\\"streamer-name-prefix\\\">${streamerName}ï¼š</span>`;\\n                    streamerMessageElement.innerHTML = namePrefix + liveInfo.dialogue[messageIndex];\\n                }\\n                \\n                updateMessageWithName();\\n                \\n                // ç‚¹å‡»æ¶ˆæ¯åˆ‡æ¢åˆ°ä¸‹ä¸€æ¡\\n                if (liveInfo.dialogue.length > 1) {\\n                    streamerMessageElement.classList.add('clickable');\\n                    streamerMessageElement.title = 'ç‚¹å‡»æŸ¥çœ‹ä¸‹ä¸€æ¡æ¶ˆæ¯';\\n                    streamerMessageElement.addEventListener('click', () => {\\n                        // æ·»åŠ ç‚¹å‡»åŠ¨ç”»æ•ˆæœ\\n                        streamerMessageElement.style.backgroundColor = 'rgba(80, 80, 80, 0.7)';\\n                        setTimeout(() => {\\n                            streamerMessageElement.style.backgroundColor = '';\\n                        }, 150);\\n                        \\n                        messageIndex = (messageIndex + 1) % liveInfo.dialogue.length;\\n                        updateMessageWithName();\\n                    });\\n                }\\n            }\\n            \\n            // æ›´æ–°ä¸»æ’­çŠ¶æ€å’Œæƒ³æ³•\\n            if (liveInfo.characterInfo && liveInfo.characterInfo.streamerStatus) {\\n                console.log(\\\"æ›´æ–°ä¸»æ’­çŠ¶æ€:\\\", liveInfo.characterInfo.streamerStatus);\\n                document.getElementById('streamerStatus').textContent = liveInfo.characterInfo.streamerStatus;\\n            } else {\\n                console.log(\\\"æ²¡æœ‰ä¸»æ’­çŠ¶æ€ä¿¡æ¯å¯æ›´æ–°\\\");\\n            }\\n            \\n            if (liveInfo.characterInfo && liveInfo.characterInfo.streamerThought) {\\n                console.log(\\\"æ›´æ–°ä¸»æ’­æƒ³æ³•:\\\", liveInfo.characterInfo.streamerThought);\\n                document.getElementById('streamerThought').textContent = liveInfo.characterInfo.streamerThought;\\n            } else {\\n                console.log(\\\"æ²¡æœ‰ä¸»æ’­æƒ³æ³•ä¿¡æ¯å¯æ›´æ–°\\\");\\n            }\\n            \\n            // æ›´æ–°å¼¹å¹•å†…å®¹\\n            if (liveInfo.barrage && liveInfo.barrage.length > 0) {\\n                // æ›¿æ¢é»˜è®¤å¼¹å¹•å†…å®¹\\n                exampleDanmaku = liveInfo.barrage.map(item => item.content);\\n            }\\n            \\n            // åˆ›å»ºæ’è¡Œæ¦œ\\n            if (liveInfo.basicInfo.rankList && liveInfo.basicInfo.rankList.length > 0) {\\n                createRankingList(liveInfo.basicInfo.rankList);\\n            }\\n            \\n            // æ ‡è®°æ•°æ®æ¥æºä¸ºXML\\n            document.getElementById('followCount').setAttribute('data-from-xml', 'true');\\n            \\n            // è‡ªåŠ¨å¯åŠ¨å¼¹å¹•\\n            toggleDanmaku();\\n            \\n            // æ·»åŠ è§‚çœ‹äººæ•°æµ®åŠ¨æ•ˆæœ\\n            if (liveInfo.basicInfo.viewerCount) {\\n                const baseViewerCount = parseInt(liveInfo.basicInfo.viewerCount, 10);\\n                const onlineCountElement = document.getElementById('onlineCount');\\n                \\n                // æ¯3-5ç§’éšæœºæ›´æ–°ä¸€æ¬¡è§‚çœ‹äººæ•°\\n                setInterval(() => {\\n                    // ç”Ÿæˆ-3%åˆ°+5%ä¹‹é—´çš„éšæœºæ³¢åŠ¨\\n                    const fluctuation = baseViewerCount * (Math.random() * 0.08 - 0.03);\\n                    const newCount = Math.max(1, Math.round(baseViewerCount + fluctuation));\\n                    \\n                    // æ›´æ–°æ˜¾ç¤º\\n                    onlineCountElement.textContent = newCount + ' äººæ­£åœ¨è§‚çœ‹';\\n                    \\n                    // æ·»åŠ çŸ­æš‚çš„é«˜äº®æ•ˆæœ\\n                    onlineCountElement.style.transition = 'color 0.3s ease';\\n                    if (fluctuation > 0) {\\n                        onlineCountElement.style.color = '#7bed9f'; // å¢åŠ æ—¶æ˜¾ç¤ºç»¿è‰²\\n                    } else {\\n                        onlineCountElement.style.color = '#ff7f50'; // å‡å°‘æ—¶æ˜¾ç¤ºæ©™è‰²\\n                    }\\n                    \\n                    // æ¢å¤åŸæ¥çš„é¢œè‰²\\n                    setTimeout(() => {\\n                        onlineCountElement.style.color = '#b0b0b0';\\n                    }, 300);\\n                }, 3000 + Math.random() * 2000); // 3-5ç§’éšæœºé—´éš”\\n            }\\n        }\\n        \\n        // è§£æURLå‚æ•°ä¸­çš„ç›´æ’­ä¿¡æ¯\\n        function parseLiveInfoFromURL() {\\n            const urlParams = new URLSearchParams(window.location.search);\\n            const liveData = urlParams.get('liveData');\\n            \\n            if (liveData) {\\n                try {\\n                    const decodedData = decodeURIComponent(liveData);\\n                    const liveInfo = parseLiveXML(decodedData);\\n                    applyLiveInfo(liveInfo);\\n                } catch (error) {\\n                    console.error('è§£æURLå‚æ•°å‡ºé”™:', error);\\n                }\\n            }\\n        }\\n\\n        // å¼¹å¹•é¢œè‰²\\n        const danmakuColors = [\\n            \\\"#ffffff\\\", // ç™½è‰²\\n            \\\"#ff6b81\\\", // ç²‰çº¢\\n            \\\"#70a1ff\\\", // è“è‰²\\n            \\\"#7bed9f\\\", // ç»¿è‰²\\n            \\\"#fffa65\\\", // é»„è‰²\\n            \\\"#ff7f50\\\", // çŠç‘šè‰²\\n            \\\"#a29bfe\\\"  // ç´«è‰²\\n        ];\\n\\n        // åˆ›å»ºå¹¶å‘é€å¼¹å¹•\\n        function createDanmaku() {\\n            const container = document.getElementById('danmakuContainer');\\n            const containerHeight = container.offsetHeight;\\n            \\n            // éšæœºé€‰æ‹©å¼¹å¹•å†…å®¹å’Œé¢œè‰²\\n            const text = exampleDanmaku[Math.floor(Math.random() * exampleDanmaku.length)];\\n            const color = danmakuColors[Math.floor(Math.random() * danmakuColors.length)];\\n            \\n            // åˆ›å»ºå¼¹å¹•å…ƒç´ \\n            const danmaku = document.createElement('div');\\n            danmaku.classList.add('danmaku');\\n            danmaku.textContent = text;\\n            danmaku.style.color = color;\\n            \\n            // éšæœºå‚ç›´ä½ç½®\\n            const top = Math.floor(Math.random() * (containerHeight - 40));\\n            danmaku.style.top = `${top}px`;\\n            \\n            // éšæœºåŠ¨ç”»æŒç»­æ—¶é—´ï¼ˆ2-4ç§’ï¼Œæ¯”ä¹‹å‰æ›´å¿«ï¼‰\\n            const duration = 6 + Math.floor(Math.random() * 2);\\n            danmaku.style.animationDuration = `${duration}s`;\\n            \\n            // æ·»åŠ åˆ°å®¹å™¨\\n            container.appendChild(danmaku);\\n            \\n            // åŠ¨ç”»ç»“æŸåç§»é™¤\\n            setTimeout(() => {\\n                danmaku.remove();\\n            }, duration * 1000);\\n        }\\n\\n        // æ›´æ–°æ—¶é—´æ˜¾ç¤º\\n        function updateTime() {\\n            // æ£€æŸ¥æ˜¯å¦åº”è¯¥æ›´æ–°æ—¶é—´ï¼ˆXMLæ•°æ®ä¼˜å…ˆï¼‰\\n            const liveTimeElement = document.getElementById('liveTime');\\n            if (liveTimeElement.getAttribute('data-from-xml') === 'true') {\\n                return; // å¦‚æœæ—¶é—´æ¥è‡ªXMLï¼Œåˆ™ä¸æ›´æ–°\\n            }\\n            \\n            const now = new Date();\\n            const hours = now.getHours().toString().padStart(2, '0');\\n            const minutes = now.getMinutes().toString().padStart(2, '0');\\n            liveTimeElement.textContent = `${hours}:${minutes}`;\\n        }\\n\\n        // åˆå§‹åŒ–å¼¹å¹•ç³»ç»Ÿ\\n        let danmakuInterval = null;\\n        let isDanmakuActive = false;\\n\\n        function toggleDanmaku() {\\n            const button = document.getElementById('startDanmaku');\\n            isDanmakuActive = !isDanmakuActive;\\n            \\n            if (isDanmakuActive) {\\n                button.classList.add('active');\\n                button.querySelector('span').textContent = 'å¼¹å¹•å·²å¼€å¯';\\n                // å¼€å§‹å¼¹å¹•å¾ªç¯\\n                danmakuInterval = setInterval(() => {\\n                    createDanmaku();\\n                }, 800);\\n            } else {\\n                button.classList.remove('active');\\n                button.querySelector('span').textContent = 'å¼¹å¹•å·²å…³é—­';\\n                // åœæ­¢å¼¹å¹•å¾ªç¯\\n                if (danmakuInterval) {\\n                    clearInterval(danmakuInterval);\\n                }\\n                // æ¸…é™¤ç°æœ‰å¼¹å¹•\\n                const container = document.getElementById('danmakuContainer');\\n                container.innerHTML = '';\\n            }\\n        }\\n\\n        document.getElementById('startDanmaku').addEventListener('click', toggleDanmaku);\\n\\n        // åˆå§‹åŒ–\\n        document.addEventListener('DOMContentLoaded', function() {\\n            // è·å–é®ç½©å±‚å…ƒç´ \\n            const overlay = document.getElementById('overlay');\\n            \\n            // è§£æç¤ºä¾‹XMLæ•°æ®å¹¶åº”ç”¨åˆ°ç•Œé¢\\n            const liveInfo = parseLiveXML(exampleLiveXML);\\n            applyLiveInfo(liveInfo);\\n            \\n            // åªæœ‰åœ¨æ²¡æœ‰ä»XMLåŠ è½½æ—¶é—´çš„æƒ…å†µä¸‹æ‰æ›´æ–°æ—¶é—´\\n            const liveTimeElement = document.getElementById('liveTime');\\n            if (liveTimeElement.getAttribute('data-from-xml') !== 'true') {\\n                updateTime();\\n                setInterval(updateTime, 60000); // æ¯åˆ†é’Ÿæ›´æ–°ä¸€æ¬¡æ—¶é—´\\n            }\\n            \\n            // æ¨¡æ‹Ÿéšæœºçš„è§‚ä¼—å’Œç‚¹èµæ•°å˜åŒ–ï¼ˆä»…åœ¨æ²¡æœ‰ä»XMLåŠ è½½æ•°æ®æ—¶ä½¿ç”¨ï¼‰\\n            const followCountElement = document.getElementById('followCount');\\n            if (followCountElement.getAttribute('data-from-xml') !== 'true') {\\n                setInterval(() => {\\n                    const followCount = document.getElementById('followCount');\\n                    const likeCount = document.getElementById('likeCount');\\n                    const onlineCount = document.getElementById('onlineCount');\\n                    \\n                    const followers = parseFloat(followCount.textContent) + (Math.random() > 0.5 ? 0.1 : -0.05);\\n                    const likes = parseFloat(likeCount.textContent) + (Math.random() > 0.6 ? 0.2 : 0);\\n                    \\n                    followCount.textContent = followers.toFixed(1) + 'ä¸‡';\\n                    likeCount.textContent = likes.toFixed(1) + 'ä¸‡';\\n                    onlineCount.textContent = Math.floor(followers * 10000) + ' äººåœ¨çº¿';\\n                }, 5000);\\n            }\\n            \\n            // äº’åŠ¨æŒ‰é’®äº‹ä»¶\\n            // åˆ›å»ºç¤¼ç‰©å¼¹çª—\\n            const giftModal = document.createElement('div');\\n            giftModal.className = 'gift-modal';\