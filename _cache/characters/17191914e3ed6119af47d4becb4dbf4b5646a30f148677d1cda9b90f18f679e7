\\n            element.scrollTop = element.scrollHeight;\\n        }\\n    }\\n    \\n    // å¼€å§‹æ‰“å­—\\n    typeChar();\\n}\\n\\nfunction playSound() {\\n    audio.currentTime = 0;\\n    audio.play();\\n}\\n\\nfunction updateTimer() {\\n    const timerElement = document.getElementById('timer');\\n    seconds++;\\n    const minutes = Math.floor(seconds / 60);\\n    const remainingSeconds = seconds % 60;\\n    timerElement.textContent = `${minutes.toString().padStart(2, '0')}:${remainingSeconds.toString().padStart(2, '0')}`;\\n\\n    // åªæœ‰åœ¨é€šè¯æœªç»“æŸæ—¶æ‰ä¿å­˜æ—¶é—´\\n    if (!localStorage.getItem('callEnded')) {\\n        localStorage.setItem('lastTimerValue', seconds.toString());\\n    }\\n}\\n\\nfunction startTimer() {\\n    // æ£€æŸ¥é€šè¯æ˜¯å¦å·²ç»“æŸ\\n    if (!localStorage.getItem('callEnded')) {\\n        // æ£€æŸ¥æ˜¯å¦æœ‰ä¹‹å‰ä¿å­˜çš„æ—¶é—´\\n        const savedSeconds = localStorage.getItem('lastTimerValue');\\n        if (savedSeconds) {\\n            seconds = parseInt(savedSeconds);\\n            // ç«‹å³æ›´æ–°æ˜¾ç¤º\\n            const minutes = Math.floor(seconds / 60);\\n            const remainingSeconds = seconds % 60;\\n            document.getElementById('timer').textContent =\\n                `${minutes.toString().padStart(2, '0')}:${remainingSeconds.toString().padStart(2, '0')}`;\\n        }\\n    } else {\\n        // å¦‚æœé€šè¯å·²ç»“æŸï¼Œé‡ç½®è®¡æ—¶å™¨\\n        seconds = 0;\\n        document.getElementById('timer').textContent = '00:00';\\n        localStorage.removeItem('callEnded');\\n        localStorage.removeItem('lastTimerValue');\\n    }\\n\\n    // æ£€æŸ¥æ˜¯å¦æœ‰å…¶ä»–æ´»åŠ¨çš„è®¡æ—¶å™¨å®ä¾‹\\n    const activeInstance = localStorage.getItem('activeTimerInstance');\\n    if (activeInstance && activeInstance !== instanceId) {\\n        // å¦‚æœæœ‰å…¶ä»–æ´»åŠ¨å®ä¾‹ï¼Œæš‚åœå®ƒ\\n        localStorage.setItem('timerPaused_' + activeInstance, 'true');\\n    }\\n\\n    // è®¾ç½®å½“å‰å®ä¾‹ä¸ºæ´»åŠ¨å®ä¾‹\\n    localStorage.setItem('activeTimerInstance', instanceId);\\n\\n    // å¼€å§‹æ–°çš„è®¡æ—¶å™¨\\n    if (!timerInterval) {\\n        timerInterval = setInterval(updateTimer, 1000);\\n    }\\n}\\n\\nfunction pauseTimer() {\\n    clearInterval(timerInterval);\\n    timerInterval = null;\\n    localStorage.setItem('timerPaused_' + instanceId, 'true');\\n}\\n\\n// è·å–å½“å‰é€šè¯è€…å§“åçš„è¾…åŠ©å‡½æ•°\\nfunction getCurrentCallerName() {\\n    try {\\n        const callerNameElement = document.getElementById('caller-name');\\n        if (!callerNameElement) {\\n            console.warn('æœªæ‰¾åˆ°caller-nameå…ƒç´ ï¼Œä½¿ç”¨é»˜è®¤åç§°');\\n            return 'æœªçŸ¥è”ç³»äºº';\\n        }\\n        \\n        const name = callerNameElement.textContent.replace(/[âœ§\\\\s]/g, '').trim();\\n        console.log(`è·å–åˆ°é€šè¯è€…å§“å: ${name}`);\\n        return name || 'æœªçŸ¥è”ç³»äºº';\\n    } catch (error) {\\n        console.error('è·å–é€šè¯è€…å§“åæ—¶å‡ºé”™:', error);\\n        return 'æœªçŸ¥è”ç³»äºº';\\n    }\\n}\\n\\nfunction endCall() {\\n    try {\\n        playSound();\\n        pauseTimer();\\n        const currentTime = document.getElementById('timer').textContent;\\n        \\n        // è·å–å½“å‰é€šè¯è€…åç§°\\n        const callerName = getCurrentCallerName();\\n        console.log(`ç»“æŸä¸ ${callerName} çš„é€šè¯, æ—¶é•¿: ${currentTime}`);\\n\\n        // æ ‡è®°é€šè¯å·²ç»“æŸ\\n        localStorage.setItem('callEnded', 'true');\\n\\n        // å‘é€æ¶ˆæ¯åˆ°èŠå¤©ç³»ç»Ÿ\\n        if (typeof triggerSlash === 'function') {\\n            triggerSlash(`/send ã€ç§èŠç•Œé¢ã€‘æŒ‚æ–­å’Œ${callerName}çš„è¯­éŸ³é€šè¯ï¼Œæ—¶é•¿${currentTime}|/trigger`);\\n        } else {\\n            console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n        }\\n    } catch (error) {\\n        console.error('ç»“æŸé€šè¯æ—¶å‡ºé”™:', error);\\n        alert('ç»“æŸé€šè¯æ—¶å‘ç”Ÿé”™è¯¯ï¼Œè¯·åˆ·æ–°é¡µé¢é‡è¯•');\\n    }\\n}\\n\\n// æ·»åŠ é—ªå…‰æ•ˆæœ\\nfunction addSparkles() {\\n    const decoration = document.querySelector('.decoration');\\n    for(let i = 0; i < 10; i++) {\\n        const sparkle = document.createElement('div');\\n        sparkle.className = 'sparkle';\\n        sparkle.style.left = Math.random() * 100 + '%';\\n        sparkle.style.top = Math.random() * 100 + '%';\\n        sparkle.style.animationDelay = Math.random() * 2 + 's';\\n        decoration.appendChild(sparkle);\\n    }\\n}\\n\\n// æ£€æŸ¥è®¡æ—¶å™¨æ˜¯å¦åº”è¯¥æš‚åœ\\nfunction checkTimerPause() {\\n    if (localStorage.getItem('timerPaused_' + instanceId) === 'true') {\\n        pauseTimer();\\n    }\\n}\\n\\n// è·å–URLå‚æ•°çš„å‡½æ•°\\nfunction getUrlParameter(name) {\\n    name = name.replace(/[\\\\[]/, '\\\\\\\\[').replace(/[\\\\]]/, '\\\\\\\\]');\\n    const regex = new RegExp('[\\\\\\\\?&]' + name + '=([^&#]*)');\\n    const results = regex.exec(location.search);\\n    return results === null ? '' : decodeURIComponent(results[1].replace(/\\\\+/g, ' '));\\n}\\n\\n// é¡µé¢åŠ è½½æ—¶çš„åˆå§‹åŒ–\\ndocument.addEventListener('DOMContentLoaded', () => {\\n    // æ£€æŸ¥URLä¸­æ˜¯å¦æœ‰é€šè¯æ•°æ®\\n    const callDataParam = getUrlParameter('callData');\\n    \\n    // å¦‚æœURLä¸­æœ‰é€šè¯æ•°æ®ï¼Œåˆ™ä½¿ç”¨URLä¸­çš„æ•°æ®\\n    if (callDataParam) {\\n        try {\\n            callData = decodeURIComponent(callDataParam);\\n        } catch (e) {\\n            console.error('è§£ç URLå‚æ•°å¤±è´¥:', e);\\n        }\\n    }\\n    \\n    // æå–é€šè¯ä¿¡æ¯å¹¶æ›´æ–°æ˜¾ç¤º\\n    try {\\n        const callInfo = extractCallInfo(callData);\\n        updateCallDisplay(callInfo);\\n        console.log('é€šè¯ä¿¡æ¯åŠ è½½æˆåŠŸ:', callInfo);\\n    } catch (error) {\\n        console.error('åŠ è½½é€šè¯ä¿¡æ¯æ—¶å‡ºé”™:', error);\\n    }\\n\\n    startTimer();\\n    addSparkles();\\n\\n    // å®šæœŸæ£€æŸ¥æ˜¯å¦åº”è¯¥æš‚åœ\\n    setInterval(checkTimerPause, 1000);\\n});\\n\\n// é¡µé¢å…³é—­æˆ–åˆ·æ–°æ—¶çš„æ¸…ç†\\nwindow.addEventListener('beforeunload', () => {\\n    if (instanceId === localStorage.getItem('activeTimerInstance')) {\\n        localStorage.removeItem('activeTimerInstance');\\n    }\\n});\\n\\n// æ·»åŠ æ–°çš„JavaScriptå‡½æ•°\\nfunction showVoiceModal() {\\n    document.getElementById('voiceModal').style.display = 'block';\\n}\\n\\nfunction cancelVoice() {\\n    document.getElementById('voiceModal').style.display = 'none';\\n}\\n\\nfunction confirmVoice() {\\n    try {\\n        const voiceInput = document.getElementById('voiceInput').value;\\n        if (voiceInput.trim() !== '') {\\n            const callerName = getCurrentCallerName();\\n            console.log(`å‘é€è¯­éŸ³å›å¤ç»™ ${callerName}: ${voiceInput}`);\\n            \\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/send ã€è¯­éŸ³é€šè¯ç•Œé¢ã€‘å›å¤å’Œ${callerName}çš„è¯­éŸ³é€šè¯ï¼š${voiceInput}|/trigger`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        } else {\\n            console.warn('è¯­éŸ³è¾“å…¥ä¸ºç©ºï¼Œæœªå‘é€æ¶ˆæ¯');\\n        }\\n        document.getElementById('voiceModal').style.display = 'none';\\n        document.getElementById('voiceInput').value = '';\\n    } catch (error) {\\n        console.error('ç¡®è®¤è¯­éŸ³è¾“å…¥æ—¶å‡ºé”™:', error);\\n        alert('å‘é€è¯­éŸ³æ—¶å‘ç”Ÿé”™è¯¯ï¼Œè¯·é‡è¯•');\\n    }\\n}\\n\\n// ä¿®æ”¹ç¬¬ä¸€ä¸ªæ§åˆ¶æŒ‰é’®çš„onclickäº‹ä»¶\\ndocument.querySelector('.call-controls button:first-child').onclick = showVoiceModal;\\n</script>\\n</body>\\n</html>\\n```\",\"trimStrings\":[],\"placement\":[2],\"disabled\":false,\"markdownOnly\":true,\"promptOnly\":false,\"runOnEdit\":true,\"substituteRegex\":0,\"minDepth\":null,\"maxDepth\":3},{\"id\":\"0d0597db-34d7-4cec-805c-4e1e07b3d786\",\"scriptName\":\"æ‰‹æœºå±å¹•ç•Œé¢(åºŸæ¡ˆ\",\"findRegex\":\"<phone>([\\\\s\\\\S]*?)</phone>\",\"replaceString\":\"```\\n<!DOCTYPE html>\\n<html>\\n<head>\\n<meta charset=\\\"UTF-8\\\">\\n<meta name=\\\"viewport\\\" content=\\\"width=device-width, initial-scale=1.0\\\">\\n<title>æ‰‹æœºå±å¹•</title>\\n<style>\\n/* åŸºæœ¬æ ·å¼è®¾ç½® */\\n* {\\n