);\\n                        \\n                        if (variables && variables.æˆ‘çš„ä¸ªäººèµ„æ–™ && variables.æˆ‘çš„ä¸ªäººèµ„æ–™.å¤´åƒ) {\\n                            // æå–å¤´åƒæ–‡ä»¶åï¼ˆè‹±æ–‡éƒ¨åˆ†å’Œåç¼€ï¼‰\\n                            const avatarMatch = variables.æˆ‘çš„ä¸ªäººèµ„æ–™.å¤´åƒ.match(/[a-zA-Z0-9_-]+\\\\.[a-zA-Z0-9]+$/);\\n                            if (avatarMatch) {\\n                                const avatarFileName = avatarMatch[0];\\n                                avatarUrl = `https://files.catbox.moe/${avatarFileName}`;\\n                            }\\n                        }\\n                    }\\n                    \\n                    // è®¾ç½®å¤´åƒ\\n                    commentAvatar.src = avatarUrl;\\n                } catch (error) {\\n                    console.error('åŠ è½½è¯„è®ºå¤´åƒæ—¶å‡ºé”™:', error);\\n                }\\n            },\\n        };\\n\\n        // ç‚¹å‡»åº•éƒ¨çš„é¦–é¡µæŒ‰é’®ï¼ŒæŸ¥çœ‹è®ºå›é¦–é¡µ\\n        function viewHome() {\\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/send ã€è®ºå›é¦–é¡µã€‘æŸ¥çœ‹è®ºå›é¦–é¡µ|/trigger`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n        \\n        // ç‚¹å‡»åº•éƒ¨çš„å‘ç°æŒ‰é’®ï¼ŒæŸ¥çœ‹è®ºå›å‘ç°é¡µ\\n        function viewDiscovery() {\\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/send ã€è®ºå›å‘ç°ã€‘æŸ¥çœ‹è®ºå›å‘ç°é¡µ|/trigger`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n        \\n        // ç‚¹å‡»åº•éƒ¨çš„æ¶ˆæ¯æŒ‰é’®ï¼ŒæŸ¥çœ‹æ¶ˆæ¯åˆ—è¡¨\\n        function viewMessages() {\\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/send ã€è®ºå›æ¶ˆæ¯åˆ—è¡¨ã€‘æŸ¥çœ‹æ¶ˆæ¯åˆ—è¡¨|/trigger`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n        \\n        // ç‚¹å‡»åº•éƒ¨çš„æˆ‘çš„æŒ‰é’®ï¼ŒæŸ¥çœ‹ä¸ªäººä¸»é¡µ\\n        function viewProfile() {\\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/send ã€ä¸ªäººä¸»é¡µã€‘æŸ¥çœ‹æˆ‘çš„ä¸»é¡µ|/trigger`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n        \\n        // ç‚¹å‡»ç›¸å…³æ¨èè·³è½¬åˆ°ç›¸å…³å¸–å­\\n        function viewRelatedPost(author, title) {\\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/send ã€è®ºå›å¸–å­ã€‘æŸ¥çœ‹${author}çš„å¸–å­ã€Š${title}ã€‹|/trigger`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n        \\n        // ç‚¹å‡»ç”¨æˆ·åæŸ¥çœ‹ä¸ªäººä¸»é¡µ\\n        function viewUserProfile(username) {\\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/send ã€ä¸ªäººä¸»é¡µã€‘æŸ¥çœ‹${username}çš„ä¸ªäººä¸»é¡µ|/trigger`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n        \\n        // ç‚¹å‡»åº•éƒ¨çš„åŠ å·æŒ‰é’®åå‘é€è®ºå›å‘å¸–\\n        function createPost() {\\n            // å°†æ–‡æœ¬åˆ†å¼€ï¼Œé¿å…è¢«æ­£åˆ™è¯†åˆ«æ›¿æ¢\\n            const part1 = 'ã€';\\n            const part2 = 'è®º';\\n            const part3 = 'å›';\\n            const part4 = 'å‘';\\n            const part5 = 'å¸–';\\n            const part6 = 'ã€‘';\\n            \\n            // åŠ¨æ€æ‹¼æ¥å­—ç¬¦ä¸²\\n            const fullString = part1 + part2 + part3 + part4 + part5 + part6;\\n            \\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/sendas name={{Char}} ${fullString}`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n        \\n        // ç»‘å®šåº•éƒ¨å¯¼èˆªæ äº‹ä»¶\\n        function bindNavBarEvents() {\\n            const navHome = document.querySelector('.bottom-navbar .nav-item:nth-child(1)');\\n            const navDiscovery = document.querySelector('.bottom-navbar .nav-item:nth-child(2)');\\n            const navMessage = document.querySelector('.bottom-navbar .nav-item:nth-child(4)');\\n            const navCreate = document.querySelector('.publish-btn-container .publish-btn');\\n            const navUser = document.querySelector('.bottom-navbar .nav-item:nth-child(5)'); // \\\"æˆ‘çš„\\\"å¯¼èˆªé¡¹\\n            \\n            navHome?.addEventListener('click', function(e) {\\n                e.preventDefault();\\n                viewHome();\\n            });\\n            \\n            navDiscovery?.addEventListener('click', function(e) {\\n                e.preventDefault();\\n                viewDiscovery();\\n            });\\n            \\n            navMessage?.addEventListener('click', function(e) {\\n                e.preventDefault();\\n                viewMessages();\\n            });\\n            \\n            navCreate?.addEventListener('click', function(e) {\\n                createPost();\\n            });\\n            \\n            navUser?.addEventListener('click', function(e) {\\n                e.preventDefault();\\n                viewUserPage();\\n            });\\n        }\\n\\n        // ä¿å­˜å¸–å­åˆ°æ”¶è—(èŠå¤©å˜é‡)\\n        async function savePostToFavorites() {\\n            try {\\n                // è·å–å¸–å­ä¿¡æ¯\\n                const postTitle = document.querySelector('.post-title').textContent;\\n                const authorName = document.querySelector('.author-name').textContent;\\n                const authorAvatar = document.querySelector('.author-avatar').src.split('?')[1] || '';\\n                \\n                // è·å–å¸–å­å†…å®¹\\n                const postTexts = Array.from(document.querySelectorAll('.post-text')).map(el => el.innerText).join('\\\\n\\\\n');\\n                \\n                // è·å–å›¾ç‰‡ç®€ä»‹(å¦‚æœæœ‰)\\n                cons