™¨å†…å®¹...');\\n        try {\\n            // ä½¿ç”¨æ­£åˆ™è¡¨è¾¾å¼åŒ¹é…<browser>æ ‡ç­¾ä¹‹é—´çš„å†…å®¹\\n            const regex = /<browser>([\\\\s\\\\S]*?)<\\\\/browser>/;\\n            const match = input.match(regex);\\n            \\n            if (!match) {\\n                console.error('æ— æ³•è§£ææµè§ˆå™¨å†…å®¹:', input);\\n                return null;\\n            }\\n            \\n            const content = match[1].trim();\\n            console.log(`æå–åˆ°å†…å®¹é•¿åº¦: ${content.length}å­—ç¬¦`);\\n            \\n            const result = {};\\n            const contentItems = [];\\n            \\n            // ä¿®æ”¹åŒ¹é…æ–¹å¼ï¼Œå¤„ç†åŒ…å«æ¢è¡Œç¬¦çš„å†…å®¹\\n            // ä½¿ç”¨çŠ¶æ€æœºæ–¹å¼å¤„ç†å¤æ‚å†…å®¹ï¼Œè€Œä¸æ˜¯ç®€å•çš„æ­£åˆ™è¡¨è¾¾å¼\\n            let state = \\\"outside\\\"; // outside æˆ– inside è¡¨ç¤ºåœ¨æ–¹æ‹¬å·å¤–æˆ–å†…\\n            let currentPos = 0;\\n            let startPos = 0;\\n            let bracketContent = \\\"\\\";\\n            let brackets = [];\\n\\n            // æ‰‹åŠ¨è§£ææ¯ä¸ªå­—ç¬¦ï¼Œå¤„ç†åµŒå¥—æ–¹æ‹¬å·å’Œæ¢è¡Œç¬¦\\n            for (let i = 0; i < content.length; i++) {\\n                if (state === \\\"outside\\\" && content[i] === '[') {\\n                    state = \\\"inside\\\";\\n                    startPos = i;\\n                } else if (state === \\\"inside\\\" && content[i] === ']') {\\n                    // æ‰¾åˆ°å®Œæ•´çš„æ–¹æ‹¬å·å†…å®¹\\n                    bracketContent = content.substring(startPos, i + 1);\\n                    brackets.push(bracketContent);\\n                    state = \\\"outside\\\";\\n                }\\n            }\\n            \\n            console.log(`æå–åˆ° ${brackets.length} ä¸ªæ–¹æ‹¬å·å†…å®¹`);\\n            \\n            // å¤„ç†æ¯ä¸ªæ‹¬å·å†…å®¹\\n            for (const bracket of brackets) {\\n                try {\\n                    // æå–æ ‡ç­¾å’Œå€¼\\n                    const bracketContent = bracket.substring(1, bracket.length - 1);\\n                    const parts = bracketContent.split('|');\\n                    \\n                    if (parts.length >= 2) {\\n                        const itemType = parts[0].trim();\\n                        \\n                        // ç‰¹æ®Šå¤„ç†å†…å®¹é¡¹ï¼Œç°åœ¨æ”¯æŒæ ‡é¢˜æ ¼å¼\\n                        if (itemType === 'å†…å®¹') {\\n                            if (parts.length >= 3) {\\n                                // æ–°æ ¼å¼ï¼š[å†…å®¹|æ ‡é¢˜|æ­£æ–‡å†…å®¹]\\n                                contentItems.push({\\n                                    æ ‡é¢˜: parts[1].trim(),\\n                                    å†…å®¹: parts[2].trim()\\n                                });\\n                                console.log(`è§£æå¸¦æ ‡é¢˜å†…å®¹é¡¹: æ ‡é¢˜=${parts[1].trim()}, å†…å®¹é•¿åº¦=${parts[2].trim().length}`);\\n                            } else {\\n                                // å…¼å®¹æ—§æ ¼å¼ï¼š[å†…å®¹|æ­£æ–‡å†…å®¹]\\n                                contentItems.push({\\n                                    æ ‡é¢˜: '',\\n                                    å†…å®¹: parts[1].trim()\\n                                });\\n                                console.log(`è§£ææ— æ ‡é¢˜å†…å®¹é¡¹: å†…å®¹é•¿åº¦=${parts[1].trim().length}`);\\n                            }\\n                        } else {\\n                            // å…¶ä»–æ ‡å‡†é¡¹ç›®\\n                            result[itemType] = parts[1].trim();\\n                            console.log(`è§£ææ ‡å‡†é¡¹: ç±»å‹=${itemType}, å€¼=${parts[1].trim()}`);\\n                        }\\n                    } else {\\n                        console.warn(`æ— æ³•è§£æçš„æ–¹æ‹¬å·å†…å®¹: ${bracket}`);\\n                    }\\n                } catch (bracketError) {\\n                    console.error(`å¤„ç†æ–¹æ‹¬å·å†…å®¹å‡ºé”™:`, bracketError);\\n                }\\n            }\\n            \\n            // å°†æ”¶é›†çš„å†…å®¹é¡¹æ·»åŠ åˆ°ç»“æœä¸­\\n            if (contentItems.length > 0) {\\n                re