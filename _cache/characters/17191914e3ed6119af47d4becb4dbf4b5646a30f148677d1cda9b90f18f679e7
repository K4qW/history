// ç¡®ä¿æœç´¢ä¸»é¡µéšè—ï¼Œå†…å®¹é¡µé¢æ˜¾ç¤º\\n            document.getElementById('home-search-container').style.display = 'none';\\n            document.getElementById('browser-content').style.display = 'block';\\n            \\n            // æ¸…ç©ºå†…å®¹åŒº\\n            const contentContainer = document.getElementById('browser-content');\\n            contentContainer.innerHTML = '';\\n            \\n            // æ›´æ–°å†…å®¹ - ä½¿ç”¨å†…å®¹æ•°ç»„\\n            if (info['å†…å®¹æ•°ç»„'] && info['å†…å®¹æ•°ç»„'].length > 0) {\\n                // åˆ›å»ºå†…å®¹å®¹å™¨ä»¥åŒ…å«æ‰€æœ‰å†…å®¹é¡¹\\n                for (let i = 0; i < info['å†…å®¹æ•°ç»„'].length; i++) {\\n                    const content = info['å†…å®¹æ•°ç»„'][i];\\n                    setTimeout(() => {\\n                        // æ£€æŸ¥å†…å®¹æ˜¯å¦æ˜¯HTMLæ ¼å¼\\n                        if (content.å†…å®¹.trim().startsWith('<') && content.å†…å®¹.includes('</')) {\\n                            // HTMLå†…å®¹ç›´æ¥æ·»åŠ \\n                            contentContainer.innerHTML += content.å†…å®¹;\\n                        } else {\\n                            // æ–‡æœ¬å†…å®¹åŒ…è£…æˆå¡ç‰‡æ ¼å¼\\n                            const cardDiv = document.createElement('div');\\n                            cardDiv.className = 'content-card';\\n                            \\n                            // æ·»åŠ æ ‡é¢˜ï¼ˆå¦‚æœæœ‰ï¼‰\\n                            if (content.æ ‡é¢˜ && content.æ ‡é¢˜.trim() !== '') {\\n                                const titleDiv = document.createElement('div');\\n                                titleDiv.className = 'card-title';\\n                                titleDiv.textContent = content.æ ‡é¢˜;\\n                                cardDiv.appendChild(titleDiv);\\n                            }\\n                            \\n                            // åˆ›å»ºå†…å®¹å®¹å™¨\\n                            const contentDiv = document.createElement('div');\\n                            contentDiv.className = 'card-content';\\n                            // å°†æ¢è¡Œç¬¦è½¬æ¢ä¸º<br>æ ‡ç­¾\\n                            contentDiv.innerHTML = content.å†…å®¹.replace(/\\\\n/g, '<br>');\\n                            \\n                            // æ·»åŠ æ—¶é—´æ ‡è®°\\n                            const timeSpan = document.createElement('div');\\n                            timeSpan.className = 'card-time';\\n                            timeSpan.textContent = getCurrentTime();\\n                            timeSpan.style.fontSize = '11px';\\n                            timeSpan.style.color = '#8E8E93';\\n                            timeSpan.style.textAlign = 'right';\\n                            timeSpan.style.marginTop = '6px';\\n                            \\n                            cardDiv.appendChild(contentDiv);\\n                            cardDiv.appendChild(timeSpan);\\n                            \\n                            contentContainer.appendChild(cardDiv);\\n                        }\\n                    }, i * 300); // æ¯æ¡æ¶ˆæ¯é—´éš”300msæ˜¾ç¤º\\n                }\\n                \\n                // æ·»åŠ ä¸‹ä¸€é¡µæŒ‰é’® - æ”¾åœ¨å†…å®¹åé¢\\n                setTimeout(() => {\\n                    const nextPageButton = document.createElement('div');\\n                    nextPageButton.className = 'next-page-button';\\n                    nextPageButton.innerHTML = `ä¸‹ä¸€é¡µ <i class=\\\"fas fa-chevron-right\\\"></i>`;\\n                    \\n                    // æ·»åŠ ç‚¹å‡»äº‹ä»¶å¤„ç†\\n                    nextPageButton.addEventListener('click', function() {\\n                        // è·å–å½“å‰é¡µé¢åç§°\\n                        const currentPageName = document.getElementById('tab-name').textContent;\\n                        // è§¦å‘ä¸‹ä¸€é¡µè¯·æ±‚\\n                        if (typeof triggerSlash === 'function') {\\n                            triggerSlash(`/send ã€æµè§ˆå™¨ç•Œé¢ã€‘æ˜¾ç¤ºä¸‹ä¸€é¡µ${currentPageName}å†…å®¹`);\\n                            \\n                            // æ˜¾ç¤ºåŠ è½½æç¤º\\n                            this.innerHTML = `<div class=\\\"loading-indicator\\\" style=\\\"margin: 0 5px;\\\"></div> åŠ è½½ä¸­...`;\\n                            this.style.pointerEvents = 'none';\\n                            this.style.opacity = '0.7';\\n                        } else {\\n                            console.log('triggerSlash å‡½æ•°ä¸å¯ç”¨ï¼Œæ— æ³•è¯·æ±‚ä¸‹ä¸€é¡µã€‚');\\n                        }\\n                    });\\n                    \\n                    contentContainer.appendChild(nextPageButton);\\n                }, (info['å†…å®¹æ•°ç»„'].length * 300) + 200); // åœ¨æ‰€æœ‰å†…å®¹æ˜¾ç¤ºåæ·»åŠ æŒ‰é’®\\n            } else {\\n                // æ²¡æœ‰å†…å®¹æ—¶æ˜¾ç¤ºé»˜è®¤ä¿¡æ¯\\n                contentContainer.innerHTML = `\\n                    <div class=\\\"content-card\\\">\\n                        <div class=\\\"card-title\\\">æ— å†…å®¹</div>\\n                        <div class=\\\"card-content\\\">æš‚æ— å¯æ˜¾ç¤ºçš„å†…å®¹</div>\\n                    </div>\\n                `;\\n            }\\n            \\n            // éšè—åŠ è½½æŒ‡ç¤ºå™¨\\n            loadingIndicator.style.display = 'none';\\n            \\n        }, 800); // æ¨¡æ‹ŸåŠ è½½å»¶è¿Ÿ\\n    }\\n\\n    // æ˜¾ç¤ºæœç´¢ä¸»é¡µ\\n    function showSearchHomepage() {\\n        document.getElementById('browser-content').style.display = 'none';\\n        document.getElementById('home-search-container').style.display = 'flex';\\n        document.getElementById('url-display').textContent = 'search://homepage';\\n        document.getElementById('tab-name').textContent = 'æœç´¢';\\n        \\n        // å½“æ˜¾ç¤ºæœç´¢ä¸»é¡µæ—¶ï¼Œåˆå§‹åŒ–ABSTRACTå­—æ¯åŠ¨ç”»\\n        initAbstractAnimation();\\n    }\\n    \\n    // åˆå§‹åŒ–ABSTRACTå­—æ¯åŠ¨ç”»\\n    function initAbstractAnimation() {\\n        const container = document.getElementById('abstract-logo');\\n        if (container && !container.dataset.initialized) {\\n            // æ¸…ç©ºå®¹å™¨\\n            container.innerHTML = '';\\n            \\n            // è®¾ç½®å·²åˆå§‹åŒ–æ ‡å¿—ï¼Œé¿å…é‡å¤åŠ¨ç”»\\n            container.dataset.initialized = 'true';\\n            \\n            // æ˜ç¡®è®¾ç½®å®¹å™¨å­—ä½“\\n            container.style.fontFamily = '\\\"zsft-ai\\\", sans-serif';\\n            container.style.fontWeight = 'normal';\\n            \\n            // ABSTRACTæ–‡å­—\\n            const text = 'ABSTRACT';\\n            \\n            // åˆ›å»ºä¸€ä¸ªç‰‡æ®µæ¥å‡å°‘DOMæ“ä½œ\\n            const fragment = document.createDocumentFragment();\\n            \\n            // å¯é€‰çš„é¢œè‰²å˜åŒ–ï¼ˆæŸ”å’Œçš„è“è‰²å˜ä½“ï¼‰\\n            const colorVariations = [\\n                'rgba(168, 199, 250, 1)',    // åŸå§‹è“è‰²\\n                'rgba(150, 187, 245, 1)',    // ç¨å¾®æš—ä¸€ç‚¹\\n                'rgba(180, 210, 255, 1)',    // ç¨å¾®äº®ä¸€ç‚¹\\n                'rgba(160, 195, 235, 1)',    // ç¨å¾®ç°ä¸€ç‚¹\\n                'rgba(175, 205, 255, 1)'     // ç¨å¾®ç´«ä¸€ç‚¹\\n            ];\\n            \\n            // ä¸ºæ¯ä¸ªå­—æ¯åˆ›å»ºå…ƒç´ \\n            for (let i = 0; i < text.length; i++) {\\n                const letter = document.createElement('span');\\n                letter.className = 'abstract-letter';\\n                letter.textContent = text[i];\\n                \\n                // æ˜ç¡®è®¾ç½®æ¯ä¸ªå­—æ¯çš„å­—ä½“\\n                letter.style.fontFamily = '\\\"zsft-ai\\\", sans-serif';\\n                letter.style.fontWeight = 'normal';\\n                \\n                // å­—æ¯é—´è·æ›´åŠ ç´§å‡‘\\n                letter.style.margin = '0 -2px';\\n                \\n                // åŸºç¡€å»¶è¿Ÿï¼Œå­—æ¯ä¾æ¬¡å‡ºç°ï¼Œä½†æœ‰å¾®å°çš„éšæœºå˜åŒ–\\n                const baseDelay = i * 0.12;\\n                const randomDelay = baseDelay + (Math.random() * 0.05); \\n                letter.style.animationDelay = `${randomDelay}s`;\\n                \\n                // éšæœºé€‰æ‹©ä¸€ä¸ªé¢œè‰²å˜ä½“\\n                const colorIndex = Math.floor(Math.random() * colorVariations.length);\\n                letter.style.color = colorVariations[colorIndex];\\n                \\n                // éšæœºè°ƒæ•´åŠ¨ç”»æŒç»­æ—¶é—´ï¼Œä½¿åŠ¨ç”»æ›´è‡ªç„¶\\n                const baseDuration = 0.7; // åŸºç¡€åŠ¨ç”»æ—¶é•¿\\n                const randomDuration = baseDuration + (Math.random() * 0.3 - 0.15);\\n                letter.style.animationDuration = `${randomDuration}s`;\\n                \\n                // éšæœºæ·»åŠ å˜æ¢åŸç‚¹ï¼Œä½¿æ¯ä¸ªå­—æ¯çš„3Dæ—‹è½¬çœ‹èµ·æ¥æ›´è‡ªç„¶\\n                const origins = ['top', 'center', 'bottom'];\\n                const randomOrigin = origins[Math.floor(Math.random() * origins.length)];\\n                letter.style.transformOrigin = `center ${randomOrigin}`;\\n                \\n                // æ·»åŠ å­—æ¯åˆ°ç‰‡æ®µ\\n                fragment.appendChild(letter);\\n            }\\n            \\n            // ä¸€æ¬¡æ€§å°†æ‰€æœ‰å­—æ¯æ·»åŠ åˆ°DOM\\n            container.appendChild(fragment);\\n            \\n            // æœ€åå¯åŠ¨æ•´ä½“çš„å…‰æ³½åŠ¨ç”»\\n            setTimeout(() => {\\n                container.classList.add('animated');\\n            }, text.length * 120 + 500); // ç­‰å¾…æ‰€æœ‰å­—æ¯åŠ¨ç”»åŸºæœ¬å®Œæˆ\\n        }\\n    }\\n\\n    // æ·»åŠ æµè§ˆå™¨äº¤äº’æ•ˆæœ\\n    function addBrowserInteractions() {\\n        // æ ‡ç­¾é¡µç‚¹å‡»æ•ˆæœ\\n        document.querySelectorAll('.browser-tab').forEach(tab => {\\n            tab.addEventListener('click', function() {\\n                const previousActiveTab = document.querySelector('.browser-tab.active');\\n                previousActiveTab.classList.remove('active');\\n                this.classList.add('active');\\n                \\n                // æ£€æŸ¥æ˜¯å¦ç‚¹å‡»çš„æ˜¯\\\"æ–°æ ‡ç­¾é¡µ\\\"\\n                if (this.textContent.trim().includes('æ–°æ ‡ç­¾é¡µ')) {\\n                    // æ˜¾ç¤ºæœç´¢ä¸»é¡µ\\n                    docum