ƒ¨ä¿¡æ¯\\n                    const headerMatch = pageContent.match(/<personal_page_header>([\\\\s\\\\S]*?)<\\\\/personal_page_header>/);\\n                    const headerContent = headerMatch ? headerMatch[1].trim() : '';\\n                    \\n                    // æå–ç¬”è®°å†…å®¹\\n                    const notesMatch = pageContent.match(/<personal_page_notes>([\\\\s\\\\S]*?)<\\\\/personal_page_notes>/);\\n                    const notesContent = notesMatch ? notesMatch[1].trim() : '';\\n                    \\n                    // æå–æ”¶è—å†…å®¹\\n                    const collectionsMatch = pageContent.match(/<personal_page_collections>([\\\\s\\\\S]*?)<\\\\/personal_page_collections>/);\\n                    const collectionsContent = collectionsMatch ? collectionsMatch[1].trim() : '';\\n                    \\n                    // æå–èµè¿‡å†…å®¹\\n                    const likesMatch = pageContent.match(/<personal_page_likes>([\\\\s\\\\S]*?)<\\\\/personal_page_likes>/);\\n                    const likesContent = likesMatch ? likesMatch[1].trim() : '';\\n                    \\n                    // è§£æå¤´éƒ¨ä¿¡æ¯\\n                    const headerData = this.extractHeaderData(headerContent);\\n                    \\n                    // è§£æç¬”è®°å¸–å­\\n                    const notesPosts = this.extractNotesPosts(notesContent, headerData);\\n                    \\n                    // è§£ææ”¶è—å¸–å­\\n                    const collectionsPosts = this.extractOtherPosts(collectionsContent);\\n                    \\n                    // è§£æèµè¿‡å¸–å­\\n                    const likesPosts = this.extractOtherPosts(likesContent);\\n                    \\n                    return {\\n                        header: headerData,\\n                        notes: notesPosts,\\n                        collections: collectionsPosts,\\n                        likes: likesPosts\\n                    };\\n                } catch (error) {\\n                    console.error('è§£æä¸ªäººä¸»é¡µæ•°æ®å¤±è´¥:', error);\\n                    return {\\n                        header: {},\\n                        notes: [],\\n                        collections: [],\\n                        likes: []\\n                    };\\n                }\\n            },\\n            \\n            // æå–å¤´éƒ¨æ•°æ®\\n            extractHeaderData(content) {\\n                if (!content) return {};\\n                \\n                const header = {};\\n                \\n                // æå–æ—¶é—´ä¿¡æ¯\\n                const timeRegex = /\\\\[æ—¶é—´\\\\|(.*?)\\\\|(.*?)\\\\]/;\\n                const timeMatch = content.match(timeRegex);\\n                if (timeMatch) {\\n                    header.date = timeMatch[1] || '';\\n                    header.time = timeMatch[2] || '';\\n                }\\n                \\n                // æå–ç”¨æˆ·ä¿¡æ¯\\n                const userRegex = /\\\\[ç”¨æˆ·ä¿¡æ¯\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/;\\n                const userMatch = content.match(userRegex);\\n                if (userMatch) {\\n                    header.username = userMatch[1] || 'æœªçŸ¥ç”¨æˆ·';\\n                    header.avatar = this.processAvatarUrl(userMatch[2]) || 'default';\\n                    header.id = userMatch[3] || '';\\n                    header.bio = userMatch[4] || '';\\n                    header.following = parseInt(userMatch[5]) || 0;\\n                    header.followers = parseInt(userMatch[6]) || 0;\\n                    header.likes = parseInt(userMatch[7]) || 0;\\n                }\\n                \\n                return header;\\n            },\\n            \\n            // æå–ç¬”è®°å¸–å­\\n            extractNotesPosts(content, headerData) {\\n                if (!content) return [];\\n                \\n                const posts = [];\\n                const lines = content.split('\\\\n');\\n                \\n                for (const line of lines) {\\n                    const trimmedLine = line.trim();\\n                    if (!trimmedLine) continue;\\n                    \\n                    // [å¸–å­|æ ‡é¢˜|å†…å®¹|å›¾ç‰‡ç®€ä»‹|ç‚¹èµæ•°|è¯„è®ºæ•°|æ ‡ç­¾]\\n                    const postRegex = /\\\\[å¸–å­\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/;\\n                    const match = trimmedLine.match(postRegex);\\n                    \\n                    if (match) {\\n                        posts.push({\\n                            title: match[1] || 'æ— æ ‡é¢˜',\\n                            description: match[2] || 'æ— å†…å®¹',\\n                            imageSummary: match[3] || 'æ— å›¾ç‰‡æè¿°',\\n                            likes: parseInt(match[4]) || 0,\\n                            comments: parseInt(match[5]) || 0,\\n                            tag: match[6] || 'æœªåˆ†ç±»',\\n                            username: headerData.username || 'è®¾è®¡çˆ±å¥½è€…', // ä½¿ç”¨ä»å¤´éƒ¨æå–çš„ç”¨æˆ·å\\n                            avatar: headerData.avatar || this.processAvatarUrl('designer-avatar.jpg') // ä½¿ç”¨ä»å¤´éƒ¨æå–çš„å¤´åƒ\\n                        });\\n                    }\\n                }\\n                \\n                return posts;\\n            },\\n            \\n            // æå–æ”¶è—å’Œèµè¿‡çš„å¸–å­\\n            extractOtherPosts(content) {\\n                if (!content) return [];\\n                \\n                const posts = [];\\n                const lines = content.split('\\\\n');\\n                \\n                for (const line of lines) {\\n                    const trimmedLine = line.trim();\\n                    if (!trimmedLine) continue;\\n                    \\n                    // [å¸–å­|å‘å¸–äºº|å‘å¸–äººå¤´åƒ|æ ‡é¢˜|å†…å®¹|å›¾ç‰‡ç®€ä»‹|ç‚¹èµæ•°|è¯„è®ºæ•°|æ ‡ç­¾]\\n                    const postRegex = /\\\\[å¸–å­\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/;\\n                    const match = trimmedLine.match(postRegex);\\n                    \\n                    if (match) {\\n                        posts.push({\\n                            username: match[1] || 'æœªçŸ¥ç”¨æˆ·',\\n                            avatar: this.processAvatarUrl(match[2]) || 'default',\\n                            title: match[3] || 'æ— æ ‡é¢˜',\\n                            description: match[4] || 'æ— å†…å®¹',\\n                            imageSummary: match[5] || 'æ— å›¾ç‰‡æè¿°',\\n                            likes: parseInt(match[6]) || 0,\\n                            comments: parseInt(match[7]) || 0,\\n                            tag: match[8] || 'æœªåˆ†ç±»'\\n                        });\\n                    }\\n                }\\n                \\n                return posts;\\n            },\\n            \\n            // è§£æ gameText\\n            parseGameText(gameText) {\\n                // ç›´æ¥è§£æä¸ªäººä¸»é¡µæ•°æ®\\n                return this.parsePersonalPageData(gameText);\\n            }\\n        };\\n\\n        // UIæ›´æ–°å™¨\\n        const UIUpdater = {\\n            // å°†å¤§æ•°å­—æ ¼å¼åŒ–ä¸ºKå’ŒM\\n            formatNumber(num) {\\n                if (!num) return '0';\\n                \\n                // å°†è¾“å…¥è½¬æ¢ä¸ºæ•°å­—\\n                num = parseInt(num);\\n                \\n                if (num >= 1000000) {\\n                    return (num / 1000000).toFixed(1).replace(/\\\\.0$/, '') + 'M';\\n                } else if (num >= 1000) {\\n                    return (num / 1000).toFixed(1).replace(/\\\\.0$/, '') + 'K';\\n                }\\n                return num.toString();\\n            },\\n            \\n            // æ›´æ–°ä¸ªäººèµ„æ–™å¤´éƒ¨\\n            updateProfileHeader(headerData) {\\n                if (!headerData) {\\n                    console.warn('æ²¡æœ‰å¤´éƒ¨æ•°æ®å¯ç”¨');\\n                    return;\\n                }\\n                \\n                // å®‰å…¨åœ°è®¾ç½®å…ƒç´ æ–‡æœ¬\\n                const setElementText = (selector, text) => {\\n                    const element = document.querySelector(selector);\\n                    if (element) {\\n                        element.textContent = text;\\n                    } else {\\n                        console.warn(`æ‰¾ä¸åˆ°å…ƒç´ : ${selector}`);\\n                    }\\n                };\\n                \\n                // æ›´æ–°ç”¨æˆ·ä¿¡æ¯\\n                setElementText('.profile-name', headerData.username || 'è®¾è®¡çˆ±å¥½è€…');\\n                setElementText('.profile-id', `å°çº¢ä¹¦å·ï¼š${headerData.id || '12345678'}`);\\n                setElementText('.profile-bio', headerData.bio || 'è¿™æ˜¯ä¸€æ®µä¸ªäººç®€ä»‹ï¼Œå¯ä»¥ä»‹ç»è‡ªå·±çš„å…´è¶£çˆ±å¥½ç­‰ã€‚');\\n                \\n                // æ›´æ–°å¤´åƒ\\n                const avatarContainer = document.querySelector('.profile-avatar');\\n                if (avatarContainer) {\\n                    // ç§»é™¤åŸæœ‰çš„å›¾æ ‡\\n                    avatarContainer.innerHTML = '';\\n                    // æ·»åŠ å¤´åƒå›¾ç‰‡\\n                    const img = document.createElement('img');\\n                    img.src = headerData.avatar;\\n                    img.alt = headerData.username || 'è®¾è®¡çˆ±å¥½è€…';\\n                    img.style.width = '100%';\\n                    img.style.height = '100%';\\n                    img.style.objectFit = 'cover';\\n                    avatarContainer.appendChild(img);\\n                }\\n                \\n                // æ›´æ–°ç»Ÿè®¡æ•°æ®\\n                const stats = document.querySelectorAll('.stat-value');\\n                if (stats.length >= 3) {\\n                    stats[0].textContent = this.formatNumber(headerData.following) || '0';\\n                    stats[1].textContent = this.formatNumber(headerData.followers) || '0';\\n                    stats[2].textContent = this.formatNumber(headerData.likes) || '0';\\n                }\\n                \\n                // æ›´æ–°æ—¶é—´\\n                setElementText('.time-display', `${headerData.date || ''} ${headerData.time || ''}`);\\n                \\n                // æ›´æ–°æ ‡é¢˜\\n                document.title = `${headerData.username || 'è®¾è®¡çˆ±å¥½è€…'} çš„ä¸ªäººä¸»é¡µ`;\\n            },\\n            \\n            // æ›´æ–°ç¬”è®°å†…å®¹\\n            updateNotes(notes) {\\n                this.updateContent('posts', notes);\\n            },\\n            \\n            // æ›´æ–°æ”¶è—å†…å®¹\\n            updateCollections(collections) {\\n                this.updateContent('collections', collections);\\n            },\\n            \\n            // æ›´æ–°èµè¿‡å†…å®¹\\n            updateLikes(likes) {\\n                this.updateContent('likes', likes);\\n            },\\n            \\n            // é€šç”¨å†…å®¹æ›´æ–°æ–¹æ³•\\n            updateContent(type, posts) {\\n                const containerId = `${type}-content`;\\n                const container = document.getElementById(containerId);\\n                if (!container) {\\n                    console.warn(`æ‰¾ä¸åˆ°å®¹å™¨: ${containerId}`);\\n                    return;\\n                }\\n                \\n                console.log(`æ›´æ–°${type}å†…å®¹ï¼Œå…±${posts ? posts.length : 0}ç¯‡å¸–å­`);\\n                \\n                // ç¡®ä¿å®¹å™¨çš„çˆ¶å…ƒç´ (content-section)æ˜¯æ˜¾ç¤ºçš„\\n                const contentSection = container.closest('.content-section');\\n                if (contentSection && !contentSection.classList.contains('active')) {\\n                    console.log(`çˆ¶å®¹å™¨${contentSection.id}æœªæ¿€æ´»ï¼Œè·³è¿‡æ›´æ–°`);\\n                    return;\\n                }\\n                \\n                // æ¸…ç©ºç€‘å¸ƒæµå®¹å™¨\\n                container.innerHTML = '';\\n                \\n                if (!posts || posts.length === 0) {\\n                    container.innerHTML = '<div class=\\\"empty-message\\\">æš‚æ— å†…å®¹</div>';\\n                    return;\\n                }\\n                \\n                // åˆ›å»ºç€‘å¸ƒæµå¸ƒå±€\\n                const waterfall = document.createElement('div');\\n                waterfall.className = 'waterfall';\\n                waterfall.id = `${type}-waterfall`;\\n                container.appendChild(waterfall);\\n                \\n                // åˆ›å»ºç€‘å¸ƒæµåˆ—\\n                let columnCount;\\n                if (window.innerWidth >= 1500) {\\n                    columnCount = 5; // è¶…å®½å±æ˜¾ç¤º5åˆ—\\n                } else if (window.innerWidth >= 1200) {\\n                    columnCount = 4; // å®½å±æ˜¾ç¤º4åˆ—\\n                } else if (window.innerWidth >= 992) {\\n                    columnCount = 3; // ä¸­ç­‰å±å¹•æ˜¾ç¤º3åˆ—\\n                } else if (window.innerWidth >= 576) {\\n                    columnCount = 2; // å°å±å¹•æ˜¾ç¤º2åˆ—\\n                } else {\\n                    columnCount = 1; // æ‰‹æœºå±å¹•æ˜¾ç¤º1åˆ—\\n                }\\n                \\n                for (let i = 0; i < columnCount; i++) {\\n                    const column = document.createElement('div');\\n                    column.className = 'waterfall-column';\\n                    waterfall.appendChild(column);\\n                }\\n                \\n                const columns = waterfall.querySelectorAll('.waterfall-column');\\n                \\n                // åˆ†é…å¸–å­åˆ°åˆ—\\n                posts.forEach((post, index) => {\\n                    const column = columns[index % columnCount];\\n                    const postCard = this.createPostCard(post, type !== 'posts');\\n                    column.appendChild(postCard);\\n                });\\n                \\n                console.log(`${type}å†…å®¹æ›´æ–°å®Œæˆ`);\\n            },\\n            \\n            // é‡å†™ createPostCard æ–¹æ³•ä»¥æ·»åŠ ç‚¹å‡»äº‹ä»¶\\n            createPostCard(post, showAuthor = false) {\\n                const card = document.createElement('div');\\n                card.className = 'post-card';\\n                \\n                // åˆ›å»ºæ ‡ç­¾å¾½ç« \\n                const tagBadge = post.tag ? `<span class=\\\"tag-badge\\\">${post.tag}</span>` : '';\\n                \\n                // å¸–å­å›¾ç‰‡å®¹å™¨\\n                let postHtml = `\\n                <div class=\\\"image-container\\\" onclick=\\\"this.classList.toggle('active-mask')\\\">\\n                    <div class=\\\"image-mask\\\">\\n                        <span>ç‚¹å‡»æŸ¥çœ‹å†…å®¹</span>\\n                    </div>\\n                    <div class=\\\"image-summary\\\">\\n                        <h4>å›¾ç‰‡ç®€ä»‹</h4>\\n                        <p>${post.imageSummary || post.description}</p>\\n                    </div>\\n                    ${tagBadge}\\n                </div>`;\\n                \\n                // è·å–æ­£ç¡®çš„ç”¨æˆ·ä¿¡æ¯\\n                let displayUsername, displayAvatar;\\n                \\n                if (showAuthor) {\\n                    // å¦‚æœæ˜¾ç¤ºå‘å¸–äººï¼Œç›´æ¥ä½¿ç”¨å¸–å­çš„ç”¨æˆ·åå’Œå¤´åƒ\\n                    displayUsername = post.username;\\n                    displayAvatar = post.avatar; // post.avatarå·²ç»åœ¨æå–é˜¶æ®µå¤„ç†è¿‡\\n                } else {\\n                    // å¦‚æœæ˜¯ç”¨æˆ·è‡ªå·±çš„å¸–å­ï¼Œä½¿ç”¨å¤´éƒ¨ç¼“å­˜çš„ç”¨æˆ·åå’Œå¤´åƒ\\n                    const headerData = window.cachedData?.header || {};\\n                    displayUsername = headerData.username || 'è®¾è®¡çˆ±å¥½è€…';\\n                    displayAvatar = headerData.avatar || 'https://files.catbox.moe/designer-avatar.jpg';\\n                }\\n                \\n                // å¸–å­å†…å®¹\\n                postHtml += `\\n                <div class=\\\"post-content\\\">\\n                    <h3 class=\\\"post-title\\\">${post.title}</h3>\\n                    <p class=\\\"post-description\\\">${post.description}</p>\\n                    <div class=\\\"post-meta\\\">\\n                        <div class=\\\"user-info\\\">\\n                            <img src=\\\"${displayAvatar}\\\" alt=\\\"${displayUsername}\\\" class=\\\"avatar\\\">\\n                            <span class=\\\"username\\\">${displayUsername}</span>\\n                        </div>\\n                        <div class=\\\"post-stats\\\">\\n                            <span class=\\\"like-btn\\\"><i class=\\\"far fa-heart\\\"></i> <span class=\\\"likes-count\\\">${this.formatNumber(post.likes)}</span></span>\\n                            <span><i class=\\\"far fa-comment\\\"></i> ${this.formatNumber(post.comments)}</span>\\n                        </div>\\n                    </div>\\n                </div>`;\\n                \\n                card.innerHTML = postHtml;\\n                \\n                // æ·»åŠ ç‚¹èµåŠŸèƒ½\\n                const likeBtn = card.querySelector('.like-btn');\\n                likeBtn.addEventListener('click', (e) => {\\n                    e.stopPropagation();\\n                    const icon = likeBtn.querySelector('i');\\n                    const likeCountElement = likeBtn.querySelector('.likes-count');\\n                    let currentLikes = likeCountElement.textContent;\\n                    \\n                    // å°†Kå’ŒMè½¬æ¢å›å®é™…æ•°å­—\\n                    if (currentLikes.endsWith('K')) {\\n                        currentLikes = parseFloat(currentLikes.replace('K', '')) * 1000;\\n                    } else if (currentLikes.endsWith('M')) {\\n                        currentLikes = parseFloat(currentLikes.replace('M', '')) * 1000000;\\n                    } else {\\n                        currentLikes = parseInt(currentLikes);\\n                    }\\n                    \\n                    if (icon.classList.contains('far')) {\\n                        // ç‚¹èµ\\n                        icon.className = 'fas fa-heart';\\n                        icon.style.color = '#ff2e4d';\\n                        likeCountElement.textContent = this.formatNumber(currentLikes + 1);\\n                    } else {\\n                        // å–æ¶ˆç‚¹èµ\\n                        icon.className = 'far fa-heart';\\n                        icon.style.color = '';\\n                        likeCountElement.textContent = this.formatNumber(currentLikes - 1);\\n                    }\\n                });\\n                \\n                // æ·»åŠ å¸–å­å¡ç‰‡ç‚¹å‡»äº‹ä»¶\\n                card.addEventListener('click', function(e) {\\n                    // å¦‚æœç‚¹å‡»çš„æ˜¯å›¾ç‰‡å®¹å™¨ã€ç‚¹èµæŒ‰é’®æˆ–ç”¨æˆ·ä¿¡æ¯ï¼Œä¸è§¦å‘å¸–å­æŸ¥çœ‹äº‹ä»¶\\n                    if (e.target.closest('.image-container') || e.target.closest('.like-btn') || e.target.closest('.user-info')) {\\n                        return;\\n                    }\\n                    \\n                    viewPost(post.title, displayUsername, post.description);\\n                });\\n                \\n                // æ·»åŠ ç”¨æˆ·ä¿¡æ¯åŒºåŸŸç‚¹å‡»äº‹ä»¶\\n                const userInfoElem = card.querySelector('.user-info');\\n                userInfoElem.addEventListener('click', function(e) {\\n                    e.stopPropagation(); // é˜²æ­¢è§¦å‘å¸–å­ç‚¹å‡»äº‹ä»¶\\n                    viewUserProfile(displayUsername);\\n                });\\n                \\n                return card;\\n            },\\n            \\n            // è·å–éšæœºå›¾æ ‡\\n            getRandomIcon() {\\n                const icons = ['palette', 'laptop-code', 'lightbulb', 'camera', 'book', 'magic'];\\n                return icons[Math.floor(Math.random() * icons.length)];\\n            },\\n            \\n            // åˆå§‹åŒ–UI\\n            initUI() {\\n                try {\\n                    console.log('å¼€å§‹åˆå§‹åŒ–UI...');\\n                    \\n                    // æå–æ•°æ®\\n                    const data = DataExtractor.parseGameText(gameText);\\n                    if (!data) {\\n                        console.error('æ— æ³•è§£ægameTextæ•°æ®');\\n                        return;\\n                    }\\n                    \\n                    console.log('æˆåŠŸè§£ææ•°æ®:', data);\\n                    \\n                    // ç¼“å­˜æ•°æ®ä»¥ä¾›é€‰é¡¹å¡åˆ‡æ¢æ—¶ä½¿ç”¨\\n                    window.cachedData = data;\\n                    \\n                    try {\\n                        // æ›´æ–°UI\\n                        this.updateProfileHeader(data.header);\\n                        console.log('å¤´éƒ¨ä¿¡æ¯æ›´æ–°å®Œæˆ');\\n                        \\n                        this.updateNotes(data.notes);\\n                        console.log('ç¬”è®°å†…å®¹æ›´æ–°å®Œæˆ');\\n                        \\n                        this.updateCollections(data.collections);\\n                        console.log('æ”¶è—å†…å®¹æ›´æ–°å®Œæˆ');\\n                        \\n                        this.updateLikes(data.likes);\\n                        console.log('èµè¿‡å†…å®¹æ›´æ–°å®Œæˆ');\\n                    } catch (updateError) {\\n                        console.error('æ›´æ–°UIéƒ¨åˆ†å¤±è´¥:', updateError);\\n                    }\\n                    \\n                    // ç»‘å®šé€‰é¡¹å¡åˆ‡æ¢äº‹ä»¶\\n                    try {\\n                        this.bindTabEvents();\\n                        console.log('é€‰é¡¹å¡äº‹ä»¶ç»‘å®šå®Œæˆ');\\n                    } catch (tabError) {\\n                        console.error('ç»‘å®šé€‰é¡¹å¡äº‹ä»¶å¤±è´¥:', tabError);\\n                    }\\n                    \\n                    console.log('UIåˆå§‹åŒ–å®Œæˆ');\\n                    \\n                    // ç¡®ä¿æ¿€æ´»ä¸€ä¸ªæ ‡ç­¾é¡µ\\n                    document.querySelector('.profile-tab.active')?.click();\\n                } catch (error) {\\n                    console.error('åˆå§‹åŒ–UIæ—¶å‡ºé”™:', error);\\n                }\\n            },\\n            \\n            // ç»‘å®šé€‰é¡¹å¡åˆ‡æ¢äº‹ä»¶\\n            bindTabEvents() {\\n                const tabs = document.querySelectorAll('.profile-tab');\\n                \\n                console.log('æ‰¾åˆ°é€‰é¡¹å¡å…ƒç´ :', tabs.length);\\n                \\n                tabs.forEach(tab => {\\n                    tab.addEventListener('click', () => {\\n                        // ç§»é™¤æ‰€æœ‰æ´»åŠ¨æ ‡ç­¾\\n                        tabs.forEach(t => t.classList.remove('active'));\\n                        // æ·»åŠ å½“å‰æ´»åŠ¨æ ‡ç­¾\\n                        tab.classList.add('active');\\n                        \\n                        // è·å–å†…å®¹ç±»å‹\\n                        const tabType = tab.getAttribute('data-tab');\\n                        console.log('ç‚¹å‡»é€‰é¡¹å¡:', tabType);\\n                        \\n                        // éšè—æ‰€æœ‰å†…å®¹åŒºåŸŸ\\n                        document.querySelectorAll('.content-section').forEach(section => {\\n                            section.classList.remove('active');\\n                        });\\n                        \\n                        // æ˜¾ç¤ºå¯¹åº”çš„å†…å®¹åŒºåŸŸ\\n                        const targetSection = document.getElementById(`${tabType}-section`);\\n                        if (targetSection) {\\n                            targetSection.classList.add('active');\\n                            console.log(`æ˜¾ç¤ºå†…å®¹åŒºåŸŸ: #${tabType}-section`);\\n                            \\n                            // ä¸ºç¡®ä¿åˆ‡æ¢åå†…å®¹èƒ½æ­£ç¡®æ˜¾ç¤ºï¼Œé‡æ–°æ›´æ–°ä¸€æ¬¡è¯¥ç±»å‹å†…å®¹\\n                            if (tabType === 'posts') {\\n                                this.updateNotes(window.cachedData ? window.cachedData.notes : []);\\n                            } else if (tabType === 'collections') {\\n                                this.updateCollections(window.cachedData ? window.cachedData.collections : []);\\n                            } else if (tabType === 'likes') {\\n                                this.updateLikes(window.cachedData ? window.cachedData.likes : []);\\n                            }\\n                        } else {\\n                            console.error(`æœªæ‰¾åˆ°å†…å®¹åŒºåŸŸ: #${tabType}-section`);\\n                        }\\n                    });\\n                });\\n            }\\n        };\\n        \\n        // é¡µé¢åŠ è½½å®Œæˆååˆå§‹åŒ–UI\\n        document.addEventListener('DOMContentLoaded', () => {\\n            UIUpdater.initUI();\\n            \\n            // æ»šåŠ¨äº‹ä»¶å¤„ç†\\n            const scrollTopBtn = document.querySelector('.scroll-top');\\n            \\n            window.addEventListener('scroll', function() {\\n                // æ˜¾ç¤º/éšè—è¿”å›é¡¶éƒ¨æŒ‰é’®\\n                if (window.scrollY > 300) {\\n                    scrollTopBtn.classList.add('visible');\\n                } else {\\n                    scrollTopBtn.classList.remove('visible');\\n                }\\n            });\\n            \\n            // è¿”å›é¡¶éƒ¨æŒ‰é’®ç‚¹å‡»äº‹ä»¶\\n            scrollTopBtn.addEventListener('click', function() {\\n                window.scrollTo({\\n                    top: 0,\\n                    behavior: 'smooth'\\n                });\\n            });\\n            \\n            // çª—å£å¤§å°å˜åŒ–æ—¶é‡æ–°åˆå§‹åŒ–UI\\n            window.addEventListener('resize', function() {\\n                UIUpdater.initUI();\\n            });\\n\\n            // ç»‘å®šåº•éƒ¨å¯¼èˆªæ äº‹ä»¶\\n            bindNavBarEvents();\\n            \\n            // ç»‘å®šæœç´¢åŠŸèƒ½\\n            bindSearchEvents();\\n            \\n            // ç»‘å®šç§ä¿¡åŠŸèƒ½\\n            bindMessageEvents();\\n            \\n            // ç»‘å®šå…³æ³¨æŒ‰é’®äº‹ä»¶\\n            bindFollowButtonEvent();\\n            \\n            // æ£€æŸ¥æ˜¯å¦å·²å…³æ³¨åšä¸»\\n            checkIfFollowed();\\n        });\\n\\n        // ç‚¹å‡»å¸–å­æŸ¥çœ‹è¯¦æƒ…\\n        function viewPost(title, username, content) {\\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/send ã€è®ºå›å¸–å­ã€‘æŸ¥çœ‹:${title}ï¼Œå‘å¸–äºº:${username}ï¼Œå†…å®¹ä¸º:${content}|/trigger`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n\\n        // ç‚¹å‡»åº•éƒ¨çš„é¦–é¡µæŒ‰é’®ï¼ŒæŸ¥çœ‹è®ºå›é¦–é¡µ\\n        function viewHome() {\\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/send ã€è®ºå›é¦–é¡µã€‘æŸ¥çœ‹è®ºå›é¦–é¡µ|/trigger`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n        \\n        // ç‚¹å‡»åº•éƒ¨çš„å‘ç°æŒ‰é’®ï¼ŒæŸ¥çœ‹è®ºå›å‘ç°é¡µ\\n        function viewDiscovery() {\\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/send ã€è®ºå›å‘ç°ã€‘æŸ¥çœ‹è®ºå›å‘ç°é¡µ|/trigger`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n        \\n        // ç‚¹å‡»åº•éƒ¨çš„æ¶ˆæ¯æŒ‰é’®ï¼ŒæŸ¥çœ‹æ¶ˆæ¯åˆ—è¡¨\\n        function viewMessages() {\\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/send ã€è®ºå›æ¶ˆæ¯åˆ—è¡¨ã€‘æŸ¥çœ‹æ¶ˆæ¯åˆ—è¡¨|/trigger`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n        \\n        // ç‚¹å‡»æœç´¢æŒ‰é’®æˆ–å›è½¦ï¼Œå‘é€æœç´¢è¯·æ±‚\\n        function performSearch() {\\n            const searchInput = document.getElementById('search-input');\\n            const keyword = searchInput.value.trim();\\n            \\n            if (!keyword) return;\\n            \\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/send ã€è®ºå›æœç´¢ã€‘æŸ¥çœ‹${keyword}çš„æœç´¢ç»“æœ|/trigger`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n        \\n        // ç‚¹å‡»åº•éƒ¨çš„åŠ å·æŒ‰é’®åå‘é€è®ºå›å‘å¸–\\n        function createPost() {\\n            // å°†æ–‡æœ¬åˆ†å¼€ï¼Œé¿å…è¢«æ­£åˆ™è¯†åˆ«æ›¿æ¢\\n            const part1 = 'ã€';\\n            const part2 = 'è®º';\\n            const part3 = 'å›';\\n            const part4 = 'å‘';\\n            const part5 = 'å¸–';\\n            const part6 = 'ã€‘';\\n            \\n            // åŠ¨æ€æ‹¼æ¥å­—ç¬¦ä¸²\\n            const fullString = part1 + part2 + part3 + part4 + part5 + part6;\\n            \\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/sendas name={{Char}} ${fullString}`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n        \\n        // ç‚¹å‡»åº•éƒ¨çš„æˆ‘çš„æŒ‰é’®ï¼ŒæŸ¥çœ‹æˆ‘çš„ä¸»é¡µ\\n        function viewUserPage() {\\n            // å°†æ–‡æœ¬åˆ†å¼€ï¼Œé¿å…è¢«æ­£åˆ™è¯†åˆ«æ›¿æ¢\\n            const part1 = 'ã€';\\n            const part2 = 'æˆ‘';\\n            const part3 = 'çš„';\\n            const part4 = 'ä¸»';\\n            const part5 = 'é¡µ';\\n            const part6 = 'ã€‘';\\n            \\n            // åŠ¨æ€æ‹¼æ¥å­—ç¬¦ä¸²\\n            const fullString = part1 + part2 + part3 + part4 + part5 + part6;\\n            \\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/sendas name={{Char}} ${fullString}`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n        \\n        // æ˜¾ç¤ºæˆ–éšè—å¼¹çª—\\n        function toggleDialog(dialogId, show) {\\n            const dialog = document.getElementById(dialogId);\\n            if (dialog) {\\n                if (show) {\\n                    dialog.style.display = 'flex';\\n                    setTimeout(() => dialog.classList.add('show'), 10);\\n                } else {\\n                    dialog.classList.remove('show');\\n                    setTimeout(() => dialog.style.display = 'none', 300);\\n                }\\n            }\\n        }\\n        \\n        // ç»‘å®šåº•éƒ¨å¯¼èˆªæ äº‹ä»¶\\n        function bindNavBarEvents() {\\n            const navHome = document.getElementById('nav-home');\\n            const navDiscovery = document.getElementById('nav-discovery');\\n            const navMessage = document.getElementById('nav-message');\\n            const navCreate = document.getElementById('nav-create');\\n            const navUser = document.querySelector('.bottom-navbar .nav-item.active'); // \\\"æˆ‘çš„\\\"å¯¼èˆªé¡¹ï¼ˆå·²æ¿€æ´»ï¼‰\\n            \\n            navHome?.addEventListener('click', function(e) {\\n                e.preventDefault(); // é˜»æ­¢é»˜è®¤çš„é”šç‚¹è¡Œä¸º\\n                viewHome();\\n            });\\n            \\n            navDiscovery?.addEventListener('click', function(e) {\\n                e.preventDefault(); // é˜»æ­¢é»˜è®¤çš„é”šç‚¹è¡Œä¸º\\n                viewDiscovery();\\n            });\\n            \\n            navMessage?.addEventListener('click', function(e) {\\n                e.preventDefault(); // é˜»æ­¢é»˜è®¤çš„é”šç‚¹è¡Œä¸º\\n                viewMessages();\\n            });\\n            \\n            navCreate?.addEventListener('click', function(e) {\\n                e.preventDefault(); // é˜»æ­¢é»˜è®¤çš„é”šç‚¹è¡Œä¸º\\n                createPost();\\n            });\\n            \\n            navUser?.addEventListener('click', function(e) {\\n                e.preventDefault(); // é˜»æ­¢é»˜è®¤çš„é”šç‚¹è¡Œä¸º\\n                viewUserPage();\\n            });\\n        }\\n        \\n        // ç»‘å®šæœç´¢åŠŸèƒ½\\n        function bindSearchEvents() {\\n            const searchInput = document.getElementById('search-input');\\n            const searchButton = document.getElementById('search-button');\\n            \\n            if (searchButton && searchInput) {\\n                searchButton.addEventListener('click', performSearch);\\n                \\n                searchInput.addEventListener('keypress', function(e) {\\n                    if (e.key === 'Enter') {\\n                        performSearch();\\n                    }\\n                });\\n            }\\n        }\\n        \\n        // ç»‘å®šç§ä¿¡åŠŸèƒ½\\n        function bindMessageEvents() {\\n            const messageBtn = document.getElementById('message-btn');\\n            const sendMessageBtn = document.getElementById('send-message-btn');\\n            const cancelMessageBtn = document.getElementById('cancel-message-btn');\\n            const messageInput = document.getElementById('message-input');\\n            \\n            // æ˜¾ç¤ºç§ä¿¡å¼¹çª—\\n            messageBtn?.addEventListener('click', function() {\\n                toggleDialog('message-dialog', true);\\n                messageInput.focus();\\n            });\\n            \\n            // å–æ¶ˆå‘é€ç§ä¿¡\\n            cancelMessageBtn?.addEventListener('click', function() {\\n                toggleDialog('message-dialog', false);\\n                messageInput.value = '';\\n            });\\n            \\n            // å‘é€ç§ä¿¡\\n            sendMessageBtn?.addEventListener('click', function() {\\n                const message = messageInput.value.trim();\\n                if (message) {\\n                    // è·å–å½“å‰ç”¨æˆ·å\\n                    const username = document.querySelector('.profile-name')?.textContent || 'ç”¨æˆ·';\\n                    \\n                    if (typeof triggerSlash === 'function') {\\n                        triggerSlash(`/send ã€è®ºå›ç§ä¿¡ã€‘{{User}}ç»™${username}å‘é€ç§ä¿¡ï¼š${message}|/trigger`);\\n                    } else {\\n                        console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n                    }\\n                    \\n                    // å…³é—­å¼¹çª—å¹¶æ¸…ç©ºè¾“å…¥\\n                    toggleDialog('message-dialog', false);\\n                    messageInput.value = '';\\n                }\\n            });\\n            \\n            // åœ¨è¾“å…¥æ¡†ä¸­æŒ‰å›è½¦ä¹Ÿå¯ä»¥å‘é€ç§ä¿¡\\n            messageInput?.addEventListener('keypress', function(e) {\\n                if (e.key === 'Enter' && messageInput.value.trim()) {\\n                    sendMessageBtn.click();\\n                }\\n            });\\n        }\\n\\n        // ä¿å­˜åšä¸»æ•°æ®åˆ°èŠå¤©å˜é‡\\n        async function saveBloggerData() {\\n            try {\\n                // æ£€æŸ¥æ˜¯å¦å·²å…³æ³¨\\n                const followBtn = document.querySelector('.follow-btn');\\n                const isFollowed = followBtn.textContent.includes('å·²å…³æ³¨');\\n                \\n                // å¦‚æœæœªå…³æ³¨ï¼Œæ‰ä¿å­˜æ•°æ®\\n                if (!isFollowed) {\\n                    // è·å–åšä¸»ä¿¡æ¯\\n                    const username = document.querySelector('.profile-name')?.textContent || 'æœªçŸ¥ç”¨æˆ·';\\n                    const userId = document.querySelector('.profile-id')?.textContent.replace('å°çº¢ä¹¦å·ï¼š', '') || '';\\n                    const bio = document.querySelector('.profile-bio')?.textContent || '';\\n                    const following = document.querySelectorAll('.stat-value')[0]?.textContent || '0';\\n                    const followers = document.querySelectorAll('.stat-value')[1]?.textContent || '0';\\n                    const likes = document.querySelectorAll('.stat-value')[2]?.textContent || '0';\\n                    \\n                    // è·å–åšä¸»å¤´åƒä¿¡æ¯\\n                    let avatarUrl = '';\\n                    const avatarImg = document.querySelector('.profile-avatar img');\\n                    if (avatarImg && avatarImg.src) {\\n                        avatarUrl = avatarImg.src;\\n                        // å¦‚æœæ˜¯å®Œæ•´URLï¼Œå°è¯•æå–æ–‡ä»¶åéƒ¨åˆ†\\n                        if (avatarUrl.includes('catbox.moe')) {\\n                            const match = avatarUrl.match(/([a-zA-Z0-9_-]+\\\\.[a-zA-Z0-9]+)$/);\\n                            if (match) {\\n                                // ä¿å­˜å¤´åƒæ ¼å¼ï¼šä¸ºä¾¿äº_æˆ‘çš„ä¸»é¡µ.htmlæå–ï¼Œä½¿ç”¨åŒ…å«ä¸­æ–‡éƒ¨åˆ†çš„æ ¼å¼\\n                                avatarUrl = `å¤´åƒ${match[1]}`;\\n                            }\\n                        } else if (window.cachedData && window.cachedData.header && window.cachedData.header.avatar) {\\n                            // å¦‚æœæ— æ³•ä»imgæ ‡ç­¾è·å–ï¼Œå°è¯•ä»ç¼“å­˜æ•°æ®è·å–\\n                            const cachedAvatar = window.cachedData.header.avatar;\\n                            const match = cachedAvatar.match(/([a-zA-Z0-9_-]+\\\\.[a-zA-Z0-9]+)$/);\\n                            if (match) {\\n                                avatarUrl = `å¤´åƒ${match[1]}`;\\n                            }\\n                        }\\n                    }\\n                    \\n                    // å°è¯•è·å–çœŸå®å§“å - é¦–å…ˆæŸ¥æ‰¾æ˜¾å¼æ ‡è®°çš„å…ƒç´ \\n                    let realName = '';\\n                    const realNameElement = document.querySelector('.profile-real-name, .real-name');\\n                    if (realNameElement && realNameElement.textContent) {\\n                        realName = realNameElement.textContent.trim();\\n                    } else {\\n                        // å¦‚æœæ²¡æœ‰æ‰¾åˆ°æ˜¾å¼æ ‡è®°çš„å…ƒç´ ï¼Œå°è¯•ä»ç¼“å­˜æ•°æ®ä¸­è·å–\\n                        if (window.cachedData && window.cachedData.header && window.cachedData.header.realName) {\\n                            realName = window.cachedData.header.realName;\\n                        } else {\\n                            // ä½¿ç”¨ç”¨æˆ·åä½œä¸ºé»˜è®¤å€¼\\n                            realName = username;\\n                        }\\n                    }\\n                    \\n                    // åˆ›å»ºä¿å­˜çš„å˜é‡å¯¹è±¡ - æ·»åŠ å¤´åƒä¿¡æ¯å’ŒçœŸå®å§“å\\n                    const dataToSave = {\\n                        å…³æ³¨çš„åšä¸»: {\\n                            [username]: {\\n                                æ˜µç§°: username,\\n                                ç”¨æˆ·ID: userId,\\n                                ä¸ªäººç®€ä»‹: bio,\\n                                å¤´åƒ: avatarUrl,\\n                                çœŸå®å§“å: realName,\\n                                å…³æ³¨æ•°: following,\\n                                ç²‰ä¸æ•°: followers,\\n                                è·èµæ•°: likes,\\n                                å…³æ³¨æ—¶é—´: new Date().toLocaleString()\\n                            }\\n                        }\\n                    };\\n                    \\n                    // ä¿å­˜æ•°æ®åˆ°èŠå¤©å˜é‡\\n                    if (typeof insertOrAssignVariables === 'function') {\\n                        await insertOrAssignVariables(dataToSave);\\n                        console.log('å·²æˆåŠŸä¿å­˜åšä¸»æ•°æ®åˆ°èŠå¤©å˜é‡:', dataToSave);\\n                        \\n                        // å¦‚æœå˜é‡ä¿å­˜æˆåŠŸï¼Œå‘é€æç¤ºä¿¡æ¯\\n                        if (typeof triggerSlash === 'function') {\\n                            triggerSlash(`/echo å·²æˆåŠŸå…³æ³¨${username}å¹¶ä¿å­˜åšä¸»æ•°æ®`);\\n                        }\\n                    } else {\\n                        // å¦‚æœå‡½æ•°ä¸å­˜åœ¨ï¼Œå°è¯•ä½¿ç”¨window.parentçš„æ–¹æ³•\\n                        console.warn('insertOrAssignVariableså‡½æ•°ä¸å¯ç”¨ï¼Œå°è¯•ä½¿ç”¨æ›¿ä»£æ–¹æ³•');\\n                        if (typeof updateVariablesWith === 'function') {\\n                            await updateVariablesWith(variables => {\\n                                if (!variables.å…³æ³¨çš„åšä¸») {\\n                                    variables.å…³æ³¨çš„åšä¸» = {};\\n                                }\\n                                variables.å…³æ³¨çš„åšä¸»[username] = {\\n                                    ç”¨æˆ·å: username,\\n                                    ç”¨æˆ·ID: userId,\\n                                    ç®€ä»‹: bio,\\n                                    å¤´åƒ: avatarUrl,\\n                                    çœŸå®å§“å: realName,\\n                                    å…³æ³¨æ•°: following,\\n                                    ç²‰ä¸æ•°: followers,\\n                                    è·èµæ•°: likes,\\n                                    å…³