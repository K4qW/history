     const variables = await getVariables();\\n                console.log('è·å–åˆ°çš„å˜é‡ï¼š', variables);\\n                \\n                // åŠ è½½ä¸ªäººèµ„æ–™\\n                loadProfile(variables);\\n                \\n                // åŠ è½½æˆ‘å‘å¸ƒçš„å¸–å­\\n                loadMyPosts(variables);\\n                \\n                // åŠ è½½æ”¶è—çš„å¸–å­\\n                loadFavorites(variables);\\n                \\n                // åŠ è½½å…³æ³¨çš„äºº\\n                loadFollowing(variables);\\n                \\n                // æ›´æ–°ç»Ÿè®¡æ•°æ®\\n                updateStats(variables);\\n            } catch (error) {\\n                console.error('åŠ è½½æ•°æ®å‡ºé”™:', error);\\n                showError('åŠ è½½æ•°æ®æ—¶å‡ºé”™: ' + error.message);\\n            }\\n        }\\n\\n        // åŠ è½½ä¸ªäººèµ„æ–™\\n        function loadProfile(variables) {\\n            // æ£€æŸ¥æ˜¯å¦æœ‰ä¿å­˜çš„ä¸ªäººèµ„æ–™\\n            if (!variables || !variables.æˆ‘çš„ä¸ªäººèµ„æ–™) {\\n                console.log('æ²¡æœ‰ä¿å­˜çš„ä¸ªäººèµ„æ–™ï¼Œä½¿ç”¨é»˜è®¤å€¼');\\n                \\n                // è®¾ç½®é»˜è®¤å€¼\\n                document.getElementById('profile-name').textContent = '{{User}}';\\n                document.getElementById('profile-id').textContent = 'å°çº¢ä¹¦å·: 12345678';\\n                document.getElementById('profile-avatar').src = 'https://files.catbox.moe/kz8xdq.JPG';\\n                return;\\n            }\\n            \\n            const profile = variables.æˆ‘çš„ä¸ªäººèµ„æ–™;\\n            \\n            // æ›´æ–°ç”¨æˆ·å\\n            if (profile.ç”¨æˆ·å) {\\n                document.getElementById('profile-name').textContent = profile.ç”¨æˆ·å;\\n            }\\n            \\n            // æ›´æ–°å°çº¢ä¹¦å·\\n            if (profile.å°çº¢ä¹¦å·) {\\n                document.getElementById('profile-id').textContent = `å°çº¢ä¹¦å·: ${profile.å°çº¢ä¹¦å·}`;\\n            }\\n            \\n            // æ›´æ–°å¤´åƒ\\n            if (profile.å¤´åƒ) {\\n                // ä½¿ç”¨processAvatarUrlå‡½æ•°å¤„ç†å¤´åƒURL\\n                document.getElementById('profile-avatar').src = processAvatarUrl(profile.å¤´åƒ);\\n                \\n                // æ›´æ–°å¤´åƒé€‰æ‹©å™¨çš„é€‰ä¸­çŠ¶æ€\\n                if (avatarSelector && avatarSelector.avatarList.includes(profile.å¤´åƒ)) {\\n                    avatarSelector.selectedAvatar = profile.å¤´åƒ;\\n                }\\n            }\\n        }\\n        \\n        // æ˜¾ç¤ºé”™è¯¯ä¿¡æ¯\\n        function showError(message) {\\n            const sections = document.querySelectorAll('.content-section');\\n            sections.forEach(section => {\\n                const emptyState = section.querySelector('.empty-state');\\n                if (emptyState) {\\n                    emptyState.innerHTML = `\\n                        <i class=\\\"fas fa-exclamation-circle\\\"></i>\\n                        <p>${message}</p>\\n                    `;\\n                }\\n            });\\n        }\\n        \\n        // æ ¼å¼åŒ–æ—¥æœŸæ—¶é—´\\n        function formatDate(dateString) {\\n            try {\\n                const date = new Date(dateString);\\n                \\n                // æ£€æŸ¥æ—¥æœŸæ˜¯å¦æœ‰æ•ˆ\\n                if (isNaN(date.getTime())) {\\n                    return dateString; // è¿”å›åŸå§‹å­—ç¬¦ä¸²\\n                }\\n                \\n                const now = new Date();\\n                const diff = now - date;\\n                \\n                // å°äº1åˆ†é’Ÿ\\n                if (diff < 60 * 1000) {\\n                    return 'åˆšåˆš';\\n                }\\n                \\n                // å°äº1å°æ—¶\\n                if (diff < 60 * 60 * 1000) {\\n                    return `${Math.floor(diff / (60 * 1000))}åˆ†é’Ÿå‰`;\\n                }\\n                \\n                // å°äº24å°æ—¶\\n                if (diff < 24 * 60 * 60 * 1000) {\\n                    return `${Math.floor(diff / (60 * 60 * 1000))}å°æ—¶å‰`;\\n                }\\n                \\n                // å°äº30å¤©\\n                if (diff < 30 * 24 * 60 * 60 * 1000) {\\n                    return `${Math.floor(diff / (24 * 60 * 60 * 1000))}å¤©å‰`;\\n                }\\n                \\n                // å¤§äº30å¤©åˆ™è¿”å›å…·ä½“æ—¥æœŸ\\n                const year = date.getFullYear();\\n                const month = date.getMonth() + 1;\\n                const day = date.getDate();\\n                \\n                return `${year}-${month.toString().padStart(2, '0')}-${day.toString().padStart(2, '0')}`;\\n            } catch (error) {\\n                console.error('æ—¥æœŸæ ¼å¼åŒ–é”™è¯¯', error);\\n                return dateString; // å‡ºé”™æ—¶è¿”å›åŸå§‹å­—ç¬¦ä¸²\\n            }\\n        }\\n        \\n        // è·å–å¹¶æ˜¾ç¤ºæˆ‘å‘å¸ƒçš„å¸–å­\\n        function loadMyPosts(variables) {\\n            const postsSection = document.getElementById('my-posts-section');\\n            \\n            // æ£€æŸ¥å˜é‡æ˜¯å¦å­˜åœ¨\\n            if (!variables || !variables['æˆ‘å‘å¸ƒçš„å¸–å­']) {\\n                console.log('æ²¡æœ‰å‘å¸ƒçš„å¸–å­æ•°æ®');\\n                return;\\n            }\\n            \\n            const postData = variables['æˆ‘å‘å¸ƒçš„å¸–å­'];\\n            \\n            // æ¸…ç©ºç©ºçŠ¶æ€ï¼Œå‡†å¤‡æ·»åŠ å†…å®¹\\n            postsSection.innerHTML = '';\\n            \\n            // åˆ›å»ºå¸–å­ç½‘æ ¼å®¹å™¨\\n            const postGrid = document.createElement('div');\\n            postGrid.className = 'post-grid';\\n            \\n            // æ·»åŠ å¸–å­å¡ç‰‡\\n            renderPostCard(postGrid, postData, false);\\n            \\n            // å°†ç½‘æ ¼æ·»åŠ åˆ°å¸–å­åŒºåŸŸ\\n            postsSection.appendChild(postGrid);\\n        }\\n        \\n        // è·å–å¹¶æ˜¾ç¤ºæ”¶è—çš„å¸–å­\\n        function loadFavorites(variables) {\\n            const favoritesSection = document.getElementById('favorites-section');\\n            \\n            // æ£€æŸ¥å˜é‡æ˜¯å¦å­˜å