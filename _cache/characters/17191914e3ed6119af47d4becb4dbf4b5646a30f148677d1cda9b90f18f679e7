\\n        </div>\\n        <div class=\\\"refresh-btn\\\">\\n            <i class=\\\"fas fa-sync\\\"></i>\\n        </div>\\n    </div>\\n\\n    <!-- ä¸»è¦å†…å®¹ -->\\n    <div class=\\\"container\\\">\\n        <!-- é¡µé¢åˆ‡æ¢é€‰é¡¹å¡ -->\\n        <div class=\\\"page-tabs\\\">\\n            <div class=\\\"page-tab active\\\" data-tab=\\\"hot\\\">çƒ­é—¨</div>\\n            <div class=\\\"page-tab\\\" data-tab=\\\"nearby\\\">é™„è¿‘</div>\\n        </div>\\n        \\n        <!-- çƒ­é—¨æ’è¡Œæ¦œå¸ƒå±€ -->\\n        <div class=\\\"rank-list\\\" id=\\\"hot-content\\\">\\n            <!-- æ’è¡Œæ¦œå†…å®¹å°†ç”±JSåŠ¨æ€ç”Ÿæˆ -->\\n        </div>\\n\\n        <!-- é™„è¿‘ç€‘å¸ƒæµå¸ƒå±€ -->\\n        <div class=\\\"waterfall\\\" id=\\\"nearby-content\\\" style=\\\"display: none;\\\">\\n            <!-- é™„è¿‘å†…å®¹å°†ç”±JSåŠ¨æ€ç”Ÿæˆ -->\\n        </div>\\n    </div>\\n\\n    <!-- åº•éƒ¨å¯¼èˆªæ  -->\\n    <div class=\\\"bottom-navbar\\\">\\n        <a href=\\\"#\\\" class=\\\"nav-item\\\" id=\\\"nav-home\\\">\\n            <i class=\\\"fas fa-home\\\"></i>\\n            <span>é¦–é¡µ</span>\\n        </a>\\n        <a href=\\\"#\\\" class=\\\"nav-item active\\\">\\n            <i class=\\\"fas fa-compass\\\"></i>\\n            <span>å‘ç°</span>\\n        </a>\\n        <div class=\\\"publish-btn-container\\\">\\n            <div class=\\\"publish-btn\\\" id=\\\"nav-create\\\">\\n                <i class=\\\"fas fa-plus\\\"></i>\\n            </div>\\n        </div>\\n        <a href=\\\"#\\\" class=\\\"nav-item\\\" id=\\\"nav-message\\\">\\n            <i class=\\\"fas fa-bell\\\"></i>\\n            <span>æ¶ˆæ¯</span>\\n        </a>\\n        <a href=\\\"#\\\" class=\\\"nav-item\\\">\\n            <i class=\\\"fas fa-user\\\"></i>\\n            <span>æˆ‘çš„</span>\\n        </a>\\n    </div>\\n\\n    <!-- å›é¡¶æŒ‰é’® -->\\n    <div class=\\\"back-to-top\\\" id=\\\"backToTop\\\">\\n        <i class=\\\"fas fa-arrow-up\\\"></i>\\n    </div>\\n\\n    <script>\\n        // å¤„ç†å¤´åƒURLï¼Œæå–è‹±æ–‡éƒ¨åˆ†å¹¶æ·»åŠ å‰ç¼€\\n        function processAvatarUrl(avatar) {\\n            // å¦‚æœå¤´åƒæ ¼å¼åŒ…å«ä¸­æ–‡ï¼Œåˆ™æå–è‹±æ–‡éƒ¨åˆ†\\n            if (/[\\\\u4e00-\\\\u9fa5]/.test(avatar)) {\\n                const match = avatar.match(/([a-zA-Z0-9]+\\\\.[a-zA-Z]+)$/);\\n                if (match) {\\n                    return `https://files.catbox.moe/${match[1]}`;\\n                }\\n            }\\n            return `https://files.catbox.moe/${avatar}`;\\n        }\\n\\n        // 1. åŸºç¡€æ¨¡æ¿å®šä¹‰\\n        let gameText = `\\n<find>$1</find>`;\\n\\n        // 2. æ•°æ®æå–å™¨\\n        const DataExtractor = {\\n            extractFindData(text) {\\n                try {\\n                    // åŒ¹é… <find> æ ‡ç­¾å†…çš„å†…å®¹\\n                    const findMatch = text.match(/<find>([\\\\s\\\\S]*?)<\\\\/find>/);\\n                    if (!findMatch) return null;\\n                    \\n                    const findContent = findMatch[1].trim();\\n                    \\n                    // æå–çƒ­é—¨å†…å®¹\\n                    const hotMatch = findContent.match(/<hot>([\\\\s\\\\S]*?)<\\\\/hot>/);\\n                    const hotContent = hotMatch ? hotMatch[1].trim() : '';\\n                    \\n                    // æå–é™„è¿‘å†…å®¹\\n                    const nearbyMatch = findContent.match(/<nearby>([\\\\s\\\\S]*?)<\\\\/nearby>/);\\n                    const nearbyContent = nearbyMatch ? nearbyMatch[1].trim() : '';\\n                    \\n                    // è§£æç½®é¡¶å¸–å’Œçƒ­é—¨å¸–å­\\n                    const hotPosts = this.extractHotPosts(hotContent);\\n                    \\n                    // è§£æé™„è¿‘å¸–å­\\n                    const nearbyPosts = this.extractNearbyPosts(nearbyContent);\\n                    \\n                    return {\\n                        hotPosts,\\n                        nearbyPosts\\n                    };\\n                } catch (error) {\\n                    console.error('è§£æå‘ç°é¡µæ•°æ®å¤±è´¥:', error);\\n                    return {\\n                        hotPosts: [],\\n                        nearbyPosts: []\\n                    };\\n                }\\n            },\\n            \\n            extractHotPosts(content) {\\n                if (!content) return [];\\n                \\n                const posts = [];\\n                \\n                // æå–ç½®é¡¶å¸–\\n                const stickyPostRegex = /\\\\[ç½®é¡¶å¸–\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/g;\\n                let stickyMatch;\\n                \\n                while ((stickyMatch = stickyPostRegex.exec(content)) !== null) {\\n                    posts.push({\\n                        username: stickyMatch[1] || 'æœªçŸ¥ç”¨æˆ·',\\n                        avatar: processAvatarUrl(stickyMatch[2]) || 'default',\\n                        title: stickyMatch[3] || 'æ— æ ‡é¢˜',\\n                        description: stickyMatch[4] || 'æ— å†…å®¹',\\n                        likes: parseInt(stickyMatch[5]) || 0,\\n                        comments: parseInt(stickyMatch[6]) || 0,\\n                isSticky: true\\n                    });\\n                }\\n                \\n                // æå–æ™®é€šçƒ­é—¨å¸–å­\\n                const postRegex = /\\\\[å¸–å­\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/g;\\n                let postMatch;\\n                \\n                while ((postMatch = postRegex.exec(content)) !== null) {\\n                    posts.push({\\n                        username: postMatch[1] || 'æœªçŸ¥ç”¨æˆ·',\\n                        avatar: processAvatarUrl(postMatch[2]) || 'default',\\n                        title: postMatch[3] || 'æ— æ ‡é¢˜',\\n                        description: postMatch[4] || 'æ— å†…å®¹',\\n                        likes: parseInt(postMatch[5]) || 0,\\n                        comments: parseInt(postMatch[6]) || 0,\\n                        isSticky: false\\n                    });\\n                }\\n                \\n                return posts;\\n            },\\n            \\n            extractNearbyPosts(content) {\\n                if (!content) return [];\\n                \\n                const nearbyPostRegex = /\\\\[å¸–å­\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/g;\\n                const posts = [];\\n                let match;\\n                \\n                while ((match = nearbyPostRegex.exec(content)) !== null) {\\n                    posts.push({\\n                        username: match[1] || 'æœªçŸ¥ç”¨æˆ·',\\n                        avatar: processAvatarUrl(match[2]) || 'default',\\n                        distance: match[3] || 'æœªçŸ¥è·ç¦»',\\n                        title: match[4] || 'æ— æ ‡é¢˜',\\n                        description: match[5] || 'æ— å†…å®¹',\\n                        imageSummary: match[6] || 'æ— å›¾ç‰‡æè¿°',\\n                        likes: parseInt(match[7]) || 0,\\n                        comments: parseInt(match[8]) || 0,\\n                        tag: match[9] || 'æœªåˆ†ç±»'\\n                    });\\n                }\\n                \\n                return posts;\\n            },\\n            \\n            parseGameText(gameText) {\\n                // ç›´æ¥è§£æå‘ç°é¡µæ•°æ®\\n                return this.extractFindData(gameText);\\n            }\\n        };\\n\\n        // 3. UIæ›´æ–°å™¨\\n        const UIUpdater = {\\n            // æ›´æ–°çƒ­é—¨æ’è¡Œæ¦œ\\n            updateHotRanks(hotPosts) {\\n                if (!hotPosts || !hotPosts.length) {\\n                    console.warn('æ²¡æœ‰çƒ­é—¨å†…å®¹å¯æ˜¾ç¤º');\\n                    return;\\n                }\\n                \\n            const hotContent = document.getElementById(\\\"hot-content\\\");\\n            hotContent.innerHTML = \\\"\\\";\\n            \\n            // è·Ÿè¸ªæ’è¡Œæ¦œç¼–å·\\n            let rankNumber = 1;\\n\\n            hotPosts.forEach((post) => {\\n                const rankItem = document.createElement(\\\"div\\\");\\n                \\n                // æ·»åŠ ç½®é¡¶æ ·å¼\\n                if (post.isSticky) {\\n                    rankItem.className = \\\"rank-item sticky\\\";\\n                    rankItem.innerHTML = `\\n                        <span class=\\\"sticky-badge\\\">ç½®é¡¶</span>\\n                        <div class=\\\"rank-image\\\">\\n                                <img src=\\\"${post.avatar}\\\" alt=\\\"${post.title}\\\">\\n                        </div>\\n                        <div class=\\\"rank-info\\\">\\n                            <div class=\\\"rank-title\\\">${post.title}</div>\\n                            <div class=\\\"rank-description\\\">${post.description}</div>\\n                            <div class=\\\"rank-meta\\\">\\n                                <div class=\\\"rank-user\\\">\\n                                    <img src=\\\"${post.avatar}\\\" alt=\\\"${post.username}\\\">\\n                                    <span>${post.username}</span>\\n                                </div>\\n                                <div class=\\\"rank-stats\\\">\\n                                    <span><i class=\\\"far fa-heart\\\"></i> ${post.likes}</span>\\n                                    <span><i class=\\\"far fa-comment\\\"></i> ${post.comments}</span>\\n                                </div>\\n                            </div>\\n                        </div>\\n                        `\\n                } else {\\n                    rankItem.className = \\\"rank-item\\\";\\n                    rankItem.innerHTML = `\\n                        <div class=\\\"rank-number\\\">${rankNumber}</div>\\n                        <div class=\\\"rank-image\\\">\\n                                <img src=\\\"${post.avatar}\\\" alt=\\\"${post.title}\\\">\\n                        </div>\\n                        <div class=\\\"rank-info\\\">\\n                            <div class=\\\"rank-title\\\">${post.title}</div>\\n                            <div class=\\\"rank-description\\\">${post.description}</div>\\n                            <div class=\\\"rank-meta\\\">\\n                                <div class=\\\"rank-user\\\">\\n                                    <img src=\\\"${post.avatar}\\\" alt=\\\"${post.username}\\\">\\n                                    <span>${post.username}</span>\\n                                </div>\\n                                <div class=\\\"rank-stats\\\">\\n                                    <span><i class=\\\"far fa-heart\\\"></i> ${post.likes}</span>\\n                                    <span><i class=\\\"far fa-comment\\\"></i> ${post.comments}</span>\\n                                </div>\\n                            </div>\\n                        </div>\\n                    `;\\n                        \\n                        // ä¸ºå‰ä¸‰åè®¾ç½®ä¸åŒæ·±åº¦çš„çº¢è‰²èƒŒæ™¯ï¼Œè€ƒè™‘åˆ°ç½®é¡¶å¸–çš„å­˜åœ¨\\n                        if (rankNumber === 1) {\\n                            rankItem.querySelector('.rank-number').style.backgroundColor = '#ff2e4d'; // ç¬¬ä¸€åï¼šæœ€æ·±çš„çº¢è‰²\\n                        } else if (rankNumber === 2) {\\n                            rankItem.querySelector('.rank-number').style.backgroundColor = '#ff6b81'; // ç¬¬äºŒåï¼šä¸­ç­‰æ·±åº¦çš„çº¢è‰²\\n                        } else if (rankNumber === 3) {\\n                            rankItem.querySelector('.rank-number').style.backgroundColor = '#ff9eb5'; // ç¬¬ä¸‰åï¼šè¾ƒæµ…çš„çº¢è‰²\\n                        } else {\\n                            rankItem.querySelector('.rank-number').style.backgroundColor = '#ffccd5'; // å…¶ä»–ï¼šæœ€æµ…çš„çº¢è‰²\\n                        }\\n                        \\n                        // å¢åŠ æ’åç¼–å·\\n                    rankNumber++;\\n                }\\n                hotContent.appendChild(rankItem);\\n            });\\n            },\\n            \\n            // æ›´æ–°é™„è¿‘ç€‘å¸ƒæµ\\n            updateNearbyWaterfall(nearbyPosts) {\\n                if (!nearbyPosts || !nearbyPosts.length) {\\n                    console.warn('æ²¡æœ‰é™„è¿‘å†…å®¹å¯æ˜¾ç¤º');\\n                    return;\\n                }\\n                \\n            // è·å–å®¹å™¨å’Œå®½åº¦\\n            const container = document.getElementById(\\\"nearby-content\\\");\\n            container.innerHTML = \\\"\\\"; // æ¸…ç©ºå†…å®¹\\n            \\n            // æ ¹æ®å±å¹•å®½åº¦ç¡®å®šåˆ—æ•°\\n            let columnCount;\\n            if (window.innerWidth >= 1200) {\\n                columnCount = 4;\\n            } else if (window.innerWidth >= 992) {\\n                columnCount = 3;\\n            } else if (window.innerWidth >= 576) {\\n                columnCount = 2;\\n            } else {\\n                columnCount = 1;\\n            }\\n            \\n            // åˆ›å»ºåˆ—\\n            const columns = [];\\n            for (let i = 0; i < columnCount; i++) {\\n                const column = document.createElement(\\\"div\\\");\\n                column.className = \\\"waterfall-column\\\";\\n                column.style.width = (100 / columnCount) + \\\"%\\\";\\n                column.style.float = \\\"left\\\";\\n                column.style.boxSizing = \\\"border-box\\\";\\n                column.style.padding = \\\"0 10px\\\";\\n                container.appendChild(column);\\n                columns.push(column);\\n            }\\n            \\n            // åˆ†é…å¸–å­åˆ°å„åˆ—\\n            nearbyPosts.forEach((post, index) => {\\n                // ç®€å•è½®è¯¢åˆ†é…åˆ°å„åˆ—ï¼Œç¡®ä¿åˆå§‹æ—¶å‡åŒ€åˆ†å¸ƒ\\n                const column = columns[index % columnCount];\\n                \\n                const postCard = document.createElement(\\\"div\\\");\\n                postCard.className = \\\"post-card\\\";\\n                    \\n                    // ä»å›¾ç‰‡ç®€ä»‹ä¸­æå–å®é™…æè¿°æ–‡æœ¬\\n                    const imageSummaryText = post.imageSummary.replace('å›¾ç‰‡ç®€ä»‹ï¼š', '');\\n                    \\n                postCard.innerHTML = `\\n                    <div class=\\\"image-container\\\">\\n                        <div class=\\\"image-mask\\\">\\n                            <span>ç‚¹å‡»æŸ¥çœ‹å†…å®¹</span>\\n                        </div>\\n                        <div class=\\\"image-summary\\\">\\n                            <h4>å›¾ç‰‡ç®€ä»‹</h4>\\n                                <p>${imageSummaryText}</p>\\n                        </div>\\n                        <div class=\\\"nearby-distance\\\">\\n                            <i class=\\\"fas fa-map-marker-alt\\\"></i> ${post.distance}\\n                        </div>\\n                    </div>\\n                    <div class=\\\"post-content\\\">\\n                        <h3 class=\\\"post-title\\\">${post.title}</h3>\\n                        <p class=\\\"post-description\\\">${post.description}</p>\\n                        <div class=\\\"post-meta\\\">\\n                            <div class=\\\"user-info\\\">\\n                                <img src=\\\"${post.avatar}\\\" alt=\\\"${post.username}\\\" class=\\\"avatar\\\">\\n                                <span class=\\\"username\\\">${post.username}</span>\\n                            </div>\\n                            <div class=\\\"post-stats\\\">\\n                                <span><i class=\\\"far fa-heart\\\"></i> ${post.likes}</span>\\n                                <span><i class=\\\"far fa-comment\\\"></i> ${post.comments}</span>\\n                            </div>\\n                        </div>\\n                    </div>\\n                `;\\n                column.appendChild(postCard);\\n                \\n                // ç»‘å®šç‚¹å‡»äº‹ä»¶\\n                const imageContainer = postCard.querySelector('.image-container');\\n                imageContainer.addEventListener('click', function() {\\n                    this.classList.toggle('active-mask');\\n                });\\n            });\\n            \\n            // æ¸…é™¤æµ®åŠ¨\\n            const clearfix = document.createElement(\\\"div\\\");\\n            clearfix.style.clear = \\\"both\\\";\\n            container.appendChild(clearfix);\\n            },\\n            \\n            // æ›´æ–°æ‰€æœ‰UI\\n            updateAllUI(data) {\\n                if (!data) {\\n                    console.error('æ— æ•ˆçš„æ•°æ®ï¼Œæ— æ³•æ›´æ–°UI');\\n                    return;\\n                }\\n                \\n                // æ›´æ–°å…¨å±€æ•°æ®å¼•ç”¨\\n                window.currentHotPosts = data.hotPosts;\\n                window.currentNearbyPosts = data.nearbyPosts;\\n                \\n                // æ ¹æ®å½“å‰é€‰ä¸­çš„tabæ˜¾ç¤ºå†…å®¹\\n                if (currentTab === \\\"hot\\\") {\\n                    document.getElementById(\\\"hot-content\\\").style.display = \\\"block\\\";\\n                    document.getElementById(\\\"nearby-content\\\").style.display = \\\"none\\\";\\n                    this.updateHotRanks(data.hotPosts);\\n                } else if (currentTab === \\\"nearby\\\") {\\n                    document.getElementById(\\\"hot-content\\\").style.display = \\\"none\\\";\\n                    document.getElementById(\\\"nearby-content\\\").style.display = \\\"block\\\";\\n                    this.updateNearbyWaterfall(data.nearbyPosts);\\n                }\\n            },\\n            \\n            // åˆå§‹åŒ–UI\\n            initializeUI() {\\n                try {\\n                    // è§£ææ¸¸æˆæ–‡æœ¬\\n                    const parsedData = DataExtractor.parseGameText(gameText);\\n                    \\n                    // æ›´æ–°UI\\n                    if (parsedData) {\\n                        this.updateAllUI(parsedData);\\n                    } else {\\n                        console.error('æ— æ³•åˆå§‹åŒ–UIï¼Œæ•°æ®è§£æå¤±è´¥');\\n                    }\\n                } catch (error) {\\n                    console.error('UIåˆå§‹åŒ–å¤±è´¥:', error);\\n                }\\n            }\\n        };\\n        \\n        // è·Ÿè¸ªå½“å‰æ˜¾ç¤ºçš„å†…å®¹ç±»å‹\\n        let currentTab = \\\"hot\\\";\\n\\n        // å…¨å±€å˜é‡å­˜å‚¨å¸–å­æ•°æ®\\n        window.currentHotPosts = [];\\n        window.currentNearbyPosts = [];\\n\\n        // é¡µé¢é€‰é¡¹å¡åˆ‡æ¢\\n        document.querySelectorAll(\\\".page-tab\\\").forEach(tab => {\\n            tab.addEventListener(\\\"click\\\", function() {\\n                // ç§»é™¤æ‰€æœ‰é€‰é¡¹å¡çš„activeç±»\\n                document.querySelectorAll(\\\".page-tab\\\").forEach(t => {\\n                    t.classList.remove(\\\"active\\\");\\n                });\\n                // ä¸ºå½“å‰ç‚¹å‡»çš„é€‰é¡¹å¡æ·»åŠ activeç±»\\n                this.classList.add(\\\"active\\\");\\n                \\n                // è·å–å½“å‰é€‰ä¸­çš„é€‰é¡¹å¡ç±»å‹\\n                const tabType = this.getAttribute(\\\"data-tab\\\");\\n                \\n                // æ ¹æ®é€‰é¡¹å¡ç±»å‹åˆ‡æ¢å†…å®¹\\n                if (tabType === \\\"hot\\\") {\\n                    currentTab = \\\"hot\\\";\\n                    document.getElementById(\\\"hot-content\\\").style.display = \\\"block\\\";\\n                    document.getElementById(\\\"nearby-content\\\").style.display = \\\"none\\\";\\n                    \\n                    // ä½¿ç”¨UIæ›´æ–°å™¨æ˜¾ç¤ºçƒ­é—¨å†…å®¹\\n                    UIUpdater.updateHotRanks(window.currentHotPosts);\\n                } else if (tabType === \\\"nearby\\\") {\\n                    currentTab = \\\"nearby\\\";\\n                    document.getElementById(\\\"hot-content\\\").style.display = \\\"none\\\";\\n                    document.getElementById(\\\"nearby-content\\\").style.display = \\\"block\\\";\\n                    \\n                    // ä½¿ç”¨UIæ›´æ–°å™¨æ˜¾ç¤ºé™„è¿‘å†…å®¹\\n                    UIUpdater.updateNearbyWaterfall(window.currentNearbyPosts);\\n                }\\n            });\\n        });\\n\\n        // 4. åˆå§‹åŒ–\\n        window.addEventListener(\\\"load\\\", function() {\\n            // åˆå§‹åŒ–UI\\n            UIUpdater.initializeUI();\\n            \\n            // ç»‘å®šæœç´¢äº‹ä»¶\\n            const searchInput = document.getElementById('search-input');\\n            const searchButton = document.getElementById('search-button');\\n            \\n            if (searchButton && searchInput) {\\n                searchButton.addEventListener('click', function() {\\n                    performSearch();\\n                });\\n                \\n                searchInput.addEventListener('keypress', function(e) {\\n                    if (e.key === 'Enter') {\\n                        performSearch();\\n                    }\\n                });\\n            }\\n            \\n            // ç»‘å®šåˆ·æ–°æŒ‰é’®ç‚¹å‡»äº‹ä»¶\\n            const refreshBtn = document.querySelector('.refresh-btn');\\n            if (refreshBtn) {\\n                refreshBtn.addEventListener('click', function() {\\n                    refreshDiscovery();\\n                });\\n            }\\n            \\n            // ç»‘å®šåº•éƒ¨å¯¼èˆªæ äº‹ä»¶\\n            bindNavBarEvents();\\n            \\n            // ç»‘å®šå¸–å­ç‚¹å‡»äº‹ä»¶\\n            bindPostClickEvents();\\n            \\n            // ç»‘å®šå›é¡¶æŒ‰é’®äº‹ä»¶\\n            initBackToTop();\\n        });\\n        \\n        // ç»‘å®šåº•éƒ¨å¯¼èˆªæ äº‹ä»¶\\n        function bindNavBarEvents() {\\n            const navHome = document.getElementById('nav-home');\\n            const navDiscovery = document.getElementById('nav-discovery');\\n            const navMessage = document.getElementById('nav-message');\\n            const navCreate = document.getElementById('nav-create');\\n            const navUser = document.querySelector('.bottom-navbar .nav-item:nth-child(5)'); // é€‰æ‹©\\\"æˆ‘çš„\\\"å¯¼èˆªé¡¹\\n            \\n            navHome?.a