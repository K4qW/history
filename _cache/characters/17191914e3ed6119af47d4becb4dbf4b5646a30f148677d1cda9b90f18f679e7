                           variables.æ”¶è—çš„å¸–å­ && \\n                                    variables.æ”¶è—çš„å¸–å­[postTitle];\\n                \\n                if (isFavorited) {\\n                    console.log(`å·²æ”¶è—å¸–å­: ${postTitle}`);\\n                    \\n                    // æ›´æ–°æ˜Ÿæ˜Ÿå›¾æ ‡ä¸ºå®å¿ƒ\\n                    starIcon.classList.remove('far');\\n                    starIcon.classList.add('fas');\\n                } else {\\n                    console.log(`æœªæ”¶è—å¸–å­: ${postTitle}`);\\n                    \\n                    // ç¡®ä¿æ˜Ÿæ˜Ÿå›¾æ ‡ä¸ºç©ºå¿ƒ\\n                    starIcon.classList.remove('fas');\\n                    starIcon.classList.add('far');\\n                }\\n            } catch (error) {\\n                console.error('æ£€æŸ¥æ”¶è—çŠ¶æ€æ—¶å‡ºé”™:', error);\\n            }\\n        }\\n\\n        // é¡µé¢åŠ è½½å®Œæˆååˆå§‹åŒ–UI\\n        document.addEventListener('DOMContentLoaded', function() {\\n            // åˆå§‹åŒ–UI\\n            UIUpdater.initializeUI();\\n            \\n            // ç»‘å®šå›¾ç‰‡æ»‘åŠ¨å™¨äº‹ä»¶\\n            initPhotoSlider();\\n            \\n            // ç»‘å®šè¯„è®ºæ’åºé€‰é¡¹\\n            initCommentSorting();\\n            \\n            // ç»‘å®šåº•éƒ¨å¯¼èˆªæ äº‹ä»¶\\n            bindNavBarEvents();\\n            \\n            // ç»‘å®šæœç´¢äº‹ä»¶\\n            const searchInput = document.getElementById('search-input');\\n            const searchButton = document.getElementById('search-button');\\n            \\n            if (searchButton && searchInput) {\\n                searchButton.addEventListener('click', function() {\\n                    performSearch();\\n                });\\n                \\n                searchInput.addEventListener('keypress', function(e) {\\n                    if (e.key === 'Enter') {\\n                        performSearch();\\n                    }\\n                });\\n            }\\n        });\\n        \\n        // ç‚¹å‡»æœç´¢æŒ‰é’®æˆ–å›è½¦ï¼Œå‘é€æœç´¢è¯·æ±‚\\n        function performSearch() {\\n            const searchInput = document.getElementById('search-input');\\n            const keyword = searchInput.value.trim();\\n            \\n            if (!keyword) return;\\n            \\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/send ã€è®ºå›æœç´¢ã€‘æŸ¥çœ‹${keyword}çš„æœç´¢ç»“æœ|/trigger`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n\\n        // å‘é€è¯„è®º\\n        function sendComment() {\\n            const commentInput = document.querySelector('.comment-input');\\n            const comment = commentInput.value.trim();\\n            \\n            if (!comment) return; // å¦‚æœè¯„è®ºä¸ºç©ºï¼Œä¸æ‰§è¡Œä»»ä½•æ“ä½œ\\n            \\n            // è·å–å½“å‰å¸–å­ä¿¡æ¯\\n            const authorName = document.querySelector('.author-name').textContent;\\n            const postTitle = document.querySelector('.post-title').textContent;\\n            \\n            // å‘é€è¯„è®º\\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/send ã€è®ºå›å¸–å­ã€‘{{User}}åœ¨${authorName}çš„å¸–å­ã€Š${postTitle}ã€‹ä¸­è¯„è®ºï¼š${comment}|/trigger`);\\n                \\n                // æ¸…ç©ºè¾“å…¥æ¡†\\n                commentInput.value = '';\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n        \\n        // å½“å‰è¦å›å¤çš„è¯„è®ºä¿¡æ¯\\n        let currentReplyInfo = {\\n            author: '',\\n            content: ''\\n        };\\n        \\n        // æ‰“å¼€å›å¤å¼¹çª—\\n        function openReplyDialog(author, content) {\\n            // ä¿å­˜å½“å‰å›å¤çš„è¯„è®ºä¿¡æ¯\\n            currentReplyInfo.author = author;\\n            currentReplyInfo.content = content;\\n            \\n            // æ›´æ–°å¼¹çª—æ ‡é¢˜\\n            document.querySelector('.reply-dialog-title').textContent = `å›å¤ ${author}`;\\n            \\n            // æ¸…ç©ºè¾“å…¥æ¡†\\n            document.getElementById('replyInput').value = '';\\n            \\n            // æ˜¾ç¤ºå¼¹çª—\\n            const dialog = document.getElementById('replyDialog');\\n            dialog.style.display = 'flex';\\n            setTimeout(() => {\\n                dialog.classList.add('show');\\n                // èšç„¦è¾“å…¥æ¡†\\n                document.getElementById('replyInput').focus();\\n            }, 10);\\n            \\n            // ç»‘å®šå¼¹çª—äº‹ä»¶\\n            bindReplyDialogEvents();\\n        }\\n        \\n        // å…³é—­å›å¤å¼¹çª—\\n        function closeReplyDialog() {\\n            const dialog = document.getElementById('replyDialog');\\n            dialog.classList.remove('show');\\n            setTimeout(() => {\\n                dialog.style.display = 'none';\\n            }, 300);\\n        }\\n        \\n        // å‘é€å›å¤\\n        function sendReply() {\\n            const replyInput = document.getElementById('replyInput');\\n            const replyContent = replyInput.value.trim();\\n            \\n            if (!replyContent) return; // å¦‚æœå›å¤å†…å®¹ä¸ºç©ºï¼Œä¸æ‰§è¡Œä»»ä½•æ“ä½œ\\n            \\n            // è·å–å½“å‰å¸–å­ä½œè€…å’Œæ ‡é¢˜\\n            const postAuthor = document.querySelector('.author-name').textContent;\\n            const postTitle = document.querySelector('.post-title').textContent;\\n            \\n            // ä½¿ç”¨triggerSlashå‘é€æ¶ˆæ¯\\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/send ã€è®ºå›å¸–å­ã€‘{{User}}åœ¨æ¥¼ä¸­æ¥¼å›å¤${currentReplyInfo.author}çš„è¯„è®º\\\"${currentReplyInfo.content.substring(0, 20)}${currentReplyInfo.content.length > 20 ? '...' : ''}\\\"ï¼š${replyContent}|/trigger`);\\n                \\n                // å…³é—­å¼¹çª—\\n                closeReplyDialog();\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n        \\n        // ç»‘å®šå›å¤å¼¹çª—äº‹ä»¶\\n        function bindReplyDialogEvents() {\\n            // å…³é—­æŒ‰é’®ç‚¹å‡»äº‹ä»¶\\n            document.querySelector('.reply-dialog-close').addEventListener('click', closeReplyDialog);\\n            \\n            // å–æ¶ˆæŒ‰é’®ç‚¹å‡»äº‹ä»¶\\n            document.getElementById('cancelReply').addEventListener('click', closeReplyDialog);\\n            \\n            // å‘é€æŒ‰é’®ç‚¹å‡»äº‹ä»¶\\n            document.getElementById('sendReply').addEventListener('click', sendReply);\\n            \\n            // è¾“å…¥æ¡†å›è½¦äº‹ä»¶\\n            document.getElementById('replyInput').addEventListener('keypress', function(e) {\\n                // æŒ‰ä¸‹Enteré”®å‘é€\\n                if (e.key === 'Enter' && !e.shiftKey) {\\n                    e.preventDefault(); // é˜»æ­¢é»˜è®¤çš„æ¢è¡Œè¡Œä¸º\\n                    sendReply();\\n                }\\n            });\\n            \\n            // ç‚¹å‡»é®ç½©å…³é—­å¼¹çª—\\n            document.getElementById('replyDialog').addEventListener('click', function(e) {\\n                if (e.target === this) {\\n                    closeReplyDialog();\\n                }\\n            });\\n        }\\n\\n        // ç‚¹å‡»åº•éƒ¨çš„æˆ‘çš„æŒ‰é’®ï¼ŒæŸ¥çœ‹æˆ‘çš„ä¸»é¡µ\\n        function viewUserPage() {\\n            // å°†æ–‡æœ¬åˆ†å¼€ï¼Œé¿å…è¢«æ­£åˆ™è¯†åˆ«æ›¿æ¢\\n            const part1 = 'ã€';\\n            const part2 = 'æˆ‘';\\n            const part3 = 'çš„';\\n            const part4 = 'ä¸»';\\n            const part5 = 'é¡µ';\\n            const part6 = 'ã€‘';\\n            \\n            // åŠ¨æ€æ‹¼æ¥å­—ç¬¦ä¸²\\n            const fullString = part1 + part2 + part3 + part4 + part5 + part6;\\n            \\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(`/sendas name={{Char}} ${fullString}`);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n        }\\n\\n        // åˆå§‹åŒ–å›¾ç‰‡æ»‘åŠ¨å™¨äº‹ä»¶\\n        function initPhotoSlider() {\\n            // è·å–æ‰€æœ‰æ»‘åŠ¨å›¾ç‰‡å®¹å™¨\\n            const sliders = document.querySelectorAll('.post-images-slider');\\n            \\n            if (!sliders || sliders.length === 0) {\\n                console.log('æ²¡æœ‰æ‰¾åˆ°å›¾ç‰‡æ»‘åŠ¨å™¨');\\n                return;\\n            }\\n            \\n            sliders.forEach(slider => {\\n                const sliderContainer = slider.querySelector('.slider-container');\\n                const dots = slider.querySelectorAll('.slider-dot');\\n                \\n                if (!sliderContainer) return;\\n                \\n                // æ·»åŠ æ»šåŠ¨äº‹ä»¶ï¼Œæ›´æ–°æŒ‡ç¤ºç‚¹çŠ¶æ€\\n                sliderContainer.addEventListener('scroll', function() {\\n                    if (!dots || dots.length === 0) return;\\n                    \\n                    const containerWidth = this.offsetWidth;\\n                    const scrollLeft = this.scrollLeft;\\n                    const activeIndex = Math.round(scrollLeft / containerWidth);\\n                    \\n                    dots.forEach((dot, index) => {\\n                        if (index === activeIndex) {\\n                            dot.classList.add('active');\\n                        } else {\\n                            dot.classList.remove('active');\\n                        }\\n                    });\\n                });\\n                \\n                // ç¡®ä¿æ»‘åŠ¨æŒ‰é’®èƒ½æ­£å¸¸å·¥ä½œ\\n                const prevBtn = slider.querySelector('.slider-prev');\\n                const nextBtn = slider.querySelector('.slider-next');\\n                \\n                if (prevBtn) {\\n                    prevBtn.addEventListener('click', function(e) {\\n                        e.stopPropagation();\\n                        if (!sliderContainer) return;\\n                        \\n                        const imageWidth = sliderContainer.querySelector('.image-container')?.offsetWidth || 0;\\n                        const scrollLeft = sliderContainer.scrollLeft;\\n                        \\n                        // è®¡ç®—ä¸Šä¸€å¼ å›¾ç‰‡çš„ä½ç½®\\n                        const prevScrollLeft = Math.max(scrollLeft - imageWidth, 0);\\n                        sliderContainer.scrollTo({\\n                            left: prevScrollLeft,\\n                            behavior: 'smooth'\\n                        });\\n                    });\\n                }\\n                \\n                if (nextBtn) {\\n                    nextBtn.addEventListener('click', function(e) {\\n                        e.stopPropagation();\\n                        if (!sliderContainer) return;\\n                        \\n                        const imageWidth = sliderContainer.querySelector('.image-container')?.offsetWidth || 0;\\n                        const scrollLeft = sliderContainer.scrollLeft;\\n                        const maxScrollLeft = sliderContainer.scrollWidth - sliderContainer.clientWidth;\\n                        \\n                        // è®¡ç®—ä¸‹ä¸€å¼ å›¾ç‰‡çš„ä½ç½®\\n                        const nextScrollLeft = Math.min(scrollLeft + imageWidth, maxScrollLeft);\\n                        sliderContainer.scrollTo({\\n                            left: nextScrollLeft,\\n                            behavior: 'smooth'\\n                        });\\n                    });\\n                }\\n                \\n                // ç¡®ä¿ç‚¹å‡»å›¾ç‰‡å®¹å™¨æ—¶åˆ‡æ¢æŸ¥çœ‹è¯¦æƒ…çŠ¶æ€\\n                const imageContainers = slider.querySelectorAll('.image-container');\\n                imageContainers.forEach(container => {\\n                    // ç§»é™¤å¯èƒ½å­˜åœ¨çš„æ—§äº‹ä»¶ç›‘å¬å™¨ï¼ˆé€šè¿‡å…‹éš†å’Œæ›¿æ¢ï¼‰\\n                    const newContainer = container.cloneNode(true);\\n                    container.parentNode.replaceChild(newContainer, container);\\n                    \\n                    // æ·»åŠ æ–°çš„ç‚¹å‡»äº‹ä»¶å¤„ç†\\n                    newContainer.addEventListener('click', function(e) {\\n                        // ç¡®ä¿ä¸æ˜¯ä»å¯¼èˆªæŒ‰é’®è§¦å‘çš„äº‹ä»¶\\n                        if (!e.target.closest('.slider-nav-btn')) {\\n                            this.classList.toggle('active-mask');\\n                            console.log('å›¾ç‰‡ç‚¹å‡»äº‹ä»¶è§¦å‘ï¼Œåˆ‡æ¢æ˜¾ç¤ºçŠ¶æ€');\\n                        }\\n                    });\\n                });\\n            });\\n        }\\n        \\n        // åˆå§‹åŒ–è¯„è®ºæ’åºé€‰é¡¹\\n        function initCommentSorting() {\\n            // ç»‘å®šè¯„è®ºæ’åºåˆ‡æ¢åŠŸèƒ½\\n            document.querySelectorAll('.sort-option').forEach(option => {\\n                option.addEventListener('click', function() {\\n                    // ç§»é™¤æ‰€æœ‰é€‰é¡¹çš„activeç±»\\n                    document.querySelectorAll('.sort-option').forEach(opt => {\\n                        opt.classList.remove('active');\\n                    });\\n                    // ä¸ºå½“å‰ç‚¹å‡»çš„é€‰é¡¹æ·»åŠ activeç±»\\n                    this.classList.add('active');\\n                    \\n                    // æ˜¾ç¤ºå¯¹åº”çš„è¯„è®ºå†…å®¹\\n                    const type = this.getAttribute('data-type');\\n                    if (type === 'new') {\\n                        document.querySelector('.new-comments').style.display = 'block';\\n                        document.querySelector('.hot-comments').style.display = 'none';\\n                    } else if (type === 'hot') {\\n                        document.querySelector('.new-comments').style.display = 'none';\\n                        document.querySelector('.hot-comments').style.display = 'block';\\n                    }\\n                });\\n            });\\n            \\n            // æ·»åŠ æ¥¼ä¸­æ¥¼å±•å¼€/æ”¶èµ·åŠŸèƒ½\\n            document.querySelectorAll('.comment-reply-count').forEach(btn => {\\n                btn.addEventListener('click', function() {\\n                    const nestedComments = this.closest('.comment-content').querySelector('.nested-comments');\\n                    if (nestedComments) {\\n                        const isVisible = nestedComments.style.display === 'block';\\n                        \\n                        // åˆ‡æ¢æ˜¾ç¤ºçŠ¶æ€\\n                        if (isVisible) {\\n                            nestedComments.style.display = 'none';\\n                            // æ›¿æ¢æ–‡æœ¬ä¸­çš„\\\"æ”¶èµ·\\\"ä¸º\\\"å±•å¼€\\\"\\n                            const count = this.textContent.match(/\\\\d+/)[0];\\n                            this.textContent = `å±•å¼€${count}æ¡å›å¤`;\\n                        } else {\\n                            nestedComments.style.display = 'block';\\n                            // æ›¿æ¢æ–‡æœ¬ä¸­çš„\\\"å±•å¼€\\\"ä¸º\\\"æ”¶èµ·\\\"\\n                            const count = this.textContent.match(/\\\\d+/)[0];\\n                            this.textContent = `æ”¶èµ·${count}æ¡å›å¤`;\\n                        }\\n                    }\\n                });\\n            });\\n            \\n            // ç»‘å®šè¯„è®ºç‚¹èµåŠŸèƒ½\\n            document.querySelectorAll('.comment-like .far.fa-heart, .nested-comment-like .far.fa-heart').forEach(heart => {\\n                heart.addEventListener('click', function(e) {\\n                    e.stopPropagation(); // é˜²æ­¢äº‹ä»¶å†’æ³¡\\n                    this.classList.toggle('fas');\\n                    this.classList.toggle('far');\\n                    const countSpan = this.nextElementSibling;\\n                    let count = parseInt(countSpan.textContent);\\n                    \\n                    if (this.classList.contains('fas')) {\\n                        count++;\\n                    } else {\\n                        count--;\\n                    }\\n                    \\n                    countSpan.textContent = count;\\n                });\\n            });\\n        }\\n    </script>\\n\\