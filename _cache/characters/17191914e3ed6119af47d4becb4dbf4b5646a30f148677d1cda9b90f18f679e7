ner.appendChild(characterElement);\\n            });\\n        }\\n        \\n        // æ˜¾ç¤ºäººç‰©è¯¦æƒ…\\n        function showCharacterDetail(characterName) {\\n            // ç¡®ä¿gameDataå·²ç»åŠ è½½\\n            if (!window.gameData || !window.gameData.characters) return;\\n            \\n            const character = window.gameData.characters[characterName];\\n            if (!character) return;\\n            \\n            // éšè—å…¶ä»–å¼¹çª—\\n            document.getElementById('charactersList').classList.remove('show');\\n            document.getElementById('locationPopup').classList.remove('show');\\n            \\n            // æ›´æ–°äººç‰©è¯¦æƒ…å†…å®¹\\n            const popup = document.getElementById('characterDetailPopup');\\n            document.getElementById('characterDetailName').textContent = character.name;\\n            document.getElementById('characterDetailAvatar').src = character.avatar;\\n            document.getElementById('characterDetailAvatar').alt = character.name;\\n            document.getElementById('characterDetailLocation').querySelector('span').textContent = character.location;\\n            document.getElementById('characterDetailStatus').textContent = character.status;\\n            \\n            // æ˜¾ç¤ºå¼¹çª—å’ŒèƒŒæ™¯\\n            document.getElementById('backdrop').classList.add('show');\\n            popup.classList.add('show');\\n        }\\n        \\n        // å…³é—­äººç‰©è¯¦æƒ…å¼¹çª—\\n        function closeCharacterDetail() {\\n            document.getElementById('characterDetailPopup').classList.remove('show');\\n            document.getElementById('backdrop').classList.remove('show');\\n        }\\n        \\n        // å‰å¾€åœ°ç‚¹å‡½æ•° - å‘é€æ¶ˆæ¯åˆ°æ­£æ–‡ç•Œé¢\\n        function goToLocation() {\\n            const locationName = document.querySelector('.location-popup-title').textContent;\\n            const locationInfo = window.gameData.locations[locationName];\\n            \\n            if (!locationInfo) return;\\n            \\n            // è·å–ä¸»è¦äººç‰©ä¿¡æ¯\\n            const mainCharactersText = locationInfo.mainCharacters && locationInfo.mainCharacters.length > 0 \\n                ? locationInfo.mainCharacters.join(', ') \\n                : 'æ— ä¸»è¦äººç‰©';\\n            \\n            // è·å–é‡è¦äº‹ä»¶ä¿¡æ¯\\n            const eventText = locationInfo.events || 'æ— é‡è¦äº‹ä»¶';\\n            \\n            // æ„å»ºæ¶ˆæ¯\\n            const message = `/send ã€æ­£æ–‡ç•Œé¢ã€‘{{User}}å†³å®šå‰å¾€${locationName}ï¼Œåœ°ç‚¹äººç‰©ï¼š${mainCharactersText}ï¼Œåœ°ç‚¹äº‹ä»¶${eventText}|/trigger`;\\n            \\n            // ä½¿ç”¨triggerSlashå‘é€æ¶ˆæ¯\\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(message);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n            \\n            // å…³é—­å¼¹çª—\\n            closeLocationPopup();\\n        }\\n        \\n        // å‘é€æ¶ˆæ¯ç»™äººç‰©\\n        function sendMessageToCharacter() {\\n            const characterName = document.getElementById('characterDetailName').textContent;\\n            const character = window.gameData.characters[characterName];\\n            \\n            if (!character) return;\\n            \\n            // æ„å»ºæ¶ˆæ¯\\n            const message = `/send ã€ç§èŠç•Œé¢ã€‘{{User}}å†³å®šå’Œ${characterName}å‘æ¶ˆæ¯ï¼Œ${characterName}ä½äº${character.location}ï¼ŒçŠ¶æ€ä¸º${character.status}|/trigger`;\\n            \\n            // ä½¿ç”¨triggerSlashå‘é€æ¶ˆæ¯\\n            if (typeof triggerSlash === 'function') {\\n                triggerSlash(message);\\n            } else {\\n                console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n            }\\n            \\n            // å…³é—­å¼¹çª—\\n            closeCharacterDetail();\\n        }\\n    </script>\\n</body>\\n</html>\\n```\",\"trimStrings\":[],\"placement\":[2],\"disabled\":false,\"markdownOnly\":true,\"promptOnly\":false,\"runO