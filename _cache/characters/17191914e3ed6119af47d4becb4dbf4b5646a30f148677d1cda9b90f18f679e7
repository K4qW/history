    const headerContent = headerMatch ? headerMatch[1].trim() : '';\\n                    const timeMatch = headerContent.match(/\\\\[æ—¶é—´\\\\|(.*?)\\\\|(.*?)\\\\]/);\\n                    const date = timeMatch ? timeMatch[1].trim() : 'æœªçŸ¥æ—¥æœŸ';\\n                    const time = timeMatch ? timeMatch[2].trim() : 'æœªçŸ¥æ—¶é—´';\\n                    \\n                    // æå–èŠå¤©æ¶ˆæ¯\\n                    const chatMatch = messageListContent.match(/<chat_message>([\\\\s\\\\S]*?)<\\\\/chat_message>/);\\n                    const chatContent = chatMatch ? chatMatch[1].trim() : '';\\n                    \\n                    // æå–ç³»ç»Ÿé€šçŸ¥\\n                    const systemMatch = messageListContent.match(/<system_message>([\\\\s\\\\S]*?)<\\\\/system_message>/);\\n                    const systemContent = systemMatch ? systemMatch[1].trim() : '';\\n                    \\n                    // æå–èµå’Œæ”¶è—\\n                    const likeMatch = messageListContent.match(/<like_and_collect>([\\\\s\\\\S]*?)<\\\\/like_and_collect>/);\\n                    const likeContent = likeMatch ? likeMatch[1].trim() : '';\\n                    \\n                    // æå–è¯„è®ºå’Œ@\\n                    const commentMatch = messageListContent.match(/<comment_and_at>([\\\\s\\\\S]*?)<\\\\/comment_and_at>/);\\n                    const commentContent = commentMatch ? commentMatch[1].trim() : '';\\n                    \\n                    // è§£æå„ç±»æ¶ˆæ¯\\n                    const chatMessages = this.extractChatMessages(chatContent);\\n                    const systemMessages = this.extractSystemMessages(systemContent);\\n                    const likeMessages = this.extractLikeMessages(likeContent);\\n                    const commentMessages = this.extractCommentMessages(commentContent);\\n                    \\n                    return {\\n                        date,\\n                        time,\\n                        chat: chatMessages,\\n                        system: systemMessages,\\n                        like: likeMessages,\\n                        comment: commentMessages\\n                    };\\n                } catch (error) {\\n                    console.error('è§£ææ¶ˆæ¯æ•°æ®å¤±è´¥:', error);\\n                    return {\\n                        date: 'æœªçŸ¥æ—¥æœŸ',\\n                        time: 'æœªçŸ¥æ—¶é—´',\\n                        chat: [],\\n                        system: [],\\n                        like: [],\\n                        comment: []\\n                    };\\n                }\\n            },\\n            \\n            extractChatMessages(content) {\\n                if (!content) return [];\\n                \\n                const messageRegex = /\\\\[æ¶ˆæ¯\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/g;\\n                const messages = [];\\n                let match;\\n                let id = 1;\\n                \\n                while ((match = messageRegex.exec(content)) !== null) {\\n                    messages.push({\\n                        id: id++,\\n                        name: match[1] || 'æœªçŸ¥ç”¨æˆ·',\\n                        avatar: this.processAvatarUrl(match[2]) || 'default',\\n                        preview: match[3] || 'æ— å†…å®¹',\\n                        time: match[4] || 'æœªçŸ¥æ—¶é—´',\\n                        unreadCount: parseInt(match[5]) || 0,\\n                        unread: parseInt(match[5]) > 0,\\n                        type: \\\"chat\\\"\\n                    });\\n                }\\n                \\n                return messages;\\n            },\\n            \\n            extractSystemMessages(content) {\\n                if (!content) return [];\\n                \\n                const messageRegex = /\\\\[ç³»ç»Ÿé€šçŸ¥\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/g;\\n                const messages = [];\\n                let match;\\n                let id = 1000; // ä½¿ç”¨ä¸åŒçš„IDèŒƒå›´\\n                \\n                while ((match = messageRegex.exec(content)) !== null) {\\n                    messages.push({\\n                        id: id++,\\n                    name: \\\"ç³»ç»Ÿé€šçŸ¥\\\",\\n                    avatar: \\\"system\\\",\\n                        preview: match[1] || 'ç³»ç»Ÿé€šçŸ¥',\\n                        time: match[2] || 'æœªçŸ¥æ—¶é—´',\\n                        unreadCount: parseInt(match[3]) || 0,\\n                        unread: parseInt(match[3]) > 0,\\n                    type: \\\"system\\\",\\n                        icon: \\\"fa-bell\\\" // é»˜è®¤å›¾æ ‡\\n                    });\\n                }\\n                \\n                return messages;\\n            },\\n            \\n            extractLikeMessages(content) {\\n                if (!content) return [];\\n                \\n                const messageRegex = /\\\\[èµå’Œæ”¶è—\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/g;\\n                const messages = [];\\n                let match;\\n                let id = 2000; // ä½¿ç”¨ä¸åŒçš„IDèŒƒå›´\\n                \\n                while ((match = messageRegex.exec(content)) !== null) {\\n                    const action = match[1] || 'èµ';\\n                    const username = match[2] || 'æœªçŸ¥ç”¨æˆ·';\\n                    const title = match[3] || 'æ— æ ‡é¢˜';\\n                    const time = match[4] || 'æœªçŸ¥æ—¶é—´';\\n                    const unreadCount = parseInt(match[5]) || 0;\\n                    \\n                    let preview = '';\\n                    if (action === 'èµ') {\\n                        preview = `èµäº†ä½ çš„ç¬”è®°ã€Š${title}ã€‹`;\\n                    } else if (action === 'æ”¶è—') {\\n                        preview = `æ”¶è—äº†ä½ çš„ç¬”è®°ã€Š${title}ã€‹`;\\n                    }\\n                    \\n                    messages.push({\\n                        id: id++,\\n                        name: username,\\n                        avatar: \\\"like\\\",\\n                        preview: preview,\\n                        time: time,\\n                        unreadCount: unreadCount,\\n                        unread: unreadCount > 0,\\n                    type: \\\"like\\\",\\n                        detail: action === 'èµ' ? \\\"å…±æ”¶åˆ°124ä¸ªèµ\\\" : \\\"å…±è¢«æ”¶è—53æ¬¡\\\" // ç¤ºä¾‹æ•°æ®\\n                    });\\n                }\\n                \\n                return messages;\\n            },\\n            \\n            extractCommentMessages(content) {\\n                if (!content) return [];\\n                \\n                const messageRegex = /\\\\[è¯„è®ºå’Œ@\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/g;\\n                const messages = [];\\n                let match;\\n                let id = 3000; // ä½¿ç”¨ä¸åŒçš„IDèŒƒå›´\\n                \\n                while ((match = messageRegex.exec(content)) !== null) {\\n                    const action = match[1] || 'è¯„è®º';\\n                    const username = match[2] || 'æœªçŸ¥ç”¨æˆ·';\\n                    const title = match[3] || 'æ— æ ‡é¢˜';\\n                    const content = match[4] || '';\\n                    const time = match[5] || 'æœªçŸ¥æ—¶é—´';\\n                    const unreadCount = parseInt(match[6]) || 0;\\n                    \\n                    let preview = '';\\n                    if (action === 'è¯„è®º') {\\n                        preview = `è¯„è®ºäº†ä½ çš„ç¬”è®°ã€Š${title}ã€‹ï¼š${content}`;\\n                    } else if (action === '@') {\\n                        preview = `@äº†ä½ ï¼š${content}`;\\n                    }\\n                    \\n                    messages.push({\\n                        id: id++,\\n                        name: username,\\n                        avatar: \\\"comment\\\",\\n                        preview: preview,\\n                        time: time,\\n                        unreadCount: unreadCount,\\n                        unread: unreadCount > 0,\\n                    type: \\\"comment\\\"\\n                    });\\n                }\\n                \\n                return messages;\\n            },\\n            \\n            parseGameText(gameText) {\\n                // ç›´æ¥è§£ææ¶ˆæ¯åˆ—è¡¨æ•°æ®\\n                return this.extractMessageData(gameText);\\n            }\\n        };\\n\\n        // è®¾ç½®å­˜å‚¨æ¶ˆæ¯æ•°æ®çš„å˜é‡ï¼ˆç”¨äºæ›¿æ¢åŸæ¥çš„é™æ€æ•°æ®ï¼‰\\n        let messageData = { chat: [], system: [], like: [], comment: [] };\\n\\n        // å½“å‰é€‰ä¸­çš„æ¶ˆæ¯ç±»å‹\\n        let currentMessageTab = \\\"chat\\\";\\n\\n        // 3. UIæ›´æ–°å™¨\\n        const UIUpdater = {\\n            // æ›´æ–°é¡µé¢æ ‡é¢˜å’Œæ—¶é—´\\n            updatePageHeader(data) {\\n                if (!data) return;\\n                \\n                // è®¾ç½®é¡µé¢æ ‡é¢˜\\n                document.title = `æ¶ˆæ¯ä¸­å¿ƒ - ${data.date} ${data.time}`;\\n                \\n                // å¯ä»¥åœ¨é¡¶éƒ¨å¯¼èˆªæ æ·»åŠ æ—¶é—´æ˜¾ç¤º\\n                const pageTitle = document.querySelector('.page-title');\\n                if (pageTitle) {\\n                    pageTitle.textContent = 'æ¶ˆæ¯ä¸­å¿ƒ';\\n                }\\n                \\n                const navTimeElem = document.createElement('div');\\n                navTimeElem.className = 'nav-time';\\n                navTimeElem.style.cssText = 'font-size: 12px; color: #666; position: absolute; right: 10px; top: 10px;';\\n                navTimeElem.textContent = `${data.date} ${data.time}`;\\n                document.querySelector('.navbar').appendChild(navTimeElem);\\n            },\\n            \\n            // æ›´æ–°æœªè¯»æ¶ˆæ¯æ•°é‡\\n            updateUnreadCounts() {\\n                // è®¡ç®—å„åˆ†ç±»æœªè¯»æ•°\\n            const unreadCounts = {\\n                    chat: messageData.chat.reduce((sum, msg) => sum + (msg.unread ? msg.unreadCount : 0), 0),\\n                    system: messageData.system.reduce((sum, msg) => sum + (msg.unread ? msg.unreadCount : 0), 0),\\n                    like: messageData.like.reduce((sum, msg) => sum + (msg.unread ? msg.unreadCount : 0), 0),\\n                    comment: messageData.comment.reduce((sum, msg) => sum + (msg.unread ? msg.unreadCount : 0), 0)\\n                };\\n            \\n            // æ›´æ–°UIä¸Šçš„æœªè¯»æ•°æ˜¾ç¤º\\n            for (const type in unreadCounts) {\\n                const badge = document.getElementById(`${type}-unread`);\\n                if (badge) {\\n                    if (unreadCounts[type] > 0) {\\n                        badge.textContent = unreadCounts[type];\\n                        badge.style.display = 'inline-block';\\n                    } else {\\n                        badge.style.display = 'none';\\n                    }\\n                }\\n            }\\n            \\n            return unreadCounts;\\n            },\\n\\n            // æ›´æ–°æ¶ˆæ¯åˆ—è¡¨\\n            updateMessageList(messageType) {\\n            const messageList = document.getElementById(\\\"message-list\\\");\\n                if (!messageList) return;\\n                \\n            messageList.innerHTML = \\\"\\\";\\n            \\n            // è·å–å¯¹åº”çš„æ¶ˆæ¯æ•°æ®\\n            let messagesToRender = messageData[messageType] || [];\\n            \\n            // å¦‚æœæ²¡æœ‰æ¶ˆæ¯ï¼Œæ˜¾ç¤ºç©ºçŠ¶æ€\\n            if (messagesToRender.length === 0) {\\n                messageList.innerHTML = `\\n                    <div class=\\\"empty-message\\\">\\n                        <i class=\\\"far fa-comment-dots\\\"></i>\\n                        <p>æš‚æ— æ¶ˆæ¯</p>\\n                    </div>\\n                `;\\n                return;\\n            }\\n            \\n            // æ¸²æŸ“æ¶ˆæ¯åˆ—è¡¨\\n            messagesToRender.forEach(message => {\\n                const messageItem = document.createElement(\\\"div\\\");\\n                messageItem.className = \\\"message-item\\\";\\n                messageItem.dataset.id = message.id;\\n                messageItem.dataset.type = message.type;\\n                \\n                // æ¶ˆæ¯å¤´åƒ\\n                let avatarHTML = '';\\n                if (message.type === \\\"system\\\") {\\n                    avatarHTML = `\\n                        <div class=\\\"message-avatar system-avatar\\\">\\n                            <i class=\\\"fas ${message.icon || 'fa-bell'}\\\"></i>\\n                        </div>\\n                    `;\\n                } else if (message.type === \\\"like\\\") {\\n                    avatarHTML = `\\n                        <div class=\\\"message-avatar like-avatar\\\">\\n                            <i class=\\\"fas fa-heart\\\"></i>\\n                        </div>\\n                    `;\\n                } else if (message.type === \\\"comment\\\") {\\n                    avatarHTML = `\\n                        <div class=\\\"message-avatar comment-avatar\\\">\\n                            <i class=\\\"fas fa-comment\\\"></i>\\n                        </div>\\n                    `;\\n                } else {\\n                    avatarHTML = `\\n                        <div class=\\\"message-avatar\\\">\\n                            <img src=\\\"${message.avatar}\\\" alt=\\\"${message.name}\\\">\\n                        </div>\\n                    `;\\n                }\\n                \\n                // æ¶ˆæ¯è¯¦æƒ…\\n                let detailHTML = '';\\n                if (message.detail) {\\n                    detailHTML = `<div class=\\\"message-detail\\\">${message.detail}</div>`;\\n                }\\n                \\n                // æœªè¯»æ ‡è®°\\n                const unreadBadge = message.unread ? '<div class=\\\"unread-badge\\\"></div>' : '';\\n                \\n                    // æœªè¯»æ•°é‡æ ‡è®°\\n                    const unreadNumber = message.unread ? `<div class=\\\"unread-number\\\">${message.unreadCount}</div>` : '';\\n                \\n                // ç»„è£…å®Œæ•´æ¶ˆæ¯é¡¹HTML\\n                messageItem.innerHTML = `\\n                    ${avatarHTML}\\n                    <div class=\\\"message-content\\\">\\n                        <div class=\\\"message-header\\\">\\n                            <div class=\\\"message-name\\\">${message.name}</div>\\n                            <div class=\\\"message-time\\\">${message.time}</div>\\n                        </div>\\n                            <div class=\\\"message-preview\\\">${message.preview}</div>\\n                        ${detailHTML}\\n                    </div>\\n                    ${unreadBadge}\\n                    ${unreadNumber}\\n                `;\\n                \\n                // æ·»åŠ ç‚¹å‡»äº‹ä»¶ - æ ‡è®°æ¶ˆæ¯ä¸ºå·²è¯»\\n                messageItem.addEventListener('click', function() {\\n                    if (message.unread) {\\n                        markMessageAsRead(message.id, message.type);\\n                    }\\n                        \\n                        // å¦‚æœæ˜¯èŠå¤©æ¶ˆæ¯ï¼Œè§¦å‘æŸ¥çœ‹ç§ä¿¡\\n                        if (message.type === \\\"chat\\\") {\\n                            viewChatMessage(message.name, message.unreadCount);\\n                        }\\n                });\\n                \\n                messageList.appendChild(messageItem);\\n            });\\n            },\\n            \\n            // æ›´æ–°æ‰€æœ‰UI\\n            updateAllUI(data) {\\n                if (!data) {\\n                    console.error('æ— æ•ˆçš„æ•°æ®ï¼Œæ— æ³•æ›´æ–°UI');\\n                    return;\\n                }\\n                \\n                this.updatePageHeader(data);\\n                this.updateUnreadCounts();\\n                this.updateMessageList(currentMessageTab);\\n            },\\n            \\n            // åˆå§‹åŒ–UI\\n            initializeUI() {\\n                try {\\n                    // è§£ææ¸¸æˆæ–‡æœ¬\\n                    const parsedData = DataExtractor.parseGameText(gameText);\\n                    \\n                    if (parsedData) {\\n                        // å°†æå–çš„æ•°æ®å­˜å‚¨åˆ°å…¨å±€å˜é‡ä¸­\\n                        messageData = {\\n                            chat: parsedData.chat || [],\\n                            system: parsedData.system || [],\\n                            like: parsedData.like || [],\\n                            comment: parsedData.comment || []\\n                        };\\n                        \\n                        // æ›´æ–°UI\\n                        this.updateAllUI(parsedData);\\n                    } else {\\n                        console.error('æ— æ³•åˆå§‹åŒ–UIï¼Œæ•°æ®è§£æå¤±è´¥');\\n                    }\\n                } catch (error) {\\n                    console.error('UIåˆå§‹åŒ–å¤±è´¥:', error);\\n                }\\n            }\\n        };\\n\\n        // æ»šåŠ¨åˆ°æ´»åŠ¨æ ‡ç­¾çš„å‡½æ•°\\n        function scrollToActiveTab() {\\n            const activeTab = document.querySelector('.message-tab.active');\\n            if (activeTab) {\\n                const tabsContainer = document.querySelector('.message-tabs');\\n                const tabRect = activeTab.getBoundingClientRect();\\n                const containerRect = tabsContainer.getBoundingClientRect();\\n                \\n                // è®¡ç®—éœ€è¦æ»šåŠ¨çš„ä½ç½®ï¼Œä½¿æ´»åŠ¨æ ‡ç­¾å±…ä¸­\\n                const scrollLeft = activeTab.offsetLeft - (containerRect.width / 2) + (tabRect.width / 2);\\n                \\n                // å¹³æ»‘æ»šåŠ¨åˆ°è¯¥ä½ç½®\\n                tabsContainer.scrollTo({\\n                    left: Math.max(0, scrollLeft),\\n                    behavior: 'smooth'\\n                });\\n            }\\n        }\\n\\n        // çª—å£å¤§å°å˜åŒ–æ—¶æ»šåŠ¨åˆ°æ´»åŠ¨æ ‡ç­¾\\n        window.addEventListener('resize', scrollToActiveTab);\\n\\n        // æ ‡è®°æ¶ˆæ¯ä¸ºå·²è¯»\\n        function markMessageAsRead(id, type) {\\n            // æ‰¾åˆ°å¹¶æ›´æ–°æ¶ˆæ¯çŠ¶æ€\\n            const message = findMessageById(id, type);\\n            if (message) {\\n                message.unread = false;\\n                \\n                // æ›´æ–°UIæ˜¾ç¤º\\n                const messageItem = document.querySelector(`.message-item[data-id=\\\"${id}\\\"]`);\\n                if (messageItem) {\\n                    const unreadBadge = messageItem.querySelector('.unread-badge');\\n                    if (unreadBadge) {\\n                        unreadBadge.remove();\\n                    }\\n                    \\n                    const unreadNumber = messageItem.querySelector('.unread-number');\\n                    if (unreadNumber) {\\n                        unreadNumber.remove();\\n                    }\\n                }\\n                \\n                // æ›´æ–°æœªè¯»æ•°é‡\\n                UIUpdater.updateUnreadCounts();\\n            }\\n        }\\n\\n        // æ ‡è®°æ‰€æœ‰æ¶ˆæ¯ä¸ºå·²è¯»\\n        function markAllAsRead() {\\n            let changed = false;\\n            \\n            // ä»…æ ‡è®°å½“å‰åˆ†ç±»çš„æ¶ˆæ¯ä¸ºå·²è¯»\\n            messageData[currentMessageTab]?.forEach(message => {\\n                if (message.unread) {\\n                    message.unread = false;\\n                    changed = true;\\n                }\\n            });\\n            \\n            // å¦‚æœæœ‰æ¶ˆæ¯çŠ¶æ€æ”¹å˜ï¼Œæ›´æ–°æ˜¾ç¤º\\n            if (changed) {\\n                UIUpdater.updateMessageList(currentMessageTab);\\n                UIUpdater.updateUnreadCounts();\\n            }\\n        }\\n\\n        // æ ¹æ®IDæŸ¥æ‰¾æ¶ˆæ¯\\n        function findMessageById(id, type) {\\n            return messageData[type]?.find(msg => msg.id === parseInt(id));\\n        }\\n\\n        // åˆå§‹åŒ–æ¶ˆæ¯æ ‡ç­¾åˆ‡æ¢äº‹ä»¶\\n        document.querySelectorAll(\\\".message-tab\\\").forEach(tab => {\\n            tab.addEventListener(\\\"click\\\", function() {\\n                // ç§»é™¤æ‰€æœ‰é€‰é¡¹å¡çš„activeç±»\\n                docume