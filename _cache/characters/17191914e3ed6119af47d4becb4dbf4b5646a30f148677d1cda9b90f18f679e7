 = timeElement ? timeElement.textContent : '11:47';\\n            }\\n\\n            // è§£ææœ€åä¸€æ¡æ¶ˆæ¯çš„æ—¶é—´\\n            const [hours, minutes] = lastTimeStr.split(':').map(Number);\\n\\n            // è®¡ç®—æ–°æ¶ˆæ¯çš„æ—¶é—´ï¼ˆæœ€åä¸€æ¡æ¶ˆæ¯æ—¶é—´+1åˆ†é’Ÿï¼‰\\n            let newMinutes = minutes + 1;\\n            let newHours = hours;\\n\\n            if (newMinutes >= 60) {\\n                newMinutes = 0;\\n                newHours = (newHours + 1) % 24;\\n            }\\n\\n            // æ ¼å¼åŒ–æ–°æ—¶é—´\\n            const timeString = `${newHours.toString().padStart(2, '0')}:${newMinutes.toString().padStart(2, '0')}`;\\n\\n            // åˆ›å»ºæ–°æ¶ˆæ¯HTML\\n            const messageHtml = `\\n                <div class=\\\"message sent\\\">\\n                    <div class=\\\"message-wrapper\\\">\\n                        <div class=\\\"message-content\\\">${messageText}</div>\\n                        <div class=\\\"message-time\\\">${timeString}</div>\\n                    </div>\\n                </div>\\n            `;\\n\\n            // æ·»åŠ æ–°æ¶ˆæ¯åˆ°èŠå¤©ç•Œé¢\\n            chatMessages.insertAdjacentHTML('beforeend', messageHtml);\\n\\n            // ä¸ºæ–°æ·»åŠ çš„æ¶ˆæ¯å…ƒç´ æ·»åŠ éŸ³æ•ˆäº‹ä»¶\\n            const newContent = chatMessages.lastElementChild.querySelector('.message-content');\\n            if (newContent) {\\n                newContent.addEventListener('click', () => {\\n                    playSound('bubble');\\n                });\\n            }\\n\\n            // æ»šåŠ¨åˆ°åº•éƒ¨å¹¶æ¸…ç©ºè¾“å…¥æ¡†\\n            chatMessages.scrollTop = chatMessages.scrollHeight;\\n            textInput.value = '';\\n            \\n            // æ›´æ–°chatDataä¸­çš„æ¶ˆæ¯è®°å½•\\n            const currentChatData = parseChat(chatData);\\n            currentChatData.messages.push({\\n                type: 'sent',\\n                content: messageText,\\n                time: timeString\\n            });\\n            \\n            // é‡æ–°æ ¼å¼åŒ–chatData\\n            let newChatData = `<message>\\\\n`;\\n            if (currentChatData.currentChat) {\\n                newChatData += `[å½“å‰èŠå¤©å¯¹è±¡|${currentChatData.currentChat.name}|${currentChatData.currentChat.avatar.replace('https://files.catbox.moe/', '')}|${currentChatData.currentChat.id || '1'}]\\\\n`;\\n            }\\n            \\n            // æ·»åŠ å¥½æ„Ÿåº¦æ•°æ®\\n            if (currentChatData.favorability > 0) {\\n                newChatData += `[å¯¹æ–¹å¥½æ„Ÿåº¦|${currentChatData.favorability}]\\\\n`;\\n            }\\n            \\n            // æ·»åŠ æ‰€æœ‰æ¶ˆæ¯\\n            currentChatData.messages.forEach(msg => {\\n                if (msg.type === 'sent') {\\n                    newChatData += `[æˆ‘æ–¹æ¶ˆæ¯|${msg.content}|${msg.time}]\\\\n`;\\n                } else if (msg.type === 'received') {\\n                    newChatData += `[å¯¹æ–¹æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.content}|${msg.time}]\\\\n`;\\n                } else if (msg.type === 'call-missed') {\\n                    newChatData += `[é€šè¯æœªæ¥é€š]\\\\n`;\\n                } else if (msg.type === 'call-ended') {\\n                    newChatData += `[é€šè¯å·²ç»“æŸ|${msg.duration}]\\\\n`;\\n                } else if (msg.type === 'recalled') {\\n                    newChatData += `[å¯¹æ–¹æ’¤å›æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.originalContent}|${msg.time}]\\\\n`;\\n                } else if (msg.type === 'browser-share') {\\n                    newChatData += `[å¯¹æ–¹åˆ†äº«é“¾æ¥æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.title}|${msg.source}|${msg.description}|${msg.url}|${msg.time}]\\\\n`;\\n                } else if (msg.type === 'transfer') {\\n                    newChatData += `[å¯¹æ–¹è½¬è´¦æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.amount}|${msg.transferTime}|${msg.time}]\\\\n`;\\n                } else if (msg.type === 'voice') {\\n                    newChatData += `[å¯¹æ–¹è¯­éŸ³æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.audioFile}|${msg.time}]\\\\n`;\\n                } else if (msg.type === 'location') {\\n                    newChatData += `[å¯¹æ–¹å®šä½å…±äº«æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.userLocation}|${msg.partnerLocation}|${msg.distance}|${msg.time}]\\\\n`;\\n                } else if (msg.type === 'news') {\\n                    newChatData += `[å¯¹æ–¹æ–°é—»æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.title}|${msg.source}|${msg.publishDate}|${msg.time}|${msg.views}|${msg.content}|${msg.keyData}|${msg.likes}|${msg.comments}]\\\\n`;\\n                } else if (msg.type === 'sticker') {\\n                    newChatData += `[å¯¹æ–¹æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|<bqb>${msg.stickerFile}</bqb>|${msg.time}]\\\\n`;\\n                } else if (msg.type === 'image') {\\n                    newChatData += `[å¯¹æ–¹æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|<img>${msg.imageFile}</img>|${msg.time}]\\\\n`;\\n                } else if (msg.type === 'file') {\\n                    newChatData += `[å¯¹æ–¹æ–‡ä»¶æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.filename}|${msg.filesize}|${msg.description}|${msg.time}]\\\\n`;\\n                } else if (msg.type === 'text-image') {\\n                    newChatData += `[å¯¹æ–¹æ–‡å­—å›¾ç‰‡æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.description}|${msg.time}]\\\\n`;\\n                }\\n            });\\n            \\n            newChatData += `</message>`;\\n            chatData = newChatData;\\n        }\\n    });\\n    \\n    // æŒ‰Enteré”®å‘é€æ¶ˆæ¯\\n    textInput.addEventListener('keypress', (e) => {\\n        if (e.key === 'Enter') {\\n            e.preventDefault(); // é˜»æ­¢é»˜è®¤è¡Œä¸º\\n            sendBtn.click(); // è§¦å‘å‘é€æŒ‰é’®ç‚¹å‡»\\n        }\\n    });\\n\\n    // ä¸