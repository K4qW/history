                        æ ‡é¢˜: postTitle,\\n                                å‘å¸–äºº: authorName,\\n                                å‘å¸–äººå¤´åƒ: authorAvatar,\\n                                å¸–å­å†…å®¹: postTexts,\\n                                å›¾ç‰‡ç®€ä»‹: imageSummaries,\\n                                å¸–å­æ ‡ç­¾: postTags,\\n                                æ”¶è—æ—¶é—´: new Date().toLocaleString()\\n                            };\\n                            return variables;\\n                        });\\n                        console.log('å·²ä½¿ç”¨æ›¿ä»£æ–¹æ³•æ”¶è—å¸–å­');\\n                        \\n                        if (typeof triggerSlash === 'function') {\\n                            triggerSlash(`/echo å·²æˆåŠŸæ”¶è—å¸–å­ã€Š${postTitle}ã€‹`);\\n                        }\\n                    } else {\\n                        console.error('æ— æ³•ä¿å­˜å˜é‡ï¼Œç›¸å…³å‡½æ•°ä¸å¯ç”¨');\\n                    }\\n                }\\n            } catch (error) {\\n                console.error('æ”¶è—å¸–å­æ—¶å‡ºé”™:', error);\\n                if (typeof triggerSlash === 'function') {\\n                    triggerSlash(`/echo æ”¶è—å¸–å­æ—¶å‡ºé”™: ${error.message}`);\\n                }\\n            }\\n        }\\n        \\n        // ä»æ”¶è—ä¸­ç§»é™¤å¸–å­\\n        async function removePostFromFavorites() {\\n            try {\\n                const postTitle = document.querySelector('.post-title').textContent;\\n                \\n                // ä»å˜é‡ä¸­åˆ é™¤æ”¶è—æ•°æ®\\n                if (typeof updateVariablesWith === 'function') {\\n                    await updateVariablesWith(variables => {\\n                        if (variables.æ”¶è—çš„å¸–å­ && variables.æ”¶è—çš„å¸–å­[postTitle]) {\\n                            delete variables.æ”¶è—çš„å¸–å­[postTitle];\\n                            \\n                            // å¦‚æœæ”¶è—çš„å¸–å­å¯¹è±¡ä¸ºç©ºï¼Œåˆ™åˆ é™¤æ•´ä¸ªå¯¹è±¡\\n                            if (Object.keys(variabl