;\\n\\n        // ç”Ÿæˆ3-10ç§’çš„éšæœºç­‰å¾…æ—¶é—´\\n        const randomWaitTime = Math.floor(Math.random() * (10000 - 3000 + 1) + 3000);\\n\\n        // éšæœºæ—¶é—´åå†³å®šæ¥å¬æˆ–æ‹’ç»\\n        callTimeout = setTimeout(() => {\\n            const randomNum = Math.random();\\n            modal.style.display = 'none';\\n            if (callAudio) callAudio.pause();\\n            if (randomNum <= 0.8) { // 80%æ¦‚ç‡æ¥å¬\\n                if (typeof triggerSlash === 'function') {\\n                    triggerSlash(`/send ã€è¯­éŸ³é€šè¯ç•Œé¢ã€‘${chatName}æ¥å¬äº†è¯­éŸ³é€šè¯|/trigger`);\\n                }\\n            } else { // 20%æ¦‚ç‡æ‹’ç»\\n                if (typeof triggerSlash === 'function') {\\n                    triggerSlash(`/send ã€è¯­éŸ³é€šè¯ç•Œé¢ã€‘${chatName}æ‹’ç»äº†è¯­éŸ³é€šè¯|/trigger`);\\n                }\\n            }\\n        }, randomWaitTime);\\n    });\\n\\n    // ç‚¹å‡»å…¶ä»–åœ°æ–¹å…³é—­è¡¨æƒ…é€‰æ‹©å™¨\\n    document.addEventListener('click', (e) => {\\n        if (!emojiBtn.contains(e.target) && !emojiPickerContainer.contains(e.target)) {\\n            emojiPickerContainer.style.display = 'none';\\n        }\\n    });\\n\\n    // é¢„åŠ è½½éŸ³é¢‘\\n    Object.values(sounds).forEach(sound => {\\n        sound.load();\\n    });\\n\\n    // åŠ¨æ€ç›‘å¬æ–°æ·»åŠ çš„æ¶ˆæ¯\\n    const observer = new MutationObserver((mutations) => {\\n        mutations.forEach((mutation) => {\\n            if (mutation.addedNodes.length) {\\n                mutation.addedNodes.forEach((node) => {\\n                    if (node.nodeType === 1) { // å…ƒç´ èŠ‚ç‚¹\\n                        const newAvatars = node.querySelectorAll('.avatar');\\n                        const newContents = node.querySelectorAll('.message-content');\\n\\n                        newAvatars.forEach(avatar => {\\n                            avatar.addEventListener('click', () => {\\n                                playSound('avatar');\\n                            });\\n                        });\\n\\n                        newContents.forEach(content => {\\n                            content.addEventListener('click', () => {\\n                                playSound('bubble');\\n                            });\\n                        });\\n                    }\\n                });\\n            }\\n        });\\n    });\\n\\n    observer.observe(chatMessages, {\\n        childList: true,\\n        subtree: true\\n    });\\n    \\n    // å¯åŠ¨æ¶ˆæ¯æ˜¾ç¤ºåŠ¨ç”»\\n    setTimeout(() => {\\n        showMessages();\\n    }, 500);\\n});\\n\\n// åˆ›å»ºä¸€ä¸ªç©ºçš„éŸ³æ•ˆå¯¹è±¡ï¼ˆä¸å†æ’­æ”¾æ¶ˆæ¯éŸ³æ•ˆï¼‰\\nconst messageSound = {\\n    currentTime: 0,\\n    play: function() { return Promise.resolve(); }\\n};\\n\\n// æ£€æŸ¥æ˜¯å¦æ˜¯æœ€æ–°çš„ç•Œé¢\\nfunction isLatestInterface() {\\n    const currentTimestamp = Date.now();\\n    const lastTimestamp = parseInt(localStorage.getItem('lastInterfaceTimestamp') || '0');\\n\\n    // æ›´æ–°æ—¶é—´æˆ³\\n    localStorage.setItem('lastInterfaceTimestamp', currentTimestamp.toString());\\n\\n    // å¦‚æœå½“å‰æ—¶é—´æˆ³ä¸ä¸Šä¸€ä¸ªæ—¶é—´æˆ³ç›¸å·®ä¸åˆ°100msï¼Œè¯´æ˜æ˜¯åŒä¸€ä¸ªç•Œé¢çš„å¤åˆ¶\\n    return (currentTimestamp - lastTimestamp) > 100;\\n}\\n\\nfunction showMessages() {\\n    // è·å–æ‰€æœ‰å¯¹æ–¹æ¶ˆæ¯\\n    const receivedMessages = Array.from(document.querySelectorAll('.message.received'));\\n    \\n    // è·å–æ‰€æœ‰é€šè¯æ¶ˆæ¯\\n    const callMessages = Array.from(document.querySelectorAll('.message.call-message'));\\n    \\n    // è·å–æ‰€æœ‰æˆ‘æ–¹æ¶ˆæ¯\\n    const sentMessages = Array.from(document.querySelectorAll('.message.sent'));\\n    \\n    // åˆå¹¶æ‰€æœ‰éœ€è¦æ˜¾ç¤ºçš„æ¶ˆæ¯\\n    const allMessages = [...receivedMessages, ...callMessages, ...sentMessages];\\n    \\n    // æŒ‰ç…§DOMé¡ºåºæ’åº\\n    allMessages.sort((a, b) => {\\n        return Array.from(a.parentNode.children).indexOf(a) - Array.from(b.parentNode.children).indexOf(b);\\n    });\\n\\n    // æ£€æŸ¥æ˜¯å¦æ˜¯æœ€æ–°ç•Œé¢\\n    const isLatest = isLatestInterface();\\n\\n    if (!isLatest) {\\n        // ä¸æ˜¯æœ€æ–°ç•Œé¢æ—¶ï¼Œä½¿ç”¨æ¸è¿›æ˜¾ç¤ºæ•ˆæœ\\n        allMessages.forEach(msg => {\\n            msg.style.display = 'none';\\n        });\\n        \\n        // ä½¿ç”¨é¡ºåºæ˜¾ç¤º\\n        setTimeout(() => {\\n            showMessagesSequentially(allMessages);\\n        }, 500);\\n        return;\\n    }\\n\\n    // æœ€æ–°ç•Œé¢æ—¶ï¼Œä½¿ç”¨æ¸è¿›æ˜¾ç¤ºæ•ˆæœ\\n    allMessages.forEach(msg => {\\n        msg.style.display = 'none';\\n    });\\n\\n    // éšæœºæ‰“å­—æ—¶é—´ï¼ˆ1-3ç§’ï¼‰\\n    function getRandomTypingTime() {\\n        return Math.floor(Math.random() * (3000 - 1000 + 1) + 1000);\\n    }\\n\\n    // éšæœºæ¶ˆæ¯é—´éš”æ—¶é—´ï¼ˆ0.5-1.5ç§’ï¼‰\\n    function getRandomDelay() {\\n        return Math.floor(Math.random() * (1500 - 500 + 1) + 500);\\n    }\\n\\n    // æ‰“å­—åŠ¨ç”»æ•ˆæœ\\n    function typeMessage(messageContent, originalContent, onComplete, isSentMessage = false) {\\n        // å¦‚æœæ˜¯å·²å‘é€çš„æ¶ˆæ¯ï¼Œç›´æ¥æ˜¾ç¤ºä¸éœ€è¦æ‰“å­—åŠ¨ç”»\\n        if (isSentMessage) {\\n            messageContent.innerHTML = originalContent;\\n            onComplete();\\n            return;\\n        }\\n        \\n        // æ£€æŸ¥æ˜¯å¦æ˜¯ç‰¹æ®Šæ¶ˆæ¯ç±»å‹\\n        const isSpecialMessage = \\n            messageContent.classList.contains('recalled') || // æ’¤å›æ¶ˆæ¯\\n            originalContent.includes('æ­¤æ¶ˆæ¯å·²æ’¤å›') || \\n            originalContent.includes('<svg') || // åŒ…å«SVGå›¾æ ‡\\n            originalContent.includes('browser-share') || // æµè§ˆå™¨åˆ†äº«\\n            originalContent.includes('transfer-bubble') || // è½¬è´¦æ¶ˆæ¯\\n            originalContent.includes('random-image-container') ||\\n            originalContent.includes('typing-indicator') ||\\n            originalContent.includes('<div') ||\\n            originalContent.includes('<img');\\n\\n        if (isSpecialMessage) {\\n            // å¯¹äºç‰¹æ®Šæ¶ˆæ¯ï¼Œç›´æ¥æ˜¾ç¤ºåŸå§‹HTMLè€Œä¸è¿›è¡Œæ‰“å­—åŠ¨ç”»\\n            messageContent.innerHTML = originalContent;\\n            onComplete();\\n            return;\\n        }\\n\\n        // åˆ›å»ºå¹¶æ˜¾ç¤ºæ‰“å­—æŒ‡ç¤ºå™¨\\n        const typingIndicator = document.createElement('div');\\n        typingIndicator.className = 'typing-indicator';\\n        typingIndicator.innerHTML = '<span class=\\\"typing-dot\\\"></span><span class=\\\"typing-dot\\\"></span><span class=\\\"typing-dot\\\"></span>';\\n\\n        messageContent.textContent = '';\\n        messageContent.appendChild(typingIndicator);\\n\\n        // æ˜¾ç¤ºæ‰“å­—æŒ‡ç¤ºå™¨çš„æ—¶é—´\\n        const typingTime = getRandomTypingTime();\\n\\n        setTimeout(() => {\\n            // æ¸å˜è¿‡æ¸¡æ•ˆæœ\\n            typingIndicator.style.transition = 'opacity 0.5s';\\n            typingIndicator.style.opacity = '0';\\n\\n            setTimeout(() => {\\n                typingIndicator.remove();\\n                messageContent.textContent = '';\\n\\n                let charIndex = 0;\\n                const typeChar = () => {\\n                    if (charIndex < originalContent.length) {\\n                        messageContent.textContent += originalContent.charAt(charIndex);\\n                        charIndex++;\\n                        setTimeout(typeChar, 50);\\n                    } else {\\n                        onComplete();\\n                    }\\n                };\\n                typeChar();\\n\\n            }, 500);\\n\\n        }, typingTime);\\n    }\\n\\n    // é€æ¡æ˜¾ç¤ºæ¶ˆæ¯\\n    function showMessagesSequentially(messages, currentIndex = 0) {\\n        if (currentIndex >= messages.length) return;\\n\\n        const message = messages[currentIndex];\\n\\n        // æ˜¾ç¤ºæ¶ˆæ¯å®¹å™¨\\n        message.style.display = 'flex';\\n\\n        // æ£€æŸ¥æ˜¯å¦æ˜¯é€šè¯æ¶ˆæ¯\\n        if (message.classList.contains('call-message')) {\\n            // é€šè¯æ¶ˆæ¯ç›´æ¥æ˜¾ç¤ºï¼Œä¸éœ€è¦æ‰“å­—åŠ¨ç”»\\n            setTimeout(() => {\\n                showMessagesSequentially(messages, currentIndex + 1);\\n            }, getRandomDelay());\\n            return;\\n        }\\n\\n        const messageContent = message.querySelector('.message-content');\\n        if (!messageContent) {\\n            // å¦‚æœæ²¡æœ‰æ‰¾åˆ°æ¶ˆæ¯å†…å®¹å…ƒç´ ï¼Œç›´æ¥æ˜¾ç¤ºä¸‹ä¸€æ¡\\n            setTimeout(() => {\\n                showMessagesSequentially(messages, currentIndex + 1);\\n            }, getRandomDelay());\\n            return;\\n        }\\n        \\n        const originalContent = messageContent.innerHTML;\\n        \\n        // æ£€æŸ¥æ˜¯å¦æ˜¯å·²å‘é€çš„æ¶ˆæ¯\\n        const isSentMessage = message.classList.contains('sent');\\n\\n        // å¼€å§‹æ‰“å­—åŠ¨ç”»\\n        typeMessage(messageContent, originalContent, () => {\\n            // å½“å‰æ¶ˆæ¯æ˜¾ç¤ºå®Œæˆåï¼Œå»¶è¿Ÿæ˜¾ç¤ºä¸‹ä¸€æ¡\\n            setTimeout(() => {\\n                showMessagesSequentially(messages, currentIndex + 1);\\n            }, getRandomDelay());\\n        }, isSentMessage);\\n    }\\n\\n    // æ·»åŠ CSSæ ·å¼\\n    const style = document.createElement('style');\\n    style.textContent = `\\n        .typing-indicator {\\n            display: inline-flex;\\n            gap: 4px;\\n            padding: 0 2px;\\n        }\\n\\n        .typing-dot {\\n            width: 4px;\\n            height: 4px;\\n            background: #666;\\n            border-radius: 50%;\\n            animation: typing-bounce 1.4s infinite;\\n        }\\n\\n        .typing-dot:nth-child(2) { animation-delay: 0.2s; }\\n        .typing-dot:nth-child(3) { animation-delay: 0.4s; }\\n\\n        @keyframes typing-bounce {\\n            0%, 60%, 100% { transform: translateY(0); }\\n            30% { transform: translateY(-4px); }\\n        }\\n    `;\\n    document.head.appendChild(style);\\n\\n    // å»¶è¿Ÿä¸€æ®µæ—¶é—´åå¼€å§‹æ˜¾ç¤ºæ¶ˆæ¯\\n    setTimeout(() => {\\n        showMessagesSequentially(allMessages);\\n    }, 1000);\\n}\\n\\nfunction handleBrowserHistoryClick(content) {\\n    // æ’­æ”¾ç‚¹å‡»éŸ³æ•ˆ\\n    const clickSound = new Audio('https://files.catbox.moe/gb0so5.mp3');\\n    clickSound.volume = 0.6; // è°ƒæ•´éŸ³é‡\\n    clickSound.play().catch(err => console.log('éŸ³é¢‘æ’­æ”¾å¤±è´¥:', err));\\n\\n    // æ·»åŠ ç‚¹å‡»è§†è§‰åé¦ˆ\\n    const preview = document.querySelector('.browser-share-preview');\\n    if (preview) {\\n        preview.style.transform = 'scale(0.98)';\\n        setTimeout(() => {\\n            preview.style.transform = 'scale(1)';\\n        }, 100);\\n    }\\n\\n    // ä»é¢„è§ˆå†…å®¹ä¸­è·å–URL\\n    const urlElement = document.querySelector('.preview-url');\\n    const url = urlElement ? urlElement.textContent : '';\\n\\n    // å‘é€æ¶ˆæ¯åˆ°èŠå¤©ç³»ç»Ÿï¼ŒåŒ…å«URLä¿¡æ¯\\n    const headerElement = document.querySelector('.header-text h2');\\n    const chatName = headerElement ? headerElement.textContent : \\\"å¯¹æ–¹\\\";\\n    console.log(\\\"è§¦å‘é“¾æ¥ç‚¹å‡»ï¼Œå³å°†å‘é€æ¶ˆæ¯:\\\", content);\\n    if (typeof triggerSlash === 'function') {\\n        triggerSlash(`/send ã€æµè§ˆå™¨ç•Œé¢ã€‘æŸ¥çœ‹${content}æµè§ˆå™¨å†…å®¹\\n${url}|/trigger`);\\n    } else {\\n        console.error(\\\"triggerSlashå‡½æ•°ä¸å¯ç”¨\\\");\\n        // æç¤ºç”¨æˆ·triggerSlashä¸å¯ç”¨\\n        showNotification(\\\"åŠŸèƒ½å—é™\\\", \\\"æ— æ³•å‘é€æ¶ˆæ¯åˆ°èŠå¤©ç³»ç»Ÿ\\\", true);\\n    }\\n}\\n\\n// å…¨å±€å˜é‡ï¼Œç”¨äºè·Ÿè¸ªå½“å‰é€‰ä¸­çš„è½¬è´¦æ°”æ³¡\\nlet currentTransferBubble = null;\\n\\n// è½¬è´¦ç›¸å…³å‡½æ•°\\nfunction showTransferModal(transferBubble) {\\n    if (!transferBubble) return;\\n    \\n    // æ’­æ”¾ç‚¹å‡»éŸ³æ•ˆ\\n    const clickSound = new Audio('https://files.catbox.moe/b4amzm.mp3');\\n    clickSound.volume = 0.6;\\n    clickSound.play().catch(err => console.log('éŸ³é¢‘æ’­æ”¾å¤±è´¥:', err));\\n    \\n    // ä¿å­˜å½“å‰è½¬è´¦æ°”æ³¡å¼•ç”¨\\n    window.currentTransferBubble = transferBubble;\\n    \\n    // è·å–è½¬è´¦æ•°æ®\\n    const amount = transferBubble.getAttribute('data-amount');\\n    const transferTime = transferBubble.getAttribute('data-time');\\n    const sender = transferBubble.getAttribute('data-sender');\\n    \\n    // è·å–å¼¹çª—å’Œé®ç½©å±‚\\n    const modal = document.querySelector('.transfer-modal');\\n    const overlay = document.querySelector('.modal-overlay');\\n    \\n    if (!modal || !overlay) {\\n        console.error('æ‰¾ä¸åˆ°è½¬è´¦å¼¹çª—æˆ–é®ç½©å±‚å…ƒç´ ');\\n        return;\\n    }\\n    \\n    // æ¸…ç©ºå¹¶åŠ¨æ€ç”Ÿæˆå¼¹çª—å†…å®¹\\n    modal.innerHTML = `\\n        <div class=\\\"modal-header\\\">\\n            <h3 class=\\\"modal-title\\\">æ”¶åˆ°è½¬è´¦</h3>\\n            <div class=\\\"modal-subtitle\\\">æ¥è‡ª <span class=\\\"transfer-sender\\\">${sender || \\\"æœªçŸ¥ç”¨æˆ·\\\"}</span></div>\\n        </div>\\n        \\n        <div class=\\\"amount-display\\\">\\n            <span class=\\\"currency\\\">Â¥</span>\\n            <span class=\\\"transfer-modal-amount\\\">${amount}</span>\\n        </div>\\n        \\n        <div class=\\\"transfer-details\\\">\\n            <div class=\\\"detail-row\\\">\\n                <div class=\\\"detail-label\\\">è½¬è´¦æ–¹å¼</div>\\n                <div class=\\\"detail-value\\\">èŠå¤©è½¬è´¦</div>\\n            </div>\\n            <div class=\\\"detail-row\\\">\\n                <div class=\\\"detail-label\\\">è½¬è´¦æ—¶é—´</div>\\n                <div class=\\\"detail-value transfer-time\\\">${transferTime}</div>\\n            </div>\\n            <div class=\\\"detail-row\\\">\\n                <div class=\\\"detail-label\\\">è½¬è´¦è¯´æ˜</div>\\n                <div class=\\\"detail-value\\\">æ— </div>\\n            </div>\\n        </div>\\n        \\n        <div class=\\\"modal-buttons\\\">\\n            <button class=\\\"modal-button cancel-btn\\\">æ‹’ç»</button>\\n            <button class=\\\"modal-button accept-btn\\\">æ¥å—</button>\\n        </div>\\n    `;\\n    \\n    // ç¡®ä¿æŒ‰é’®äº‹ä»¶ç»‘å®š\\n    const cancelBtn = modal.querySelector('.cancel-btn');\\n    const acceptBtn = modal.querySelector('.accept-btn');\\n    \\n    if (cancelBtn) {\\n        cancelBtn.addEventListener('click', hideTransferModal);\\n    }\\n    \\n    if (acceptBtn) {\\n        acceptBtn.addEventListener('click', acceptTransfer);\\n    }\\n    \\n    // è°ƒæ•´å±‚çº§å’Œä½ç½®ï¼Œç¡®ä¿åœ¨æœ€é¡¶å±‚æ˜¾ç¤º\\n    document.body.appendChild(overlay);\\n    document.body.appendChild(modal);\\n    \\n    modal.style.zIndex = '10000';\\n    overlay.style.zIndex = '9999';\\n    \\n    // æ˜¾ç¤ºæ¨¡æ€çª—å’Œé®ç½©å±‚\\n    modal.style.display = 'block';\\n    overlay.style.display = 'block';\\n}\\n\\nfunction hideTransferModal() {\\n    const modal = document.querySelector('.transfer-modal');\\n    const overlay = document.querySelector('.modal-overlay');\\n    \\n    if (modal && overlay) {\\n        modal.style.display = 'none';\\n        overlay.style.display = 'none';\\n        window.currentTransferBubble = null;\\n    }\\n}\\n\\nfunction acceptTransfer() {\\n    // ä½¿ç”¨å½“å‰æ“ä½œçš„è½¬è´¦æ°”æ³¡\\n    if (window.currentTransferBubble) {\\n        const transferStatus = window.currentTransferBubble.querySelector('.transfer-status');\\n        if (transferStatus) {\\n            transferStatus.textContent = 'å·²æ¥æ”¶';\\n            transferStatus.style.color = '#4CAF50';\\n        }\\n    }\\n    \\n    // æ’­æ”¾æ¥æ”¶éŸ³æ•ˆ\\n    const acceptSound = new Audio('https://files.catbox.moe/gb0so5.mp3');\\n    acceptSound.volume = 0.5;\\n    acceptSound.play().catch(err => console.log('éŸ³é¢‘æ’­æ”¾å¤±è´¥:', err));\\n    \\n    // éšè—æ¨¡æ€çª—å’Œé®ç½©å±‚\\n    hideTransferModal();\\n}\\n\\n// å®šä½æ¶ˆæ¯ç›¸å…³å‡½æ•°\\nfunction toggleLocationMap(previewElement) {\\n    const mapElement = previewElement.nextElementSibling;\\n    const arrowElement = previewElement.querySelector('.location-arrow');\\n    \\n    if (mapElement.style.display === 'none') {\\n        // æ˜¾ç¤ºåœ°å›¾\\n        mapElement.style.display = 'block';\\n        mapElement.classList.add('showing');\\n        arrowElement.style.transform = 'rotate(90deg)';\\n        \\n        // æ’­æ”¾å±•å¼€éŸ³æ•ˆ\\n        const expandSound = new Audio('https://files.catbox.moe/gb0so5.mp3');\\n        expandSound.volume = 0.3;\\n        expandSound.play().catch(err => console.log('éŸ³é¢‘æ’­æ”¾å¤±è´¥:', err));\\n    } else {\\n        // éšè—åœ°å›¾\\n        mapElement.classList.remove('showing');\\n        arrowElement.style.transform = 'rotate(0deg)';\\n        \\n        // æ·»åŠ è¿‡æ¸¡åŠ¨ç”»åéšè—\\n        setTimeout(() => {\\n            mapElement.style.display = 'none';\\n        }, 300);\\n        \\n        // æ’­æ”¾æ”¶èµ·éŸ³æ•ˆ\\n        const collapseSound = new Audio('https://files.catbox.moe/b4amzm.mp3');\\n        collapseSound.volume = 0.3;\\n        collapseSound.play().catch(err => console.log('éŸ³é¢‘æ’­æ”¾å¤±è´¥:', err));\\n    }\\n}\\n\\n// æ–°é—»æ¶ˆæ¯ç›¸å…³å‡½æ•°\\nfunction showNewsModal(title, source, publishDate, views, content, keyData, likes, comments) {\\n    // æ’­æ”¾ç‚¹å‡»éŸ³æ•ˆ\\n    const clickSound = new Audio('https://files.catbox.moe/gb0so5.mp3');\\n    clickSound.volume = 0.5;\\n    clickSound.play().catch(err => console.log('éŸ³é¢‘æ’­æ”¾å¤±è´¥:', err));\\n    \\n    // è·å–å¼¹çª—å’Œé®ç½©å±‚\\n    const modal = document.querySelector('.news-modal');\\n    const overlay = document.querySelector('.news-modal-overlay');\\n    \\n    if (!modal || !overlay) {\\n        console.error('æ‰¾ä¸åˆ°æ–°é—»å¼¹çª—æˆ–é®ç½©å±‚å…ƒç´ ');\\n        return;\\n    }\\n    \\n    // å¤„ç†å¯èƒ½çš„æ•°æ®é—®é¢˜\\n    // å°è¯•è§£ç ç¼–ç è¿‡çš„å†…å®¹\\n    try {\\n        content = content ? decodeURIComponent(content) : '';\\n        keyData = keyData ? decodeURIComponent(keyData) : '';\\n    } catch (e) {\\n        console.error('è§£ç å†…å®¹æ—¶å‡ºé”™:', e);\\n        // å¦‚æœè§£ç å‡ºé”™ï¼Œä¿æŒåŸæ ·\\n    }\\n    \\n    // æ£€æŸ¥likesæ˜¯å¦åŒ…å«\\\"|\\\"åˆ†éš”ç¬¦ï¼Œå¦‚æœæ˜¯åˆ™è¯´æ˜åˆå¹¶äº†ç‚¹èµå’Œè¯„è®ºæ•°\\n    let likesValue = likes || '0';\\n    let commentsValue = comments || '0';\\n    \\n    if (likesValue && likesValue.includes('|')) {\\n        const parts = likesValue.split('|');\\n        likesValue = parts[0]; // ç¬¬ä¸€éƒ¨åˆ†æ˜¯ç‚¹èµæ•°\\n        commentsValue = parts[1] || '0'; // ç¬¬äºŒéƒ¨åˆ†æ˜¯è¯„è®ºæ•°\\n    }\\n    \\n    // æ¸…ç©ºå¹¶åŠ¨æ€ç”Ÿæˆå¼¹çª—å†…å®¹\\n    modal.innerHTML = `\\n        <div class=\\\"news-modal-close\\\" onclick=\\\"closeNewsModal()\\\">Ã—</div>\\n        \\n        <div class=\\\"news-header\\\">\\n            <div class=\\\"news-publisher\\\">\\n                <img src=\\\"https://files.catbox.moe/8to7e5.png\\\" class=\\\"publisher-icon\\\">\\n                <span class=\\\"publisher-name\\\">${source}</span>\\n            </div>\\n            <h3 class=\\\"news-modal-title\\\">${title}</h3>\\n            <div class=\\\"news-stats\\\">\\n                <span class=\\\"news-publish-date\\\">${publishDate}</span>\\n                <span class=\\\"stats-separator\\\">â€¢</span>\\n                <span class=\\\"news-views\\\">é˜…è¯» ${views}</span>\\n            </div>\\n        </div>\\n        \\n        <div class=\\\"news-content\\\">\\n            <p>${content}</p>\\n            ${keyData ? `\\n                <div class=\\\"news-key-data\\\">\\n                    <div class=\\\"key-data-title\\\">å…³é”®æ•°æ®</div>\\n                    <div class=\\\"key-data-content\\\">${keyData}</div>\\n                </div>\\n            ` : ''}\\n        </div>\\n        \\n        <div class=\\\"news-footer\\\">\\n            <div class=\\\"news-actions\\\">\\n                <div class=\\\"action-item\\\">\\n                    <svg viewBox=\\\"0 0 24 24\\\" class=\\\"action-icon\\\">\\n                        <path fill=\\\"currentColor\\\" d=\\\"M12 21.35l-1.45-1.32C5.4 15.36 2 12.28 2 8.5 2 5.42 4.42 3 7.5 3c1.74 0 3.41.81 4.5 2.09C13.09 3.81 14.76 3 16.5 3 19.58 3 22 5.42 22 8.5c0 3.78-3.4 6.86-8.55 11.54L12 21.35z\\\"/>\\n                    </svg>\\n                    <span class=\\\"action-count likes-count\\\">${likesValue}</span>\\n                </div>\\n                <div class=\\\"action-item\\\">\\n                    <svg viewBox=\\\"0 0 24 24\\\" class=\\\"action-icon\\\">\\n                        <path fill=\\\"currentColor\\\" d=\\\"M21.99 4c0-1.1-.89-2-1.99-2H4c-1.1 0-2 .9-2 2v12c0 1.1.9 2 2 2h14l4 4-.01-18z\\\"/>\\n                    </svg>\\n                    <span class=\\\"action-count comments-count\\\">${commentsValue}</span>\\n                </div>\\n            </div>\\n            <div class=\\\"news-share-buttons\\\">\\n                <svg viewBox=\\\"0 0 24 24\\\" class=\\\"share-button\\\">\\n                    <path fill=\\\"currentColor\\\" d=\\\"M18 16.08c-.76 0-1.44.3-1.96.77L8.91 12.7c.05-.23.09-.46.09-.7s-.04-.47-.09-.7l7.05-4.11c.54.5 1.25.81 2.04.81 1.66 0 3-1.34 3-3s-1.34-3-3-3-3 1.34-3 3c0 .24.04.47.09.7L8.04 9.81C7.5 9.31 6.79 9 6 9c-1.66 0-3 1.34-3 3s1.34 3 3 3c.79 0 1.5-.31 2.04-.81l7.12 4.16c-.05.21-.08.43-.08.65 0 1.61 1.31 2.92 2.92 2.92 1.61 0 2.92-1.31 2.92-2.92s-1.31-2.92-2.92-2.92z\\\"/>\\n                </svg>\\n                <svg viewBox=\\\"0 0 24 24\\\" class=\\\"share-button\\\">\\n                    <path fill=\\\"currentColor\\\" d=\\\"M17 3H7c-1.1 0-1.99.9-1.99 2L5 21l7-3 7 3V5c0-1.1-.9-2-2-2z\\\"/>\\n                </svg>\\n            </div>\\n        </div>\\n    `;\\n    \\n    // è°ƒæ•´å±‚çº§å’Œä½ç½®\\n    document.body.appendChild(overlay);\\n    document.body.appendChild(modal);\\n    \\n    modal.style.zIndex = '10001';\\n    overlay.style.zIndex = '10000';\\n    \\n    // æ˜¾ç¤ºå¼¹çª—å’Œé®ç½©å±‚\\n    modal.style.display = 'block';\\n    overlay.style.display = 'block';\\n    \\n    // æ·»åŠ é®ç½©å±‚ç‚¹å‡»å…³é—­äº‹ä»¶\\n    overlay.onclick = closeNewsModal;\\n    \\n    // æ ¹æ®å†…å®¹è°ƒæ•´å¼¹çª—ä½ç½®\\n    setTimeout(() => {\\n        if (modal.offsetHeight > window.innerHeight * 0.8) {\\n            modal.style.height = '80vh';\\n            modal.style.overflowY = 'auto';\\n        } else {\\n            modal.style.height = 'auto';\\n            modal.style.overflowY = 'visible';\\n        }\\n    }, 10);\\n}\\n\\nfunction closeNewsModal() {\\n    const modal = document.querySelector('.news-modal');\\n    const overlay = document.querySelector('.news-modal-overlay');\\n    \\n    if (modal && overlay) {\\n        // æ’­æ”¾å…³é—­éŸ³æ•ˆ\\n        const closeSound = new Audio('https://files.catbox.moe/b4amzm.mp3');\\n        closeSound.volume = 0.3;\\n        closeSound.play().catch(err => console.log('éŸ³é¢‘æ’­æ”¾å¤±è´¥:', err));\\n        \\n        // éšè—å¼¹çª—å’Œé®ç½©\\n        modal.style.display = 'none';\\n        overlay.style.display = 'none';\\n    }\\n}\\n\\n// å¤„ç†æ–°é—»å¼¹çª—å…³é—­\\nfunction closeNewsModal() {\\n    document.querySelector('.news-modal').style.display = 'none';\\n    document.querySelector('.news-modal-overlay').style.display = 'none';\\n    \\n    // æ’­æ”¾å…³é—­éŸ³æ•ˆ\\n    const clickSound = document.getElementById('clickSound');\\n    clickSound.currentTime = 0;\\n    clickSound.play();\\n}\\n\\n// ä¿å­˜æ¶ˆæ¯åˆ°ä¸–ç•Œä¹¦\\nasync function saveMessages() {\\n    try {\\n        // æ’­æ”¾æç¤ºéŸ³\\n        try {\\n            const clickSound = document.getElementById('clickSound');\\n            if (clickSound) {\\n                clickSound.currentTime = 0;\\n                clickSound.play().catch(() => {\\n                    // æ’­æ”¾å¤±è´¥æ—¶ä½¿ç”¨å¤‡ç”¨å£°éŸ³\\n                    const fallbackSound = new Audio('https://assets.mixkit.co/active_storage/sfx/2568/2568-preview.mp3');\\n                    fallbackSound.play().catch(err => console.log('æ‰€æœ‰éŸ³æ•ˆæ’­æ”¾å¤±è´¥'));\\n                });\\n            } else {\\n                // ä½¿ç”¨å¤‡ç”¨å£°éŸ³\\n                const fallbackSound = new Audio('https://assets.mixkit.co/active_storage/sfx/2568/2568-preview.mp3');\\n                fallbackSound.play().catch(err => console.log('å¤‡ç”¨éŸ³æ•ˆæ’­æ”¾å¤±è´¥'));\\n            }\\n        } catch (soundError) {\\n            console.log('éŸ³æ•ˆå¤„ç†å‡ºé”™:', soundError);\\n        }\\n\\n        // 1. è·å–å½“å‰èŠå¤©å¯¹è±¡ä¿¡æ¯\\n        const chatInfo = parseChat(chatData);\\n        if (!chatInfo.currentChat || !chatInfo.currentChat.id) {\\n            showNotification('ä¿å­˜å¤±è´¥', 'æ— æ³•è·å–èŠå¤©å¯¹è±¡ID', true);\\n            return;\\n        }\\n\\n        const characterId = chatInfo.currentChat.id;\\n        \\n        // å¤„ç†å¤´åƒURLï¼Œç§»é™¤å‰ç¼€\\n        const avatarWithoutPrefix = chatInfo.currentChat.avatar.replace('https://files.catbox.moe/', '');\\n        \\n        // åˆ›å»ºæ¶ˆæ¯æ•°æ®å¯¹è±¡ï¼Œç¡®ä¿å¤´åƒURLä¸åŒ…å«å‰ç¼€\\n        const messageData = {\\n            characterName: chatInfo.currentChat.name,\\n            characterAvatar: avatarWithoutPrefix,\\n            messages: chatInfo.messages.map(msg => {\\n                // åˆ›å»ºæ¶ˆæ¯çš„å‰¯æœ¬\\n                const msgCopy = {...msg};\\n                \\n                // å¦‚æœæ¶ˆæ¯ä¸­åŒ…å«å¤´åƒå­—æ®µï¼Œä¹Ÿç§»é™¤å‰ç¼€\\n                if (msgCopy.avatar && typeof msgCopy.avatar === 'string') {\\n                    msgCopy.avatar = msgCopy.avatar.replace('https://files.catbox.moe/', '');\\n                }\\n                \\n                return msgCopy;\\n            }),\\n            favorability: chatInfo.favorability, // æ·»åŠ å¥½æ„Ÿåº¦ä¿¡æ¯\\n            lastUpdated: new Date().toISOString()\\n        };\\n\\n        // 2. å¤„ç†JSONå­—ç¬¦ä¸²ä¸­çš„ç‰¹æ®Šå­—ç¬¦\\n        const contentJson = JSON.stringify(messageData)\\n            .replace(/\\\\|/g, '\\\\\\\\|')\\n            .replace(/\\\"/g, '\\\\\\\\\\\"')\\n            .replace(/'/g, \\\"\\\\\\\\'\\\");\\n\\n        // 3. ä¿å­˜åˆ°ä¸–ç•Œä¹¦\\n        if (typeof triggerSlash === 'function') {\\n            console.log(`ä¿å­˜èŠå¤©è®°å½•åˆ°ä¸–ç•Œä¹¦ï¼Œè§’è‰²ID: ${characterId}`);\\n            await triggerSlash(`/setentryfield file=worldbook uid=${characterId} field=content \\\"${contentJson}\\\"`);\\n            await triggerSlash(`/setentryfield file=worldbook uid=${characterId} field=constant `);\\n            await triggerSlash(`/setentryfield file=worldbook uid=${characterId} field=position 4`);\\n            await triggerSlash(`/setentryfield file=worldbook uid=${characterId} field=role 0`);\\n            \\n            showNotification('ä¿å­˜æˆåŠŸ', `å·²å°†ä¸ ${chatInfo.currentChat.name} çš„èŠå¤©è®°å½•ä¿å­˜åˆ°ä¸–ç•Œä¹¦`);\\n            console.log(`æˆåŠŸä¿å­˜èŠå¤©è®°å½•ï¼Œè§’è‰²ID: ${characterId}`);\\n        } else {\\n            showNotification('ä¿å­˜å¤±è´¥', 'æ— æ³•ä½¿ç”¨triggerSlashå‡½æ•°', true);\\n            console.error('triggerSlashå‡½æ•°ä¸å¯ç”¨');\\n        }\\n    } catch (error) {\\n        console.error('ä¿å­˜èŠå¤©è®°å½•å¤±è´¥:', error);\\n        showNotification('ä¿å­˜å¤±è´¥', 'ä¿å­˜èŠå¤©è®°å½•æ—¶å‘ç”Ÿé”™è¯¯', true);\\n    }\\n}\\n\\n// æ˜¾ç¤ºé€šçŸ¥æç¤º\\nfunction showNotification(title, message, isError = false) {\\n    try {\\n        // æ’­æ”¾æç¤ºéŸ³\\n        const notificationSound = document.getElementById('notificationSound');\\n        if (notificationSound) {\\n            notificationSound.currentTime = 0;\\n            notificationSound.play().catch(err => {\\n                console.log('é€šçŸ¥éŸ³æ•ˆæ’­æ”¾å¤±è´¥:', err);\\n                // å¦‚æœæ‰¾ä¸åˆ°æŒ‡å®šçš„éŸ³æ•ˆå…ƒç´ ï¼Œä½¿ç”¨æ›¿ä»£éŸ³æ•ˆ\\n                const fallbackSound = new Audio('https://assets.mixkit.co/active_storage/sfx/3005/3005-preview.mp3');\\n                fallbackSound.play().catch(err => console.log('æ›¿ä»£éŸ³æ•ˆæ’­æ”¾å¤±è´¥:', err));\\n            });\\n        } else {\\n            // å¦‚æœæ‰¾ä¸åˆ°æŒ‡å®šçš„éŸ³æ•ˆå…ƒç´ ï¼Œä½¿ç”¨æ›¿ä»£éŸ³æ•ˆ\\n            const fallbackSound = new Audio('https://assets.mixkit.co/active_storage/sfx/3005/3005-preview.mp3');\\n            fallbackSound.play().catch(err => console.log('æ›¿ä»£éŸ³æ•ˆæ’­æ”¾å¤±è´¥:', err));\\n        }\\n        \\n        // åˆ›å»ºé€šçŸ¥å…ƒç´ \\n        const notification = document.createElement('div');\\n        notification.className = 'notification';\\n        if (isError) {\\n            notification.classList.add('error');\\n        }\\n        notification.innerHTML = `\\n            <div class=\\\"notification-title\\\">${title}</div>\\n            <div class=\\\"notification-message\\\">${message}</div>\\n        `;\\n        \\n        document.body.appendChild(notification);\\n        \\n        // æ˜¾ç¤ºåŠ¨ç”»\\n        setTimeout(() => {\\n            notification.classList.add('show');\\n        }, 10);\\n        \\n        // è‡ªåŠ¨å…³é—­\\n        setTimeout(() => {\\n            notification.classList.remove('show');\\n            setTimeout(() => {\\n                notification.remove();\\n            }, 300);\\n        }, 3000);\\n    } catch (error) {\\n        console.error('æ˜¾ç¤ºé€šçŸ¥å¤±è´¥:', error);\\n        // ä½¿ç”¨alertä½œä¸ºå¤‡ç”¨\\n        alert(`${title}: ${message}`);\\n    }\\n}\\n\\n// åŠ è½½èŠå¤©è®°å½•\\nasync function loadMessages(characterId) {\\n    try {\\n        if (!characterId) {\\n            console.error('æœªæä¾›è§’è‰²IDï¼Œæ— æ³•åŠ è½½èŠå¤©è®°å½•');\\n            return false;\\n        }\\n        \\n        if (typeof triggerSlashWithResult !== 'function') {\\n            console.error('triggerSlashWithResultå‡½æ•°ä¸å¯ç”¨');\\n            return false;\\n        }\\n        \\n        // ä»ä¸–ç•Œä¹¦è·å–èŠå¤©è®°å½•\\n        const content = await triggerSlashWithResult(`/getentryfield file=worldbook field=content ${characterId}`);\\n        \\n        if (!content || content.trim() === '') {\\n            console.log(`æœªæ‰¾åˆ°è§’è‰²ID ${characterId} çš„èŠå¤©è®°å½•`);\\n            return false;\\n        }\\n        \\n        try {\\n            // è§£æJSONæ•°æ®\\n            const messageData = JSON.parse(content);\\n            \\n            if (!messageData || !messageData.messages || !Array.isArray(messageData.messages)) {\\n                console.error('èŠå¤©è®°å½•æ ¼å¼æ— æ•ˆ');\\n                return false;\\n            }\\n            \\n            // é‡å»ºèŠå¤©æ•°æ®æ ¼å¼\\n            let newChatData = `<message>\\\\n`;\\n            newChatData += `[å½“å‰èŠå¤©å¯¹è±¡|${messageData.characterName}|${messageData.characterAvatar.replace('https://files.catbox.moe/', '')}|${characterId}]\\\\n`;\\n            \\n            // æ·»åŠ å¥½æ„Ÿåº¦æ•°æ®\\n            if (messageData.favorability && messageData.favorability > 0) {\\n                newChatData += `[å¯¹æ–¹å¥½æ„Ÿåº¦|${messageData.favorability}]\\\\n`;\\n            }\\n            \\n            // æ·»åŠ æ¶ˆæ¯\\n            messageData.messages.forEach(msg => {\\n                switch (msg.type) {\\n                    case 'received':\\n                        newChatData += `[å¯¹æ–¹æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.content}|${msg.time}]\\\\n`;\\n                        break;\\n                    case 'recalled':\\n                        newChatData += `[å¯¹æ–¹æ’¤å›æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.originalContent}|${msg.time}]\\\\n`;\\n                        break;\\n                    case 'browser-share':\\n                        newChatData += `[å¯¹æ–¹åˆ†äº«é“¾æ¥æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.title}|${msg.source}|${msg.description}|${msg.url}|${msg.time}]\\\\n`;\\n                        break;\\n                    case 'transfer':\\n                        newChatData += `[å¯¹æ–¹è½¬è´¦æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.amount}|${msg.transferTime}|${msg.time}]\\\\n`;\\n                        break;\\n                    case 'voice':\\n                        newChatData += `[å¯¹æ–¹è¯­éŸ³æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.audioFile}|${msg.time}]\\\\n`;\\n                        break;\\n                    case 'location':\\n                        newChatData += `[å¯¹æ–¹å®šä½å…±äº«æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.userLocation}|${msg.partnerLocation}|${msg.distance}|${msg.time}]\\\\n`;\\n                        break;\\n                    case 'news':\\n                        newChatData += `[å¯¹æ–¹æ–°é—»æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.title}|${msg.source}|${msg.publishDate}|${msg.time}|${msg.views}|${msg.content}|${msg.keyData}|${msg.likes}|${msg.comments}]\\\\n`;\\n                        break;\\n                    case 'text-image':\\n                        newChatData += `[å¯¹æ–¹æ–‡å­—å›¾ç‰‡æ¶ˆæ¯|${msg.avatar.replace('https://files.catbox.moe/', '')}|${msg.description}|${msg.time}]\\\\n`;\\n                        break;\\n                    case 'call-missed':\\n                        newChatData += `[é€šè¯æœªæ¥é€š]\\\\n`;\\n                        break;\\n                    case 'call-ended':\\n                        newChatData += `[é€šè¯å·²ç»“æŸ|${msg.duration}]\\\\n`;\\n                        break;\\n                }\\n            });\\n            \\n            newChatData += `</message>`;\\n            \\n            // æ›´æ–°èŠå¤©æ•°æ®\\n            chatData = newChatData;\\n            \\n            // æ›´æ–°æ˜¾ç¤º\\n            const chatInfo = parseChat(chatData);\\n            updateChatDisplay(chatInfo);\\n            \\n            showNotification('åŠ è½½æˆåŠŸ', `å·²åŠ è½½ä¸ ${messageData.characterName} çš„èŠå¤©è®°å½•`);\\n            return true;\\n        } catch (error) {\\n            console.error('è§£æèŠå¤©è®°å½•å¤±è´¥:', error);\\n            return false;\\n        }\\n    } catch (error) {\\n        console.error('åŠ è½½èŠå¤©è®°å½•å¤±è´¥:', error);\\n        return false;\\n    }\\n}\\n\\n// åœ¨é¡µé¢åŠ è½½æ—¶å°è¯•æ¢å¤èŠå¤©è®°å½•\\ndocument.addEventListener('DOMContentLoaded', async function() {\\n    // è§£æå½“å‰èŠå¤©æ•°æ®\\n    const chatInfo = parseChat(chatData);\\n    \\n    // å¦‚æœæœ‰è§’è‰²IDï¼Œå°è¯•åŠ è½½èŠå¤©è®°å½•\\n    if (chatInfo.currentChat && chatInfo.currentChat.id) {\\n        // ç­‰å¾…100msç¡®ä¿ç•Œé¢å·²åŠ è½½\\n        setTimeout(async () => {\\n            // å¦‚æœå½“å‰æ²¡æœ‰æ¶ˆæ¯ï¼Œå°è¯•ä»ä¸–ç•Œä¹¦åŠ è½½\\n            if (chatInfo.messages.length <= 1) {\\n                await loadMessages(chatInfo.currentChat.id);\\n            }\\n        }, 100);\\n    }\\n});\\n\\n// æ·»åŠ å£°éŸ³æ¶ˆæ¯æ’­æ”¾å‡½æ•°\\nfunction playVoice(container) {\\n    // æŸ¥æ‰¾æ‰€æœ‰æ­£åœ¨æ’­æ”¾çš„è¯­éŸ³æ¶ˆæ¯ï¼Œå¹¶æš‚åœå®ƒä»¬\\n    document.querySelectorAll('.voice-message-container.playing').forEach(el => {\\n        if (el !== container) {\\n            el.classList.remove('playing');\\n            const audio = el.querySelector('audio');\\n            if (audio) {\\n                audio.pause();\\n                audio.currentTime = 0;\\n                el.querySelector('.voice-progress').style.width = '0%';\\n            }\\n        }\\n    });\\n    \\n    const audio = container.querySelector('audio');\\n    if (!audio) return;\\n    \\n    if (audio.paused) {\\n        // æ’­æ”¾éŸ³é¢‘\\n        audio.play().then(() => {\\n            container.classList.add('playing');\\n            \\n            // æ›´æ–°è¿›åº¦æ¡\\n            const updateProgress = () => {\\n                if (audio.duration) {\\n                    const progress = (audio.currentTime / audio.duration) * 100;\\n                    container.querySelector('.voice-progress').style.width = `${progress}%`;\\n                }\\n                \\n                if (!audio.paused) {\\n                    requestAnimationFrame(updateProgress);\\n                }\\n            };\\n            \\n            updateProgress();\\n            \\n            // æ’­æ”¾å®Œæˆæ—¶\\n            audio.onended = () => {\\n                container.classList.remove('playing');\\n                container.querySelector('.voice-progress').style.width = '0%';\\n                audio.currentTime = 0;\\n            };\\n        }).catch(err => console.log('éŸ³é¢‘æ’­æ”¾å¤±è´¥:', err));\\n    } else {\\n        // æš‚åœéŸ³é¢‘\\n        audio.pause();\\n        container.classList.remove('playing');\\n    }\\n}\\n\\n// æ·»åŠ æ›´æ–°éŸ³é¢‘æ—¶é•¿æ˜¾ç¤ºçš„å‡½æ•°\\nfunction updateDuration(audioElement) {\\n    if (!audioElement) return;\\n    \\n    // ç¡®ä¿éŸ³é¢‘å…ƒç´ å·²ç»åŠ è½½å…ƒæ•°æ®\\n    if (audioElement.readyState === 0) {\\n        audioElement.addEventListener('loadedmetadata', function() {\\n            updateDurationDisplay(audioElement);\\n        });\\n    } else {\\n        updateDurationDisplay(audioElement);\\n    }\\n}\\n\\n// è¾…åŠ©å‡½æ•°ï¼šæ ¼å¼åŒ–å¹¶æ˜¾ç¤ºéŸ³é¢‘æ—¶é•¿\\nfunction updateDurationDisplay(audioElement) {\\n    if (!audioElement || !audioElement.duration) return;\\n    \\n    // æŸ¥æ‰¾çˆ¶å®¹å™¨ä¸­çš„æ—¶é•¿æ˜¾ç¤ºå…ƒç´ \\n    const container = audioElement.closest('.voice-message-container');\\n    if (!container) return;\\n    \\n    const durationElement = container.querySelector('.voice-duration');\\n    if (!durationElement) return;\\n    \\n    // æ ¼å¼åŒ–æ—¶é•¿ï¼ˆä¾‹å¦‚ï¼š1:23ï¼‰\\n    const totalSeconds = Math.round(audioElement.duration);\\n    const minutes = Math.floor(totalSeconds / 60);\\n    const seconds = totalSeconds % 60;\\n    const formattedDuration = `${minutes}:${seconds.toString().padStart(2, '0')}`;\\n    \\n    // æ›´æ–°æ˜¾ç¤º\\n    durationElement.textContent = formattedDuration;\\n}\\n\\n// æ’­æ”¾è¯­éŸ³æ¶ˆæ¯\\nfunction playVoice(element) {\\n    try {\\n        if (window.audioManager && typeof window.audioManager.playVoice === 'function') {\\n            // ä½¿ç”¨æ–°çš„éŸ³é¢‘ç®¡ç†å™¨\\n            window.audioManager.playVoice(element);\\n            return;\\n        }\\n        \\n        // ä»¥ä¸‹æ˜¯å…¼å®¹ä»£ç ï¼Œä¿ç•™åŸæœ‰åŠŸèƒ½\\n        // è·å–éŸ³é¢‘å…ƒç´ \\n        const audioElement = element.querySelector('audio');\\n        if (!audioElement) return;\\n        \\n        // è·å–è¿›åº¦æ¡å…ƒç´ \\n        const progressBar = element.querySelector('.voice-progress');\\n        if (!progressBar) return;\\n        \\n        // å¦‚æœå½“å‰æ­£åœ¨æ’­æ”¾çš„ä¸æ˜¯è¿™ä¸ªéŸ³é¢‘ï¼Œå…ˆæš‚åœå…¶ä»–æ­£åœ¨æ’­æ”¾çš„éŸ³é¢‘\\n        const allAudios = document.querySelectorAll('.voice-message-container audio');\\n        allAudios.forEach(audio => {\\n            if (audio !== audioElement && !audio.paused) {\\n                audio.pause();\\n                // é‡ç½®å…¶ä»–éŸ³é¢‘çš„è¿›åº¦æ¡\\n                const otherContainer = audio.closest('.voice-message-container');\\n                if (otherContainer) {\\n                    const otherProgress = otherContainer.querySelector('.voice-progress');\\n                    if (otherProgress) {\\n                        otherProgress.style.width = '0%';\\n                    }\\n                }\\n            }\\n        });\\n        \\n        // æ’­æ”¾/æš‚åœåˆ‡æ¢\\n        if (audioElement.paused) {\\n            // æ’­æ”¾å‰å…ˆæ’­æ”¾ç‚¹å‡»éŸ³æ•ˆ\\n            const clickSound = document.getElementById('clickSound');\\n            if (clickSound) {\\n                clickSound.currentTime = 0;\\n                clickSound.play().catch(err => console.log('ç‚¹å‡»éŸ³æ•ˆæ’­æ”¾å¤±è´¥:', err));\\n            }\\n            \\n            // æ’­æ”¾è¯­éŸ³\\n            audioElement.play().then(() => {\\n                // æ›´æ–°è¿›åº¦æ¡\\n                const updateProgress = () => {\\n                    if (audioElement.duration) {\\n                        const percent = (audioElement.currentTime / audioElement.duration) * 100;\\n                        progressBar.style.width = `${percent}%`;\\n                    }\\n                    \\n                    if (!audioElement.paused) {\\n                        requestAnimationFrame(updateProgress);\\n                    }\\n                };\\n                \\n                updateProgress();\\n            }).catch(err => {\\n                console.error('è¯­éŸ³æ’­æ”¾å¤±è´¥:', err);\\n                showNotification('æ’­æ”¾å¤±è´¥', 'æ— æ³•æ’­æ”¾è¯­éŸ³æ¶ˆæ¯', true);\\n            });\\n            \\n            // ç›‘å¬æ’­æ”¾ç»“æŸäº‹ä»¶\\n            audioElement.onended = function() {\\n                progressBar.style.width = '0%';\\n            };\\n        } else {\\n            // æš‚åœæ’­æ”¾\\n            audioElement.pause();\\n        }\\n    } catch (error) {\\n        console.error('æ’­æ”¾è¯­éŸ³æ¶ˆæ¯å¤±è´¥:', error);\\n        showNotification('æ’­æ”¾å¤±è´¥', 'æ’­æ”¾è¯­éŸ³æ¶ˆæ¯æ—¶å‘ç”Ÿé”™è¯¯', true);\\n    }\\n}\\n    // å›¾ç‰‡æŸ¥çœ‹å™¨åŠŸèƒ½\\n    function showImageViewer(imgSrc) {\\n        // æ’­æ”¾ç‚¹å‡»éŸ³æ•ˆ\\n        const clickSound = new Audio('https://files.catbox.moe/gb0so5.mp3');\\n        clickSound.volume = 0.5;\\n        clickSound.play().catch(err => console.log('éŸ³é¢‘æ’­æ”¾å¤±è´¥:', err));\\n        \\n        // åˆ›å»ºå›¾ç‰‡æŸ¥çœ‹å™¨å…ƒç´ \\n        const viewer = document.createElement('div');\\n        viewer.className = 'image-viewer-overlay';\\n        viewer.style.position = 'fixed';\\n        viewer.style.top = '0';\\n        viewer.style.left = '0';\\n        viewer.style.width = '100%';\\n        viewer.style.height = '100%';\\n        viewer.style.backgroundColor = 'rgba(0, 0, 0, 0.9)';\\n        viewer.style.display = 'flex';\\n        viewer.style.justifyContent = 'center';\\n        viewer.style.alignItems = 'center';\\n        viewer.style.zIndex = '10000';\\n        viewer.style.opacity = '0';\\n        viewer.style.transition = 'opacity 0.3s ease';\\n        \\n        // åˆ›å»ºå›¾ç‰‡å…ƒç´ \\n        const img = document.createElement('img');\\n        img.src = imgSrc;\\n        img.style.maxWidth = '90%';\\n        img.style.maxHeight = '90%';\\n        img.style.borderRadius = '8px';\\n        img.style.boxShadow = '0 4px 20px rgba(0, 0, 0, 0.3)';\\n        img.style.transform = 'scale(0.9)';\\n        img.style.transition = 'transform 0.3s ease';\\n        \\n        // åˆ›å»ºå…³é—­æŒ‰é’®\\n        const closeBtn = document.createElement('div');\\n        closeBtn.innerHTML = 'Ã—';\\n        closeBtn.style.position = 'absolute';\\n        closeBtn.style.top = '20px';\\n        closeBtn.style.right = '20px';\\n        closeBtn.style.color = 'white';\\n        closeBtn.style.fontSize = '32px';\\n        closeBtn.style.cursor = 'pointer';\\n        closeBtn.style.width = '40px';\\n        closeBtn.style.height = '40px';\\n        closeBtn.style.display = 'flex';\\n        closeBtn.style.justifyContent = 'center';\\n        closeBtn.style.alignItems = 'center';\\n        closeBtn.style.borderRadius = '50%';\\n        closeBtn.style.background = 'rgba(0, 0, 0, 0.5)';\\n        \\n        // æ·»åŠ å…ƒç´ åˆ°æŸ¥çœ‹å™¨\\n        viewer.appendChild(img);\\n        viewer.appendChild(closeBtn);\\n        document.body.appendChild(viewer);\\n        \\n        // è§¦å‘é‡æ’åæ˜¾ç¤º\\n        setTimeout(() => {\\n            viewer.style.opacity = '1';\\n            img.style.transform = 'scale(1)';\\n        }, 10);\\n        \\n        // ç‚¹å‡»å…³é—­æŒ‰é’®å…³é—­æŸ¥çœ‹å™¨\\n        closeBtn.addEventListener('click', () => {\\n            closeImageViewer(viewer);\\n        });\\n        \\n        // ç‚¹å‡»èƒŒæ™¯å…³é—­æŸ¥çœ‹å™¨\\n        viewer.addEventListener('click', (e) => {\\n            if (e.target === viewer) {\\n                closeImageViewer(viewer);\\n            }\\n        });\\n    }\\n    \\n    // å…³é—­å›¾ç‰‡æŸ¥çœ‹å™¨\\n    function closeImageViewer(viewer) {\\n        // æ’­æ”¾å…³é—­éŸ³æ•ˆ\\n        const closeSound = new Audio('https://files.catbox.moe/b4amzm.mp3');\\n        closeSound.volume = 0.3;\\n        closeSound.play().catch(err => console.log('éŸ³é¢‘æ’­æ”¾å¤±è´¥:', err));\\n        \\n        // æ·»åŠ å…³é—­åŠ¨ç”»\\n        const img = viewer.querySelector('img');\\n        viewer.style.opacity = '0';\\n        if (img) img.style.transform = 'scale(0.9)';\\n        \\n        // åŠ¨ç”»ç»“æŸåç§»é™¤å…ƒç´ \\n        setTimeout(() => {\\n            document.body.removeChild(viewer);\\n        }, 300);\\n    }\\n\\n    // æ·»åŠ æ˜¾ç¤ºæ–‡ä»¶è¯¦æƒ…å¼¹çª—çš„å‡½æ•°\\n    function showFileModal(element) {\\n        // æ’­æ”¾ç‚¹å‡»éŸ³æ•ˆ\\n        const clickSound = new Audio('https://files.catbox.moe/gb0so5.mp3');\\n        clickSound.volume = 0.5;\\n        clickSound.play().catch(err => console.log('éŸ³é¢‘æ’­æ”¾å¤±è´¥:', err));\\n\\n        // è·å–æ–‡ä»¶ä¿¡æ¯\\n        const filename = element.getAttribute('data-filename');\\n        const filesize = element.getAttribute('data-filesize');\\n        const description = element.getAttribute('data-description');\\n        const fileType = element.getAttribute('data-file-type');\\n        \\n        // åˆ›å»ºå¼¹çª—HTML\\n        let fileIconClass = 'docx';\\n        let fileIconSvg = '';\\n        \\n        // æ ¹æ®æ–‡ä»¶æ‰©å±•åè®¾ç½®ä¸åŒçš„å›¾æ ‡å’Œæ ·å¼\\n        switch(fileType) {\\n            case 'docx':\\n                fileIconClass = 'docx';\\n                fileIconSvg = '<svg viewBox=\\\"0 0 24 24\\\"><path fill=\\\"white\\\" d=\\\"M14,2H6A2,2 0 0,0 4,4V20A2,2 0 0,0 6,22H18A2,2 0 0,0 20,20V8L14,2M18,20H6V4H13V9H18V20M9,13H15V15H9V13M9,17H13V19H9V17Z\\\"/></svg>';\\n                break;\\n            case 'pptx':\\n                fileIconClass = 'pptx';\\n                fileIconSvg = '<svg viewBox=\\\"0 0 24 24\\\"><path fill=\\\"white\\\" d=\\\"M6,2H14L20,8V20A2,2 0 0,1 18,22H6A2,2 0 0,1 4,20V4A2,2 0 0,1 6,2M13,3.5V9H18.5L13,3.5M8,11V13H16V11H8M8,15V17H13V15H8Z\\\"/></svg>';\\n                break;\\n            case 'xlsx':\\n                fileIconClass = 'xlsx';\\n                fileIconSvg = '<svg viewBox=\\\"0 0 24 24\\\"><path fill=\\\"white\\\" d=\\\"M6,2H14L20,8V20A2,2 0 0,1 18,22H6A2,2 0 0,1 4,20V4A2,2 0 0,1 6,2M13,3.5V9H18.5L13,3.5M17,11H13V13H17V11M17,15H13V17H17V15M11,11H7V13H11V11M11,15H7V17H11V15Z\\\"/></svg>';\\n                break;\\n            case 'zip':\\n                fileIconClass = 'zip';\\n                fileIconSvg = '<svg viewBox=\\\"0 0 24 24\\\"><path fill=\\\"white\\\" d=\\\"M14,17H12V15H10V13H12V15H14M14,9H12V11H14V13H12V11H10V9H12V7H10V5H12V7H14M19,3H5C3.89,3 3,3.89 3,5V19A2,2 0 0,0 5,21H19A2,2 0 0,0 21,19V5C21,3.89 20.1,3 19,3Z\\\"/></svg>';\\n                break;\\n            case 'pdf':\\n                fileIconClass = 'pdf';\\n                fileIconSvg = '<svg viewBox=\\\"0 0 24 24\\\"><path fill=\\\"white\\\" d=\\\"M20,2H8A2,2 0 0,0 6,4V16A2,2 0 0,0 8,18H20A2,2 0 0,0 22,16V4A2,2 0 0,0 20,2M20,16H8V4H20V16M4,6H2V20A2,2 0 0,0 4,22H18V20H4V6M16,12V9H9V12H16Z\\\"/></svg>';\\n                break;\\n            default:\\n                fileIconClass = 'docx';\\n                fileIconSvg = '<svg viewBox=\\\"0 0 24 24\\\"><path fill=\\\"white\\\" d=\\\"M6,2A2,2 0 0,0 4,4V20A2,2 0 0,0 6,22H18A2,2 0 0,0 20,20V8L14,2H6M6,4H13V9H18V20H6V4M8,12V14H16V12H8M8,16V18H13V16H8Z\\\"/></svg>';\\n        }\\n        \\n        // åˆ›å»ºå½“å‰æ—¥æœŸå­—ç¬¦ä¸²\\n        const now = new Date();\\n        const dateString = `${now.getFullYear()}-${(now.getMonth()+1).toString().padStart(2, '0')}-${now.getDate().toString().padStart(2, '0')}`;\\n        \\n        // åˆ›å»ºæ¨¡æ€æ¡†HTML\\n        const modalHTML = `\\n            <div class=\\\"file-modal-header\\\">\\n                <div class=\\\"file-modal-icon ${fileIconClass}\\\">\\n                    ${fileIconSvg}\\n                </div>\\n                <div class=\\\"file-modal-title\\\">${filename}</div>\\n            </div>\\n            <div class=\\\"file-modal-info\\\">\\n                <div class=\\\"file-modal-detail\\\">\\n                    <div class=\\\"file-modal-label\\\">æ–‡ä»¶å¤§å°</div>\\n                    <div class=\\\"file-modal-value\\\">${filesize}</div>\\n                </div>\\n                <div class=\\\"file-modal-detail\\\">\\n                    <div class=\\\"file-modal-label\\\">åˆ›å»ºæ—¥æœŸ</div>\\n                    <div class=\\\"file-modal-value\\\">${dateString}</div>\\n                </div>\\n                <div class=\\\"file-modal-detail\\\">\\n                    <div class=\\\"file-modal-label\\\">æè¿°</div>\\n                    <div class=\\\"file-modal-value\\\">${description || 'æ— æè¿°'}</div>\\n                </div>\\n            </div>\\n            <div class=\\\"file-modal-actions\\\">\\n                <button class=\\\"file-modal-button file-modal-download\\\">ä¸‹è½½</button>\\n                <button class=\\\"file-modal-button file-modal-close\\\">å…³é—­</button>\\n            </div>\\n        `;\\n        \\n        // è·å–æˆ–åˆ›å»ºæ¨¡æ€æ¡†å…ƒç´ \\n        let fileModal = document.querySelector('.file-modal');\\n        let fileModalOverlay = document.querySelector('.file-modal-overlay');\\n        \\n        if (!fileModal) {\\n            fileModal = document.createElement('div');\\n            fileModal.className = 'file-modal';\\n            document.querySelector('.phone-container').appendChild(fileModal);\\n        }\\n        \\n        if (!fileModalOverlay) {\\n            fileModalOverlay = document.createElement('div');\\n            fileModalOverlay.className = 'file-modal-overlay';\\n            document.querySelector('.phone-container').appendChild(fileModalOverlay);\\n        }\\n        \\n        // è®¾ç½®æ¨¡æ€æ¡†å†…å®¹\\n        fileModal.innerHTML = modalHTML;\\n        \\n        // æ˜¾ç¤ºæ¨¡æ€æ¡†\\n        fileModal.style.display = 'block';\\n        fileModalOverlay.style.display = 'block';\\n        \\n        // æ·»åŠ æŒ‰é’®äº‹ä»¶\\n        const closeBtn = fileModal.querySelector('.file-modal-close');\\n        const downloadBtn = fileModal.querySelector('.file-modal-download');\\n        \\n        closeBtn.addEventListener('click', () => {\\n            fileModal.style.display = 'none';\\n            fileModalOverlay.style.display = 'none';\\n        });\\n        \\n        downloadBtn.addEventListener('click', () => {\\n            // å…³é—­æ–‡ä»¶æ¨¡æ€æ¡†\\n            fileModal.style.display = 'none';\\n            fileModalOverlay.style.display = 'none';\\n            \\n            // æ’­æ”¾ç‚¹å‡»éŸ³æ•ˆï¼ˆè€Œä¸æ˜¯ä¸‹è½½éŸ³æ•ˆï¼‰\\n            const clickSound = document.ge