š„æ ¼å¼è§£æ\\n            const oldFormatRegex = /\\\\[è¯­éŸ³é€šè¯ä¸­\\\\]\\\\s*åå­—ï¼š(.*?)\\\\s*å¤´åƒï¼š(.*?)\\\\s*å†…å¿ƒï¼š(.*?)\\\\s*å†…å®¹ï¼š([\\\\s\\\\S]*)/;\\n            const oldMatch = input.match(oldFormatRegex);\\n            \\n            if (oldMatch) {\\n                console.log('ä½¿ç”¨æ—§æ ¼å¼æˆåŠŸè§£æ');\\n                return {\\n                    name: oldMatch[1].trim(),\\n                    avatar: oldMatch[2].trim(),\\n                    thoughts: oldMatch[3].trim(),\\n                    content: oldMatch[4].trim()\\n                };\\n            }\\n            \\n            console.warn('æ‰€æœ‰è§£æå°è¯•å¤±è´¥ï¼Œè¿”å›é»˜è®¤å€¼');\\n            return {\\n                name: 'æœªçŸ¥',\\n                avatar: '',\\n                thoughts: 'æš‚æ— ',\\n                content: 'æš‚æ— é€šè¯è®°å½•'\\n            };\\n        }\\n        \\n        const content = callMatch[1].trim();\\n        console.log(`æå–åˆ°å†…å®¹é•¿åº¦: ${content.length}å­—ç¬¦`);\\n        \\n        // ä½¿ç”¨æ›´å¥å£®çš„æ­£åˆ™è¡¨è¾¾å¼åŒ¹é…æ ¼å¼ [æ ‡ç­¾|å†…å®¹]\\n        // æ·»åŠ (?=\\\\]) ä½¿æ­£åˆ™è¡¨è¾¾å¼èƒ½å¤Ÿå¤„ç†æ— æ¢è¡Œç¬¦çš„æƒ…å†µ\\n        const nameRegex = /\\\\[åå­—\\\\|(.*?)(?=\\\\])/;\\n        const avatarRegex = /\\\\[å¤´åƒ\\\\|(.*?)(?=\\\\])/;\\n        const thoughtsRegex = /\\\\[å†…å¿ƒ\\\\|(.*?)(?=\\\\])/;\\n        const contentRegex = /\\\\[å†…å®¹\\\\|([\\\\s\\\\S]*?)(?=\\\\]|$)/;\\n        \\n        const nameMatch = content.match(nameRegex);\\n        const avatarMatch = content.match(avatarRegex);\\n        const thoughtsMatch = content.match(thoughtsRegex);\\n        const contentMatch = content.match(contentRegex);\\n        \\n        // è®°å½•è§£æç»“æœ\\n        if (nameMatch) console.log(`æå–åˆ°åå­—: ${nameMatch[1].trim()}`);\\n        if (avatarMatch) console.log(`æå–åˆ°å¤´åƒ: ${avatarMatch[1].trim()}`);\\n        if (thoughtsMatch) console.log(`æå–åˆ°å†…å¿ƒæƒ³æ³•ï¼Œé•¿åº¦: ${thoughtsMatch[1].trim().length}å­—ç¬¦`);\\n        if (contentMatch) console.log(`æå–åˆ°é€šè¯å†…å®¹ï¼Œé•¿åº¦: ${contentMatch[1].trim().length}å­—ç¬¦`);\\n        \\n        // å¦‚æœä¸»è¦æ­£åˆ™è¡¨è¾¾å¼æ²¡æœ‰åŒ¹é…åˆ°å†…å®¹ï¼Œå°è¯•ä½¿ç”¨å¤‡ç”¨æ­£åˆ™è¡¨è¾¾å¼\\n        let name = nameMatch ? nameMatch[1].trim() : '';\\n        let avatar = avatarMatch ? avatarMatch[1].trim() : '';\\n        let thoughts = thoughtsMatch ? thoughtsMatch[1].trim() : '';\\n        let callContent = contentMatch ? contentMatch[1].trim() : '';\\n        \\n        // å¤‡ç”¨è§£ææ–¹æ³•\\n        if (!name || !avatar || !thoughts || !callContent) {\\n            console.log('ä¸»è¦è§£æä¸å®Œæ•´ï¼Œä½¿ç”¨å¤‡ç”¨è§£ææ–¹æ³•...');\\n            \\n            // ä½¿ç”¨ä¼ ç»Ÿçš„æ­£åˆ™è¡¨è¾¾å¼ä½œä¸ºå¤‡é€‰\\n            const backupNameMatch = content.match(/\\\\[åå­—\\\\|(.*?)\\\\]/);\\n            const backupAvatarMatch = content.match(/\\\\[å¤´åƒ\\\\|(.*?)\\\\]/);\\n            const backupThoughtsMatch = content.match(/\\\\[å†…å¿ƒ\\\\|(.*?)\\\\]/);\\n            const backupContentMatch = content.match(/\\\\[å†…å®¹\\\\|([\\\\s\\\\S]*?)\\\\]/);\\n            \\n            if (!name && backupNameMatch) {\\n                name = backupNameMatch[1].trim();\\n                console.log(`å¤‡ç”¨æ–¹æ³•æå–åˆ°åå­—: ${name}`);\\n            }\\n            \\n            if (!avatar && backupAvatarMatch) {\\n                avatar = backupAvatarMatch[1].trim();\\n                console.log(`å¤‡ç”¨æ–¹æ³•æå–åˆ°å¤´åƒ: ${avatar}`);\\n            }\\n            \\n            if (!thoughts && backupThoughtsMatch) {\\n                thoughts = backupThoughtsMatch[1].trim();\\n                console.log(`å¤‡ç”¨æ–¹æ³•æå–åˆ°å†…å¿ƒæƒ³æ³•`);\\n            }\\n            \\n            if (!callContent && backupContentMatch) {\\n                callContent = backupContentMatch[1].trim();\\n                console.log(`å¤‡ç”¨æ–¹æ³•æå–åˆ°é€šè¯å†…å®¹`);\\n            }\\n        }\\n        \\n        // å°è¯•ç¬¬ä¸‰ç§è§£ææ–¹æ³•ï¼šç›´æ¥æŸ¥æ‰¾æ ‡ç­¾å¹¶åˆ†å‰²\\n        if (!name || !avatar || !thoughts || !callContent) {\\n            console.log('å°è¯•ç¬¬ä¸‰ç§è§£ææ–¹æ³•ï¼šç›´æ¥æŸ¥æ‰¾æ ‡ç­¾...');\\n            \\n            if (!name && content.includes('[åå­—|')) {\\n                const startIndex = content.indexOf('[åå­—|') + 4;\\n                const endIndex = content.indexOf(']', startIndex);\\n                if (endIndex > startIndex) {\\n                    name = content.substring(startIndex, endIndex).trim();\\n                    console.log(`ç¬¬ä¸‰ç§æ–¹æ³•æå–åˆ°åå­—: ${name}`);\\n                }\\n            }\\n            \\n            if (!avatar && content.includes('[å¤´åƒ|')) {\\n                const startIndex = content.indexOf('[å¤´åƒ|') + 4;\\n                const endIndex = content.indexOf(']', startIndex);\\n                if (endIndex > startIndex) {\\n                    avatar = content.substring(startIndex, endIndex).trim();\\n                    console.log(`ç¬¬ä¸‰ç§æ–¹æ³•æå–åˆ°å¤´åƒ: ${avatar}`);\\n                }\\n            }\\n            \\n            if (!thoughts && content.includes('[å†…å¿ƒ|')) {\\n                const startIndex = content.indexOf('[å†…å¿ƒ|') + 4;\\n                const endIndex = content.indexOf(']', startIndex);\\n                if (endIndex > startIndex) {\\n                    thoughts = content.substring(startIndex, endIndex).trim();\\n                    console.log(`ç¬¬ä¸‰ç§æ–¹æ³•æå–åˆ°å†…å¿ƒæƒ³æ³•`);\\n                }\\n            }\\n            \\n            if (!callContent && content.includes('[å†…å®¹|')) {\\n                const startIndex = content.indexOf('[å†…å®¹|') + 4;\\n                const endIndex = content.indexOf(']', startIndex);\\n                if (endIndex > startIndex) {\\n                    callContent = content.substring(startIndex, endIndex).trim();\\n                    console.log(`ç¬¬ä¸‰ç§æ–¹æ³•æå–åˆ°é€šè¯å†…å®¹`);\\n                }\\n            }\\n        }\\n        \\n        // ç¡®ä¿æ‰€æœ‰å¿…è¦å­—æ®µéƒ½æœ‰å€¼\\n        name = name || 'æœªçŸ¥';\\n        avatar = avatar || '';\\n        thoughts = thoughts || 'æš‚æ— ';\\n        callContent = callContent || 'æš‚æ— é€šè¯è®°å½•';\\n        \\n        console.log('é€šè¯æ•°æ®è§£æå®Œæˆ');\\n        return {\\n            name: name,\\n            avatar: avatar,\\n            thoughts: thoughts,\\n            content: callContent\\n        };\\n    } catch (error) {\\n        console.error('è§£æé€šè¯æ•°æ®æ—¶å‡ºé”™:', error);\\n        return {\\n            name: 'è§£æé”™è¯¯',\\n            avatar: '',\\n            thoughts: 'è§£ææ•°æ®æ—¶å‘ç”Ÿé”™è¯¯',\\n            content: 'æš‚æ— é€šè¯è®°å½•'\\n        };\\n    }\\n}\\n\\n// æ›´æ–°é€šè¯æ˜¾ç¤ºçš„å‡½æ•°\\nfunction updateCallDisplay(callInfo) {\\n    // æ›´æ–°å¤´åƒ\\n    const avatarElement = document.getElementById('caller-avatar');\\n    if (avatarElement && callInfo.avatar) {\\n        // å¤„ç†å¤´åƒè·¯å¾„ï¼Œæ”¯æŒæ··åˆä¸­è‹±æ–‡æ–‡ä»¶å\\n        let avatarPath = '';\\n        if (callInfo.avatar.startsWith('http')) {\\n            avatarPath = callInfo.avatar;\\n        } else {\\n            // æå–æ–‡ä»¶åä¸­çš„è‹±æ–‡éƒ¨åˆ†ï¼ˆé€šå¸¸æ˜¯æœ€åçš„IDå’Œæ‰©å±•åï¼‰\\n            // åŒ¹é…æ¨¡å¼ï¼šéhttpå¼€å¤´çš„æ–‡ä»¶åï¼Œæå–æœ«å°¾çš„è‹±æ–‡å­—æ¯/æ•°å­—å’Œæ‰©å±•åéƒ¨åˆ†\\n            const filenameParts = callInfo.avatar.match(/([a-zA-Z0-9]+\\\\.[a-zA-Z]+)$/);\\n            if (filenameParts && filenameParts[1]) {\\n                // æ‰¾åˆ°ç¬¦åˆæ¡ä»¶çš„è‹±æ–‡éƒ¨åˆ†\\n                avatarPath = `https://files.catbox.moe/${filenameParts[1]}`;\\n                console.log(`æå–çš„å¤´åƒæ–‡ä»¶å: ${filenameParts[1]}`);\\n            } else {\\n                // æœªæ‰¾åˆ°ç¬¦åˆæ¡ä»¶çš„éƒ¨åˆ†ï¼Œä½¿ç”¨æ•´ä¸ªæ–‡ä»¶å\\n                avatarPath = `https://files.catbox.moe/${callInfo.avatar}`;\\n                console.log(`ä½¿ç”¨å®Œæ•´å¤´åƒæ–‡ä»¶å: ${callInfo.avatar}`);\\n            }\\n        }\\n        avatarElement.src = avatarPath;\\n    }\\n    \\n    // æ›´æ–°åç§°\\n    const nameElement = docum