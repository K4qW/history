ˆå¯¹è®ºå›å¸–å­ç‰¹å®šç»“æ„\\n                const bracketRegex = /\\\\[(æ ‡é¢˜|å‘å¸ƒè€…|æµè§ˆ|è¯„è®º|å†…å®¹|å¸–å­è¯„è®º)[^\\\\[\\\\]]*?\\\\]/g;\\n                let bracketMatch;\\n                \\n                while ((bracketMatch = bracketRegex.exec(content)) !== null) {\\n                    brackets.push(bracketMatch[0]);\\n                }\\n                \\n                console.log(`æå–åˆ° ${brackets.length} ä¸ªæ–¹æ‹¬å·å†…å®¹`);\\n                \\n                // å¤„ç†æ¯ä¸ªæ–¹æ‹¬å·å†…å®¹\\n                for (const bracket of brackets) {\\n                    try {\\n                        // æå–æ ‡ç­¾å’Œå€¼\\n                        const bracketContent = bracket.substring(1, bracket.length - 1);\\n                        const separatorIndex = bracketContent.indexOf('|');\\n                        \\n                        if (separatorIndex !== -1) {\\n                            const itemType = bracketContent.substring(0, separatorIndex).trim();\\n                            const itemContent = bracketContent.substring(separatorIndex + 1).trim();\\n                            \\n                            // æ ¹æ®é¡¹ç›®ç±»å‹å¤„ç†\\n                            if (itemType === 'æ ‡é¢˜' || itemType === 'å‘å¸ƒè€…' || itemType === 'æµè§ˆ' || \\n                                itemType === 'è¯„è®º' || itemType === 'å†…å®¹') {\\n                                result.å¸–å­ä¿¡æ¯[itemType] = itemContent;\\n                                console.log(`æå–åˆ°å¸–å­ä¿¡æ¯: ${itemType} = ${itemContent.substring(0, 30)}${itemContent.length > 30 ? '...' : ''}`);\\n                            } else if (itemType === 'å¸–å­è¯„è®º') {\\n                                const commentParts = itemContent.split('|');\\n                                \\n                                if (commentParts.length >= 3) {\\n                                    const comment = {\\n                                        ç”¨æˆ·å: commentParts[0].trim(),\\n                                        æ—¶é—´: commentParts[1].trim(),\\n                                        å†…å®¹: commentParts[2].trim(),\\n                                        æ¥¼ä¸­æ¥¼: []\\n                                    };\\n                                    \\n                                    // æå–æ¥¼ä¸­æ¥¼å›å¤ï¼ˆå¦‚æœæœ‰çš„è¯ï¼‰\\n                                    if (commentParts.length > 3) {\\n                                        for (let i = 3; i < commentParts.length; i += 2) {\\n                                            if (i + 1 < commentParts.length) {\\n                                                comment.æ¥¼ä¸­æ¥¼.push({\\n                                                    ç”¨æˆ·å: commentParts[i].trim(),\\n                                                    å†…å®¹: commentParts[i + 1].trim()\\n                                                });\\n                                                console.log(`æå–åˆ°æ¥¼ä¸­æ¥¼å›å¤: ${commentParts[i].trim()} -> ${commentParts[i + 1].substring(0, 20)}${commentParts[i + 1].length > 20 ? '...' : ''}`);\\n                                            }\\n                                        }\\n                                    }\\n                                    \\n                                    result.è¯„è®ºåˆ—è¡¨.push(comment);\\n                                    console.log(`æå–åˆ°è¯„è®º: ${comment.ç”¨æˆ·å} äº ${comment.æ—¶é—´}, å†…å®¹: ${comment.å†…å®¹.substring(0, 20)}${comment.å†…å®¹.length > 20 ? '...' : ''}`);\\n                                } else {\\n                                    console.warn(`è¯„è®ºæ ¼å¼ä¸æ­£ç¡®æˆ–å‚æ•°ä¸è¶³: ${bracket}`);\\n                                }\\n                            } else {\\n                                console.warn(`æœªçŸ¥çš„é¡¹ç›®ç±»å‹: ${itemType}`);\\n                            }\\n                        } else {\\n                            console.warn(`æ— æ³•è§£æçš„æ–¹æ‹¬å·å†…å®¹: ${bracket}`);\\n                        }\\n                    } catch (bracketError) {\\n                        console.error(`å¤„ç†æ–¹æ‹¬å·å†…å®¹å‡ºé”™:`, bracketError);\\n                    }\\n                }\\n                \\n                console.log(`è§£æç»“æœ: ${Object.keys(result.å¸–å­ä¿¡æ¯).length} ä¸ªå¸–å­ä¿¡æ¯å­—æ®µ, ${result.è¯„è®ºåˆ—è¡¨.length} æ¡è¯„è®º`);\\n                return result;\\n            } catch (error) {\\n                console.error('è§£æè®ºå›å¸–å­å†…å®¹æ—¶å‡ºé”™:', error);\\n                return {\\n                    å¸–å­ä¿¡æ¯: {\\n                        æ ‡é¢˜: 'è§£æå‡ºé”™',\\n                        å‘å¸ƒè€…: 'ç³»ç»Ÿ',\\n                        æµè§ˆ: '0',\\n                        è¯„è®º: '0',\\n                        å†…å®¹: 'å¸–å­å†…å®¹è§£æå¤±è´¥ï¼Œè¯·æ£€æŸ¥æ ¼å¼ã€‚'\\n                    },\\n                    è¯„è®ºåˆ—è¡¨: []\\n                };\\n            }\\n        }\\n\\n        // æ›´æ–°å¸–å­æ˜¾ç¤ºçš„å‡½æ•°\\n        function updatePostDisplay(postInfo) {\\n            if (!postInfo) {\\n                console.error('å¸–å­ä¿¡æ¯ä¸ºç©ºï¼Œæ— æ³•æ›´æ–°æ˜¾ç¤º');\\n                // æ˜¾ç¤ºé”™è¯¯ä¿¡æ¯\\n                document.getElementById('post-title').textContent = 'å†…å®¹åŠ è½½å¤±è´¥';\\n                document.getElementById('author-name').textContent = 'ç³»ç»Ÿ';\\n                document.getElementById('post-views').textContent = '0';\\n                document.getElementById('post-comments').textContent = '0';\\n                document.getElementById('post-content').textContent = 'æ— æ³•åŠ è½½å¸–å­å†…å®¹ï¼Œè¯·ç¨åå†è¯•ã€‚';\\n                return;\\n            }\\n            \\n            // æ›´æ–°å¸–å­æ ‡é¢˜å’ŒåŸºæœ¬ä¿¡æ¯\\n            document.getElementById('post-title').textContent = postInfo.å¸–å­ä¿¡æ¯.æ ‡é¢˜ || 'æ— æ ‡é¢˜';\\n            document.getElementById('author-name').textContent = postInfo.å¸–å­ä¿¡æ¯.å‘å¸ƒè€… || 'åŒ¿å';\\n            document.getElementById('post-views').textContent = postInf