\\n            <div class=\\\"category\\\">é˜…è¯»</div>\\n            <div class=\\\"category\\\">ç”µå½±</div>\\n            <div class=\\\"category\\\">éŸ³ä¹</div>\\n        </div>\\n\\n        <!-- ç€‘å¸ƒæµå¸ƒå±€ -->\\n        <div class=\\\"waterfall\\\" id=\\\"waterfall\\\">\\n            <!-- åˆ—å®¹å™¨å°†ç”±JavaScriptåŠ¨æ€ç”Ÿæˆ -->\\n        </div>\\n    </div>\\n\\n    <!-- åº•éƒ¨å¯¼èˆªæ  -->\\n    <div class=\\\"bottom-navbar\\\">\\n        <a href=\\\"#\\\" class=\\\"nav-item active\\\" id=\\\"nav-home\\\">\\n            <i class=\\\"fas fa-home\\\"></i>\\n            <span>é¦–é¡µ</span>\\n        </a>\\n        <a href=\\\"#\\\" class=\\\"nav-item\\\" id=\\\"nav-discovery\\\">\\n            <i class=\\\"fas fa-compass\\\"></i>\\n            <span>å‘ç°</span>\\n        </a>\\n        <div class=\\\"publish-btn-container\\\">\\n            <div class=\\\"publish-btn\\\" id=\\\"nav-create\\\">\\n                <i class=\\\"fas fa-plus\\\"></i>\\n            </div>\\n        </div>\\n        <a href=\\\"#\\\" class=\\\"nav-item\\\" id=\\\"nav-message\\\">\\n            <i class=\\\"fas fa-bell\\\"></i>\\n            <span>æ¶ˆæ¯</span>\\n        </a>\\n        <a href=\\\"#\\\" class=\\\"nav-item\\\">\\n            <i class=\\\"fas fa-user\\\"></i>\\n            <span>æˆ‘çš„</span>\\n        </a>\\n    </div>\\n\\n    <!-- å›é¡¶æŒ‰é’® -->\\n    <div class=\\\"back-to-top\\\" id=\\\"backToTop\\\">\\n        <i class=\\\"fas fa-arrow-up\\\"></i>\\n    </div>\\n\\n    <script>\\n        // 1. åŸºç¡€æ¨¡æ¿å®šä¹‰\\n        let gameText = `\\n<home>$1</home>`;\\n\\n        // 2. æ•°æ®æå–å™¨\\n        const DataExtractor = {\\n            // å¤„ç†å¤´åƒURLï¼Œæå–è‹±æ–‡éƒ¨åˆ†å¹¶æ·»åŠ å‰ç¼€\\n            processAvatarUrl(avatar) {\\n                // å¦‚æœå¤´åƒæ ¼å¼åŒ…å«ä¸­æ–‡ï¼Œåˆ™æå–è‹±æ–‡éƒ¨åˆ†\\n                if (/[\\\\u4e00-\\\\u9fa5]/.test(avatar)) {\\n                    const match = avatar.match(/([a-zA-Z0-9]+\\\\.[a-zA-Z]+)$/);\\n                    if (match) {\\n                        return `https://files.catbox.moe/${match[1]}`;\\n                    }\\n                }\\n                return `https://files.catbox.moe/${avatar}`;\\n            },\\n            extractHomeData(text) {\\n                try {\\n                    // åŒ¹é… <home> æ ‡ç­¾å†…çš„å†…å®¹\\n                    const homeMatch = text.match(/<home>([\\\\s\\\\S]*?)<\\\\/home>/);\\n                    if (!homeMatch) return null;\\n                    \\n                    const homeContent = homeMatch[1].trim();\\n                    \\n                    // æå–æ—¶é—´ä¿¡æ¯\\n                    const timeMatch = homeContent.match(/\\\\[æ—¶é—´\\\\|(.*?)\\\\|(.*?)\\\\]/);\\n                    const date = timeMatch ? timeMatch[1].trim() : 'æœªçŸ¥æ—¥æœŸ';\\n                    const time = timeMatch ? timeMatch[2].trim() : 'æœªçŸ¥æ—¶é—´';\\n                    \\n                    // æå–æ¨èå†…å®¹\\n                    const recommendMatch = homeContent.match(/<recommend>([\\\\s\\\\S]*?)<\\\\/recommend>/);\\n                    const recommendContent = recommendMatch ? recommendMatch[1].trim() : '';\\n                    \\n                    // æå–å…³æ³¨å†…å®¹\\n                    const followMatch = homeContent.match(/<follow>([\\\\s\\\\S]*?)<\\\\/follow>/);\\n                    const followContent = followMatch ? followMatch[1].trim() : '';\\n                    \\n                    // è§£ææ¨èå¸–å­\\n                    const recommendPosts = this.extractPosts(recommendContent);\\n                    \\n                    // è§£æå…³æ³¨å¸–å­\\n                    const followPosts = this.extractPosts(followContent);\\n                    \\n                    return {\\n                        date,\\n                        time,\\n                        recommendPosts,\\n                        followPosts\\n                    };\\n                } catch (error) {\\n                    console.error('è§£æé¦–é¡µæ•°æ®å¤±è´¥:', error);\\n                    return {\\n                        date: 'æœªçŸ¥æ—¥æœŸ',\\n                        time: 'æœªçŸ¥æ—¶é—´',\\n                        recommendPosts: [],\\n                        followPosts: []\\n                    };\\n                }\\n            },\\n            \\n            extractPosts(content) {\\n                if (!content) return [];\\n                \\n                const postRegex = /\\\\[å¸–å­\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\]/g;\\n                const posts = [];\\n                let match;\\n                \\n                while ((match = postRegex.exec(content)) !== null) {\\n                    posts.push({\\n                        username: match[1] || 'æœªçŸ¥ç”¨æˆ·',\\n                        avatar: this.processAvatarUrl(match[2]) || 'default',\\n                        title: match[3] || 'æ— æ ‡é¢˜',\\n                        description: match[4] || 'æ— å†…å®¹',\\n                        imageSummary: match[5] || 'æ— å›¾ç‰‡æè¿°',\\n                        likes: parseInt(match[6]) || 0,\\n                        comments: parseInt(match[7]) || 0,\\n                        tag: match[8] || 'æœªåˆ†ç±»'\\n                    });\\n                }\\n                \\n                return posts;\\n            },\\n            \\n            parseGameText(gameText) {\\n                // ç›´æ¥è§£æé¦–é¡µæ•°æ®\\n                return this.extractHomeData(gameText);\\n            }\\n        };\\n\\n        // 3. UIæ›´æ–°å™¨\\n        const UIUpdater = {\\n            // æ›´æ–°é¡µé¢æ ‡é¢˜å’Œæ—¶é—´\\n            updatePageHeader(data) {\\n                if (!data) return;\\n                \\n                // è®¾ç½®é¡µé¢æ ‡é¢˜\\n                document.title = `å°çº¢ä¹¦ - ${data.date} ${data.time}`;\\n                \\n                // å¯ä»¥åœ¨é¡¶éƒ¨å¯¼èˆªæ æ·»åŠ æ—¶é—´æ˜¾ç¤º\\n                const navTimeElem = document.createElement('div');\\n                navTimeElem.className = 'nav-time';\\n                navTimeElem.style.cssText = 'font-size: 12px; color: #666; position: absolute; right: 10px; top: 10px;';\\n                navTimeElem.textContent = `${data.date} ${data.time}`;\\n                document.querySelector('.navbar').appendChild(navTimeElem);\\n            },\\n            \\n            // æ›´æ–°æ¨èé¡µé¢\\n            updateRecommendContent(recommendPosts) {\\n                if (!recommendPosts || !recommendPosts.length) {\\n                    console.warn('æ²¡æœ‰æ¨èå†…å®¹å¯æ˜¾ç¤º');\\n                    return;\\n                }\\n                \\n                // ä¿å­˜æ•°æ®åˆ°å…¨å±€ä»¥ä¾¿åˆ‡æ¢æ—¶ä½¿ç”¨\\n                window.currentRecommendPosts = recommendPosts;\\n                \\n                // å¦‚æœå½“å‰æ˜¯æ¨èé¡µé¢ï¼Œç›´æ¥æ›´æ–°å†…å®¹\\n                if (currentTab === \\\"recommend\\\") {\\n                    initWaterfall();\\n                }\\n                \\n                // æ ¹æ®æ ‡ç­¾æ›´æ–°åˆ†ç±»èœå•\\n                this.updateCategories(recommendPosts);\\n            },\\n            \\n            // æ›´æ–°å…³æ³¨é¡µé¢\\n            updateFollowContent(followPosts) {\\n                if (!followPosts || !followPosts.length) {\\n                    console.warn('æ²¡æœ‰å…³æ³¨å†…å®¹å¯æ˜¾ç¤º');\\n                    return;\\n                }\\n                \\n                // ä¿å­˜æ•°æ®åˆ°å…¨å±€ä»¥ä¾¿åˆ‡æ¢æ—¶ä½¿ç”¨\\n                window.currentFollowPosts = followPosts;\\n                \\n                // å¦‚æœå½“å‰æ˜¯å…³æ³¨é¡µé¢ï¼Œç›´æ¥æ›´æ–°å†…å®¹\\n                if (currentTab === \\\"follow\\\") {\\n                    initWaterfall();\\n                }\\n            },\\n            \\n            // æ ¹æ®å¸–å­æ ‡ç­¾æ›´æ–°åˆ†ç±»èœå•\\n            updateCategories(posts) {\\n                if (!posts || !posts.length) return;\\n                \\n                // æå–æ‰€æœ‰æ ‡ç­¾\\n                const tags = ['æ¨è'];\\n                posts.forEach(post => {\\n                    if (post.tag && !tags.includes(post.tag)) {\\n                        tags.push(post.tag);\\n                    }\\n                });\\n                \\n                // æ›´æ–°åˆ†ç±»èœå•\\n                const categoriesContainer = document.querySelector('.categories');\\n                categoriesContainer.innerHTML = '';\\n                \\n                tags.forEach((tag, index) => {\\n                    const categoryElem = document.createElement('div');\\n                    categoryElem.className = 'category' + (index === 0 ? ' active' : '');\\n                    categoryElem.textContent = tag;\\n                    categoryElem.addEventListener('click', function() {\\n                        // ç§»é™¤æ‰€æœ‰activeç±»\\n                        document.querySelectorAll('.category').forEach(c => {\\n                            c.classList.remove('active');\\n                        });\\n                        // ä¸ºå½“å‰ç‚¹å‡»çš„æ ‡ç­¾æ·»åŠ activeç±»\\n                        this.classList.add('active');\\n                        \\n                        // æ ¹æ®æ ‡ç­¾ç­›é€‰å†…å®¹\\n                        if (tag === 'æ¨è') {\\n                            // æ˜¾ç¤ºå…¨éƒ¨å†…å®¹\\n                            initWaterfall();\\n                        } else {\\n                            // ç­›é€‰ç‰¹å®šæ ‡ç­¾çš„å†…å®¹\\n                            const filteredPosts = currentTab === \\\"recommend\\\" \\n                                ? window.currentRecommendPosts.filter(post => post.tag === tag)\\n                                : window.currentFollowPosts.filter(post => post.tag === tag);\\n                            \\n                            displayFilteredPosts(filteredPosts);\\n                        }\\n                    });\\n                    categoriesContainer.appendChild(categoryElem);\\n                });\\n            },\\n            \\n            // æ›´æ–°æ‰€æœ‰UI\\n            updateAllUI(data) {\\n                if (!data) {\\n                    console.error('æ— æ•ˆçš„æ•°æ®ï¼Œæ— æ³•æ›´æ–°UI');\\n                    return;\\n                }\\n                \\n                this.updatePageHeader(data);\\n                this.updateRecommendContent(data.recommendPosts);\\n                this.updateFollowContent(data.followPosts);\\n            },\\n            \\n            // åˆå§‹åŒ–UI\\n            initializeUI() {\\n                try {\\n                    // è§£ææ¸¸æˆæ–‡æœ¬\\n                    const parsedData = DataExtractor.parseGameText(gameText);\\n                    \\n                    // æ›´æ–°UI\\n                    if (parsedData) {\\n                        this.updateAllUI(parsedData);\\n                    } else {\\n                        console.error('æ— æ³•åˆå§‹åŒ–UIï¼Œæ•°æ®è§£æå¤±è´¥');\\n                    }\\n                } catch (error) {\\n                    console.error('UIåˆå§‹åŒ–å¤±è´¥:', error);\\n                }\\n            }\\n        };\\n\\n        // è·Ÿè¸ªå½“å‰æ˜¾ç¤ºçš„å†…å®¹ç±»å‹\\n        let currentTab = \\\"recommend\\\";\\n        \\n        // å…¨å±€å˜é‡å­˜å‚¨å¸–å­æ•°æ®\\n        window.currentRecommendPosts = [];\\n        window.currentFollowPosts = [];\\n\\n        // åˆå§‹åŒ–ç€‘å¸ƒæµ\\n        function initWaterfall() {\\n            const waterfall = document.getElementById(\\\"waterfall\\\");\\n            waterfall.innerHTML = \\\"\\\"; // æ¸…ç©ºå®¹å™¨\\n\\n            // æ ¹æ®å±å¹•å®½åº¦å†³å®šåˆ—æ•°\\n            let columnCount;\\n            if (window.innerWidth >= 1200) {\\n                columnCount = 4;\\n            } else if (window.innerWidth >= 992) {\\n                columnCount = 3;\\n            } else if (window.innerWidth >= 576) {\\n                columnCount = 2;\\n            } else {\\n                columnCount = 1;\\n            }\\n\\n            // åˆ›å»ºåˆ—å®¹å™¨\\n            for (let i = 0; i < columnCount; i++) {\\n                const column = document.createElement(\\\"div\\\");\\n                column.className = \\\"waterfall-column\\\";\\n                waterfall.appendChild(column);\\n            }\\n\\n            // æ ¹æ®å½“å‰é€‰ä¸­çš„æ ‡ç­¾æ˜¾ç¤ºå¯¹åº”å†…å®¹\\n            if (currentTab === \\\"recommend\\\") {\\n                displayPosts(window.currentRecommendPosts);\\n            } else if (currentTab === \\\"follow\\\") {\\n                displayPosts(window.currentFollowPosts);\\n            }\\n        }\\n\\n        // æ˜¾ç¤ºç­›é€‰åçš„å¸–å­\\n        function displayFilteredPosts(filteredPosts) {\\n            const waterfall = document.getElementById(\\\"waterfall\\\");\\n            waterfall.innerHTML = \\\"\\\"; // æ¸…ç©ºå®¹å™¨\\n\\n            // æ ¹æ®å±å¹•å®½åº¦å†³å®šåˆ—æ•°\\n            let columnCount;\\n            if (window.innerWidth >= 1200) {\\n                columnCount = 4;\\n            } else if (window.innerWidth >= 992) {\\n                columnCount = 3;\\n            } else if (window.innerWidth >= 576) {\\n                columnCount = 2;\\n            } else {\\n                columnCount = 1;\\n            }\\n\\n            // åˆ›å»ºåˆ—å®¹å™¨\\n            for (let i = 0; i < columnCount; i++) {\\n                const column = document.createElement(\\\"div\\\");\\n                column.className = \\\"waterfall-column\\\";\\n                waterfall.appendChild(column);\\n            }\\n\\n            // æ˜¾ç¤ºç­›é€‰çš„å¸–å­\\n            displayPosts(filteredPosts);\\n        }\\n\\n        // æ˜¾ç¤ºå¸–å­\\n        function displayPosts(postsToDisplay) {\\n            if (!postsToDisplay || postsToDisplay.length === 0) {\\n                const waterfall = document.getElementById(\\\"waterfall\\\");\\n                const emptyMessage = document.createElement('div');\\n                emptyMessage.style.cssText = 'text-align: center; padding: 50px 0; color: #999; width: 100%;';\\n                emptyMessage.textContent = 'æš‚æ— å†…å®¹';\\n                waterfall.appendChild(emptyMessage);\\n                return;\\n            }\\n            \\n            const columns = document.querySelectorAll(\\\".waterfall-column\\\");\\n            const columnCount = columns.length;\\n\\n            // æ‰¾å‡ºé«˜åº¦æœ€å°çš„åˆ—\\n            function getMinHeightColumn() {\\n                let minHeight = Infinity;\\n                let minIndex = 0;\\n\\n                columns.forEach((column, index) => {\\n                    const height = column.offsetHeight;\\n                    if (height < minHeight) {\\n                        minHeight = height;\\n                        minIndex = index;\\n                    }\\n                });\\n\\n                return columns[minIndex];\\n            }\\n\\n            // å°†å¸–å­æ·»åŠ åˆ°é«˜åº¦æœ€å°çš„åˆ—\\n            postsToDisplay.forEach(post => {\\n                const column = getMinHeightColumn();\\n                \\n                const postCard = document.createElement(\\\"div\\\");\\n                postCard.className = \\\"post-card\\\";\\n                \\n                // åˆ›å»ºå¸–å­æ ‡ç­¾å¾½ç« \\n                const tagBadge = post.tag ? `<span class=\\\"tag-badge\\\">${post.tag}</span>` : '';\\n                \\n                postCard.innerHTML = `\\n                    <div class=\\\"image-container\\\" onclick=\\\"this.classList.toggle('active-mask')\\\">\\n                        <div class=\\\"image-mask\\\">\\n                            <span>ç‚¹å‡»æŸ¥çœ‹å†…å®¹</span>\\n                        </div>\\n                        <div class=\\\"image-summary\\\">\\n                            <h4>å›¾ç‰‡ç®€ä»‹</h4>\\n                            <p>${post.imageSummary.replace('å›¾ç‰‡ç®€ä»‹ï¼š', '')}</p>\\n                        </div>\\n                        ${tagBadge}\\n                    </div>\\n                    <div class=\\\"post-content\\\">\\n                        <h3 class=\\\"