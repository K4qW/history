/ æ›´æ–°è¯„è®ºè®¡æ•°\\n                const commentsCount = document.querySelector('.comments-count');\\n                if (commentsCount) {\\n                    // ä½¿ç”¨ä»å¤´éƒ¨ä¿¡æ¯ä¸­è·å–çš„è¯„è®ºè®¡æ•°ï¼Œè€Œä¸æ˜¯ä»å½“å‰å‚æ•°è®¡ç®—\\n                    const statsItems = document.querySelectorAll('.post-stats-actions .stat-item');\\n                    if (statsItems.length >= 3) {\\n                        commentsCount.textContent = statsItems[2].querySelector('span').textContent;\\n                    } else {\\n                        // å›é€€åˆ°è®¡ç®—å½“å‰å¯è§è¯„è®ºæ•°é‡\\n                        commentsCount.textContent = latestComments.length + hotComments.length;\\n                    }\\n                }\\n                \\n                // é‡æ–°ç»‘å®šæ¥¼ä¸­æ¥¼è¯„è®ºå±•å¼€/æ”¶èµ·åŠŸèƒ½\\n                this.bindCommentEvents();\\n            },\\n            \\n            // æ›´æ–°ç›¸å…³æ¨è\\n            updateRelatedPosts(data) {\\n                if (!data || !data.relatedPosts || data.relatedPosts.length === 0) {\\n                    console.warn('æ²¡æœ‰ç›¸å…³å¸–å­æ•°æ®å¯æ˜¾ç¤º');\\n                    return;\\n                }\\n                \\n                const relatedContainer = document.querySelector('.related-list');\\n                if (!relatedContainer) return;\\n                \\n                // æ¸…ç©ºç°æœ‰å†…å®¹\\n                relatedContainer.innerHTML = '';\\n                \\n                // æ·»åŠ ç›¸å…³å¸–å­\\n                data.relatedPosts.forEach(post => {\\n                    const postItem = document.createElement('div');\\n                    postItem.className = 'related-item';\\n                    \\n                    postItem.innerHTML = `\\n                        <div class=\\\"related-info\\\">\\n                            <div class=\\\"related-post-title\\\">${post.title}</div>\\n                            <div class=\\\"related-author\\\">${post.author}</div>\\n                        </div>\\n                    `;\\n                    \\n                    // æ·»åŠ ç‚¹å‡»äº‹ä»¶\\n                    postItem.addEventListener('click', function() {\\n                        viewRelatedPost(post.author, post.title);\\n                    });\\n                    \\n                    relatedContainer.appendChild(postItem);\\n                });\\n            },\\n            \\n            // ç»‘å®šè¯„è®ºåŒºäº‹ä»¶\\n            bindCommentEvents() {\\n                // ç»‘å®šæ¥¼ä¸­æ¥¼è¯„è®ºå±•å¼€/æ”¶èµ·åŠŸèƒ½\\n                document.querySelectorAll('.comment-reply-count').forEach(btn => {\\n                    btn.addEventListener('click', function() {\\n                        const nestedComments = this.closest('.comment-content').querySelector('.nested-comments');\\n                        if (nestedComments) {\\n                            // è·å–è®¡ç®—åçš„æ ·å¼\\n                            const computedStyle = window.getComputedStyle(nestedComments);\\n                            const isVisible = computedStyle.display !== 'none';\\n                            \\n                            // åˆ‡æ¢æ˜¾ç¤ºçŠ¶æ€\\n                            if (isVisible) {\\n                                nestedComments.style.display = 'none';\\n                                // æ›¿æ¢æ–‡æœ¬ä¸­çš„\\\"æ”¶èµ·\\\"ä¸º\\\"å±•å¼€\\\"\\n                                const count = this.textContent.match(/\\\\d+/)[0];\\n                                this.textContent = `å±•å¼€${count}æ¡å›å¤`;\\n                            } else {\\n                                nestedComments.style.display = 'block';\\n                                // æ›¿æ¢æ–‡æœ¬ä¸­çš„\\\"å±•å¼€\\\"ä¸º\\\"æ”¶èµ·\\\"\\n                                const count = this.textContent.match(/\\\\d+/)[0];\\n                                this.textContent = `æ”¶èµ·${count}æ¡å›å¤`;\\n                            }\\n                        }\\n                    });\\n                });\\n                \\n                // ç»‘å®šè¯„è®ºå›å¤æŒ‰é’®ç‚¹å‡»äº‹ä»¶\\n                document.querySelectorAll('.comment-reply').forEach(replyBtn => {\\n                    replyBtn.addEventListener('click', function() {\\n                        const commentItem = this.closest('.comment-item');\\n                        const commentAuthor = commentItem.querySelector('.comment-author').textContent;\\n                        const commentText = commentItem.querySelector('.comment-text').textContent;\\n                        \\n                        // æ‰“å¼€å›å¤å¼¹çª—\\n                        openReplyDialog(commentAuthor, commentText);\\n                    });\\n                });\\n                \\n                // ç»‘å®šæ¥¼ä¸­æ¥¼è¯„è®ºå›å¤æŒ‰é’®ç‚¹å‡»äº‹ä»¶\\n                document.querySelectorAll('.nested-comment-reply').forEach(replyBtn => {\\n                    replyBtn.addEventListener('click', function() {\\n                        const nestedCommentItem = this.closest('.nested-comment-item');\\n                        const commentAuthor = nestedCommentItem.querySelector('.nested-comment-author').textContent;\\n                        const commentText = nestedCommentItem.querySelector('.nested-comment-text').textContent;\\n                        \\n                        // æ‰“å¼€å›å¤å¼¹çª—\\n                        openReplyDialog(commentAuthor, commentText);\\n                    });\\n                });\\n                \\n                // ç»‘å®šè¯„è®ºç‚¹èµåŠŸèƒ½\\n                document.querySelectorAll('.comment-like .far.fa-heart, .nested-comment-like .far.fa-heart').forEach(heart => {\\n                    heart.addEventListener('click', function() {\\n                        this.classList.toggle('fas');\\n                        this.classList.toggle('far');\\n                        const countSpan = this.nextElementSibling;\\n                        let count = parseInt(countSpan.textContent);\\n                        \\n                        if (this.classList.contains('fas')) {\\n                            count++;\\n                        } else {\\n                            count--;\\n                        }\\n                        \\n                        countSpan.textContent = count;\\n                    });\\n                });\\n            },\\n            \\n            // æ›´æ–°æ‰€æœ‰UI\\n            updateAllUI(data) {\\n                if (!data) {\\n                    console.error('æ— æ•ˆçš„æ•°æ®ï¼Œæ— æ³•æ›´æ–°UI');\\n                    return;\\n                }\\n                \\n                // æ›´æ–°å¸–å­å¤´éƒ¨ä¿¡æ¯\\n                this.updatePostHeader(data.postHeader);\\n                \\n                // æ›´æ–°è¯„è®ºåŒº\\n                this.updateComments(data.latestComments, data.hotComments);\\n                \\n                // æ›´æ–°ç›¸å…³æ¨è\\n                this.updateRelatedPosts(data);\\n                \\n                // é‡æ–°ç»‘å®šæ‰€æœ‰äº¤äº’äº‹ä»¶\\n                this.bindEvents();\\n            },\\n            \\n            // ç»‘å®šæ‰€æœ‰äº¤äº’äº‹ä»¶\\n            bindEvents() {\\n                // ç»‘å®šè¯„è®ºåŒºäº‹ä»¶\\n                this.bindCommentEvents();\\n                \\n                // ç»‘å®šç‚¹èµåŠŸèƒ½\\n                document.querySelectorAll('.post-stats-actions .far.fa-heart').forEach(heart => {\\n                    heart.addEventListener('click', function() {\\n                        this.classList.toggle('fas');\\n                        this.classList.toggle('far');\\n                        const countSpan = this.nextElementSibling;\\n                        let count = parseInt(countSpan.textContent);\\n                        \\n                        if (this.classList.contains('fas')) {\\n                            count++;\\n                        } else {\\n                            count--;\\n                        }\\n                        \\n                        countSpan.textContent = count;\\n                    });\\n                });\\n                \\n                // ç»‘å®šæ”¶è—åŠŸèƒ½\\n                document.querySelectorAll('.post-stats-actions .far.fa-star, .post-stats-actions .fas.fa-star').forEach(star => {\\n                    star.addEventListener('click', async function() {\\n                        // åˆ‡æ¢å›¾æ ‡æ ·å¼\\n                        this.classList.toggle('fas');\\n                        this.classList.toggle('far');\\n                        const countSpan = this.nextElementSibling;\\n                        let count = parseInt(countSpan.textContent);\\n                        \\n                        // æ›´æ–°æ˜¾ç¤ºçš„æ”¶è—æ•°\\n                        if (this.classList.contains('fas')) {\\n                            count++;\\n                            // æ”¶è—å¸–å­ï¼Œä¿å­˜åˆ°èŠå¤©å˜é‡\\n                            await savePostToFavorites();\\n                        } else {\\n                            count--;\\n                            // å–æ¶ˆæ”¶è—ï¼Œä»èŠå¤©å˜é‡ä¸­åˆ é™¤\\n                            await removePostFromFavorites();\\n                        }\\n                        \\n                        countSpan.textContent = count;\\n                    });\\n                });\\n                \\n                // ç»‘å®šè¯„è®ºå‘é€åŠŸèƒ½\\n                const commentInput = document.querySelector('.comment-input');\\n                const commentSendBtn = document.querySelector('.comment-send-btn');\\n                \\n                if (commentInput && commentSendBtn) {\\n                    // ç‚¹å‡»å‘é€æŒ‰é’®å‘é€è¯„è®º\\n                    commentSendBtn.addEventListener('click', function() {\\n                        sendComment();\\n                    });\\n                    \\n                    // åœ¨è¾“å…¥æ¡†ä¸­æŒ‰å›è½¦ä¹Ÿå¯ä»¥å‘é€è¯„è®º\\n                    commentInput.addEventListener('keypress', function(e) {\\n                        if (e.key === 'Enter') {\\n                            sendComment();\\n                        }\\n                    });\\n                }\\n                \\n                // ä¸ªäººä¸»é¡µæŒ‰é’®ç‚¹å‡»äº‹ä»¶\\n                const profileBtn = document.querySelector('.profile-btn');\\n                if (profileBtn) {\\n                    profileBtn.addEventListener('click', function() {\\n                        // è·å–ä½œè€…åç§°\\n                        const authorName = document.querySelector('.author-name')?.textContent || 'æœªçŸ¥ä½œè€…';\\n                        viewUserProfile(authorName);\\n                    });\\n                }\\n                \\n                // ç»‘å®šè¿”å›æŒ‰é’®\\n                const backButton = document.querySelector('.back-button');\\n                if (backButton) {\\n                    backButton.addEventListener('click', function() {\\n                        // è¿”å›ä¸Šä¸€é¡µ\\n                        window.history.back();\\n                    });\\n                }\\n                \\n                // ç»‘å®šè¯„è®ºæ’åºåˆ‡æ¢åŠŸèƒ½\\n                document.querySelectorAll('.sort-option').forEach(option => {\\n                    option.addEventListener('click', function() {\\n                        // ç§»é™¤æ‰€æœ‰é€‰é¡¹çš„activeç±»\\n                        document.querySelectorAll('.sort-option').forEach(opt => {\\n                            opt.classList.remove('active');\\n                        });\\n                        // ä¸ºå½“å‰ç‚¹å‡»çš„é€‰é¡¹æ·»åŠ activeç±»\\n                        this.classList.add('active');\\n                        \\n                        // æ˜¾ç¤ºå¯¹åº”çš„è¯„è®ºå†…å®¹\\n                        const type = this.getAttribute('data-type');\\n                        if (type === 'new') {\\n                            document.querySelector('.new-comments').style.display = 'block';\\n                            document.querySelector('.hot-co