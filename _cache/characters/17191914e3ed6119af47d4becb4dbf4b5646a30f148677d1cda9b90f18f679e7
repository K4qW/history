<button class=\\\"add-tag-btn\\\">ç¡®å®š</button>\\n                    </div>\\n                </div>\\n                <div class=\\\"word-count\\\">0/10 ä¸ªæ ‡ç­¾</div>\\n            </div>\\n\\n            <div class=\\\"preview-section\\\">\\n                <div class=\\\"preview-title\\\">é¢„è§ˆ</div>\\n                <div class=\\\"preview-card\\\">\\n                    <div class=\\\"preview-header\\\">\\n                        <div class=\\\"preview-avatar\\\">\\n                            <img src=\\\"data:image/svg+xml;base64,\\\" alt=\\\"ç”¨æˆ·å¤´åƒ\\\">\\n                        </div>\\n                        <div class=\\\"preview-user-info\\\">\\n                            <div class=\\\"preview-user\\\">ç”¨æˆ·å</div>\\n                            <div class=\\\"preview-time\\\">åˆšåˆšå‘å¸ƒ</div>\\n                        </div>\\n                    </div>\\n                    <div class=\\\"preview-content\\\">\\n                        <div class=\\\"preview-post-title\\\">è¿™é‡Œæ˜¯æ ‡é¢˜é¢„è§ˆ</div>\\n                        <div class=\\\"preview-text\\\">è¿™é‡Œæ˜¯å†…å®¹é¢„è§ˆ...</div>\\n                        <div class=\\\"preview-image-container\\\">\\n                            <!-- å›¾ç‰‡ä¼šåœ¨è¿™é‡ŒåŠ¨æ€ç”Ÿæˆ -->\\n                        </div>\\n                        <div class=\\\"preview-tags\\\">\\n                            <!-- æ ‡ç­¾å°†åœ¨è¿™é‡ŒåŠ¨æ€ç”Ÿæˆ -->\\n                        </div>\\n                    </div>\\n                </div>\\n            </div>\\n\\n            <div class=\\\"nav-buttons\\\">\\n                <button class=\\\"nav-btn prev-btn\\\">ä¸Šä¸€æ­¥</button>\\n                <button class=\\\"nav-btn next-btn\\\">å‘é€</button>\\n            </div>\\n        </div>\\n    </div>\\n\\n    <script>\\n        // è¿™é‡Œæ˜¯åŸºæœ¬çš„åˆ‡æ¢æ­¥éª¤çš„è„šæœ¬ï¼Œå®é™…åŠŸèƒ½æš‚æ—¶ä¸å®ç°\\n        document.addEventListener('DOMContentLoaded', function() {\\n            const steps = document.querySelectorAll('.edit-step');\\n            const stepDots = document.querySelectorAll('.step-dot');\\n            const stepLabels = document.querySelectorAll('.step-label');\\n            const prevButtons = document.querySelectorAll('.prev-btn');\\n            const nextButtons = document.querySelectorAll('.next-btn');\\n            \\n            let currentStep = 0;\\n            \\n            // åˆå§‹åŒ–æ—¶æ‰‹åŠ¨æ›´æ–°æ­¥éª¤æ˜¾ç¤º\\n            updateStepDisplay();\\n            \\n            // ç«‹å³è°ƒç”¨updatePreviewä»¥åŠ è½½ç”¨æˆ·èµ„æ–™\\n            updatePreview();\\n            \\n            // æ›´æ–°æ­¥éª¤æ˜¾ç¤º\\n            function updateStepDisplay() {\\n                steps.forEach((step, index) => {\\n                    if (index === currentStep) {\\n                        step.classList.add('active');\\n                    } else {\\n                        step.classList.remove('active');\\n                    }\\n                });\\n                \\n                stepDots.forEach((dot, index) => {\\n                    if (index < currentStep) {\\n                        dot.classList.add('completed');\\n                        dot.classList.remove('active');\\n                    } else if (index === currentStep) {\\n                        dot.classList.add('active');\\n                        dot.classList.remove('completed');\\n                    } else {\\n                        dot.classList.remove('active', 'completed');\\n                    }\\n                });\\n                \\n                stepLabels.forEach((label, index) => {\\n                    if (index === currentStep) {\\n                        label.classList.add('active');\\n                    } else {\\n                        label.classList.remove('active');\\n                    }\\n                });\\n                \\n                // æ›´æ–°è¿æ¥çº¿çš„æ ·å¼\\n                const stepLines = document.querySelectorAll('.step-line');\\n                stepLines.forEach((line, index) => {\\n                    // æ³¨æ„ï¼šç¬¬ä¸€ä¸ªæ­¥éª¤æ²¡æœ‰è¿æ¥çº¿\\n                    if (index < currentStep) {\\n                        line.classList.add('active');\\n                    } else {\\n                        line.classList.remove('active');\\n                    }\\n                });\\n                \\n                // å¦‚æœåˆ°äº†é¢„è§ˆæ­¥éª¤ï¼Œæ›´æ–°é¢„è§ˆå†…å®¹\\n                if (currentStep === 2) {\\n                    updatePreview();\\n                }\\n            }\\n            \\n            // æ›´æ–°é¢„è§ˆå†…å®¹\\n            function updatePreview() {\\n                const titleInput = document.querySelector('.input-title');\\n                const contentInput = document.querySelector('.input-content');\\n                const previewTitle = document.querySelector('.preview-post-title');\\n                const previewText = document.querySelector('.preview-text');\\n                const previewImageContainer = document.querySelector('.preview-image-container');\\n                const previewUser = document.querySelector('.preview-user');\\n                const previewAvatar = document.querySelector('.preview-avatar img');\\n                \\n                // æ›´æ–°æ ‡é¢˜å’Œå†…å®¹\\n                previewTitle.textContent = titleInput.value || \\\"è¿™é‡Œæ˜¯æ ‡é¢˜é¢„è§ˆ\\\";\\n                previewText.textContent = contentInput.value || \\\"è¿™é‡Œæ˜¯å†…å®¹é¢„è§ˆ...\\\";\\n                \\n                // å°è¯•è·å–ç”¨æˆ·èµ„æ–™æ•°æ®\\n                if (typeof getVariables === 'function') {\\n                    // å¼‚æ­¥è·å–ç”¨æˆ·å¤´åƒå’Œç”¨æˆ·å\\n                    getVariables().then(variables => {\\n                        // æ£€æŸ¥æ˜¯å¦æœ‰ä¿å­˜çš„ä¸ªäººèµ„æ–™\\n                        if (variables && variables.æˆ‘çš„ä¸ªäººèµ„æ–™) {\\n                            const profile = variables.æˆ‘çš„ä¸ªäººèµ„æ–™;\\n                            \\n                            // æ›´æ–°ç”¨æˆ·å\\n                            if (profile.ç”¨æˆ·å) {\\n                                previewUser.textContent = profile.ç”¨æˆ·å;\\n                            } else {\\n                                previewUser.textContent = '{{User}}';\\n                            }\\n                            \\n                            // æ›´æ–°å¤´åƒ\\n                            if (profile.å¤´åƒ) {\\n                                previewAvatar.src = processAvatarUrl(profile.å¤´åƒ);\\n                            }\\n                        } else {\\n                            // ä½¿ç”¨é»˜è®¤å€¼\\n                            previewUser.textContent = '{{User}}';\\n                        }\\n                    }).catch(error => {\\n                        console.error('è·å–ç”¨æˆ·èµ„æ–™æ—¶å‡ºé”™:', error);\\n                        // ä½¿ç”¨é»˜è®¤å€¼\\n                        previewUser.textContent = '{{User}}';\\n                    });\\n                } else {\\n                    // éSTç¯å¢ƒï¼Œä½¿ç”¨é»˜è®¤å€¼\\n                    previewUser.textContent = '{{User}}';\\n                }\\n                \\n                // æ›´æ–°å›¾ç‰‡\\n                previewImageContainer.innerHTML = '';\\n                const imageItems = document.querySelectorAll('.image-item');\\n                imageItems.forEach((item, index) => {\\n                    const caption = item.querySelector('textarea').value || \\\"æš‚æ— å›¾ç‰‡ç®€ä»‹\\\";\\n                    const previewImage = document.createElement('div');\\n                    previewImage.className = 'preview-image';\\n                    previewImage.onclick = function() {\\n                        this.classList.toggle('active-mask');\\n                    };\\n                    \\n                    // æ·»åŠ é®ç½©å±‚\\n                    const imageMask = document.createElement('div');\\n                    imageMask.className = 'preview-image-mask';\\n                    imageMask.i