           break;\\n            default:\\n                // å¦‚æœæœ‰è‡ªå®šä¹‰ç®€ä»‹ï¼Œåˆ™ä½¿ç”¨è‡ªå®šä¹‰ç®€ä»‹ï¼Œå¦åˆ™ä½¿ç”¨é»˜è®¤æ¨¡æ¿\\n                const customIdentityDesc = document.getElementById('customIdentityDesc').value.trim();\\n                if (customIdentityDesc) {\\n                    behaviorRules = customIdentityDesc.replace(/è§’è‰²/g, characterName);\\n                } else {\\n                    behaviorRules = `1. ${characterName}èº«ä»½ä¸ºç©å®¶çš„${identity}\\\\n2. ã€${identity}è¡Œä¸ºå®ˆåˆ™é¢„ç•™ä½ç½®ã€‘`;\\n                }\\n        }\\n        \\n        const content = `<character_rules>\\\\nå½“å‰${characterName}çš„åœ¨çº¿ä¸ŠèŠå¤©å’Œçº¿ä¸‹è¡¨ç°å¿…é¡»éµå¾ªä»¥ä¸‹è§„åˆ™ï¼Œå’ŒåŸæœ‰äººè®¾å†²çªæ—¶ä»¥æ­¤ä¸ºå‡†ï¼š\\\\n${behaviorRules}\\\\n</character_rules>`;\\n        \\n        // åˆ›å»ºå‘½ä»¤åˆ—è¡¨\\n        const commands = [];\\n        \\n        // è®¾ç½®æ¡ç›®å†…å®¹\\n        commands.push(`/setentryfield file=worldbook uid=${entryId} field=content ${content}`);\\n        commands.push(`/setentryfield file=worldbook uid=${entryId} field=constant 0`);\\n        commands.push(`/setentryfield file=worldbook uid=${entryId} field=position 4`);\\n        commands.push(`/setentryfield file=worldbook uid=${entryId} field=role 0`);\\n        \\n        // æ‰§è¡Œå‘½ä»¤\\n        if (typeof triggerSlash === 'function') {\\n            triggerSlash(commands.join('|'));\\n        } else {\\n            console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n        }\\n        \\n        closeDialog('identityDialog');\\n    }\\n    \\n    function selectIdentity(identity) {\\n        // ä¿ç•™æ—§å‡½æ•°ä»¥å…¼å®¹å¯èƒ½çš„å…¶ä»–è°ƒç”¨\\n        selectIdentityButton(identity);\\n    }\\n    \\n    function clearIdentity() {\\n        const characterName = document.getElementById('selectedCharacterName').value;\\n        \\n        // æ ¹æ®è§’è‰²åç§°ç¡®å®šå¯¹åº”çš„æ¡ç›®ID\\n        let entryId = 0;\\n        switch(characterName) {\\n            case 'æ—è‹¥æ›¦':