         const postsCount = document.getElementById('posts-count');\\n            const favoritesCount = document.getElementById('favorites-count');\\n            const followingCount = document.getElementById('following-count');\\n            \\n            // è®¡ç®—æˆ‘å‘å¸ƒçš„å¸–å­æ•°é‡\\n            if (variables && variables['æˆ‘å‘å¸ƒçš„å¸–å­']) {\\n                // å¦‚æœæ˜¯å•ä¸ªå¸–å­å¯¹è±¡ï¼Œè®¡æ•°ä¸º1\\n                postsCount.textContent = '1';\\n            } else {\\n                postsCount.textContent = '0';\\n            }\\n            \\n            // è®¡ç®—æ”¶è—çš„å¸–å­æ•°é‡\\n            if (variables && variables['æ”¶è—çš„å¸–å­']) {\\n                favoritesCount.textContent = Object.keys(variables['æ”¶è—çš„å¸–å­']).length;\\n            } else {\\n                favoritesCount.textContent = '0';\\n            }\\n            \\n            // è®¡ç®—å…³æ³¨çš„åšä¸»æ•°é‡\\n            if (variables && variables['å…³æ³¨çš„åšä¸»']) {\\n                followingCount.textContent = Object.keys(variables['å…³æ³¨çš„åšä¸»']).length;\\n            } else {\\n                followingCount.textContent = '0';\\n            }\\n        }\\n        \\n        // æ¸²æŸ“ä¸€ä¸ªå¸–å­å¡ç‰‡å¹¶æ·»åŠ åˆ°å®¹å™¨\\n        function renderPostCard(container, post, isFavorite) {\\n            const postCard = document.createElement('div');\\n            postCard.className = 'post-card';\\n            \\n            // è·å–å¸–å­æ ‡é¢˜\\n            const title = isFavorite ? post.æ ‡é¢˜ : post.title;\\n            \\n            // è·å–å¸–å­å†…å®¹\\n            const content = isFavorite ? post.å¸–å­å†…å®¹ : post.content;\\n\\n            // è·å–å›¾ç‰‡ç®€ä»‹\\n            let imageSummary = '';\\n            if (isFavorite && post.å›¾ç‰‡ç®€ä»‹) {\\n                imageSummary = post.å›¾ç‰‡ç®€ä»‹;\\n            } else if (!isFavorite && post.imageCaptions && post.imageCaptions.length > 0) {\\n                imageSummary = post.imageCaptions[0];\\n            }\\n            \\n            // è·å–å¸–å­æ ‡ç­¾\\n            let tags = [];\\n            if (isFavorite) {\\n                // æ”¶è—çš„å¸–å­å¯èƒ½å°†æ ‡ç­¾å­˜ä¸ºå­—ç¬¦ä¸²ï¼Œéœ€è¦æ‹†åˆ†\\n                if (typeof post.å¸–å­æ ‡ç­¾ === 'string') {\\n                    tags = post.å¸–å­æ ‡ç­¾.split('|').map(tag => tag.trim()).filter(tag => tag);\\n                } else if (Array.isArray(post.å¸–å­æ ‡ç­¾)) {\\n                    tags = post.å¸–å­æ ‡ç­¾;\\n                }\\n            } else {\\n                // æˆ‘å‘å¸ƒçš„å¸–å­æ ‡ç­¾åº”è¯¥æ˜¯æ•°ç»„\\n                tags = post.tags || [];\\n            }\\n            \\n            // è·å–æ—¶é—´\\n            const timeStr = isFavorite ? (post.æ”¶è—æ—¶é—´ || '') : (post.createTime || '');\\n            const formattedTime = formatDate(timeStr);\\n            \\n            // æ„å»ºå¸–å­å¡ç‰‡HTML\\n            postCard.innerHTML = `\\n                <div class=\\\"image-container\\\">\\n                    <div class=\\\"image-mask\\\">\\n                        <span>ç‚¹å‡»æŸ¥çœ‹å†…å®¹</span>\\n                    </div>\\n                    <div class=\\\"image-summary\\\">\\n                        <h4>å›¾ç‰‡ç®€ä»‹</h4>\\n                        <p>${imageSummary || 'æš‚æ— å›¾ç‰‡æè¿°'}</p>\\n                    </div>\\n                </div>\\n                <div class=\\\"post-content\\\">\\n                    <div class=\\\"post-title\\\">${title || 'æ— æ ‡é¢˜'}</div>\\n                    <div class=\\\"post-text\\\">${content || 'æ— å†…å®¹'}</div>\\n                    <div class=\\\"post-meta\\\">\\n                        <div class=\\\"post-time\\\">${formattedTime}</div>\\n                    </div>\\n                    ${tags.length > 0 ? `\\n                        <div class=\\\"post-tags\\\">\\n                            ${tags.map(tag => `<div class=\\\"post-tag\\\">${tag}</div>`).join('')}\\n                        </div>\\n                    ` : ''}\\n                    <div class=\\\"