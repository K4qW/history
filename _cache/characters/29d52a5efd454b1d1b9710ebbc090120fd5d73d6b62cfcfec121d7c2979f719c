Sparkles();\\n\\n    // å®šæœŸæ£€æŸ¥æ˜¯å¦åº”è¯¥æš‚åœ\\n    setInterval(checkTimerPause, 1000);\\n});\\n\\n// é¡µé¢å…³é—­æˆ–åˆ·æ–°æ—¶çš„æ¸…ç†\\nwindow.addEventListener('beforeunload', () => {\\n    if (instanceId === localStorage.getItem('activeTimerInstance')) {\\n        localStorage.removeItem('activeTimerInstance');\\n    }\\n});\\n\\n// æ·»åŠ æ–°çš„JavaScriptå‡½æ•°\\nfunction showVoiceModal() {\\n    document.getElementById('voiceModal').style.display = 'block';\\n}\\n\\nfunction cancelVoice() {\\n    document.getElementById('voiceModal').style.display = 'none';\\n}\\n\\nfunction confirmVoice() {\\n    const voiceInput = document.getElementById('voiceInput').value;\\n    if (voiceInput.trim() !== '') {\\n        if (typeof triggerSlash === 'function') {\\n            triggerSlash(`/send å›å¤è¯­éŸ³é€šè¯å’Œ$1ï¼š${voiceInput}|/trigger`);\\n        } else {\\n            console.error('triggerSlash å‡½æ•°ä¸å¯ç”¨');\\n        }\\n    }\\n    document.getElementById('voiceModal').style.display = 'none';\\n    document.getElementById('voiceInput').value = '';\\n}\\n\\n// ä¿®æ”¹ç¬¬ä¸€ä¸ªæ§åˆ¶æŒ‰é’®çš„onclickäº‹ä»¶\\ndocument.querySelector('.call-controls button:first-child').onclick = showVoiceModal;\\n</script>\\n</body>\\n</html>\\n```\",\"trimStrings\":[],\"placement\":[2],\"disabled\":false,\"markdownOnly\":true,\"promptOnly\":false,\"runOnEdit\":false,\"substituteRegex\":false,\"minDepth\":null,\"maxDepth\":5},{\"id\":\"fb00459a-76b2-4168-aebb-912a1163bdec\",\"scriptName\":\"æµè§ˆå™¨\",\"findRegex\":\"\\\\[æµè§ˆå™¨ç•Œé¢\\\\]\\\\s*æ—¶é—´ï¼š(.*?)\\\\s*ç½‘é¡µåï¼š(.*?)\\\\s*ç½‘å€ï¼š(.*?)\\\\s*ç½‘é¡µå†…å®¹ï¼š([\\\\s\\\\S]*)\",\"replaceString\":\"```html\\n<!DOCTYPE html>\\n<html>\\n<head>\\n<meta charset=\\\"UTF-8\\\">\\n<meta name=\\\"viewport\\\" content=\\\"width=device-width, initial-scale=1.0\\\">\\n<title>Minimal Phone Layout</title>\\n<style>\\n/* åŸæœ‰æ ·å¼ä¿æŒä¸å˜ */\\n* {\\n    margin: 0;\\n    padding: 0;\\n    box-sizing: border-box;\\n}\\n\\nbody {\\n    di