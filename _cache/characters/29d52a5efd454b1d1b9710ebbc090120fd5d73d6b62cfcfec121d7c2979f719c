\\n            'https://files.catbox.moe/tggs7k.wav',\\n            'https://files.catbox.moe/8h2thb.wav',\\n            'https://files.catbox.moe/2hcfna.wav'\\n        ].map(url => {\\n            const audio = new Audio(url);\\n            audio.load();\\n            return audio;\\n        });\\n\\n        function playRandomSound() {\\n            const randomIndex = Math.floor(Math.random() * soundEffects.length);\\n            soundEffects[randomIndex].currentTime = 0;\\n            soundEffects[randomIndex].play();\\n        }\\n\\n        function stopDragging() {\\n            isDragging = false;\\n        }\\n\\n        function createCherryBlossom() {\\n            const blossom = document.createElement('div');\\n            blossom.classList.add('cherry-blossom');\\n            blossom.style.left = Math.random() * 100 + 'vw';\\n            const size = Math.random() * 15 + 5;\\n            blossom.style.width = size + 'px';\\n            blossom.style.height = size + 'px';\\n            blossom.style.animationDuration = (Math.random() * 5 + 4) + 's';\\n            blossom.style.opacity = Math.random() * 0.5 + 0.3;\\n            const pink = Math.random() * 15 + 187;\\n            blossom.style.background = `rgba(255, ${pink}, ${pink + 10}, 0.8)`;\\n            blossom.style.borderRadius = '50%';\\n            blossom.style.filter = 'blur(1.5px)';\\n            document.body.appendChild(blossom);\\n            setTimeout(() => {\\n                blossom.remove();\\n            }, parseFloat(blossom.style.animationDuration) * 1000);\\n        }\\n\\n        setInterval(createCherryBlossom, 800);\\n    </script>\\n</body>\\n</html>\\n```\",\"trimStrings\":[],\"placement\":[2],\"disabled\":false,\"markdownOnly\":true,\"promptOnly\":false,\"runOnEdit\":false,\"substituteRegex\":false,\"minDepth\":null,\"maxDepth\":5},{\"id\":\"981d3904-5263-4bbd-9b30-a3b80302ab60\",\"scriptName\":\"è®ºå›å¸–å­\",\"findRegex\":\"/<è®ºå›å¸–å­\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(